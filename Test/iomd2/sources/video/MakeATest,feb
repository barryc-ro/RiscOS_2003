rmensure ddeutils 0.00 rmload system:modules.ddeutils
echo Assembling...
objasm -FROM <obey$dir>.s.videomain -TO <obey$dir>.o.%0 -predefine "testname SETS \"%0\"" -NoCache -List <obey$dir>.list.%0 -width 200 -length 0 
echo Linking...
link <obey$dir>.o.%0 -bin -c++ -o <obey$dir>.bin.%0
echo Output thing...
disem <obey$Dir>.bin.%0 <obey$dir>.rom.%0
echo Done.



