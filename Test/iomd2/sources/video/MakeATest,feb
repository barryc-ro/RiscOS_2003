rmensure ddeutils 0.00 rmload system:modules.ddeutils
echo Assembling...
objasm -FROM <obey$dir>.s.videomain -TO <obey$dir>.o.t_%0 -predefine "testname SETS \"t_%0\"" -NoCache -List <obey$dir>.list.t_%0 -width 200 -length 0 
echo Linking...
link <obey$dir>.o.t_%0 -bin -c++ -o <obey$dir>.bin.t_%0
echo Output thing...
disem <obey$Dir>.bin.t_%0 <obey$dir>.rom.%0
copy <obey$dir>.rom.%0 <romimage$dir>.video.%0 ~CVF
echo Done.
