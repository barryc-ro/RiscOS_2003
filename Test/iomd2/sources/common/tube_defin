; Address definitions for using the fasttube

tube            *       &60000000
fasttube        *       &61000000
dump_r0         *       &62000000
dump_r1         *       &62000004
dump_r2         *       &62000008
dump_r3         *       &6200000C
dump_r4         *       &62000010
dump_r5         *       &62000014
dump_r6         *       &62000018
dump_r7         *       &6200001C
dump_r8         *       &62000020
dump_r9         *       &62000024
dump_r10        *       &62000028
dump_r11        *       &6200002C
dump_r12        *       &62000030
dump_r13        *       &62000034
dump_r14        *       &62000038
dump_r15        *       &6200003C


; Some character definitions for use with the fasttube

LF      	*       10                      ; Linefeed character
CR      	*       13                      ; Carrage return character
CtrlD   	*       4                       ; Stop simulation by printing this character

		END
