; IOMD2 address map

main_rom_base	*	&00000000
ext_rom_base	*	&01000000
vram_base	*	&02000000
io_base		*	&03000000
reserved_1	*	&04000000
expansion_base	*	&08000000
sdram0_base	*	&10000000
