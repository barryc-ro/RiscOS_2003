sdram2_base	*	&18000000
sdram3_base	*	&1C000000
sdram4_base	*	&20000000
sdram5_base	*	&24000000
sdram6_base	*	&28000000
sdram7_base	*	&2C000000
ssram_base	*	&30000000
reserved_2	*	&34000000
reserved_3	*	&38000000
cacheflush_base	*	&3C000000
local_bus_base	*	&40000000

