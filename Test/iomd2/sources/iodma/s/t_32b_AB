
	GBLL	iodma_Test_Routing
iodma_Test_Routing	SETL	{FALSE}

; this file generates a test case for a single channel transfer, on buffer A,
; with word width transfers, copying from the bottom of a page at sdram0_base,
; to the top of the page

	MACRO
	iodma_DoTheTests

	iodma_Copy_Test_Data		sdram0_base, 16
	iodma_Copy_Test_Data		sdram0_base + &1000, 16
t1	iodma_DoTest_DoubleBuffer_BufA	sdram0_base, sdram0_base + &1000-16, 16, sdram0_base+&1000, sdram0_base + &2000-16, 16, 1

	MEND

	END
