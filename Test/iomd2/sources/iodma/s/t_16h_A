; with word width transfers, copying from the bottom of a page at sdram0_base,
; to the top of the page

	MACRO
	iodma_DoTheTests

	iodma_Copy_Test_Data		sdram0_base, 32
t1	iodma_DoTest_SingleBuffer_BufA	sdram0_base, sdram0_base + &1000-32, 32, 2

	MEND

	END
