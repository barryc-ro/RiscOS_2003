sb              RN      9

HighestOSEntry  *       OS_MapInIO

                ^       0,sb
OSheader        #       4
OSentries       #       4*(HighestOSEntry+1)
TimerPeriods    #       4*2
TIMER_BASE_Address      #       4
INT_BASE_Address        #       4
GPIO_BASE_Address       #       4
KEYBOARD_BASE_Address   #       4


;Colour LCD (Video) workspace
;
CLCD_Address        #      4
CLCD_bpp            #     4

HAL_WsSize      *       :INDEX:@

                END
