# Project:   CDManager


# Toolflags:
CCflags = -c -depend !Depend -IC: -throwback  -DTINYSTUBS  -zM 
C++flags = -c -depend !Depend -IC: -throwback
Linkflags = -aif -c++ -o $@
ObjAsmflags = -throwback -NoCache -depend !Depend
CMHGflags = 
LibFileflags = -c -o $@
Squeezeflags = -o $@
TLinkflags = -rmf -c++ -o $@ 


# Final targets:
@.CDManager:   @.o.main @.o.resources @.o.swicode @.o.veneer @.o.modhdr \
        @.^.common.commonlibm 
        tlink $(tlinkflags) @.o.main @.o.resources @.o.swicode @.o.veneer \
        @.o.modhdr @.^.common.commonlibm 


# User-editable dependencies:
@.c.resources:  @.Messages
        mkresource CDManager Messages


# Static dependencies:
@.o.main:   @.c.main
        cc $(ccflags) -o @.o.main @.c.main 
@.o.resources:   @.c.resources
        cc $(ccflags) -o @.o.resources @.c.resources 
@.o.swicode:   @.c.swicode
        cc $(ccflags) -o @.o.swicode @.c.swicode 
@.o.veneer:   @.s.veneer
        objasm $(objasmflags) -from @.s.veneer -to @.o.veneer
@.o.modhdr:   @.cmhg.modhdr
        cmhg @.cmhg.modhdr -o @.o.modhdr


# Dynamic dependencies:
o.resources:	c.resources
o.resources:	h.resources
o.veneer: s.veneer
o.main:	c.main
o.main:	h.main
o.main:	C:h.swis
o.main:	C:h.kernel
o.main:	C:h.kernel
o.main:	C:h.defines
o.main:	C:h.kernel
o.main:	C:h.debug
o.main:	h.msgs
o.main:	C:h.kernel
o.main:	C:h.dbmalloc
o.main:	h.driverops
o.main:	h.resources
o.main:	h.swicode
o.swicode:	c.swicode
o.swicode:	h.main
o.swicode:	C:h.swis
o.swicode:	C:h.kernel
o.swicode:	C:h.kernel
o.swicode:	C:h.defines
o.swicode:	C:h.kernel
o.swicode:	C:h.debug
o.swicode:	h.msgs
o.swicode:	C:h.kernel
o.swicode:	C:h.dbmalloc
o.swicode:	h.driverops
o.swicode:	h.swicode
o.swicode:	h.asm
