#define RedBook_SectorSize                  2352
#define RedBook_FramesPerSecond               75

#define ISO9660_LogicalSectorSize           2048
#define Log2_ISO9660_LogicalSectorSize        11

#define ISO9660_SessionOffsetForPVD           16
