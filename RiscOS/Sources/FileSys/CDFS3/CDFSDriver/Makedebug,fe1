# Project:   CDFSDvrDBG


# Toolflags:
CCflags = -c -depend !Depend -IC: -throwback  -Ddebugging=1  -fah 
C++flags = -c -depend !Depend -IC: -throwback
Linkflags = -rmf -c++ -o $@ 
ObjAsmflags = -throwback -NoCache -depend !Depend
CMHGflags = -depend !Depend -throwback -IADFS::Tim.$.CDSystem.components.CDFSDriver.cmhg.Hdr 
LibFileflags = -c -o $@
Squeezeflags = -o $@
TLinkflags = -aif -c++ -o $@


# Final targets:
@.CDFSDvrDBG:   @.o.Hdr @.o.messages C:o.stubs C:o.msgs C:o.debug C:o.dbmalloc \
        @.o.cdfsswi @.o.swi @.o.globals @.o.cdfsdriver @.o.command @.o.aux @.o.drive 
        link $(linkflags) @.o.Hdr @.o.messages C:o.stubs C:o.msgs C:o.debug \
        C:o.dbmalloc @.o.cdfsswi @.o.swi @.o.globals @.o.cdfsdriver @.o.command @.o.aux @.o.drive 


# User-editable dependencies:
@.o.messages: @.Res.UK.Messages 
        resgen messages_file @.o.messages @.Res.UK.Messages Resources.CDFSDriver.Messages 
@.h.module: @.cmhg.Hdr 
        cmhg -d @.h.module @.cmhg.Hdr -o @.o.Hdr 

# Static dependencies:
@.o.Hdr:   @.cmhg.Hdr
        cmhg @.cmhg.Hdr -o @.o.Hdr
@.o.cdfsswi:   @.s.cdfsswi
        objasm $(objasmflags) -from @.s.cdfsswi -to @.o.cdfsswi
@.o.swi:   @.c.swi
        cc $(ccflags) -o @.o.swi @.c.swi 
@.o.globals:   @.c.globals
        cc $(ccflags) -o @.o.globals @.c.globals 
@.o.cdfsdriver:   @.c.cdfsdriver
        cc $(ccflags) -o @.o.cdfsdriver @.c.cdfsdriver 
@.o.command:   @.c.command
        cc $(ccflags) -o @.o.command @.c.command 
@.o.aux:   @.c.aux
        cc $(ccflags) -o @.o.aux @.c.aux 
@.o.drive:   @.c.drive
        cc $(ccflags) -o @.o.drive @.c.drive 


# Dynamic dependencies:
o.cdfsswi: s.cdfsswi
o.globals:	c.globals
o.globals:	h.globals
