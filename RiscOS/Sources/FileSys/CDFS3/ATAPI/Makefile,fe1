# Project:   ATAPI


# Toolflags:
CCflags = -c -depend !Depend -IC: -throwback -ffah 
C++flags = -c -depend !Depend -IC: -throwback
Linkflags = -aif -c++ -o $@
ObjAsmflags = -throwback -NoCache -depend !Depend
CMHGflags = 
LibFileflags = -c -o $@
Squeezeflags = -o $@
TLinkflags = -rmf -c++ -o $@ 


# Final targets:
@.ATAPI:   @.o.main @.o.modhdr @.o.resources @.o.register @.o.atapi \
        @.^.common.commonlibm @.o.drives 
        TLink $(TLinkflags) @.o.main @.o.modhdr @.o.resources @.o.register \
        @.o.atapi @.^.common.commonlibm @.o.drives 


# User-editable dependencies:
@.c.resources:  @.Messages
        mkresource ATAPI Messages


# Static dependencies:
@.o.main:   @.c.main
        cc $(ccflags) -o @.o.main @.c.main 
@.o.modhdr:   @.cmhg.modhdr
        cmhg @.cmhg.modhdr -o @.o.modhdr
@.o.resources:   @.c.resources
        cc $(ccflags) -o @.o.resources @.c.resources 
@.o.register:   @.c.register
        cc $(ccflags) -o @.o.register @.c.register 
@.o.atapi:   @.c.atapi
        cc $(ccflags) -o @.o.atapi @.c.atapi 
@.o.drives:   @.c.drives
        cc $(ccflags) -o @.o.drives @.c.drives 


# Dynamic dependencies:
o.resources:	c.resources
o.resources:	h.resources
o.main:	c.main
o.main:	h.main
o.main:	C:h.swis
o.main:	C:h.kernel
o.main:	C:h.kernel
o.main:	C:h.defines
o.main:	C:h.kernel
o.main:	C:h.debug
o.main:	C:h.msgs
o.main:	C:h.kernel
o.main:	C:h.dbmalloc
o.main:	h.driverops
o.main:	h.atapiops
o.main:	h.drives
o.main:	h.resources
o.main:	h.register
o.register:	c.register
o.register:	h.main
o.register:	C:h.swis
o.register:	C:h.kernel
o.register:	C:h.kernel
o.register:	C:h.defines
o.register:	C:h.kernel
o.register:	C:h.debug
o.register:	C:h.msgs
o.register:	C:h.kernel
o.register:	C:h.dbmalloc
o.register:	h.driverops
o.register:	h.atapiops
o.register:	h.drives
o.register:	h.register
o.register:	h.atapi
o.register:	h.drives
o.register:	h.cdmgr
o.atapi:	c.atapi
o.atapi:	h.main
o.atapi:	C:h.swis
o.atapi:	C:h.kernel
o.atapi:	C:h.kernel
o.atapi:	C:h.defines
o.atapi:	C:h.kernel
o.atapi:	C:h.debug
o.atapi:	C:h.msgs
o.atapi:	C:h.kernel
o.atapi:	C:h.dbmalloc
o.atapi:	h.driverops
o.atapi:	h.atapiops
o.atapi:	h.drives
o.atapi:	h.register
o.atapi:	h.atapi
o.atapi:	h.drives
o.drives:	c.drives
o.drives:	h.main
o.drives:	C:h.swis
o.drives:	C:h.kernel
o.drives:	C:h.kernel
o.drives:	C:h.defines
o.drives:	C:h.kernel
o.drives:	C:h.debug
o.drives:	C:h.msgs
o.drives:	C:h.kernel
o.drives:	C:h.dbmalloc
