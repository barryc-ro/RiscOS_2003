/*->c.csfserr */


#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <ctype.h>
#include <stdarg.h>


#include "kernel.h"
#include "swis.h"


#include "h.xerrs"

#include "h.etc"



#include "h.csfserr"







_kernel_oserror * geterror(int errno)
{
 _kernel_oserror * err;
 int               len;

 err=&mb_malloc_failed;
 while(err->errnum<0x18400)
 {
  if(err->errnum==errno) break;
  len=strlen(err->errmess);
  len=((len+1+3) & (~3))+sizeof(int);
  err=(_kernel_oserror *)(((char*)err)+len);
 }

 if(err->errnum<0x18400) return(err);
 else                    return(&mb_nfserr_BadParameters);
}



