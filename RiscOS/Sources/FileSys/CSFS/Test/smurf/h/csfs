#ifndef __csfs_h
#define __csfs_h

#define CSFS_Services                   0x04e980
#define CSFS_Register                   0x04e981
#define CSFS_DeRegister                 0x04e982
#define CSFS_Mount                      0x04e983
#define CSFS_Dismount                   0x04e984
#define CSFS_Update                     0x04e985
#define CSFS_Release                    0x04e986
#define CSFS_Refresh                    0x04e987

#endif
