;->cmhg.csfs
;

initialisation-code: CSFS_initialise

service-call-handler: CSFS_service

title-string: CSFS

help-string: CSFS 0.05

date-string: 24 Apr 1997

swi-chunk-base-number: 0x4E980

swi-handler-code: CSFS_swi

swi-decoding-table: CSFS,Services,Register,DeRegister,Mount,Dismount,Update

generic-veneers: callback_entry/callback_handler



command-keyword-table: CSFS_command

CSFS(min-args:0, max-args:0,
  help-text:      "*CSFS selects CSFS as the current filing system\r"
                  "Syntax:\t*CSFS\r",
  invalid-syntax: "Syntax:\t*CSFS\r\n" ),

CSFSServices(min-args:0, max-args:0,
  help-text:      "*CSFSServices lists the current transport services that have been registered with CSFS.\r"
                  "Syntax:\t*CSFSServices\r",
  invalid-syntax: "Syntax:\t*CCSFSServices\r\n"),

Mount(min-args:4, max-args:13, fs-command:,
  help-text:      "*Mount mount server based file system.\r"
                  "Syntax:\t*Mount -service <service name> -name <mount name>\r\n"
                  "[-writecache] [-host <host name>]\r\n"
                  "[-path <mount path>]\r\n"
                  "[-user <user name>] [-passwd <password>]\r\n"
  invalid-syntax: "Syntax:\t*Mount -service <service name> -name <mount name>\r\n"
                  "[-writecache] [-host <host name>]\r\n"
                  "[-path <mount path>]\r\n"
                  "[-user <user name>] [-passwd <password>]\r\n"),

Release(min-args:1, max-args:1, fs-command:,
  help-text:      "*Release unlock files.\r"
                  "Syntax:\t*release <path>\r"
  invalid-syntax: "Syntax:\t*release <path>\r\n"),

Refresh(min-args:1, max-args:1, fs-command:,
  help-text:      "*Refresh refresh CSFS directory listing.\r"
                  "Syntax:\t*refresh <path>\r"
  invalid-syntax: "Syntax:\trefresh <path>\r\n"),

Update(min-args:1, max-args:2, fs-command:,
  help-text:      "*Update upload changed files to server.\r"
                  "Syntax:\t*update [-upload] <path>\r"
  invalid-syntax: "Syntax:\t*update [-upload] <path>\r\n"),

Dismount(min-args:1, max-args:1, fs-command:,
  help-text:      "*Dismount a mounted directory.\r"
                  "Syntax:\t*dismount <mount name>\r"
  invalid-syntax: "Syntax:\t*dismount <mount name>\r\n"),

Back(min-args:0, max-args:0, fs-command:,
  help-text:      "*Back swaps current and previous directories.\r"
                  "Syntax:\t*Back\r",
  invalid-syntax: "Syntax:\t*Back\r\n"),

URD(min-args:0, max-args:1, fs-command:,
  help-text:      "*URD sets the user root directory.\r"
                  "Syntax:\t*URD [<directory>]\r",
  invalid-syntax: "Syntax:\t*URD [<directory>]\r\n"),

NoURD(min-args:0, max-args:0, fs-command:,
  help-text:      "*NoURD unsets the URD.\r"
                  "Syntax:\t*NoURD\r",
  invalid-syntax: "Syntax:\t*NoURD\r\n"),

NoDir(min-args:0, max-args:0, fs-command:,
  help-text:      "*NoDir unsets the current directory.\r"
                  "Syntax:\t*NoDir\r",
  invalid-syntax: "Syntax:\t*NoDir\r\n"),

NoLib(min-args:0, max-args:0, fs-command:,
  help-text:      "*NoLib unsets the library.\r"
                  "Syntax:\t*NoLib\r",
  invalid-syntax: "Syntax:\t*NoLib\r\n")

