;->cmhg.csfs
;

initialisation-code: CSFS_initialise

service-call-handler: CSFS_service

title-string: CSFS

help-string: CSFS 0.06

date-string: 24 Apr 1997

swi-chunk-base-number: 0x4E980

swi-handler-code: CSFS_swi

swi-decoding-table: CSFS,Services,Register,DeRegister,Mount,Dismount,Update,Release,Refresh

generic-veneers: callback_entry/callback_handler

command-keyword-table: CSFS_command

CSFS(add-syntax:, min-args:0, max-args:0,
  help-text: "*CSFS selects CSFS as the current filing system\n"
  invalid-syntax: "Syntax:\t*CSFS\n" ),

CSFSServices(add-syntax:, min-args:0, max-args:0,
  help-text: "*CSFSServices lists the current transport services that have been registered with CSFS.\n"
  invalid-syntax: "Syntax:\t*CCSFSServices\n"),

Mount(add-syntax:, min-args:4, max-args:13, fs-command:,
  help-text: "*Mount mount server based file system.\n"
  invalid-syntax:"Syntax:\t*Mount -service <service name> -name <mount name> [-writecache] [-host <host name>] [-path <mount path>] [-user <user name>] [-passwd <password>]\n"),

Update(add-syntax:, min-args:0, max-args:2, fs-command:,
  help-text: "*Update upload changed files to server.\n"
  invalid-syntax: "Syntax:\t*update [-upload] <path>\n"),

Release(add-syntax:, min-args:0, max-args:1, fs-command:,
  help-text: "*Release unlock files.\n"
  invalid-syntax: "Syntax:\t*release <path>\n"),

Refresh(add-syntax:, min-args:0, max-args:1, fs-command:,
  help-text: "*Refresh refresh CSFS directory listing.\n"
  invalid-syntax: "Syntax:\trefresh <path>\n"),

Dismount(add-syntax:, min-args:1, max-args:1, fs-command:,
  help-text: "*Dismount a mounted directory.\n"
  invalid-syntax: "Syntax:\t*dismount <mount name>\n")

