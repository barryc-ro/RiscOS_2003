; Version number file > version

        GBLS    version
version SETS    "3.27"          ; RiscOS 3.10 versions are >= 2.67
                                ; RiscOS Black versions are >= 2.92

        GBLS    date
date    SETS    "16 Apr 1996"

        GBLA    max_fix
max_fix SETA    12

        GBLL    test_version

test_version SETL {FALSE}
;test_version SETL {TRUE}

        GBLL    A1
        GBLL    FDC1772
        GBLL    Top16Write
        GBLL    IOMD

        END
