/*->h.constants */


#define XTYPE 0xF89

#define DATA  0xFFD


#define OURREAD   0x1
#define OURWRITE  0x2
#define LOCK      0x8
#define THEMREAD  0x10
#define THEMWRITE 0x20

#define DEFATT (OURREAD|OURWRITE|THEMREAD|THEMWRITE)


#define CARRY    (1<<29)
#define OVERFLOW (1<<28)
#define LRINDEX  11



