#define geterror(a) (&(a))


byte *outbuf;            /* buffer for rle look-back */
byte *outptr;


int outpos;              /* absolute position in outfile */
int outcnt;              /* current position in outbuf */



buffer * zsrc;
buffer * zdest;



/* read a byte; return 8 if byte available, 0 if not */
int ReadByte(UWORD * x)
{
 if(zsrc->p<zsrc->end)
 {
  *x=*zsrc->p++;
  return(8);
 }
 else
 {
  fillsrcbuffer(zsrc);
  if(zsrc->p<zsrc->end)
  {
   *x=*zsrc->p++;
   return(8);
  }
  else return(0);
 }
}








