rmload <Obey$Dir>.rm.CronTest
crontest reset
crontest command -wait 1:0
crontest command -wait 0:1
crontest command -delay 1:0
crontest command -delay 0:1
crontest swifn -delay 1:0
crontest swifn -delay 0:1
crontest swifn -delay5 1:0
crontest swifn -delay5 0:1
crontest swicmd -delay 1:0
crontest swicmd -delay 0:1
crontest swicmd -delay5 1:0
crontest swicmd -delay5 0:1
crontest status
