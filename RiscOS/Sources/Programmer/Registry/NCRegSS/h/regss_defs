// header file for REGSS_DEFS.H

/* Begin-Prologue */
#ifndef REGSS_DEFS_H
#define REGSS_DEFS_H

/* globals for all support modules */
#define REGISTRY_SUPPORT_INITSWI   0x0
#define REGISTRY_SUPPORT_READSWI   0x1
#define REGISTRY_SUPPORT_WRITESWI  0x2

#define NCRegistry_SupportInit     0x4d387
#define NCRegistry_SupportExit     0x4d388
/* identifiers for Registry Support modules */

#define REGISTRY_SUPPORT_REGISTRY 0x0
#define REGISTRY_SUPPORT_SCRIPT   0x1

/* Registry support notification service call */
#define REGISTRY_SVC_SUPPORT 0xbc
/* and associate values */
#define REGISTRY_SUPPORT_SVC_STARTING 0x0
#define REGISTRY_SUPPORT_SVC_DYING    0x1

/* identifiers for registry special tags */

#define REGSS_ISP_DOMAIN         0x1
#define REGSS_NFS_MOUNT          0x2
#define REGSS_URL_INIT           0x3
#define REGSS_SEND_EXPECT        0x4
#define REGSS_NCF_INFO		 0x5
#define REGSS_NCD_INFO		 0x6

/* identifiers for this support module */

#define REGSS_SWIBASE            0x4d3c0
/* Begin-Epilogue */
#endif
/* End_Epilogue */

