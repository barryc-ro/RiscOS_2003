#include "@.HP_SDK_LIB.global_types.h"




/*****************************************************************************************/
/***                                                                                   ***/
/***   General Variable Definitions                                                    ***/
/***                                                                                   ***/
/*****************************************************************************************/

#define  ON                             TRUE
#define  OFF                            FALSE

#define  FORCE_PARALLEL                 TRUE      // For debugging only - code always uses parallel

/*****************************************************************************************/
/***                                                                                   ***/
/***   Debugging Definitions                                                           ***/
/***                                                                                   ***/
/*****************************************************************************************/

#define  Debug                          FALSE
#define  Verbose                        TRUE



/*****************************************************************************************/
/***                                                                                   ***/
/***   PDumperHPS  Definitions                                                         ***/
/***                                                                                   ***/
/*****************************************************************************************/


#define  PDUMPER_NO                             66
#define  PDRIVER_VERSION_REQ                    100
#define  PDUMPER_SUPPORTED_CALLS__MASK          0x01FF
#define  PDUMPER_SUPPORTTED_STRIPS__MASK        0x03FF
#define  PDRIVER_DP_NUM                         7

#define  PDUMPER_REGISTER                       0x80000000
#define  PDUMPER_UNREGISTER                     0x80000001

#define  PDUMPER_REASON__SET_DRIVER             0x00
#define  PDUMPER_REASON__OUTPUT_DUMP            0x01
#define  PDUMPER_REASON__COLOUR_SET             0x02
#define  PDUMPER_REASON__START_PAGE             0x03
#define  PDUMPER_REASON__END_PAGE               0x04
#define  PDUMPER_REASON__START_JOB              0x05
#define  PDUMPER_REASON__ABORT_JOB              0x06
#define  PDUMPER_REASON__MISC_OP                0x07


#define  DEVICE_ID_SIZE                         512
#define  DPI                                    300
#define  PAGE_WIDTH                             (8 * DPI)
#define  PAGE_HEIGHT                            ((11 * DPI) + (DPI /2))

#define  __Parallel_OP                          0x00042EC1



