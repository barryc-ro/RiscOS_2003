# PDumperLe

CCOPTS		= -c -depend !Depend -IC: -throwback -zM -fah
DBCCOPTS	= $(CCOPTS) -dDEBUG
RMLINKOPTS	= -rmf -o $@
LINKOPTS	= -aif -o $@
OAOPTS		= -throwback -depend !Depend
CMHGOPTS	=

all: @.PDumperLe @.dPDumperLe

.suffixes: .o .dbo .c .cmhg .s

.c.o:;		cc $(CCOPTS) -o $@ $<
.c.dbo:;	cc $(DBCCOPTS) -o $@ $<
.s.o:;		objasm $(OAOPTS) -from $< -to $@
.cmhg.o:;	cmhg $(CMHGOPTS) $< -o $@

OBJS	=	@.o.fcifkelv @.o.fcintfc @.o.fckelvin @.o.fcprint @.o.fcrotate @.o.module @.o.riscos \
			@.o.rm @.o.errors @.o.veneer @.o.pdumper @.o.pdsupport @.o.lexsupport @.o.rawsprite \
			c:o.stubs

DBOBJS	=	@.dbo.fcifkelv @.dbo.fcintfc @.dbo.fckelvin @.dbo.fcprint @.dbo.fcrotate @.dbo.module \
			@.dbo.riscos @.o.rm @.o.errors @.o.veneer @.dbo.pdumper @.dbo.pdsupport @.dbo.lexsupport \
			@.dbo.rawsprite c:o.stubs 

@.PDumperLe: $(OBJS)
	link $(RMLINKOPTS) $(OBJS)

@.dPDumperLe: $(DBOBJS)
	link $(RMLINKOPTS) $(DBOBJS)


