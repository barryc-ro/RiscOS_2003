/***************************************************************************
 *                Printer Specific Structure Defines
 ***************************************************************************/

#define MAX_CARRIER_RAMP_SIZE 800
#define MAX_PAPER_RAMP_SIZE 800
#define MAX_RAMP_TABLE_CNT 16

/* Structure to keep printer specific hardware data */

struct tHwData
{
	unsigned char bTestPrintDir;
	unsigned char bInstallCartridge;
	unsigned char bMonoFirePulse;
	unsigned char bColorFirePulse;
	unsigned char bHomeStepCnt;
	unsigned short wPaperLoadStepCntA;
	unsigned short wPaperLoadStepCntB;
	unsigned short wPaperEjectStepCnt;
	unsigned short wManualFeedStepCnt;
	unsigned char bPaperRampTableSpeed;							/*Currently always 0 */
	unsigned char bPaperRampTableType[MAX_RAMP_TABLE_CNT];
	unsigned char bPaperRampTableData[MAX_PAPER_RAMP_SIZE];
	unsigned short wPaperRampSRAMOffset;
	unsigned short wPaperRampTableLength;
	unsigned char bAccelStepCnt3;
	unsigned char bAlphaStepCnt3;
	unsigned char bDecelStepCnt3;
	unsigned short wDecelOffsetAddr3;
	unsigned char bAccelStepCnt16;
	unsigned char bAlphaStepCnt16;
	unsigned char bDecelStepCnt16;
	unsigned short wDecelOffsetAddr16;
	unsigned char bAccelStepCnt25;
	unsigned char bAlphaStepCnt25;
	unsigned char bDecelStepCnt25;
	unsigned short wDecelOffsetAddr25;
	unsigned char bCarrRampTableData[MAX_CARRIER_RAMP_SIZE];
	unsigned short wCarrRampSRAMOffset;
	unsigned short wCarrRampTableLength;
	unsigned short wPrintSRAMOffset;
	unsigned char bPrtCarrDirection;
	unsigned short wPrtStartPosition;
	unsigned short wPrtCarrStopPosition;
	unsigned short wPrtCarrStartPosition;
	unsigned short bPrtCarrSpeed;
	unsigned char bPosCarrDirection;
	unsigned short wPosCarrStopPosition;
	unsigned short wPosCarrStartPosition;
	unsigned short bPosCarrSpeed;
	unsigned short wCurrCarrPos;
	unsigned short wColorHeadOffset;
	unsigned short wMonoHeadOffset;
	int nMonoLeftBidiAlign;
	int nMonoRightBidiAlign;
	int nColorLeftBidiAlign;
	int nColorRightBidiAlign;

	long alHwDotCount[MAX_MAINT_PARMS][MAX_NUM_INKS];			/* for printhead maintenance */
};

#define HW_BUFF_SIZE (long)sizeof(struct tHwData)
#define PRT_CMD_HDR_SIZE      8

struct tPrintCmd
{
	unsigned char abSwathBuff[HI_RES_PRINT_BUFF_SIZE];
};

#define PRT_CMD_SIZE sizeof(struct tPrintCmd)

