 *       Printer Maintenance Defines
 *************************************************************/

#define UNCAP_SPIT_CNT           128
#define UNCAP_WIPE_CNT           1

#define BETWEEN_PAGE_SPIT_CNT    128
#define BETWEEN_PAGE_WIPE_CNT    1

#define INSTALL_SPIT_CNT         448
#define INSTALL_WIPE_CNT         2

// #define DEBUG_MAINT

