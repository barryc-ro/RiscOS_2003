#else
#define OUTPUT(device, lp, size)   SendCommand((void FAR *)\
                                   device->lpLXGenericPtr, lp, size)
#endif

#elif defined(FC_WIN_32)
#define _fmemcpy      memcpy
#define _fstrcpy      strcpy
#define _fstrcmp      strcmp
#define _fstrcat      strcat
