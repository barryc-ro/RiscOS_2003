#ifndef LOBYTE
#define LOBYTE(w)     ((ByteType)(w))
#define HIBYTE(w)     ((ByteType)(((WordType)(w) >> 8) & 0xFF))
#define LOWORD(l)     ((WordType)(DwordType)(l))
#define HIWORD(l)     ((WordType)((((DwordType)(l)) >> 16) & 0xFFFF))
#endif
#ifndef FALSE
#define FALSE         0
#define TRUE          1
#endif


