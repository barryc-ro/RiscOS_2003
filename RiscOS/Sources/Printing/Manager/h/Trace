#ifdef Trace
   #define Ftracef(s) PROCftracef(s)
   #define Ftrace_Send(e,p,t) PROCftrace_send(e,p,t)
   #define Ftrace_Receive(e,p) PROCftrace_receive(e,p)
   #define Ftrace_Queu(q) PROCftrace_queu(q)
#else
   #define Ftracef(s)
   #define Ftrace_Send(e,p,t)
   #define Ftrace_Receive(e,p)
   #define Ftrace_Queu(q)
#endif
