extern void *private_word_pointer ;

extern char module_name[] ;

#ifndef TRUE
# define TRUE 1
# define FALSE 0
#endif

/*
 * Upcall & Service numbers 
 */

#define UpCallV                         0x1d

#define UpCall_MediaNotPresent          1
#define UpCall_MediaNotKnown            2
#define UpCall_ModifyingFile            3
#define UpCall_Freeway                  12

#define Service_FreewayStarting         0x95
#define Service_FreewayTerminating      0x96

/*
 * Our nice SWI chunk number
 */

#define SWI_CHUNK_NUMBER 0x47c00
