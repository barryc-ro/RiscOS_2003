typedef struct 
{
    int sblimit;
    al_table table;
} alloc_table;

#define SBINFO(b,g,q) { (b) ? (b) - 1 : 0, g, (g) ? ((q) == 3 ? 2 : (q)) : (q) == 2 ? 0 : (q) - 3 }
