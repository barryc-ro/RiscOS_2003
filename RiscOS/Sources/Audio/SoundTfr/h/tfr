typedef void VoidFunc(void);

VoidFunc tfr_play_start;
VoidFunc tfr_play_proc;
VoidFunc tfr_play_end;

VoidFunc tfr_capture_start;
VoidFunc tfr_capture_proc;
VoidFunc tfr_capture_end;

VoidFunc tfr_memory_play_start;
VoidFunc tfr_memory_play_proc;
VoidFunc tfr_memory_play_end;

VoidFunc tfr_memory_capture_start;
VoidFunc tfr_memory_capture_proc;
VoidFunc tfr_memory_capture_end;

