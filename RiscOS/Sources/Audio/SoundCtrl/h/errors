
#define Error_Base         0x813100

#define Error_UKFunction   (Error_Base + 1)
#define Error_UKChannel    (Error_Base + 2)
#define Error_UKRes        (Error_Base + 3)
#define Error_UKEnc        (Error_Base + 4)
#define Error_BadKey       (Error_Base + 5)
#define Error_SoundBlaster (Error_Base + 6)
#define Error_HWNotFound   (Error_Base + 7)
