# Project:   SoundCtrlDBG


# Toolflags:
CCflags = -c -depend !Depend -IC: -throwback  -Ddebugging=1  -zM -fah 
C++flags = -c -depend !Depend -IC: -throwback
Linkflags = -rmf -c++ -o $@ 
ObjAsmflags = -throwback -NoCache -depend !Depend
CMHGflags = 
LibFileflags = -c -o $@
Squeezeflags = -o $@
TLinkflags = -rmf -c++ -o $@ 


# Final targets:
@.SndCtrlDBG:   @.o.Hdr @.o.main @.o.swi @.o.globals @.o.ss \
        @.^.common.commonlibm @.o.delay @.o.dmatfr @.o.messages 
        tlink $(tlinkflags) @.o.Hdr @.o.main @.o.swi @.o.globals @.o.ss \
        @.^.common.commonlibm @.o.delay @.o.dmatfr @.o.messages 


# User-editable dependencies:
@.o.messages: @.Res.UK.Messages 
        resgen messages_file @.o.messages @.Res.UK.Messages Resources.SoundCtrl.Messages
@.h.module: @.cmhg.Hdr 
        cmhg -d @.h.module @.cmhg.Hdr -o @.o.Hdr 


# Static dependencies:
@.o.Hdr:   @.cmhg.Hdr
        cmhg @.cmhg.Hdr -o @.o.Hdr
@.o.main:   @.c.main
        cc $(ccflags) -o @.o.main @.c.main 
@.o.swi:   @.c.swi
        cc $(ccflags) -o @.o.swi @.c.swi 
@.o.globals:   @.c.globals
        cc $(ccflags) -o @.o.globals @.c.globals 
@.o.ss:   @.c.ss
        cc $(ccflags) -o @.o.ss @.c.ss 
@.o.delay:   @.s.delay
        objasm $(objasmflags) -from @.s.delay -to @.o.delay
@.o.dmatfr:   @.s.dmatfr
        objasm $(objasmflags) -from @.s.dmatfr -to @.o.dmatfr


# Dynamic dependencies:
o.globals:	c.globals
o.globals:	h.globals
o.globals:	h.SoundCtrl
o.delay: s.delay
o.delay: hdr:ListOpts
o.delay: hdr:Macros
o.delay: hdr:System
o.delay: Hdr:SWIs
o.delay: Hdr:CPU.Generic26
o.delay: Hdr:IO.GenericIO
o.delay: Hdr:RISCOS
o.swi:	c.swi
o.swi:	C:h.kernel
o.swi:	C:h.swis
o.swi:	C:h.defines
o.swi:	C:h.kernel
o.swi:	C:h.swiextra
o.swi:	C:h.swis
o.swi:	C:h.debug
o.swi:	C:h.msgs
o.swi:	C:h.kernel
o.swi:	h.SoundCtrl
o.swi:	h.SS
o.swi:	h.module
o.swi:	h.errors
o.swi:	h.globals
o.swi:	h.SoundCtrl
o.swi:	h.DMA
o.swi:	h.ss
o.swi:	h.dmatfr
o.swi:	h.irq
o.ss:	c.ss
o.ss:	h.ss
o.main:	c.main
o.main:	C:h.kernel
o.main:	C:h.swis
o.main:	C:h.defines
o.main:	C:h.kernel
o.main:	C:h.dbmalloc
o.main:	C:h.debug
o.main:	C:h.msgs
o.main:	C:h.kernel
o.main:	C:h.swiextra
o.main:	C:h.swis
o.main:	h.module
o.main:	h.globals
o.main:	h.SoundCtrl
o.main:	h.errors
o.main:	h.ss
o.main:	h.irq
o.main:	h.delay
o.dmatfr: s.dmatfr
o.dmatfr: hdr:ListOpts
o.dmatfr: hdr:Macros
o.dmatfr: hdr:System
o.dmatfr: Hdr:SWIs
o.dmatfr: Hdr:CPU.Generic26
o.dmatfr: Hdr:IO.GenericIO
o.dmatfr: Hdr:RISCOS
o.dmatfr: hdr.CVars
o.dmatfr: hdr.globals
o.dmatfr: hdr.SS
o.dmatfr: hdr.DMATfr
