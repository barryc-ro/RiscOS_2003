 * The line numbers to seek to for various info in the armovie header.

 */


#ifndef __arline_h
# define __arline_h

enum
{
    arline_Ident,
    arline_Name,
    arline_Date,
    arline_Author,
    arline_VideoType,
    arline_XSize,
    arline_YSize,
    arline_PixelDepth,
    arline_FrameRate,
    arline_SoundType,
    arline_SoundRate,
    arline_SoundChannels,
    arline_SoundPrecision,
    arline_FramesPerChunk,
    arline_NChunks,
    arline_EvenChunkSize,
    arline_OddChunkSize,
    arline_CatalogueOffset,
    arline_SpriteOffset,
    arline_SpriteSize,
    arline_KeyFrameOffset
};

#endif

/* eof arline.h */
