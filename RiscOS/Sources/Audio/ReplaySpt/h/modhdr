#ifndef __cmhg_defs_h
#define __cmhg_defs_h

#define  Replay_00             0x048b00
#define  Replay_RegisterSprites  0x048b00
#define  Replay_DeRegisterSprites  0x048b01
#define  Replay_TimebarPaintAddress  0x048b02
#define  Replay_Version        0x048b03
#define  Replay_ReadFileHeader  0x048b04
#define  Replay_ReadVideoCodecInfo  0x048b05
#define  Replay_ReadSoundCodecInfo  0x048b06
#define  Replay_SoundCode      0x048b07
#define  Replay_ReadAccessInfo  0x048b08

#define  CMD_ReplayCheckVer    0
#define  CMD_ReplaySupportInfo  1

#endif
