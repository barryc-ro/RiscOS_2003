#ifndef __audswitch_h
  #define __audswitch_h

  #pragma force_top_level
  #pragma include_only_once


  #ifndef AudioSwitch_AudioMix
    #define AudioSwitch_00                  0x055440
    #define AudioSwitch_AudioMix            0x055440
    #define AudioSwitch_IACAudio            0x055441
    #define AudioSwitch_Audio_Enabled   1
    #define AudioSwitch_Audio_Disabled  0
    #define AudioSwitch_Audio_Get      -1

    #define AudioSwitch_IAC_Disabled    0
    #define AudioSwitch_IAC_Enabled     1
  #endif


#endif
