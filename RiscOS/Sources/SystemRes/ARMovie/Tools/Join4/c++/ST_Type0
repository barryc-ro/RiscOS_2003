#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include "ST_Type0.h"

////////////////////////////////////////////////////////////////////////////

SoundTrack_Type0::SoundTrack_Type0() : SoundTrack() {

    noChunks = 0;
    maxChunksize = 0;

} // end SoundTrack_Type0::SoundTrack_Type0


////////////////////////////////////////////////////////////////////////////

int SoundTrack_Type0::readChunkSize(int chunk) {

    chunk = chunk; // shut up compiler warning

    return 0;

} // end SoundTrack_Type0::readChunkSize


////////////////////////////////////////////////////////////////////////////

void SoundTrack_Type0::load(char *dest, int chunkNo) {

    // shut up compiler warning
    *dest = *dest;
    chunkNo = chunkNo;

} // end SoundTrack_Type0::load


