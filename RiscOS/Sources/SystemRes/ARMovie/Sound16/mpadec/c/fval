int main (int argc, char **argv)
{
    int r, c;
    int a2ncyc, a8ncyc;
    a2ncyc = 0;  a8ncyc = 0;
    for (r = 0; r < 32; ++r)
    {
	for (c = 0; c < 16; ++c)
	{
	    int v = window_G16[r*16+c];
	    int p;
	    printf ("%7d", v);
	    if (-(1<<8) <= v && v < (1<<8))
		a8ncyc += 3;
	    else if (-(1 << 16) <= v && v < (1 << 16))
		a8ncyc += 4;
	    else if (-(1 << 24) <= v && v < (1 << 24))
		a8ncyc += 5;
	    else
		a8ncyc += 6;
	    if (v < 0)
		v = -v;
	    p = 1; 
	    while (p <= 15 && v >= (1 << p*2))
		++p;
	    a2ncyc += p;
	}
	printf ("\n");
    }
    printf ("wbits = %d, total mul cycles/chan = %d (a2m) %d (a8m)\n", WBITS, a2ncyc, a8ncyc);
}
