    Revolting hack to allow C to access variables declared in ARM code.  Only
    needed when compiling to Module code, but does no harm to leave in (except
    offending ones sensibilities).
*/

#ifndef _HACK_DEC_
#define _HACK_DEC_

void cpels(void);
void pels(void);
void lines(void);
void long_vectors(void);
void mv_outside_frame(void);
void stream(void);
void stream_len(void);

#include "hack_on.h"

#endif
