#include "error.h"
#include <stdlib.h>
#include <stdio.h>
#include "setenv.h"
#include "Logger.h"
int t_dotrace=0;
void t_bugmsg(char *t)
{
  if(t_dotrace==1) {
    t_LogOut(t);
    fflush(stderr);
  }
}
void error(char *c)
{
  if(c!=NULL)
  {
   fprintf(stderr,c);
   setenv("ARMovie$CompressionStatus",c);
   exit(1);
  }
}
char t_BugBuff[256];
