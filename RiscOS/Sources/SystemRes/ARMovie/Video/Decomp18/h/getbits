void ainitbits _ANSI_ARGS_((unsigned char *source));
unsigned int ashowbits _ANSI_ARGS_((int n));
unsigned int agetbits1 _ANSI_ARGS_((void));
void aflushbits _ANSI_ARGS_((int n));
unsigned int agetbits _ANSI_ARGS_((int n));

void binitbits _ANSI_ARGS_((unsigned char *source));
unsigned int bshowbits _ANSI_ARGS_((int n));
unsigned int bgetbits1 _ANSI_ARGS_((void));
void bflushbits _ANSI_ARGS_((int n));
unsigned int bgetbits _ANSI_ARGS_((int n));
unsigned int pint _ANSI_ARGS_((unsigned int i,int num));

#define BITASM
#undef TESTMAKE


#ifdef TESTMAKE
#ifdef BITASM

#define getbits(n) pint(agetbits(n),n)
#define getbits1(n) agetbits1()
#define flushbits(n) aflushbits(n)
#define showbits(n) ashowbits(n)
#define initbits(n) ainitbits(n)

#else

#define getbits(n) pint(bgetbits(n),n)
#define getbits1(n) bgetbits1()
#define flushbits(n) bflushbits(n)
#define showbits(n) bshowbits(n)
#define initbits(n) binitbits(n)


#endif
#else

#ifdef BITASM

#define getbits(n) agetbits(n)
#define getbits1(n) agetbits1()
#define flushbits(n) aflushbits(n)
#define showbits(n) ashowbits(n)
#define initbits(n) ainitbits(n)

#else

#define getbits(n) bgetbits(n)
