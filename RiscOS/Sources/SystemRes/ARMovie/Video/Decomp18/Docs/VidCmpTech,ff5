      0 36 atan dup cos 8 mul 404 exch sub
      exch sin 8 mul 256 exch sub lineto
   stroke
grestore
gsave
   newpath
      404 256 8 3 36 0 tgifarrowtip
   closepath fill
grestore

% TEXT
0 setgray
/Courier findfont [17 0 0 -17 0 0] makefont setfont
   gsave
      412 262 moveto (Used for) show
      412 279 moveto (interpolation) show
   grestore

% TEXT
0 setgray
/Courier findfont [17 0 0 -17 0 0] makefont setfont
   gsave
      428 358 moveto () show
   grestore

grestore
tgifsavedpage restore
end
