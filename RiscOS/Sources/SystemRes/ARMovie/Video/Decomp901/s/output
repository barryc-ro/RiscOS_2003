XOS_File	EQU	&20008

	AREA	|Test$$Code|,CODE,READONLY

	GET	hdr.plook

	EXPORT store
store
	; r0 is dest
	; r1 is pixval
	; r2 is lookup
	plook 	r1, r3, r2
	str	r3, [r0]
	movs	pc, lr

; ----------------------------------------------------------------
	EXPORT loop_l
loop_l
	; r0 = ysrc
	; r1 = OutputCursor
	; r2 = U|V
	; r3 = table
	stmfd	sp!,{r4-r6,lr}
	;
	ldr	r6, [r0]
	and	r4, r6, #&ff
	orr	r5, r2, r4, lsr#3
	plook	r5, r4, r3
	str	r4, [r1],#4
	;
	and	r4, r6, #&ff00
	orr	r5, r2, r4, lsr#3+8
	plook	r5, r4, r3
	str	r4, [r1],#4
	;
	and	r4, r6, #&ff0000
	orr	r5, r2, r4, lsr#3+16
	plook	r5, r4, r3
	str	r4, [r1],#4
	;
	and	r4, r6, #&ff000000
	orr	r5, r2, r4, lsr#3+24
	plook	r5, r4, r3
	str	r4, [r1],#4
	;
	ldmfd	sp!,{r4-r6,pc}^

	EXPORT	PatchTable
PatchTable
	ptable

; --------------------------------------------------------------------------------
	EXPORT savemem
savemem
	stmfd	sp!,{r4-r5,lr}
	mov	r4, r0
	mov	r5, r1
	mov	r1, r2
	mov	r0, #10
	mov	r2,   #&ff0
	orr	r2, r2, #&d
	swi	XOS_File
	ldmfd	sp!,{r4-r5,pc}^



	END
