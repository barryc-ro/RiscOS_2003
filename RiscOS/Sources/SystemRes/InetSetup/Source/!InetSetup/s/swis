	AREA	|SWI$$Code|, READONLY, CODE

Toolbox_ObjectMiscOp		*	&44EC6
ActionButton_SetClickShow	*	132

	EXPORT actionbutton_set_click_show
actionbutton_set_click_show
	MOV	R12,R13
	STMFD	R13!,{R4,R5,LR}
	LDMIA	R12,{R5}
	MOV	R4,R3
	MOV	R3,R2
	MOV	R2,#ActionButton_SetClickShow
	SWI	Toolbox_ObjectMiscOp
	LDMFD	R13!,{R4,R5,PC}^

	END
