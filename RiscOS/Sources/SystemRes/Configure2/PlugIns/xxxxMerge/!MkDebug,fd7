Dir <Obey$Dir>
Remove Resources.BootMerge.!RunImage
Remove Resources.FontMerge.!RunImage
Remove Resources.SysMerge.!RunImage
CDir <Boot$Dir>.RO380Hook.Res.Configure.!BootSetup
CDir <Boot$Dir>.RO380Hook.Res.Configure.!FontSetup
amu_machine install LDFLAGS=-Debug COMPONENT=BootMerge INSTDIR=<Boot$Dir>.RO380Hook.Res.Configure.!BootSetup
amu_machine install LDFLAGS=-Debug COMPONENT=FontMerge INSTDIR=<Boot$Dir>.RO380Hook.Res.Configure.!FontSetup
amu_machine install LDFLAGS=-Debug COMPONENT=SysMerge INSTDIR=<Boot$Dir>.RO380Hook.Res.Configure
