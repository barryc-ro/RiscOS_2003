#include "ns_int.h"

		/* NS & MNS configuration structure for:
		 *
		 *    PRI,
		 *    ETSI-like variants
		 *
		 *    NAI 0: Terminal
		 */

CONST struct ns_config ns_config_data_TE1 = {

	{0},							/* confirm */

#	if (NS_SPF_CONFIG == ON)

		{
			NS_CONFIG_TYPE_STANDARD,	/* config_type: Type of NS configuration structure */
		},

#	endif
	1,								/* number of network access */

		/* Network access table */

	{

			/* NAI 0: PRI, Terminal */

		{
			0,						/* nai */
			PRI,					/* type */
			FG_TE_NT,				/* function grouping */
			23,						/* nb of B channels */
			0,						/* nb of outgoing calls */
			0,						/* nb of incoming calls */
			23,						/* nb of mixed calls */
			POINT_TO_POINT,			/* configuration */
			ALL_SERVICES,			/* subscription type */
			{0},					/* SPID (Services Profile ID) */
			{0},					/* NS timers values (0 for implicit values) */
			{0},					/* DL */
			ON,						/* T309 option */
			0,						/* country */
			0,						/* network/operator */
			1,						/* DL ces */
			0						/* interface Nai */
		},

	},

	0,								/* country where NS is running */
	E10,							/* was AT9 network operator */
	OFF,							/* check transparent IE */

};


/*--------------------------------------------------------------------------*/

		/* NS & MNS configuration structure for:
		 *
		 *    PRI,
		 *    ETSI-like variants
		 *
		 *    NAI 0: Network
		 */
CONST struct ns_config ns_config_data_TE2 = {

	{0},							/* confirm */

#	if (NS_SPF_CONFIG == ON)

		{
			NS_CONFIG_TYPE_STANDARD,	/* config_type: Type of NS configuration structure */
		},

#	endif

	1,								/* number of network access */

		/* Network access table */

	{

			/* NAI 1: PRI, Network */

		{
			1,						/* nai */
			PRI,					/* type */
			FG_TE_NT,				/* function grouping */
			23,						/* nb of B channels */
			0,						/* nb of outgoing calls */
			0,						/* nb of incoming calls */
			23,						/* nb of mixed calls */
			POINT_TO_POINT,			/* configuration */
			ALL_SERVICES,			/* subscription type */
			{0},					/* SPID (Services Profile ID) */
			{0},					/* NS timers values (0 for implicit values) */
			{0},					/* DL timers values */
			OFF,					/* Timer T309 option */
			0,						/* country */
			0,						/* network/operator */
			1,						/* DL ces */
			1						/* interface Nai */
		},

	},

	0,								/* country where NS is running */
	E10,							/* was AT9 network operator */
	OFF,							/* check transparent IE */

};

/*EOF*/
