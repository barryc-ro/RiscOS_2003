#ifndef IT_PAR
#define IT_PAR

	/* INTERRUPT CONFIGURATION */

#define	IT_STACK_D_MSG_LOW_MARK			25		/* message stack low mark */
#define	IT_STACK_D_MSG_MIDDLE_MARK		30		/* message stack middle mark */
#define	IT_STACK_D_MSG_HIGH_MARK			35		/* message stack high mark */

#define	IT_STACK_D_BUFFER_LOW_MARK		25		/* buffer stack low mark */
#define	IT_STACK_D_BUFFER_MIDDLE_MARK		30		/* buffer stack middle mark */
#define	IT_STACK_D_BUFFER_HIGH_MARK		35		/* buffer stack high mark */

#define IT_STACK_D_MSG_CONG_LOW_MARK     	5		/* message cpu congestion low mark */
#define IT_STACK_D_MSG_CONG_HIGH_MARK    	9		/* message cpu congestion high mark */

#define IT_STACK_D_BUFFER_CONG_LOW_MARK	5		/* buffer cpu congestion low mark */
#define IT_STACK_D_BUFFER_CONG_HIGH_MARK 	9		/* buffer cpu congestion high mark */

#if 0
					/* Sizing constants - default values */

#define	IT_STACK_D_MSG_LOW_MARK				8			/* message stack low mark */
#define	IT_STACK_D_MSG_MIDDLE_MARK			10			/* message stack middle mark */
#define	IT_STACK_D_MSG_HIGH_MARK			12			/* message stack high mark */
#define IT_STACK_D_MSG_CONG_LOW_MARK    	2			/* message cpu congestion low mark */
#define IT_STACK_D_MSG_CONG_HIGH_MARK   	6			/* message cpu congestion high mark */


#define	IT_STACK_D_BUFFER_LOW_MARK			6			/* buffer stack low mark */
#define	IT_STACK_D_BUFFER_MIDDLE_MARK		8			/* buffer stack middle mark */
#define	IT_STACK_D_BUFFER_HIGH_MARK			10			/* buffer stack high mark */

#define IT_STACK_D_BUFFER_CONG_LOW_MARK		2			/* buffer cpu congestion low mark */
#define IT_STACK_D_BUFFER_CONG_HIGH_MARK	5			/* buffer cpu congestion high mark */

#endif

/*-------------------------------------------------------------------------*/

#endif

