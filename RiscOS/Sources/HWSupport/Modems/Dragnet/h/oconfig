					0,						/* nai */
					BRI,					/* access type (BRI or PRI) */
					FG_TE_NET,				/* function grouping */
					2,						/* nb of B channels */
					3,						/* nb of SAP */
					{0, 127, 1}				/* TEI list */
				},

					/* SAP descriptions */

				{

					{
						DL_SAPI_SIG,		/* sapi */
						ENT_DL_D,			/* entity */
						128,				/* modulo */
						1,					/* k */
						1*SCD,				/* timer T200 (or T201) value (0 for implicit value) */
						10*SCD,				/* timer T203 (or T202) value (0 for implicit value) */
						0,					/* timer for TEI assignement procedure */
						3,					/* retry nb (N201) */
						3,					/* nb of connection enpoints */
						260					/* I or UI frames max size of information field (n201) */
					},

					{
						DL_SAPI_MDL,		/* sapi */
						ENT_DL_D,			/* entity */
						128,				/* modulo */
						1,					/* k */
						1*SCD,				/* timer T200 (or T201) value (0 for implicit value) */
						10*SCD,				/* timer T203 (or T202) value (0 for implicit value) */
						2*SCD,				/* timer for TEI assignement procedure */
						3,					/* retry nb (N201) */
						3,					/* nb of connection enpoints */
						260,				/* I or UI frames max size of information field (n201) */
					},


					{
						DL_SAPI_X25,		/* sapi */
						ENT_DL_D,			/* entity */
						128,				/* modulo */
						3,					/* k */
						1*SCD,				/* timer T200 (or T201) value (0 for implicit value) */
						10*SCD,				/* timer T203 (or T202) value (0 for implicit value) */
						0,					/* timer for TEI assignement procedure */
						3,					/* retry nb (N201) */
						3,					/* nb of connection enpoints */
						260,				/* I or UI frames max size of information field (n201) */

					},

				}

			},


				/* second access 1 : SCI HDTM */

			{

					/* NA description */

				{
					1,						/* nai */
					SCI,					/* access type */
					DL_DTE,					/* function grouping */
					1,						/* nb of B channels */
					1,						/* nb of SAP */
				},

					/* SAP descriptions */

				{

					{
						64,					/* sapi */
						ENT_DL_B,			/* entity */
						128,				/* modulo */
						7,					/* k */
						1*SCD,				/* timer T200 (or T201) value (0 for implicit value) */
						10*SCD,				/* timer T203 (or T202) value (0 for implicit value) */
						0,					/* unused */
						3,					/* retry nb (N201) */
						1,					/* nb of connection enpoints */
						128,				/* I or UI frames max size of information field (n201) */
					},

				}

			}


		},

		OFF,

			/* DL RELAY : (0, DL_SAPI_X25) <---> (1, 64, 0) */

		{

			{
				ON,								/* Flag relay ON */

				0,								/* NAI 	1 */
				DL_SAPI_X25,					/* SAPI 1 */
				2,								/* CES	1 */

#				if 1

						/* Loop back */

					0,							/* NAI 	1 */
					DL_SAPI_X25,				/* SAPI 1 */
					2							/* CES	1 */

#				else

					1, 							/* NAI 	2 */
					64,							/* SAPI 2 */
					0							/* CES	2 */

#				endif
			},

		}

	};


#	else	/* } DL_LAP4K OFF: { */

#		if DL_LAPF == ON	/* { */

#	if 1

			/* DL & MDL configuration structure for a LAPF TERMINAL */

	CONST struct dl_config dl_config_data = {

		{0},								/* confirm */

		1,									/* number of network access */

			/* Network Access description table */
		{

				/* access 0 : LAPF */

			{

					/* NA description */

				{
					0,						/* nai */
					SCI,					/* Single CHannel Interface */
					DL_DTE,					/* DTE side */
					1,						/* nb of channels */
					1,						/* nb of SAP */
					{0},					/* TEI list */
					0,						/* TEI time assignment at : CONFIGURATION, NA ACTIVATION or USAGE time*/
					0,						/* TEI time removal at    : NA DEACTIVATION, POWER DOWN */

					{
						1,					/* Virtual channel ID : 1, 2 or DL_VC_ID_NIL */
						3,					/* number of DLCI in the Virtual Channel */

							/* DLCI LIST */

						{DL_DLCI_NIL, DL_DLCI_NIL, DL_DLCI_NIL},
					},


				},

					/* SAP descriptions */

				{

					{
						DL_SAPI_LAPF_L3,	/* sapi */
						ENT_DL_F,			/* entity */
						128,				/* modulo */
						1,					/* k */
						1*SCD,				/* timer T200 (or T201) value (0 for implicit value) */
						10*SCD,				/* timer T203 (or T202) value (0 for implicit value) */
						0,					/* timer for TEI assignement procedure */
						3,					/* retry nb (N201) */
						3,					/* nb of connection enpoints */
						260,				/* I or UI frames max size of information field (n201) */
					},

