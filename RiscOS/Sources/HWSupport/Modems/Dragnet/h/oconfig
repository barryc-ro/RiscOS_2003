						10*SCD,				/* timer T203 (or T202) value (0 for implicit value) */
						0,					/* timer for TEI assignement procedure */
						3,					/* retry nb (N201) */
						3,					/* nb of connection enpoints */
						260,				/* I or UI frames max size of information field (n201) */
						ENT_L3,				/* L3 entity ID */

#						if DL_FC == ON
							0,				/* outgoing flow control low mark */
							0,				/* outgoing flow control high mark */
#						endif

#						if DL_XID == ON || DL_IDENTIFICATION == ON
							0,				/* XID retransmission timer value */
							0,				/* XID retransmission max value */

#							if DL_IDENTIFICATION == ON
								0, 			/* identification profile number */
#							endif

#						endif

#						if DL_ISDN_BIOS == ON
							0,				/* ISDN-BIOS LAPB facilities ON or OFF in thz SAP */
#						endif

						0,					/* OFF : congestion is managed / ON : no congestion management */
						0,					/* teledisket profile : ON or OFF */

						2,					/* Lapf address field format */
						1,					/* Lapf Dicard elligibility */
						0,					/* DLCI or DL-CORE */
					},

				}

			},

		},

		OFF

