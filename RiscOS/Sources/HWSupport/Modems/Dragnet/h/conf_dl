#include "dl_int.h"

/*
				TE							 NT2                    NT1
		----------------		 ------------------------       --------------
		|              |         |          |           |       |  Public    |
		|  FG_TE_NT    |---------| FG_NT_TE | FG_NT_NET |-------|  Network   |
		|              |  0 BRI  |          |           | 1 PRI |            |
		----------------         ------------------------       --------------
			  User <=============>  Network	|  User <============>  Network

*/

		/* DL & MDL configuration structure for:
		 *
		 *    PRI,
		 *    ETSI-like variants
		 *
		 *    NAI 0: Terminal
		 */

CONST struct dl_config dl_config_data_TE1 = {

	{0},								/* confirm */

	1,									/* number of network access */

		/* Network Access description table */
	{

			/* NAI 0: PRI, Terminal */

		{

				/* NA description */

			{
				0,						/* nai */
				PRI,					/* access type (BRI or PRI) */
				FG_TE_NT,				/* function grouping */
				23,						/* nb of B channels */
				2,						/* nb of SAP */

					/* TEI list */
				{
					0,					/* TEI for CES #0 (broadcast) */
					0,					/* TEI for CES #1 (Signalling) */
				},

					/* TEI management */

				DL_TEI_TIME_CONFIGURATION,	/* TEI assignment */
				0,							/* TEI removal */

			},

				/* SAP descriptions */

			{

				{
					DL_SAPI_SIG,		/* sapi */
					ENT_DL_D,			/* entity */
					128,				/* modulo */
					1,					/* k orig 7 */
					1*SCD,				/* timer T200 value (0 for implicit value) */
					10*SCD,				/* timer T203 value (0 for implicit value) */
					0,					/* N/A */
					3,					/* retry nb (N201) */
					2,					/* nb of connection enpoints: 1 broadcast, 1 Signalling */
					260					/* I or UI frames max size of information field (n201) */
				},

				{
					DL_SAPI_MDL,		/* sapi */
					ENT_DL_D,			/* entity */
					128,				/* modulo */
					1,					/* k */
					1*SCD,				/* timer T200 value (0 for implicit value) */
					10*SCD,				/* timer T203 value (0 for implicit value) */
					1*SCD,				/* timer T201 (TEI-Check-Response) */
					2,					/* retry nb (N201) */
					1,					/* nb of connection enpoints */
					260					/* I or UI frames max size of information field (n201) */
				},

			}

		},	/* End of NAI 0 */

	},	/* End of NA table */

	DL_ATT,	/* uchar att_ni_flag:
			   AT&T, NI or Northern Telecom option flag :
					OFF		Other network
					DL_ATT	AT&T Network
					DL_NI	National ISDN
					DL_NT   Nothern Telecom
			*/

};

/*--------------------------------------------------------------------------*/

		/* DL & MDL configuration structure for:
		 *
		 *    PRI,
		 *    ETSI-like variants
		 *
		 *    NAI 1: Network
		 */

CONST struct dl_config dl_config_data_TE2 = {

	{0},								/* confirm */

	1,									/* number of network access */

		/* Network Access description table */
	{

			/* NAI 1: PRI, Network */

		{

				/* NA description */

			{
				1,						/* nai */
				PRI,					/* access type (BRI or PRI) */
				FG_TE_NT,				/* function grouping */
				23,						/* nb of B channels */
				2,						/* nb of SAP */

					/* TEI list */
				{
					0,					/* TEI for CES #0 (broadcast) */
					0,					/* TEI for CES #1 (Signalling) */
				},

					/* TEI management */

				DL_TEI_TIME_CONFIGURATION,	/* TEI assignment */
				0,							/* TEI removal */

			},

				/* SAP descriptions */

			{

				{
					DL_SAPI_SIG,		/* sapi */
					ENT_DL_D,			/* entity */
					128,				/* modulo */
					7,					/* k */
					1*SCD,				/* timer T200 value (0 for implicit value) */
					10*SCD,				/* timer T203 value (0 for implicit value) */
					0,					/* N/A */
					3,					/* retry nb (N201) */
					2,					/* nb of connection enpoints */
					260					/* I or UI frames max size of information field (n201) */
				},

				{
					DL_SAPI_MDL,		/* sapi */
					ENT_DL_D,			/* entity */
					128,				/* modulo */
					1,					/* k */
					1*SCD,				/* timer T200 value (0 for implicit value) */
					10*SCD,				/* timer T203 value (0 for implicit value) */
					1*SCD,				/* timer T201 (TEI-Check-Response) */
					2,					/* retry nb (N201) */
					1,					/* nb of connection enpoints */
					260					/* I or UI frames max size of information field (n201) */
				},

			}

		},	/* End of NAI 1 */

	},	/* End of NA table */

	DL_ATT,	/* uchar att_ni_flag:
			   AT&T, NI or Northern Telecom option flag :
					OFF		Other network
					DL_ATT	AT&T Network
					DL_NI	National ISDN
					DL_NT   Nothern Telecom
			*/

};

