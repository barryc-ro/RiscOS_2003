#include "llglob_d.h"

#include "dpglob.h"
#include "z80glob.h"
#include "z80flash.h"
#include "allglobs.h"

#if defined(VPCM_CLIENT) || defined(VPCM_SERVER)

#ifdef VPCM_SERVER
# include "dpvpv8d.h"
# include "dpvpded.h"
#else
# if defined(DRAGNET2)
#  include "dpdrv80.h"
# else
#  include "dpvpv80.h"
#endif // DRAGNET2
#ifdef V92
#  include "dpvpde0.h"
#endif	// V92
#endif // VPCM_SERVER


// Download everything for phase 1/2
// for V.PCM.  (Client or central site.)
void dp_download_PCM_P12(void)
{
	DOWNLOAD_DSP(wVP_V8Wk0);	// download section .v8wk
#if defined(VPCM_SERVER) || defined (V92)
	DOWNLOAD_DSP(wVP_DE000);
#endif // VPCM_SERVER
}
#endif
