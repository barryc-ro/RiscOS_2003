 ; => GetAll
        TTL     The Podule manager for Arthur.

        GET     Hdr:ListOpts
        GET     Hdr:Macros
        GET     Hdr:System
        GET     Hdr:Machine.<Machine>
	$GetIO
        GET     Hdr:CMOS
        GET     Hdr:ModHand
        GET     Hdr:Debug
        GET     Hdr:Podule
        GET     Hdr:FSNumbers
        GET     Hdr:HighFSI
        GET     Hdr:NewErrors
        GET     Hdr:Services
        GET     Hdr:Tokens
        GET     Hdr:Proc
        GET     Hdr:Symbols
        GET     Hdr:MsgTrans
        GET     Hdr:PoduleReg

        GET     VersionASM

        GBLL    ReleaseVersion
ReleaseVersion  SETL    True

        GBLS    GetHelpTexts
  [ International_Help <> 0
GetHelpTexts    SETS    "GET HelpTexts"
  |
GetHelpTexts    SETS    "GET s.HelpTokens"
  ]

	AREA Podule_code, CODE, READONLY


        GET     s.Module
        GET     s.Interface
        GET     s.Commands
        GET     s.Errors
        GET     s.ROMExtend
        $GetHelpTexts
        GET     s.MsgCode

        END
