#ifndef UEIBLAST_H
#define UEIBLAST_H

#define UEIIRBlast_SWI_Chunk  0x4f180
#define UEIIRBlast_Error_Block 0x00811f00

enum
{
    UEIIRBlast_BlastToDevice = UEIIRBlast_SWI_Chunk,
    UEIIRBlast_BlastToTV,
    UEIIRBlast_BlastToVCR,
    UEIIRBlast_BlastToCable,
    UEIIRBlast_BlastToSatellite
};

enum
{
    UEIIRBlast_Error_UnknownDevice,
    UEIIRBlast_Error_DeviceNumberOutOfRange,
    UEIIRBlast_Error_CodeNumberOutOfRange
};

#endif
