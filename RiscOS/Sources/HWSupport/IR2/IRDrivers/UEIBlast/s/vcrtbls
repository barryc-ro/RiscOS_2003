;V00
;RCA - VCR1
;9205.04


V00TAB
	DCB	&02		;00-00-0010 (1 BPF:2 DIF FLD)
	DCB	&BA		;10111-010 (24 FPC:CARRIER 2)
	DCB	132		;8mS (4+128)
	DCB	113		;MARK HIGH = 224*17.58 = 3.938mS
	DCB	44		;SPACE = 3.936mS (3.93)
	DCB	14		;SYNC HIGH = 28*17.58 = 492uS
	DCB	31		;"0" SPACE = 1.968mS (1.98)
	DCB	14		;"0" BURST HIGH
	DCB	21		;"1" SPACE = 984uS (942)
	DCB	14		;"1" BURST HIGH
;
V00COD
	DCB	&1F	;TVVCR - 1FAE05
	DCB	&AE
	DCB	&05
	DCB	&1E	;RECRD - 1E8E17
	DCB	&8E
	DCB	&17
	DCB	&1E	;PAUSE - 1E6E19
	DCB	&6E
	DCB	&19
	DCB	&1E	;STOP  - 1E0E1F
	DCB	&0E
	DCB	&1F
	DCB	&1E	;REWND - 1E2E1D
	DCB	&2E
	DCB	&1D
	DCB	&1E	;PLAY  - 1EAE15
	DCB	&AE
	DCB	&15
	DCB	&1E	;F FWD - 1E3E1C
	DCB	&3E
	DCB	&1C
	DCB	&1D	;Power Toggle - 1D5E2A (was POWER - 1C4E3B)
	DCB	&5E
	DCB	&2A
	DCB	&1D	;CH UP - 1D2E2D
	DCB	&2E
	DCB	&2D
	DCB	&1D	;CH DN - 1D3E2C
	DCB	&3E
	DCB	&2C
	DCB	&1C	;DIG 0 - 1CFE30
	DCB	&FE
	DCB	&30
	DCB	&1C	;DIG 1 - 1CEE31
	DCB	&EE
	DCB	&31
	DCB	&1C	;DIG 2 - 1CDE32
	DCB	&DE
	DCB	&32
	DCB	&1C	;DIG 3 - 1CCE33
	DCB	&CE
	DCB	&33
	DCB	&1C	;DIG 4 - 1CBE34
	DCB	&BE
	DCB	&34
	DCB	&1C	;DIG 5 - 1CAE35
	DCB	&AE
	DCB	&35
	DCB	&1C	;DIG 6 - 1C9E36
	DCB	&9E
	DCB	&36
