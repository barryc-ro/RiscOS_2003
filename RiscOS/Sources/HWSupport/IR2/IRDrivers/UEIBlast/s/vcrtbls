	;END                   	;;END V00

;V01
;CAPEHART
;
V01TAB
	DCB	&82		;10-00-0010 (1 BPF:2 DIF FLD)
	DCB	&FE		;11111-110 (32 FPC:CARRIER 6)
	DCB	&04		;PREAMBLE=04
	DCB	173		;MARK = 18.0mS (EXPRESSED AS 9.0mS)
	DCB	67		;SPACE = 9.0mS (9.04)
	DCB	21		;SYNC = 1.1mS
	DCB	41		;"0" SPACE = 3.4mS (3.42)
	DCB	21		;"0" BURST = 1.1mS
	DCB	23		;"1" SPACE = 1.1mS (1.12)
	DCB	21		;"1" BURST = 1.1mS
;
V01COD
	DCB	&0D	;TVVCR - B0
	DCB	&1E	;RECRD - 78
	DCB	&0E	;PAUSE - 70
	DCB	&0C	;STOP  - 30
	DCB	&07	;REWND - E0
	DCB	&04	;PLAY  - 20
	DCB	&05	;F FWD - A0
	DCB	&08	;POWER - 10
	DCB	&00	;CH UP - 00
	DCB	&01	;CH DN - 80
	DCB	&FF	;DIG 0 - DUNSEL
	DCB	&FF	;DIG 1 - DUNSEL
	DCB	&FF	;DIG 2 - DUNSEL
	DCB	&FF	;DIG 3 - DUNSEL
	DCB	&FF	;DIG 4 - DUNSEL
	DCB	&FF	;DIG 5 - DUNSEL
	DCB	&FF	;DIG 6 - DUNSEL
	DCB	&FF	;DIG 7 - DUNSEL
	DCB	&FF	;DIG 8 - DUNSEL
	DCB	&FF	;DIG 9 - DUNSEL
;
	;END			;;END V01
;V02
;SANYO
;
V02TAB
	DCB	&82	;10-00-0010 (1 BPF:2 DIF FLD)
	DCB	&FE	;11111-110 (32 FPC:CARRIER 6)
	DCB	&31	;PREAMBLE=31
	DCB	173	;MARK = 9.0mS
	DCB	47	;SPACE = 4.5mS (4.48)
	DCB	11	;SYNC = 560uS
	DCB	28	;"0" SPACE = 1.68mS (1.63)
	DCB	11	;"0" BURST = 560uS
	DCB	16	;"1" SPACE = 560uS (572)
	DCB	11	;"1" BURST = 560uS
;
V02COD
	DCB	&08	;TVVCR - 10
	DCB	&15	;RECRD - A8
	DCB	&11	;PAUSE - 88
	DCB	&10	;STOP  - 08
	DCB	&12	;REWND - 48
	DCB	&14	;PLAY  - 28
	DCB	&13	;F FWD - C8
	DCB	&00	;POWER - 00
	DCB	&01	;CH UP - 80
	DCB	&02	;CH DN - 40
	DCB	&1D	;DIG 0 - B8
	DCB	&04	;DIG 1 - 20
	DCB	&05	;DIG 2 - A0
	DCB	&06	;DIG 3 - 60
	DCB	&07	;DIG 4 - E0
	DCB	&0C	;DIG 5 - 30
	DCB	&0D	;DIG 6 - B0
	DCB	&0E	;DIG 7 - 70
	DCB	&0F	;DIG 8 - F0
	DCB	&1C	;DIG 9 - 38
;
	;END			;;END V02

