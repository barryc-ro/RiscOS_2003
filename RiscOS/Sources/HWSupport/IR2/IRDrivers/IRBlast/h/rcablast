#ifndef RCABLAST_H
#define RCABLAST_H

#define RCAIRBlast_SWI_Chunk  0x5f040
#define RCAIRBlast_Error_Block 0xf8000

enum
{
    RCAIRBlast_BlastToDevice = RCAIRBlast_SWI_Chunk,
    RCAIRBlast_BlastToTV,
    RCAIRBlast_BlastToVCR,
    RCAIRBlast_BlastToCable
};

enum
{
    RCAIRBlast_Error_UnknownDevice,
    RCAIRBlast_Error_DeviceNumberOutOfRange,
    RCAIRBlast_Error_CodeNumberOutOfRange
};

#endif
