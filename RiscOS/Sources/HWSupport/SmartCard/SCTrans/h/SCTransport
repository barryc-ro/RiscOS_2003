/* > SCTransport.h

 * Exported SWI definitions for SCTransport

 */

#ifndef SCTransport_Open
#define SCTransport_Open                0x04d400
#define SCTransport_RetrieveATR         0x04d401
#define SCTransport_Close               0x04d402
#define SCTransport_IssueCommand        0x04d403
#define SCTransport_ReadResponse        0x04d404
#define SCTransport_StartTrans          0x04d405
#define SCTransport_EndTrans            0x04d406
#define SCTransport_Error               0x04d407
#define SCTransport_GetErrorText        0x04d408
#define SCTransport_Status              0x04d409
#define SCTransport_SlotInfo            0x04d40a
#define SCTransport_InitSlot2           0x04d40b
#define SCTransport_ReadSlot2           0x04d40c
#define SCTransport_PowerControl        0x04d40d
#define SCTransport_CardStatus          0x04d40e
#endif

