#define PMSDEBUG 0                      /* Set to 0 for no debug, or 1 to enable debug */
void pms_debug_dump(void);              /* Dump debug log to disc file */
void pms_debug(char *, ...);            /* Write debug text to memory buffer, printf format */
void pms_debug_start(char *);           /* Start debug log and save filename */
char *pms_convert_hex(void *, int);     /* Convert buffer to hex string */


