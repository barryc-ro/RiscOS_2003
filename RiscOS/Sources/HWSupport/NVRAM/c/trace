/* trace.c
 *
 *      General purpose debugging code.
 */

/* From CLib */
#include <stdio.h>
#include <stdarg.h>
#include "swis.h"

#ifdef DEBUG

#include "trace.h"

void
_tracef( char *fmt, ... )
{
    __va_list args;

    (void)_swix( 0x40100, 0 );
    va_start( args, fmt );
    vprintf( fmt, args );
    va_end( args );
    (void)_swix( 0x40101, 0 );
}

#endif
