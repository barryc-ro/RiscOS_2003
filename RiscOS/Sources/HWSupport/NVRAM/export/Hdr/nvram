	SUBT Exported NVRAM constants
OldOpt	SETA	{OPT}
	OPT	OptNoList+OptNoP1List

SWIClass	SETS	NVRAMSWI_Name

; NVRAM SWI numbers.
		^	NVRAMSWI_Base
		AddSWI	Read		; &4EE00
		AddSWI	Write		; &4EE01

; NVRAM_Read errors.
NVRAM_READ_TAGERR       *	(-1)
NVRAM_READ_BUFERR       *	(-4)

; NVRAM_Write errors.
NVRAM_WRITE_TAGERR      *	(-1)
NVRAM_WRITE_LOCKERR     *	(-2)

	OPT	OldOpt
	END

