/* trace.h
 *
 *      General purpose debugging code.
 */

#ifndef __trace_h
#define __trace_h

/* Make _ == , so we can have multiple arguments in a macro. */
#define _ ,

#ifdef DEBUG

#define TRACE(args) _tracef(args)

extern void _tracef( char *fmt, ... );

#else

#define TRACE(args)

#endif

#endif
