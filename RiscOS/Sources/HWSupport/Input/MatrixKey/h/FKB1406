

// FKB1406 constants


#define KEYBOARD_TYPE 3




#define ROW_SIZE                7
#define COLUMN_SIZE             14

#define NUMBER_OF_KEY_LAYERS    2

// special keys for FKB1406 keyboard


#define ALT_KEY_ROW            1
#define ALT_KEY_CLM            1

#define CTRL_KEY_ROW           1
#define CTRL_KEY_CLM           4

#define FUNC_KEY_ROW           1
#define FUNC_KEY_CLM           5

#define ESC_KEY_ROW            1
#define ESC_KEY_CLM            6

#define SFTL_KEY_ROW           2
#define SFTL_KEY_CLM           3

#define SFTR_KEY_ROW           2
#define SFTR_KEY_CLM           13
