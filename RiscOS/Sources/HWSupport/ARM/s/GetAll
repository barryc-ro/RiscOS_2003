; > Sources.GetAll

;---------------------------------------------------------------------------
; Module template.
;---------------------------------------------------------------------------

        AREA    |ARM$$Code|, CODE, READONLY, PIC

Module_BaseAddr

  [ {TRUE} ;so we can do secret stuff for StrongARM, ARM810
CP15 CP 15
C0  CN  0
C1  CN  1
C2  CN  2
C15 CN 15
  ]

              GBLL    ARM810support
ARM810support SETL    {FALSE}

;---------------------------------------------------------------------------
;       Get the headers.
;
        GET     Hdr:ListOpts
        GET     Hdr:Macros
        GET     Hdr:System
        GET     Hdr:Machine.<Machine>
        GET     Hdr:ModHand
        GET     Hdr:Services
        GET     Hdr:MsgTrans
        GET     Hdr:ResourceFS
        GET     Hdr:NdrDebug
        GET     Hdr:DDVMacros
        GET     Hdr:Proc
        GET     Hdr:CMOS
        GET     Hdr:MEMM.ARM600
        GET     Hdr:FSNumbers
        GET     Hdr:NewErrors

        GET     VersionASM


;---------------------------------------------------------------------------
;       Define global variables.
;
                GBLL    hostvdu

debug           SETL    false
hostvdu         SETL    true

cache           SETD    false

    [ :LNOT: :DEF: standalone
                GBLL    standalone
standalone      SETL    false
    ]

                GBLL    checkprocessor
checkprocessor  SETL    false

;---------------------------------------------------------------------------
;       Get the sources.
;
                GBLS    GetResFiles
 [ standalone
GetResFiles     SETS    "GET    s.ResFiles"
 |
GetResFiles     SETS    ""
 ]

                GBLS    Command
Command         SETS    "Cache"


        GET     s.Front
        GET     s.Errors
        GET     s.Module
        $GetResFiles
        GET     s.Cache
	GET	s.ARM810clk
        GET     s.MsgTrans

        END
