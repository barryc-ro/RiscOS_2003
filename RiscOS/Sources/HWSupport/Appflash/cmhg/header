title-string:           AppFlash
help-string:            AppFlash 0.23 (PreRelease)
initialisation-code:    module_initialise
finalisation-code:      module_finalise
swi-chunk-base-number:  0x50A00
swi-handler-code:       module_swi
swi-decoding-table:     AppFlash Update Checksum

command-keyword-table: module_cmd
AppFlash_Update(min-args: 1,
 max-args: 1,
 invalid-syntax: "Syntax: *AppFlash_Update <Flash Image location>\n",
 help-text: "*AppFlash_Update erases the Application flash, then writes the\n\
 ROM Image file at <Flash Image location> into the Application flash.\n\
 Syntax: *AppFlash_Update <Flash Image location>"),
AppFlash_Write(min-args: 1,
 max-args: 1,
 invalid-syntax: "Syntax: *AppFlash_Write <Flash Image location>\n",
 help-text: "*AppFlash_Write writes the ROM Image file at <Flash Image location>\n\
  into the Application flash.\n\
 Syntax: *AppFlash_Write <Flash Image location>"),
AppFlash_Erase(min-args: 0,
 max-args: 0,
 invalid-syntax: "Syntax: *AppFlash_Erase\n",
 help-text: "*AppFlash_Erase erases the Application flash.\n\
 Syntax: *AppFlash_Erase"),
AppFlash_WriteWord(min-args: 1,
 max-args: 1,
 invalid-syntax: "Syntax: *AppFlash_WriteWord <address>\n",
 help-text: "*AppFlash_WriteWord writes a word into the flash, without erasure.\n\
 Syntax: *AppFlash_WriteWord <address>")
