/******************************************************************************

      File: module.h
   Purpose: header file for module entry points
    Author: A.Pirozek
   History: 25-03-98

 ******************************************************************************/

#ifndef __module_h
#define __module_h

#define NOTUSED(f) { f = f; }

#ifndef TRUE
#define TRUE 1
#endif

#ifndef FALSE
#define FALSE 0
#endif

#ifndef ERROR
#define ERROR -1
#endif

#ifndef NULL
#define NULL '\0'
#endif

typedef unsigned int  uint;
typedef unsigned char uchar;
typedef unsigned long ulong;

#endif /* __module_h */
