#ifndef __desfunc_h
#define __desfunc_h

void des_encrypt(uchar *a, uchar *b, uchar *c, int n);
void des_decrypt(uchar *a, uchar *b, uchar *c, int n);
void des_triple_encrypt(uchar *a, uchar *b, uchar *sx, uchar *dx);
void des_triple_decrypt(uchar *a, uchar *b, uchar *sx, uchar *dx);

#endif
