// header file for ISPC_CMDS.H

/* Begin-Prologue */
#ifndef ISPC_CMDS_H
#define ISPC_CMDS_H
#undef DECLARE
#undef INIT
#ifdef PROTOTYPES_INTERNAL
#define DECLARE
#define INIT(x) =(x)
#else
#define DECLARE extern
#define INIT(x)
#endif
/* End-Prologue */

DECLARE int cmd_ispc_nullcmd(char *, int);
DECLARE int cmd_ispc_status(char *, int);
DECLARE int cmd_ispc_urc(char *, int);
DECLARE int cmd_ispc_showargs(char *, int);
DECLARE int cmd_ispc_showscript(char *, int);
DECLARE int cmd_ispc_parsescript(char *, int);

#define MAXCMD 18
#define ISPCAPTURE_VAR_NAME    "ISPCapture$URL"

DECLARE char cmdargs[512];

struct cmdent {
        int (*cmd_call)();      /* swi handler */
};



#ifdef PROTOTYPES_INTERNAL
struct cmdent ispc_cmdent[MAXCMD + 1] = {
    cmd_ispc_status,
    cmd_ispc_urc,
    cmd_ispc_showargs,
    cmd_ispc_showscript,
    cmd_ispc_parsescript,
    cmd_ispc_nullcmd,
    cmd_ispc_nullcmd,
    cmd_ispc_nullcmd,
    cmd_ispc_nullcmd,
    cmd_ispc_nullcmd,
    cmd_ispc_nullcmd,
    cmd_ispc_nullcmd,
    cmd_ispc_nullcmd,
    cmd_ispc_nullcmd,
    cmd_ispc_nullcmd,
    cmd_ispc_nullcmd,
    cmd_ispc_nullcmd,
    cmd_ispc_nullcmd,
    0,
};
#else
extern struct cmdent ispc_cmdent[MAXCMD+1];
#endif


/* Begin-Epilogue */
#undef PROTOTYPES_INTERNAL
#undef INIT
#undef DECLARE
#endif
/* End_Epilogue */






