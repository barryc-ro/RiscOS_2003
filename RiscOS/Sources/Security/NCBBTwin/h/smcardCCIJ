#ifndef SMCARDCCIJ_H
#define SMCARDCCIJ_H

#define FUNAI_CARD          	0x80
#define FUNAI_PHONE_NUMBER  	0x81
#define FUNAI_ACCOUNT_ID    	0x82
#define FUNAI_ACCOUNT_PASSWORD 	0x83
#define FUNAI_DNS1  	    	0x84
#define FUNAI_DNS2  	    	0x85
#define FUNAI_EMAIL_ID 	    	0x86
#define FUNAI_EMAIL_PASSWORD 	0x87
#define FUNAI_EMAIL_ADDRESS 	0x88
#define FUNAI_POP_SERVER    	0x89
#define FUNAI_SMTP_SERVER   	0x8a
#define FUNAI_URL   	    	0x8b
#define FUNAI_GATEWAY 	    	0x8c
#define FUNAI_PROXY 	    	0x8d
#define FUNAI_TIMESERVER    	0x8e
#define FUNAI_MAIL_URL	    	0x8f
#endif
