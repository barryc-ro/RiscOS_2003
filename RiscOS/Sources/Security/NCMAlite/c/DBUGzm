/* DEBUG stuff */

#include <stdio.h>
#include <string.h>
#include <stdarg.h>
#define PROTOTYPES_INTERNAL
#include "DBUG.H"

#ifdef ENABLE_DEBUGGING
void DBUG_PRINTF(int level, char *fmt, ...)
{
  va_list ap;
  char debug_line[256];

  va_start(ap, fmt);
  vsprintf(debug_line, fmt, ap);
  dprintf(("", debug_line));
  va_end(ap);
}
#endif /* ENABLE_DEBUGGING */

