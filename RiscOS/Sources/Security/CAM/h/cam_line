// header file for CAM_LINE.H

/* Begin-Prologue */
#ifndef CAM_LINE_H
#define CAM_LINE_H
#undef DECLARE
#undef INIT
#ifdef PROTOTYPES_INTERNAL
#define DECLARE
#define INIT(x) =(x)
#else
#define DECLARE extern
#define INIT(x)
#endif
/* End-Prologue */

DECLARE void cam_pause(void);
DECLARE int cam_resume(void);
DECLARE void cam_halt(void);

DECLARE unsigned int line_status INIT(0);

#define LINE_UP   0
#define LINE_DOWN 1

/* Begin-Epilogue */
#undef PROTOTYPES_INTERNAL
#undef INIT
#undef DECLARE
#endif
/* End_Epilogue */

