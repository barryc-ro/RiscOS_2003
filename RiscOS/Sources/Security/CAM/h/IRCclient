/* NNTPclient header file */

/* some macros  */
#define UNUSED(x) (x=x)
#define DEBUG if(debug)
#define QUIET if(!quiet)
#define LOG if(logfile)
#define FLAG(x) (x?"yes":"no")


/* Domain Name Service SWIs provided by the InetDB module  */
#define Internet_GetHostByName	     0x46000
#define Internet_GetHostByAddr	     0x46001

/* bit of boolean stuff */

#define TRUE                   1
#define FALSE		       0

#ifndef NULL
#define	NULL		       0
#endif

/* various help related string defines */

/* other miscellaneous defines */

/* prototypes for external functions  */

extern void bcopy(char *, char *, int);
extern void bcmp(char *, char *, int);
extern void bzero(char *, int);

