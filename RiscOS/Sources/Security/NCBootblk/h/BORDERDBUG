// header file for BORDERDBUG.s

/* Begin-Prologue */
#ifndef BORDERDBUG_H
#define BORDERDBUG_H
#undef DECLARE
#undef INIT
#ifdef PROTOTYPES_INTERNAL
#define DECLARE
#define INIT(x) =(x)
#else
#define DECLARE extern
#define INIT(x)
#endif
/* End-Prologue */

DECLARE void border(int);
DECLARE void border_cycle(int *);

#define BORDER_RED border(0)
#define BORDER_GREEN border(1)
#define BORDER_BLUE border(2)
#define BORDER_YELLOW border(3)
#define BORDER_MAGENTA border(4)
#define BORDER_PURPLE border(4)
#define BORDER_CYAN border(5)
#define BORDER_ORANGE border(6)
#define BORDER_BLACK border(7)
#define BORDER_WHITE border(8)
/* Begin-Epilogue */
#undef PROTOTYPES_INTERNAL
#undef INIT
#undef DECLARE
#endif
/* End_Epilogue */
