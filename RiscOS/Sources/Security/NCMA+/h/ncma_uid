// header file for NCMA_UID.H

/* Begin-Prologue */
#ifndef NCMA_UID_H
#define NCMA_UID_H
#undef DECLARE
#undef INIT
#ifdef PROTOTYPES_INTERNAL
#define DEFINE_GLOBALS_HERE
#define DECLARE
#define INIT(x) =(x)
#else
#define DECLARE extern
#define INIT(x)
#endif
/* End-Prologue */
DECLARE int init_unique_id(void);
DECLARE int render_hex_id(char *);

#include "smcard.h"
#include "ncma_ROM.h"

#ifdef DEFINE_GLOBALS_HERE
HardwareSerial unique_id = {
	 NCI_SERIAL_VERSION,
	 NCI_SERIAL_MANASSIGNED,
	 ROM_MANUFACTURE_ID,
	 ROM_MODEL,
	 ROM_BIOS_VERSION,
	 ROM_NCOS_VERSION,
	 ROM_SERIAL_LEN
	 };
#else
extern HardwareSerial unique_id;
#endif


/* Begin-Epilogue */
#undef PROTOTYPES_INTERNAL
#undef INIT
#undef DECLARE
#endif
/* End_Epilogue */

