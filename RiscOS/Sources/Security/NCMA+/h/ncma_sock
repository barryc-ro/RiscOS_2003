// header file for NCMA_SOCK.h

/* Begin-Prologue */
#ifndef NCMA_SOCK_H
#define NCMA_SOCK_H
#undef DECLARE
#undef INIT
#ifdef PROTOTYPES_INTERNAL
#define DECLARE
#define INIT(x) =(x)
#else
#define DECLARE extern
#define INIT(x)
#endif
/* End-Prologue */

DECLARE int ncma_socket INIT(0);
DECLARE int open_socket(void);
DECLARE void close_socket(int);

/* Begin-Epilogue */
#undef PROTOTYPES_INTERNAL
#undef INIT
#undef DECLARE
#endif
/* End_Epilogue */

