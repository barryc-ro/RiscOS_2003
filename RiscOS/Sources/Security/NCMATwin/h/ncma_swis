// header file for NCMA_SWIS.H

/* Begin-Prologue */
#ifndef NCMA_SWIS_H
#define NCMA_SWIS_H
#undef DECLARE
#undef INIT
#ifdef PROTOTYPES_INTERNAL
#define DECLARE
#define INIT(x) =(x)
#else
#define DECLARE extern
#define INIT(x)
#endif
/* End-Prologue */

/* reason codes for MiscOp SWI */
#define NCMA_MISCOP_PRECONNECT     0x0
#define NCMA_MISCOP_CONNECTIONTYPE 0x1
#define NCMA_MISCOP_DISABLESLOT2   0x2

#define MAXSWI 18

struct swient {
        int (*swi_call)();      /* swi handler */
};


DECLARE int ncma_nullswi(_kernel_swi_regs *);
DECLARE int ncma_enquiry(_kernel_swi_regs *);
DECLARE int ncma_miscop(_kernel_swi_regs *);

#ifdef PROTOTYPES_INTERNAL
struct swient ncma_ent[MAXSWI + 1] = {
    ncma_enquiry,
    ncma_miscop,
    ncma_nullswi,
    ncma_nullswi,
    ncma_nullswi,
    ncma_nullswi,
    ncma_nullswi,
    ncma_nullswi,
    ncma_nullswi,
    ncma_nullswi,
    ncma_nullswi,
    ncma_nullswi,
    ncma_nullswi,
    ncma_nullswi,
    ncma_nullswi,
    ncma_nullswi,
    ncma_nullswi,
    ncma_nullswi,
    0,
};
#else
extern struct swient ncma_ent[MAXSWI+1];
#endif


/* Begin-Epilogue */
#undef PROTOTYPES_INTERNAL
#undef INIT
#undef DECLARE
#endif
/* End_Epilogue */






