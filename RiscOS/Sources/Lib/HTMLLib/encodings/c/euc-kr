#include <stdlib.h>

#include "encoding.h"

#include "iso2022.h"
#include "euc-kr.h"

/*
 * Routines for encoding EUC-KR
 * Number: 38
 * Names: EUC-KR
 *        csEUCKR
 */

static void euc_kr_reset(Encoding *e)
{
    iso2022_reset(e);
    /* Select G1 = KS C 5601 */
    iso2022_read(e, NULL, "\x1B\x24\x29\x43", 4, NULL);
}

Encoding enc_euc_kr =
{
    iso2022_read,
    euc_kr_reset,
    iso2022_ws_size,
    iso2022_delete
};
