#
# SSLLeay/crypto/lhash/Makefile
#
# RISC OS version.
#
.SUFFIXES: .c .o

DIR=lhash

WIPE=wipe

CFLAGS=-I${INCLUDES} ${DEFINES} ${EFLAGS}
WPFLAGS=FR~C~V

LIBOBJ=lhash.o lh_stats.o

export_hdrs:
       @|

export_libs: ${LIBOBJ}
       @|

clean:
       ${WIPE} o.* ${WPFLAGS}

.c.o:; ${CC} ${CFLAGS} -c $*.c
