#
# SSLLeay/crypto/conf/Makefile
#
# RISC OS version.
#
.SUFFIXES: .c .o

DIR=conf

WIPE=wipe

CFLAGS=-I${INCLUDES} ${DEFINES} ${EFLAGS}
WPFLAGS=FR~C~V

ERR=conf
ERRC=conf_err

LIBOBJ=	conf.o $(ERRC).o

export_hdrs:
       @|

export_libs: ${LIBOBJ}
       @|

clean:
       ${WIPE} o.* ${WPFLAGS}

.c.o:; ${CC} ${CFLAGS} -c $*.c
