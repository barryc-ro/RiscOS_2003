#
# SSLLeay/crypto/bf/Makefile
#
# RISC OS version.
#
.SUFFIXES: .c .o

DIR=bf

CFLAGS=-I${INCLUDES} ${DEFINES} ${EFLAGS}
WPFLAGS=FR~C~V
WIPE=wipe

BF_ENC=		bf_enc.o

LIBOBJ= bf_skey.o bf_ecb.o $(BF_ENC) bf_cbc.o bf_cfb64.o bf_ofb64.o

export_hdrs:
       @|

export_libs: ${LIBOBJ}
       @|

clean:
       ${WIPE} o.* ${WPFLAGS}

.c.o:; ${CC} ${CFLAGS} -c $*.c
