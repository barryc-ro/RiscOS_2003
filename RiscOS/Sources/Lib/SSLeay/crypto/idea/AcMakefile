.SUFFIXES: .c .o

DIR=idea

WIPE=wipe

CFLAGS=-I${INCLUDES} ${DEFINES} ${EFLAGS}
WPFLAGS=FR~C~V

LIBOBJ=i_cbc.o i_cfb64.o i_ofb64.o i_ecb.o i_skey.o

export_hdrs:
       @|

export_libs: ${LIBOBJ}
       @|

clean:
       ${WIPE} o.* ${WPFLAGS}

.c.o:; ${CC} ${CFLAGS} -c $*.c
