#
# SSLLeay/crypto/sha/Makefile
#
# RISC OS version.
#
.SUFFIXES: .c .o

DIR=sha

WIPE=wipe

CFLAGS=-I${INCLUDES} ${DEFINES} ${EFLAGS}
WPFLAGS=FR~C~V

LIBOBJ=sha_dgst.o sha1dgst.o sha_one.o sha1_one.o

export_hdrs:
       @|

export_libs: ${LIBOBJ}
       @|

clean:
       ${WIPE} o.* ${WPFLAGS}

.c.o:; ${CC} ${CFLAGS} -c $*.c
