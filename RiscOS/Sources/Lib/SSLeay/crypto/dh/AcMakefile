#
# SSLLeay/crypto/dh/Makefile
#
# RISC OS version.
#
.SUFFIXES: .c .o

DIR=dh

WIPE=wipe

CFLAGS=-I${INCLUDES} ${DEFINES} ${EFLAGS}
WPFLAGS=FR~C~V

ERR=dh
ERRC=dh_err

LIBOBJ= dh_gen.o dh_key.o dh_lib.o dh_check.o $(ERRC).o

export_hdrs:
       @|

export_libs: ${LIBOBJ}
       @|

clean:
       ${WIPE} o.* ${WPFLAGS}

.c.o:; ${CC} ${CFLAGS} -c $*.c
