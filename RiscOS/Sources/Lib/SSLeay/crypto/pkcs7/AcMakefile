#
# SSLLeay/crypto/pkcs7/Makefile
#
# RISC OS version.
#
.SUFFIXES: .c .o

DIR=pkcs7

WIPE=wipe

CFLAGS=-I${INCLUDES} ${DEFINES} ${EFLAGS}
WPFLAGS=FR~C~V

LIBOBJ= pk7_lib.o pkcs7err.o pk7_doit.o

export_hdrs:
       @|

export_libs: ${LIBOBJ}
       @|

clean:
       ${WIPE} o.* ${WPFLAGS}

.c.o:; ${CC} ${CFLAGS} -c $*.c
