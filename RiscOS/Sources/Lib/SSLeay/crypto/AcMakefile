#
# SSLLeay/crypto/Makefile
#
# RISC OS version.
#

.SUFFIXES: .c .o

WIPE=wipe

CFLAGS=-I${INCLUDES} ${DEFINES} ${EFLAGS}
WPFLAGS=FR~C~V

LIBDIR = <Lib$Dir>.SSLeay

LIBOBJ= cryptlib.o mem.o cversion.o

export_hdrs:
       @|

export_libs: ${LIBOBJ}
       @|

clean:
       ${WIPE} o.* ${WPFLAGS}

.c.o:; ${CC} ${CFLAGS} -c $*.c
