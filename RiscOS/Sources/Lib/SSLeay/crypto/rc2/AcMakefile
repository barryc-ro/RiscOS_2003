#
# SSLLeay/crypto/rc2/Makefile
#
# RISC OS version.
#
.SUFFIXES: .c .o

DIR=rc2

WIPE=wipe

CFLAGS=-I${INCLUDES} ${DEFINES} ${EFLAGS}
WPFLAGS=FR~C~V

ERR=bio
ERRC=bio_err

LIBOBJ=rc2_ecb.o rc2_skey.o rc2_cbc.o rc2cfb64.o rc2ofb64.o

export_hdrs:
       @|

export_libs: ${LIBOBJ}
       @|

clean:
       ${WIPE} o.* ${WPFLAGS}

.c.o:; ${CC} ${CFLAGS} -c $*.c
