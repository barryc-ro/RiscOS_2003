#
# SSLLeay/crypto/err/Makefile
#
# RISC OS version.
#
.SUFFIXES: .c .o

DIR=err

WIPE=wipe

CFLAGS=-I${INCLUDES} ${DEFINES} ${EFLAGS}
WPFLAGS=FR~C~V

LIBOBJ=err.o err_all.o err_prn.o

export_hdrs:
       @|

export_libs: ${LIBOBJ}
       @|

clean:
       ${WIPE} o.* ${WPFLAGS}

.c.o:; ${CC} ${CFLAGS} -c $*.c
