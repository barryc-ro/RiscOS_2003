#
# SSLLeay/crypto/md/Makefile
#
# RISC OS version.
#
.SUFFIXES: .c .o

DIR=md

WIPE=wipe

CFLAGS=-I${INCLUDES} ${DEFINES} ${EFLAGS}
WPFLAGS=FR~C~V

LIBOBJ=md2_dgst.o md5_dgst.o md2_one.o md5_one.o

export_hdrs:
       @|

export_libs: ${LIBOBJ}
       @|

clean:
       ${WIPE} o.* ${WPFLAGS}

.c.o:; ${CC} ${CFLAGS} -c $*.c
