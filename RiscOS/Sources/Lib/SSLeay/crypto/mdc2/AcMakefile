#
# SSLLeay/crypto/mdc2/Makefile
#
# RISC OS version.
#
.SUFFIXES: .c .o

DIR=mdc2

WIPE=wipe

CFLAGS=-I${INCLUDES} ${DEFINES} ${EFLAGS}
WPFLAGS=FR~C~V

LIBOBJ=mdc2dgst.o mdc2_one.o

export_hdrs:
       @|

export_libs: ${LIBOBJ}
       @|

clean:
       ${WIPE} o.* ${WPFLAGS}

.c.o:; ${CC} ${CFLAGS} -c $*.c
