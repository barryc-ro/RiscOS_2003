#
# SSLLeay/ssl/Makefile
#
# RISC OS version.
#
.SUFFIXES: .c .o

DIR=ssl

AR=libfile
WIPE=wipe

CFLAGS=-I${INCLUDES} ${DEFINES} ${EFLAGS}
WPFLAGS=FR~C~V

LIBDIR = <Lib$Dir>.SSLeay

ERR=ssl
ERRC=ssl_err

LIBOBJ= \
	s2_meth.o  s2_srvr.o  s2_clnt.o  s2_lib.o  s2_pkt.o s2_enc.o \
	s3_meth.o  s3_srvr.o  s3_clnt.o  s3_lib.o  s3_pkt.o s3_enc.o s3_both.o \
	s23_meth.o s23_srvr.o s23_clnt.o s23_lib.o s23_pkt.o \
	ssl_lib.o ssl_err2.o ssl_cert.o ssl_sess.o \
	ssl_ciph.o ssl_stat.o ssl_rsa.o \
	ssl_asn1.o ssl_txt.o ssl_algs.o \
	bio_ssl.o $(ERRC).o

export_hdrs:
       @|

export_libs: ${LIBOBJ}
       ${AR} -c -o ${LIBDIR}.o.${DIR} ${LIBOBJ}
       @echo SSL LIBRARY EXPORTED SUCCESSFULLY

clean:
       ${WIPE} o.* ${WPFLAGS}

.c.o:; ${CC} ${CFLAGS} -c $*.c
