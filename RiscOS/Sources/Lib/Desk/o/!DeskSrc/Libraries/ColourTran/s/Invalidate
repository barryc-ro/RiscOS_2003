        GET     Desk.sh.macros
        GET     Desk.sh.RegDefs
        GET     ^.h.SWInames
;
        PREAMBLE
        STARTCODE Desk_ColourTrans_InvalidateCache
;
        STMFD   sp!, {lr}
        SWI     XDesk_SWI_ColourTrans_InvalidateCache
        Desk_Error2_CheckV
        LDMFD   sp!, {pc}^
;
        END
