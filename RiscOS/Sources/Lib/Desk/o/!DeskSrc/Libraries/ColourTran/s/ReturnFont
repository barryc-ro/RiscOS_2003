;-------------------------------------------------------------------------------
; Desk_ColourTrans_ReturnFontColours(Desk_ct_fontblock);
; Copyright 1993 Shaun Blackmore

        GET     Desk.sh.macros
        GET     Desk.sh.RegDefs
        GET     ^.h.SWInames
;
        PREAMBLE
        STARTCODE Desk_ColourTrans_ReturnFontColours
;
        MOV     ip, sp
        STMFD   sp!, {r4, lr}
        MOV     r4, r0
        LDR     r0, [r4, #0]
        LDR     r1, [r4, #4]
        LDR     r2, [r4, #8]
        LDR     r3, [r4, #12]
        SWI     XDesk_SWI_ColourTrans_ReturnFontColours
        Desk_Error2_CheckV
        STR     r1, [r4, #4]
        STR     r2, [r4, #8]
        STR     r3, [r4, #12]
        LDMFD   sp!, {r4, pc}^
;
        END
