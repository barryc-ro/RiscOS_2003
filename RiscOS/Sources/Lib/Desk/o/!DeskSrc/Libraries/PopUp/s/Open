        GET     Desk.sh.regdefs
        GET     ^.h.swinos
        GET     Desk.sh.macros

; Desk_os_error *Desk_PopUp_Open(Desk_popup_block *params)

        PREAMBLE
        STARTCODE Desk_PopUp_Open
;
        STMFD   sp!, {lr}
        SWI     Desk_SWI_PopUp_Open + Desk_XOS_Bit
        Desk_Error2_CheckV
        LDMFD   sp!, {pc}^
;
        END
