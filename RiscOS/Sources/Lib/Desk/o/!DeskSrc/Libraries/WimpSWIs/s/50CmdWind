;       Author                 : John H. Winters
;
;       Function               : Opens a command window.
;
;
;       Modification history.
;       Date                   :  03 Apr 1993
;       Author                 :  Shaun Blackmore
;
;
;============================================================================
;
;  Include files.
;
;============================================================================
;
        GET     Desk.sh.regdefs
        GET     h.swinos
        GET     Desk.sh.macros
;
;============================================================================
;
;  Code.
;
;============================================================================
;
        PREAMBLE
        STARTCODE Desk_Wimp_CommandWindow
;
;        STMFD   sp!, {lr}
        MOV     ip, lr

        SWI     Desk_SWI_Wimp_CommandWindow ;+ Desk_XOS_Bit
        MOVVC   a1, #0

        MOVS    pc, ip
;        LDMFD   sp!, {pc}^
;
        END
