;       Author                 : John H. Winters
;
;       Function               : Reads a pixel translation ?
;
;
;       Modification history.
;
;       Version                : (Reflect in header IDENT)
;       Date                   :
;       Author                 :
;       Changes                :
;
;
;============================================================================
;
;  Include files.
;
;============================================================================
;
        GET     Desk.sh.regdefs
        GET     h.swinos
        GET     Desk.sh.macros
;
;============================================================================
;
;  Code.
;
;============================================================================
;
        PREAMBLE
        STARTCODE Desk_Wimp_ReadPixTrans
;
        MOV     ip, sp
        STMFD   sp!, {v1-v4, lr}
        LDR     v4, [ip, #0]
        MOV     v3, a4
        SWI     Desk_SWI_Wimp_ReadPixTrans + Desk_XOS_Bit
	Desk_Error2_CheckV
        LDMFD   sp!, {v1-v4, pc}^
;
        END
