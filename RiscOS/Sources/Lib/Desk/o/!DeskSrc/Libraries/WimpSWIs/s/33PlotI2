        GET     Desk.sh.regdefs
        GET     ^.h.swinos
        GET     Desk.sh.macros
	
	PREAMBLE
	
	STARTCODE Desk_Wimp_PlotIcon2
	
	STMFD	sp!, { r4, r5, lr}
       
        ;	; These are graphics origin when not in redraw loop.
        ;	; This info ommited from PRMs, but see example code
        ;	; in PRMs 5a-516
	MOV	r5, r2
	MOV	r4, r1
	
	MOV	r1, r0
	SWI     Desk_SWI_Wimp_PlotIcon
	MOVVC	r0, #0
	LDMFD   sp!, {r4, r5, pc}^
	
        END
