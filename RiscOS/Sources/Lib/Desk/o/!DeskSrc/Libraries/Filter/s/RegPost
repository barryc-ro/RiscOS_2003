        GET     Desk.sh.regdefs
        GET     ^.h.swinos
        GET     Desk.sh.macros

;        void Desk_Filter_RegisterPostFilter(char *FilterName, FilterHandler handler, int R12, int TaskHandle, int EventMask);

        PREAMBLE
        STARTCODE Desk_Filter_RegisterPostFilter
;
        MOV     ip, sp
        STMFD   sp!, {r4, lr}
        LDR     r4, [ip]
        SWI     Desk_SWI_Filter_RegisterPostFilter
        Desk_Error2_CheckV
        LDMFD   sp!, {r4, pc}^
;
        END
