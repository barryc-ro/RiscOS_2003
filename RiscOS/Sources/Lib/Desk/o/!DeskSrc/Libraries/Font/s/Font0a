        GET     Desk.sh.regdefs
        GET     ^.h.swinos
        GET     Desk.sh.macros

;Desk_os_error *Desk_Font_SetFont(FontHandle font);



        PREAMBLE
        STARTCODE Desk_Font_SetFont
;
        STMFD   sp!, {lr}
        SWI     Desk_SWI_Font_SetFont + Desk_XOS_Bit
        Desk_Error2_CheckV
        ;MOVVC   r0, #0
        LDMFD   sp!, {pc}^
;
        END
