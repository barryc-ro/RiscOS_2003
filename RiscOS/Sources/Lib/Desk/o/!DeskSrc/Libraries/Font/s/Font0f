        GET     Desk.sh.regdefs
        GET     ^.h.swinos
        GET     Desk.sh.macros

;Desk_os_error *Desk_Font_ReadScaleFactor(int *xscale, int *yscale);

        PREAMBLE
        STARTCODE Desk_Font_ReadScaleFactor
;
        STMFD   sp!, {r4, lr}
        MOV     r3, r0
        MOV     r4, r1
        SWI     Desk_SWI_Font_ReadScaleFactor + Desk_XOS_Bit
        Desk_Error2_CheckV
        STR     r1, [r3, #0]
        STR     r2, [r4, #0]
        ;MOVVC   r0, #0
        LDMFD   sp!, {r4, pc}^
;
        END
