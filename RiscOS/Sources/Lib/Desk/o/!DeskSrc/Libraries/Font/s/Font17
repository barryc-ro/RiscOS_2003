        GET     Desk.sh.regdefs
        GET     ^.h.swinos
        GET     Desk.sh.macros

;Desk_os_error *Desk_Font_StringBBox(char *string, FontInfo *info);


        PREAMBLE
        STARTCODE Desk_Font_StringBBox
;
        STMFD   sp!, {r4, r5, lr}
        MOV     r5, r1
        MOV     r1, r0
        SWI     Desk_SWI_Font_StringBBox + Desk_XOS_Bit
        Desk_Error2_CheckV
        STMIA   r5, {r1, r2, r3, r4}
        ;MOVVC   r0, #0
        LDMFD   sp!, {r4, r5, pc}^
        END
