        GET     Desk.sh.regdefs
        GET     ^.h.swinos
        GET     Desk.sh.macros

;Desk_os_error *Desk_Font_ListFonts(char *name, int *count);

        PREAMBLE
        STARTCODE Desk_Font_ListFonts
;
        STMFD   sp!, {r4, lr}
        MOV     r4, r1
        MOV     r3, #-1
        LDR     r2, [r4, #0]
        MOV     r1, r0
        SWI     Desk_SWI_Font_ListFonts + Desk_XOS_Bit
        Desk_Error2_CheckV
        STR     r2, [r4, #0]
        ;MOVVC   r0, #0
        LDMFD   sp!, {r4, pc}^
;
        END
