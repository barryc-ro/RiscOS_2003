        GET     Desk.sh.regdefs
        GET     ^.h.swinos
        GET     Desk.sh.macros

;Desk_os_error *Desk_Font_SetThresholds(FontThresholds *t);


        PREAMBLE
        STARTCODE Desk_Font_SetThresholds
;
        STMFD   sp!, {lr}
        MOV     r1, r0
        SWI     Desk_SWI_Font_SetThresholds + Desk_XOS_Bit
        Desk_Error2_CheckV
        ;MOVVC   r0, #0
        LDMFD   sp!, {pc}^
        END
