        GET     Desk.sh.regdefs
        GET     ^.h.swinos
        GET     Desk.sh.macros

;Desk_os_error *Desk_Font_ReadInfo(FontHandle font, FontInfo *info);

;       r0=font
;       r1=pointer to definition block

;       [r5, #0]  = min x
;       [r5, #4]  = min y
;       [r5, #8]  = max x
;       [r5, #12] = max y

        PREAMBLE
        STARTCODE Desk_Font_ReadInfo
;
        STMFD   sp!, {r4, r5, lr}
        MOV     r5, r1
        SWI     Desk_SWI_Font_ReadInfo + Desk_XOS_Bit
        Desk_Error2_CheckV
        STMIA   r5, {r1, r2, r3, r4}
        ;MOVVC   r0, #0
        LDMFD   sp!, {r4, r5, pc}^
;
        END
