        GET     Desk.sh.regdefs
        GET     ^.h.swinos
        GET     Desk.sh.macros

;Desk_os_error *Desk_Font_SetPalette(int Desk_back_log, int Desk_fore_log, int offset, int Desk_back_phys, int Desk_fore_phys);
;
; r0=Desk_back_log
; r1=Desk_fore_log
; r2=offset
; r3=Desk_back_phys
; [ip, #0]=Desk_fore_phys

        PREAMBLE
        STARTCODE Desk_Font_SetPalette
;
        MOV     ip, sp
        STMFD   sp!, {r4, r5, lr}
        LDR     r5, [ip, #0]
        MOV     r4, r3
        MOV     r3, r2
        MOV     r2, r1
        MOV     r1, r0
        SWI     Desk_SWI_Font_SetPalette + Desk_XOS_Bit
        Desk_Error2_CheckV
        ;MOVVC   r0, #0
        LDMFD   sp!, {r4, r5, pc}^
        END
