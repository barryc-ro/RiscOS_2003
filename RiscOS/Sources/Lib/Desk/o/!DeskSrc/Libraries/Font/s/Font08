        GET     Desk.sh.regdefs
        GET     ^.h.swinos
        GET     Desk.sh.macros

;Desk_os_error *Desk_Font_ConverttoOS(int x, int y, int *xout, int *yout)



        PREAMBLE
        STARTCODE Desk_Font_ConverttoOS
;
        STMFD   sp!, {r4, lr}
        MOV     r4, r3
        MOV     r3, r2
        MOV     r2, r1
        MOV     r1, r0
        SWI     Desk_SWI_Font_ConverttoOS + Desk_XOS_Bit
        Desk_Error2_CheckV
        STR     r1, [r3, #0]
        STR     r2, [r4, #0]
        ;MOVVC   r0, #0
        LDMFD   sp!, {r4, pc}^
;
        END
