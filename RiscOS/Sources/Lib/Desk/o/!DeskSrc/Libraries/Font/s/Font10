        GET     Desk.sh.regdefs
        GET     ^.h.swinos
        GET     Desk.sh.macros

;Desk_os_error *Desk_Font_SetScaleFactor(int xscale, int yscale);

        PREAMBLE
        STARTCODE Desk_Font_SetScaleFactor
;
        STMFD   sp!, {lr}
        MOV     r2, r1
        MOV     r1, r0
        SWI     Desk_SWI_Font_SetScaleFactor + Desk_XOS_Bit
        Desk_Error2_CheckV
        ;MOVVC   r0, #0
        LDMFD   sp!, {pc}^
;
        END
