        GET     Desk.sh.regdefs
        GET     ^.h.swinos
        GET     Desk.sh.macros

;Desk_os_error *Desk_Font_Caret(int colour, int height, int flags, int x, int y)



        PREAMBLE
        STARTCODE Desk_Font_Caret
;
        MOV     ip, sp
        STMFD   sp!, {r4, lr}
        LDR     r4, [ip, #0]
        SWI     Desk_SWI_Font_Caret + Desk_XOS_Bit
        Desk_Error2_CheckV
        ;MOVVC   r0, #0
        LDMFD   sp!, {r4, pc}^
;
        END
