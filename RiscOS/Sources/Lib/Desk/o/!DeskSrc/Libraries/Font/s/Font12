        GET     Desk.sh.regdefs
        GET     ^.h.swinos
        GET     Desk.sh.macros

;Desk_os_error *Desk_Font_SetFontColours(FontHandle font, int Desk_back_colour, int Desk_fore_colour, int offset);

        PREAMBLE
        STARTCODE Desk_Font_SetFontColours
;
        STMFD   sp!, {lr}
        SWI     Desk_SWI_Font_SetFontColours + Desk_XOS_Bit
        Desk_Error2_CheckV
        ;MOVVC   r0, #0
        LDMFD   sp!, {pc}^
        END
