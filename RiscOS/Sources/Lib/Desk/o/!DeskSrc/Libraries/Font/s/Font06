        GET     Desk.sh.regdefs
        GET     ^.h.swinos
        GET     Desk.sh.macros

;Desk_os_error *Desk_Font_Paint(char *, int options, int x, int y);

;   r0=char*s
;   r1=options
;   r2=x
;   r3=y

        PREAMBLE
        STARTCODE Desk_Font_Paint
;
        STMFD   sp!, {r4, lr}
        MOV     r4, r3
        MOV     r3, r2
        MOV     r2, r1
        MOV     r1, r0
        SWI     Desk_SWI_Font_Paint + Desk_XOS_Bit
        Desk_Error2_CheckV
        ;MOVVC   r0, #0
        LDMFD   sp!, {r4, pc}^
;
        END
