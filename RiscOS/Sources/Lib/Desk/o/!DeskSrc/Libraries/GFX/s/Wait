;-------------------------------------------------------------------------------
; Desk_GFX_Wait (void)
; by Jason Williams 1993
;
        GET     Desk.sh.Macros
        GET     Desk.sh.RegDefs
        GET     ^.h.SWINos
;
        PREAMBLE
        STARTCODE Desk_GFX_Wait
;
        STMFD   sp!, {lr}
        MOV     a1, #19
        SWI     Desk_XSWI_OS_Byte
        Desk_Error2_CheckV
        LDMFD   sp!, {pc}^
;
        END
