;-------------------------------------------------------------------------------
; Desk_OS_WriteN
;
        GET     Desk.sh.Macros
        GET     Desk.sh.RegDefs
        GET     ^.h.SWINos
;
        PREAMBLE
        STARTCODE Desk_GFX_WriteN
;
        STMFD   sp!, {lr}
        SWI     Desk_XSWI_OS_WriteN
        Desk_Error2_CheckV
        LDMFD   sp!, {pc}^
;
        END
