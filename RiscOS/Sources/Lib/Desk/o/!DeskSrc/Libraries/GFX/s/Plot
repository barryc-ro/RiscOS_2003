;-------------------------------------------------------------------------------
; Desk_GFX_Plot (int plotcode, int x, int y)
; by Jason Williams 1993
;
        GET     Desk.sh.Macros
        GET     Desk.sh.RegDefs
        GET     ^.h.SWINos
;
        PREAMBLE
        STARTCODE Desk_GFX_Plot
;
        STMFD   sp!, {lr}
        SWI     Desk_XSWI_OS_Plot
        Desk_Error2_CheckV
        LDMFD   sp!, {pc}^
;
        END
