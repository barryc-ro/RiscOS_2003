;-------------------------------------------------------------------------------
; Desk_GFX_CLG(void)
; by Jason Williams 1993
;
        GET     Desk.sh.Macros
        GET     Desk.sh.RegDefs
        GET     ^.h.SWINos
;
        PREAMBLE
        STARTCODE Desk_GFX_CLG
;
        STMFD   sp!, {lr}
        SWI     Desk_XSWI_OS_WriteI + 16
        Desk_Error2_CheckV
        LDMFD   sp!, {pc}^
;
        END
