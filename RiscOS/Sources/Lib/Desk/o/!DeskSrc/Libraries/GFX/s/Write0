;-------------------------------------------------------------------------------
; Desk_OS_Write0
;
        GET     Desk.sh.Macros
        GET     Desk.sh.RegDefs
        GET     ^.h.SWINos
;
        PREAMBLE
        STARTCODE Desk_GFX_Write0
;
        STMFD   sp!, {lr}
        SWI     Desk_XSWI_OS_Write0
        Desk_Error2_CheckV
        LDMFD   sp!, {pc}^
;
        END
