; Desk_backtr_sup.s
; Machine code support function for the backtrace() function
; (c) Paul Field 1995


        AREA |C$$code|, CODE, READONLY

        EXPORT  Desk_BackTrace_SupportCurrentFrame
Desk_BackTrace_SupportCurrentFrame
        STR      fp, [a1, #&18]
        STR      sp, [a1, #&1c]
        STR      sl, [a1, #&24]!
        MOVS     pc, lr

        END
