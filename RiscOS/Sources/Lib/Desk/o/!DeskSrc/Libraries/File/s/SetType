;   Version: 1.00 (11 Aug 1993)
;   Purpose: SWI veneer for file operations - set file type

	GET	Desk.sh.regdefs
	GET	h.swinos
	GET	Desk.sh.macros
;
	PREAMBLE
	STARTCODE Desk_File_SetType
;
; extern Desk_os_error *Desk_File_SetType(const char *filename, int);
;
	STMFD	sp!, {lr}

	MOV	a3, a2
	MOV	a2, a1
	MOV	a1, #18

	SWI	Desk_SWI_OS_File + Desk_XOS_Bit
	Desk_Error2_CheckV
	;MOVVC	a1, #0

	LDMFD	sp!, {pc}^
;
	END
