        GET     Desk.sh.regdefs
        GET     ^.h.swinos
        GET     Desk.sh.macros

; void Desk_DragASprite_Start(int flags, void *spritearea, char *name, Desk_wimp_box *bound, Desk_wimp_box *parent);

        PREAMBLE
        STARTCODE Desk_DragASprite_Start
;
        MOV     ip, sp
        STMFD   sp!, {r4, lr}
        LDR     r4, [ip, #0]
        SWI     Desk_SWI_DragASprite_Start + Desk_XOS_Bit
        Desk_Error2_CheckV
        ;MOVVC   r0, #0
        LDMFD   sp!, {r4, pc}^
;
        END
