        GET     Desk.sh.regdefs
        GET     ^.h.swinos
        GET     Desk.sh.macros

; void Desk_DragASprite_Stop(void);

        PREAMBLE
        STARTCODE Desk_DragASprite_Stop
;
        STMFD   sp!, {lr}
        SWI     Desk_SWI_DragASprite_Stop + Desk_XOS_Bit
        Desk_Error2_CheckV
        ;MOVVC   r0, #0
        LDMFD   sp!, {pc}^
;
        END
