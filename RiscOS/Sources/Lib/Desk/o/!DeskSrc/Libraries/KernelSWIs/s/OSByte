;   Purpose: Generic veneer to the Desk_OS_Byte SWI.

        GET     Desk.sh.regdefs
        GET     h.swinos
        GET     Desk.sh.macros

; Desk_os_error *Desk_OS_Byte(Desk_osbyte_number reason, int r1in, int r2in,
;                                         int *r1out, int *r2out);

        PREAMBLE
        STARTCODE Desk_OS_Byte
;
        MOV     ip, sp
        STMFD   sp!, {lr}
        SWI     Desk_SWI_OS_Byte + Desk_XOS_Bit
        Desk_Error2_CheckV
        ;MOVVC   r0, #0
        CMP     r3, #0
        STRNE   r1, [r3]
        LDR     r3, [ip]
        CMP     r3, #0
        STRNE   r2, [r3]
        LDMFD   sp!, {pc}^
;
        END
