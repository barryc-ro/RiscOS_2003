;
;       Title                  : Sprite 16U
;       System                 : Sprite Library
;       Version                : 1.0
;       Copyright              : (C) John Winters
;       Date                   : 9th February, 1992
;       Author                 : John H. Winters
;
;       Function               : Issues an Desk_OS_SpriteOp 16.
;
;
;       Modification history.
;
;       Version                : (Reflect in header IDENT)
;       Date                   :
;       Author                 :
;       Changes                :
;
;
;============================================================================
;
;  Include files.
;
;============================================================================
;
        GET     Desk.sh.regdefs
        GET     ^.h.swinos
        GET     Desk.sh.macros
;
;============================================================================
;
;  Code.
;
;============================================================================
;
        PREAMBLE
        STARTCODE Desk_Sprite_Get
;
        MOV     ip, sp
        STMFD   sp!, {a1, a2, a3, a4, v1, v2, v3, v4, v5, lr}
        LDMFD   sp!, {a2, a3, a4, v1}
        LDMFD   ip!, {v2, v3, v4}
        MOV     a1, #16
        ADD     a1, a1, #256
        SWI     Desk_OS_SpriteOp + Desk_XOS_Bit
        Desk_Error2_CheckV
        LDR   ip, [ip, #0]
        STR   a3, [ip, #0]
        MOV   a1, #0
        LDMFD   sp!, {v1, v2, v3, v4, v5, pc}^
;
        END
