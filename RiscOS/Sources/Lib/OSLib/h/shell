 * SWI names and SWI reason codes *
 **********************************/
#undef  Shell_Create
#define Shell_Create                            0x405C0
#undef  XShell_Create
#define XShell_Create                           0x605C0
#undef  Shell_Destroy
#define Shell_Destroy                           0x405C1
#undef  XShell_Destroy
#define XShell_Destroy                          0x605C1

/************************
 * Constant definitions *
 ************************/
