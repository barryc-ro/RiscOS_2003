FILES = ${HDRS} ${CHEADERS} ${LIBS} ${CSTRONG} CHelp o.OSLib

all: $(FILES)

release: ${FILES}
   Copy h          OSLib:h                   ~CFLNR~V
   Copy Types.h    OSLib:h.types             ~CFLNR~V
   Copy Macros.h   OSLib:h.macros            ~CFLNR~V
   Copy Hdr        OSLib:Hdr                 ~CFLNR~V
   Copy Types.Hdr  OSLib:Hdr.Types           ~CFLNR~V
   Copy Macros.Hdr OSLib:Hdr.Macros          ~CFLNR~V
   Copy o.OSLib    OSLib:o.OSLib             ~CFLNR~V
   Copy ReadMe     OSLib:ReadMe              ~CFLNR~V
   Copy ChangeLog  OSLib:ChangeLog           ~CFLNR~V
   Copy CStrong    <StrongHelp$Dir>.HelpData ~CFLNR~V

o.OSLib:
        libfile -c -o $@

#------------------------------------------------------------------------
# RISC OS Build targets:
#

export: export_${PHASE}

export_hdrs:
        @echo ${COMPONENT}: export complete (hdrs)

export_libs: ${HDRS} ${CHEADERS} ${LIBS} o.OSLib dirs
        Copy h          OSLib:h                   ~CFLNR~V
        Copy Types.h    OSLib:h.types             ~CFLNR~V
        Copy Macros.h   OSLib:h.macros             ~CFLNR~V
        Copy Hdr        OSLib:Hdr                 ~CFLNR~V
        Copy Types.Hdr  OSLib:Hdr.Types           ~CFLNR~V
        Copy Macros.Hdr OSLib:Hdr.Macros          ~CFLNR~V
        Copy o.OSLib    OSLib:o.OSLib             ~CFLNR~V
        @echo ${COMPONENT}: export complete (libs)

clean:
        @echo ${COMPONENT}: *** NOT cleaned *** clean locally if required

clean_all:
        ${WIPE} o.* ${WFLAGS}
        @echo ${COMPONENT}: cleaned

clean_lib: clean_all

dirs:
        ${MKDIR} l
        ${MKDIR} ${LIBDIR}
        ${MKDIR} ${LIBDIR}.OSLib
        ${MKDIR} ${LIBDIR}.OSLib.h
        ${MKDIR} ${LIBDIR}.OSLib.o

# End of RISC OS Build targets
#------------------------------------------------------------------------

cplusplusheaders: ${CPLUSPLUSHEADERS}
hdrs: ${HDRS}
libs: ${LIBS}
objasm: ${OBJASM}
cheaders: ${CHEADERS}
cstrong: ${CSTRONG}
examples: ${EXAMPLES}

clean_all: clean_h++ clean_chelp clean_o clean_s clean_h clean_hdr clean_asmhelp clean_cstrong

clean_h++:
   -Destroy ${CPLUSPLUSHEADERS}

clean_chelp:
   -Destroy ${CHELP}

clean_o:
   -Destroy ${LIBS}

clean_s:
   -Destroy ${OBJASM}

clean_h:
   -Destroy ${CHEADERS}

clean_hdr:
   -Destroy ${HDRS}

clean_asmhelp:
   -Destroy ${ASMHELP}

clean_cstrong:
   -Destroy ${CSTRONG}

.SUFFIXES: .o .h .h++ .def .ch .Hdr .ah .Examples .Stamps .Help

.def.s:
   DefMod -s -byte_wide ByteWide < $< > $@

.def.h:
   DefMod -h < $< > $@

.def.h++:
   DefMod -h++ < $< > $@

.def.Hdr:
   DefMod -hdr < $< > $@

.def.ch:
   DefMod -chelp < $< > $@

.def.ah:
   DefMod -asmhelp < $< > $@

.def.o:
   DefMod -l -o l.$* -byte_wide ByteWide < $<
   LibFile -c -o $@ -via ViaFile
   -LibFile -d o.OSLib l.$*.*
   LibFile -i -o o.OSLib -via ViaFile
   Wipe l.$* ~C~FR~V

.def.Help:
   DefMod -cstrong -o CStrong.$* < $<
   Create $@

.Examples.Stamps:
   cc -c -throwback -depend !Depend -IOS: -wp -fc -D__swi -o $@ $<
   Create $@ #Truncate file since we don't actually need it

CHelp: oC Types.CHelp Macros.CHelp ${CHELP}
   Kitten oC Types.CHelp Macros.CHelp ${CHELP} > CHelp
   SetType CHelp Text

AsmHelp: oAsm Types.AHelp Macros.AHelp ${ASMHELP}
   Kitten oAsm Types.AHelp Macros.AHelp ${ASMHELP} > AsmHelp
   SetType AsmHelp Text

