        [       :LNOT: :DEF: Get_Types
        GBLS    Get_Types
        ]
        [       :LNOT: :DEF: Types_Hdr
Get_Types                       SETS    "GET OS:Hdr.Types"
        |
Get_Types                       SETS    ""
        ]
        $Get_Types

        [       :LNOT: :DEF: Get_OS
        GBLS    Get_OS
        ]
        [       :LNOT: :DEF: OS_Hdr
Get_OS                          SETS    "GET OS:Hdr.OS"
        |
Get_OS                          SETS    ""
        ]
        $Get_OS

        [       :LNOT: :DEF: Get_Wimp
        GBLS    Get_Wimp
        ]
        [       :LNOT: :DEF: Wimp_Hdr
Get_Wimp                        SETS    "GET OS:Hdr.Wimp"
        |
Get_Wimp                        SETS    ""
        ]
        $Get_Wimp

        [       :LNOT: :DEF: Help_Hdr
        GBLS    Help_Hdr

;Symbols for constants
Message_HelpRequest             *       &502
Message_HelpReply               *       &503

;Symbols for structure offsets and sizes
                                ^       0
Help_MessageRequest_pos         #       OS_Coord
Help_MessageRequest_buttons     #       Bits
Help_MessageRequest_w           #       Wimp_W
Help_MessageRequest_i           #       Wimp_I
Help_MessageRequest             *       @

                                ^       0
Help_MessageReply_reply         #       236*Char
Help_MessageReply               *       @


        ]
        END
