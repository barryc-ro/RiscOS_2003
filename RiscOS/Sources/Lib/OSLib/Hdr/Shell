        [       :LNOT: :DEF: Get_Types
        GBLS    Get_Types
        ]
        [       :LNOT: :DEF: Types_Hdr
Get_Types                       SETS    "GET OS:Hdr.Types"
        |
Get_Types                       SETS    ""
        ]
        $Get_Types

        [       :LNOT: :DEF: Get_OS
        GBLS    Get_OS
        ]
        [       :LNOT: :DEF: OS_Hdr
Get_OS                          SETS    "GET OS:Hdr.OS"
        |
Get_OS                          SETS    ""
        ]
        $Get_OS

        [       :LNOT: :DEF: Shell_Hdr
        GBLS    Shell_Hdr

;Symbols for constants
Error_ShellCreation             *       &900
Error_ShellRemoval              *       &901

;Symbols for SWI's and SWI reason codes
XShell_Create                   *       &605C0
Shell_Create                    *       &405C0

XShell_Destroy                  *       &605C1
Shell_Destroy                   *       &405C1

        ]
        END
