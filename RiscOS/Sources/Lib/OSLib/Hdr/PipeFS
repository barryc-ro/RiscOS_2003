        [       :LNOT: :DEF: Get_Types
        GBLS    Get_Types
        ]
        [       :LNOT: :DEF: Types_Hdr
Get_Types                       SETS    "GET OS:Hdr.Types"
        |
Get_Types                       SETS    ""
        ]
        $Get_Types

        [       :LNOT: :DEF: Get_OS
        GBLS    Get_OS
        ]
        [       :LNOT: :DEF: OS_Hdr
Get_OS                          SETS    "GET OS:Hdr.OS"
        |
Get_OS                          SETS    ""
        ]
        $Get_OS

        [       :LNOT: :DEF: PipeFS_Hdr
        GBLS    PipeFS_Hdr

;Symbols for constants
Error_PipeFSNoBlocking          *       &12F00

;Symbols for SWI's and SWI reason codes
UpCall_SleepNoMore              *       &7
   ;Entry
   ;  R0 = &7
   ;  R1 = pollword (pointer to Int)

        ]
        END
