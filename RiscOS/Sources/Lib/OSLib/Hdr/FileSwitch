;Symbols for SWI's and SWI reason codes
XOS_BGet                        *       &2000A
OS_BGet                         *       &A
   ;Entry
   ;  R1 = file (OS_F)
   ;Exit
   ;  R0 = c (Char)

BGetV                           *       &A
   ;Entry
   ;  R1 = file (OS_F)
   ;  R9 = &A
   ;Exit
   ;  R0 = c (Char)

XOS_BPut                        *       &2000B
OS_BPut                         *       &B
   ;Entry
   ;  R0 = c (Char)
   ;  R1 = file (OS_F)

BPutV                           *       &B
   ;Entry
   ;  R0 = c (Char)
   ;  R1 = file (OS_F)
   ;  R9 = &B

Service_StartUpFS               *       &12
   ;Entry
   ;  R1 = &12
   ;  R2 = fs (Int)

Service_FSRedeclare             *       &40
   ;Entry
   ;  R1 = &40

Service_CloseFile               *       &68
   ;Entry
   ;  R1 = &68
   ;  R2 -> file_name (String)
   ;  R3 = count (Int)
   ;Exit
   ;  R3 = count_out (Int)

        ]
        END
