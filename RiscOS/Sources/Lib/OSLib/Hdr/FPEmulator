        [       :LNOT: :DEF: Get_Types
        GBLS    Get_Types
        ]
        [       :LNOT: :DEF: Types_Hdr
Get_Types                       SETS    "GET OS:Hdr.Types"
        |
Get_Types                       SETS    ""
        ]
        $Get_Types

        [       :LNOT: :DEF: Get_OS
        GBLS    Get_OS
        ]
        [       :LNOT: :DEF: OS_Hdr
Get_OS                          SETS    "GET OS:Hdr.OS"
        |
Get_OS                          SETS    ""
        ]
        $Get_OS

        [       :LNOT: :DEF: FPEmulator_Hdr
        GBLS    FPEmulator_Hdr

;Symbols for SWI's and SWI reason codes
XFPEmulator_Version             *       &60480
FPEmulator_Version              *       &40480
   ;Exit
   ;  R0 = version (Int)

        ]
        END
