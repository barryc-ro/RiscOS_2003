#ifndef unicode_iso10646_h
#define unicode_iso10646_h

typedef unsigned int UCS4;
typedef unsigned short UCS2;

#define NULL_UCS4 ((UCS4)0xFFFFFFFF)
#define NULL_UCS2 ((UCS2)0xFFFF)

#endif
