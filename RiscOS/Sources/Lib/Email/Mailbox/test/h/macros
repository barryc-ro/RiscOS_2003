#ifndef macros_h
#define macros_h

/* =============================================================================
 * Define macros
 */

#define M_min(a, b) ((a) > (b) ? (b) : (a))
#define M_max(a, b) ((a) > (b) ? (a) : (b))

#define M_unused(a) (void) (a)

#endif
