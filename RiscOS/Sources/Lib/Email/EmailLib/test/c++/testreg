extern "C"
{
  #include <stdio.h>
}
#include "../hdr_reg.h"
#include "../header.h"
#include "../from.h"
#include "../date.h"

int main()
{
  header header1(FALSE, 0, 0, 0, TRUE);
  date   date1(FALSE, 0, 0, 0, TRUE);
  from   from1(FALSE, 0, 0, 0, TRUE);
  
  hdr_registry::display_count();
  printf("Header NFE_HDR_TYPE: %d\n", header1.GetType());
  printf("Date   NFE_HDR_TYPE: %d\n", date1.GetType());
  printf("From   NFE_HDR_TYPE: %d\n", from1.GetType());
  
  return 0;  
}
