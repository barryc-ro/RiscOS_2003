/* ************************************************************************** */


/* ************************************************************************** */
/*                                                                            */
/* Globals to this file                                                       */
/*                                                                            */
/* ************************************************************************** */
EmailLibVersion* EmailLibVersion::_instance = 0;


/* ************************************************************************** */
/*                                                                            */
/* Object methods                                                             */
/*                                                                            */
/* ************************************************************************** */

EmailLibVersion::EmailLibVersion()
                :Version(lib_name,
                 Module_MajorVersion,
                 Module_MinorVersion,
                 Module_Version,
                 Module_Date)
{
}


Version* EmailLibVersion::Instance(void)
{
  if (_instance == 0)
    _instance = new EmailLibVersion();

  return _instance;
}
