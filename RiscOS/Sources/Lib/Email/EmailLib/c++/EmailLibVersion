#include "Version.h"
//#include "VersionNum" 		// use once we switch to srccommit


/* ************************************************************************** */
/*                                                                            */
/* Constants                                                                  */
/*                                                                            */
/* ************************************************************************** */

// Until we switch EmailLib to srccommit, define the tokens here
#define Module_MajorVersion_CMHG     	0.52
#define Module_MinorVersion_CMHG
#define Module_Date_CMHG      		11 Nov 1998
#define Module_MajorVersion     	"0.52"
#define Module_Version                  52
#define Module_MinorVersion		""
#define Module_Date      		"11 Nov 1998"
const char lib_name[] = "EmailLib Library";


/* ************************************************************************** */
/*                                                                            */
/* New enumerated types                                                       */
/*                                                                            */
/* ************************************************************************** */


/* ************************************************************************** */
/*                                                                            */
/* Function Prototypes - Private to this file                                 */
