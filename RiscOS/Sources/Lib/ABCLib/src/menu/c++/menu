// Acorn Base Class Library - menu.c++
//
//      Class implementation of the menu object.
//

#include "toolbox.h"
#include "menu.h"


Menu::Menu()
{
}

Menu::Menu( unsigned int flags, void *name_or_template )
{
    Create( flags, name_or_template );
}

Menu::Menu( ObjectId oid )
{
    Attach( oid );
}

Menu::~Menu()
{
}
