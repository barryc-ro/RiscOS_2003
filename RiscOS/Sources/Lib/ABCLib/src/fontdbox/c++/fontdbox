// Acorn Base Class Library - fontdbox.c++
//
//      Implementation of the FontDbox class.
//

#include "toolbox.h"
#include "fontdbox.h"


FontDbox::FontDbox()
{
}

FontDbox::FontDbox( unsigned int flags, void *name_or_template )
{
    Create( flags, name_or_template );
}

FontDbox::FontDbox( ObjectId oid )
{
    Attach( oid );
}

FontDbox::~FontDbox()
{
}
