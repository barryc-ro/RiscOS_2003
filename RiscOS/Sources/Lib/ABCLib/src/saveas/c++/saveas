// Acorn Base Class Library - saveas.c++
//
//      Implementation of the SaveAs class.
//

#include "toolbox.h"
#include "saveas.h"


SaveAs::SaveAs()
{
}

SaveAs::SaveAs( unsigned int flags, void *name_or_template )
{
    Create( flags, name_or_template );
}

SaveAs::SaveAs( ObjectId oid )
{
    Attach( oid );
}

SaveAs::~SaveAs()
{
}
