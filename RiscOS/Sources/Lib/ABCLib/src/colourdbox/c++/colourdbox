#include "toolbox.h"
#include "colourdbox.h"


ColourDbox::ColourDbox()
{
}

ColourDbox::ColourDbox( unsigned int flags, void *name_or_template )
{
    Create( flags, name_or_template );
}

ColourDbox::ColourDbox( ObjectId oid )
{
    Attach( oid );
}

ColourDbox::~ColourDbox()
{
}
