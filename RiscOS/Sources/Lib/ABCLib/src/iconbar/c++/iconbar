// Acorn Base Class Library - iconbar.c++
//
//      Implementation of the iconbar class.
//

#include "toolbox.h"
#include "iconbar.h"


Iconbar::Iconbar()
{
}

Iconbar::Iconbar( unsigned int flags, void *name_or_template )
{
    Create( flags, name_or_template );
}

Iconbar::Iconbar( ObjectId oid )
{
    Attach( oid );
}

Iconbar::~Iconbar()
{
}
