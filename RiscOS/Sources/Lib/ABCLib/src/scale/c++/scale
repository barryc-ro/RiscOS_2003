#include "toolbox.h"
#include "scale.h"


Scale::Scale()
{
}

Scale::Scale( unsigned int flags, void *name_or_template )
{
    Create( flags, name_or_template );
}

Scale::Scale( ObjectId oid )
{
    Attach( oid );
}

Scale::~Scale()
{
}
