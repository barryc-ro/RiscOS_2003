// Acorn BAse Class Library - fileinfo.c++
//
//      Implementation of the FileInfo class.
//

#include "toolbox.h"
#include "fileinfo.h"


FileInfo::FileInfo()
{
}

FileInfo::FileInfo( unsigned int flags, void *name_or_template )
{
    Create( flags, name_or_template );
}

FileInfo::FileInfo( ObjectId oid )
{
    Attach( oid );
}

FileInfo::~FileInfo()
{
}
