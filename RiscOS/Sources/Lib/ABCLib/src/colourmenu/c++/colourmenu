// Acorn Base Class Library - colourmenu.c++
//
//      Class implementation of the ColourMenu object.
//

#include "toolbox.h"
#include "colourmenu.h"


ColourMenu::ColourMenu()
{
}

ColourMenu::ColourMenu( unsigned int flags, void *name_or_template )
{
    Create( flags, name_or_template );
}

ColourMenu::ColourMenu( ObjectId oid )
{
    Attach( oid );
}

ColourMenu::~ColourMenu()
{
}
