// Acorn Base Class Library - quit.c++
//
//      Implementation of the Quit class.
//

#include "toolbox.h"
#include "quit.h"

#include "trace.h"


Quit::Quit()
{
}

Quit::Quit( unsigned int flags, void *name_or_template )
{
    Create( flags, name_or_template );
}

Quit::Quit( ObjectId oid )
{
    Attach( oid );
}

Quit::~Quit()
{
}
