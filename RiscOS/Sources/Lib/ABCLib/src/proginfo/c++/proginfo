// Acorn Base Class Library - proginfo.c++
//
//      Implementation of the ProgInfo class.
//

#include "toolbox.h"
#include "proginfo.h"


ProgInfo::ProgInfo()
{
}

ProgInfo::ProgInfo( unsigned int flags, void *name_or_template )
{
    Create( flags, name_or_template );
}

ProgInfo::ProgInfo( ObjectId oid )
{
    Attach( oid );
}

ProgInfo::~ProgInfo()
{
}
