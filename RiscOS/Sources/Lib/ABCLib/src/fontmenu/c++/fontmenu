// Acorn Base Class Library - fontmenu.c++
//
//      Class implementation of the FontMenu object.
//

#include "toolbox.h"
#include "fontmenu.h"


FontMenu::FontMenu()
{
}

FontMenu::FontMenu( unsigned int flags, void *name_or_template )
{
    Create( flags, name_or_template );
}

FontMenu::FontMenu( ObjectId oid )
{
    Attach( oid );
}

FontMenu::~FontMenu()
{
}
