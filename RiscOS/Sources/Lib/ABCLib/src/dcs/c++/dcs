// Acorn Base Class Library - dcs.h
//
//      Class implementation of the Discard/Cancel/Save object.
//

#include "toolbox.h"
#include "dcs.h"


DCS::DCS()
{
}

DCS::DCS( unsigned int flags, void *name_or_template )
{
    Create( flags, name_or_template );
}

DCS::DCS( ObjectId oid )
{
    Attach( oid );
}

DCS::~DCS()
{
}
