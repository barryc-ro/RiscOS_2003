        EXPORT  |__SIG_DFL|
        EXPORT  |__SIG_ERR|
        EXPORT  |__SIG_IGN|

|__SIG_DFL|     *       -1
|__SIG_ERR|     *       -2
|__SIG_IGN|     *       -3

        END
