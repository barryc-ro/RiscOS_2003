#include "error.h"
#include "nosup.h"

void NoSupport(char *type)
{
    Error("You have double clicked an %s file.\n\nUpgrading MovieFS may allow you to play these files.\n\nSee the ReadMe file for more information.", type);
}

void NoSupport2(char *type, char *form)
{
    Error("You have double clicked an %s file\nthat uses %s video compression.\n\nUpgrading MovieFS may allow you to play these files.\n\nSee the ReadMe file for more information.", type, form);
}
