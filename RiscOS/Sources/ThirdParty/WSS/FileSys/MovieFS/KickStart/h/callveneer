typedef void (*cv_t)(int *, int *, int);
static int callveneercode[] =
{
    0xE1A0C00D, 0xE92D4FF0, 0xE081C102, 0xE890000F, 0xE1A0E00F,
    0xE1A0F00C, 0xE8FD8FF0, 0xE5940014, 0xE5850014, 0xE5940018,
    0xE5850018, 0xE3A00000
 /* Equivalent to:
        MOV     R12,R13
        STMDB   R13!,{R4-R11,R14}
        ADD     R12,R1,R2,LSL #2
        LDMIA   R0,{R0-R3}
        MOV     R14,PC
        MOV     PC,R12
        LDMIA   R13!,{R4-R11,PC}^
        LDR     R0,[R4,#20]
        STR     R0,[R5,#20]
        LDR     R0,[R4,#24]
        STR     R0,[R5,#24]
        MOV     R0,#0
  */
};
static cv_t callveneer /*= (cv_t) callveneercode*/;
