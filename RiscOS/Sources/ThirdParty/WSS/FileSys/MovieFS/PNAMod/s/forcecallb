	GET h.RegNames

        AREA    |C$$code|, CODE, READONLY

        EXPORT  ForceCallBack
ForceCallBack
	; Called in privd mode.
        STMFD R13!,{R8-R9, R14}

        ; Drop to user mode
	CMP	PC,PC	; EQ => 32bit, NE=> 26bit
	MRSEQ	R8,cpsr
        MOVNE	R8,PC
        BIC	R9,R8,#3
	MSREQ	cpsr,R9
        TEQNEP	PC,R9

        ; Call a dummy SWI
        SWI 0x20042	; OS_ReadMonotonicTime

        ; Go back to supervisor mode
        SWI 0x16+(1<<17); OS_EnterOS

        ; Go back to original state
	CMP	PC,PC
        TEQNEP	PC,R8
	MSREQ	cpsr,R8
        MOV	R0,R0

        LDMFD R13!,{R8-R9, R14}
	MOV PC,R14
	END
