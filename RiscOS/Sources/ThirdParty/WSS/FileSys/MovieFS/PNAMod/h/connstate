#ifndef _CONNSTATE_
#define _CONNSTATE_

typedef enum
{
    CONN_STATE_NEW,
    CONN_STATE_RESOLVING,
    CONN_STATE_CONNECTING,
    CONN_STATE_PROXY,
    CONN_STATE_GREETING,
    CONN_STATE_CHALLENGING,
    CONN_STATE_REQUESTING,
    CONN_STATE_READY,
    CONN_STATE_FINISHED,
    CONN_STATE_CLOSED,
    CONN_STATE_FAILED
} conn_state_t;


#endif
