#ifndef _PQUEUE_
#define _PQUEUE_


typedef struct pqueue_s *pqueue_t;


pqueue_t PQueue(int);

void PQueueFree(pqueue_t);

void PQueuePush(pqueue_t, void *);

void *PQueuePop(pqueue_t);

void *PQueueLook(pqueue_t, int);

#endif
