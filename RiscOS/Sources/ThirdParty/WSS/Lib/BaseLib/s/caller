        AREA    |C$$code|, CODE, READONLY
|x$codeseg|

        EXPORT caller

caller ;(void)
        MOV R0, R11
        TEQ R0, #0
 [  {CONFIG}<>26 ; 32 bit
        MOVEQ PC, R14
 |
        MOVEQS PC, R14
 ]
        LDR R0, [R0, #-12]
        TEQ R0, #0
 [  {CONFIG}<>26 ; 32 bit
        MOVEQ PC, R14
 |
        MOVEQS PC, R14
 ]
        LDR R0, [R0]
        BIC R0, R0, #&3
        BIC R0, R0, #&FC000000
        SUB R0, R0, #16
        LDR R1, [R0]
        MOV R2, R1, LSR#8
        TEQ R2, #&FF0000
        SUBNE R0, R0, #4
        LDRNE R1, [R0]
        MOVNE R2, R1, LSR#8
        TEQNE R2, #&FF0000
        ANDEQ R1, R1, #&FF
        SUBEQ R0, R0, R1
        ADRNE R0, noname
 [  {CONFIG}<>26 ; 32 bit
        MOV PC, R14
 |
        MOVS PC, R14
 ]

noname
        DCB "<anonymous>"
        DCB 0

	END
