#ifndef _STREXT_
#define _STREXT_

typedef unsigned short unicode_t;

char *strdup(char *);

int ci_strcmp(char *, char *);

int ustrlen(unicode_t *);

int ustrcmp(unicode_t *, unicode_t *);

unicode_t *ustrdup(unicode_t *);

unicode_t *ustrrchr(unicode_t *, unicode_t);

#endif
