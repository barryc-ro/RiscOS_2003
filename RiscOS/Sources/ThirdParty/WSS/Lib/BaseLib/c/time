#include "kernel.h"
#include "swis.h"
#include "time.h"

int Time(void)
{
    _kernel_swi_regs regs;

    _kernel_swi(OS_ReadMonotonicTime, &regs, &regs);
    return (unsigned int) regs.r[0];
}
