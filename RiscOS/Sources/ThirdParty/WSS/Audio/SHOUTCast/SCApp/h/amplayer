#ifndef AMPLAYER_H

#define AMPLAYER_H

#define AMPlayer_Play                   0x052e00
#define AMPlayer_Stop                   0x052e01
#define AMPlayer_Pause                  0x052e02
#define AMPlayer_Locate                 0x052e03
#define AMPlayer_Info                   0x052e04
#define AMPlayer_Control                0x052e05
#define AMPlayer_Plugin                 0x052e06
#define AMPlayer_FileInfo               0x052e07
#define AMPlayer_StreamOpen             0x052e08
#define AMPlayer_StreamClose            0x052e09
#define AMPlayer_StreamGiveData         0x052e0a
#define AMPlayer_StreamInfo             0x052e0b
#define AMPlayer_MetaDataPollChange	0x052e0c
#define AMPlayer_MetaDataLookup         0x052e0d

#endif
