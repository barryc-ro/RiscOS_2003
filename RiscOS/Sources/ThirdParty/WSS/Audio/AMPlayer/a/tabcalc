;
; Code using FPE to build tables etc.
; (C) 1999 Thomas Olsson

#include "h.defs"

#include "a.asmhdr"

	AREA	|C$$code|, CODE, READONLY

        ; unops -------------------------

	FUNC tofpl			; R0=(fpl) F0
	STMFD R13!,{R14}
	CMFE F0,#1
	MOVGE R0,#&7FFFFFFF
 [ ATPCS32
	LDMGEFD R13!,{PC}
 |
	LDMGEFD R13!,{PC}^
 ]
	CNFE F0,#1
	MOVLT R0,#&80000000
 [ ATPCS32
	LDMLTFD R13!,{PC}
 |
	LDMLTFD R13!,{PC}^
 ]
	LDFD F1,dtf_sh31
	MUFD F0,F0,F1
	FIXDZ R0,F0
 [ ATPCS32
	LDMFD R13!,{PC}
 |
	LDMFD R13!,{PC}^
 ]
	FUNCEND

dtf_sh31 DCD &41E00000,0

	; Table setup FPE functions -----------------

	FUNC make_xmuls	; R0->mul
			; R1=exp
			; On exit:
			; R0=tofpl((mul/2^out_SH)*2^(exp/3))
	STMFD R13!,{R14}
	MOV R2,#1<<out_SH
	LDFD F0,[R0]
	FLTD F1,R2
	DVFD F0,F0,F1
	MVFD F1,#2
	FLTD F2,R1
	DVFD F2,F2,#3
	POWD F1,F1,F2
	MUFD F0,F0,F1
	BL tofpl
 [ ATPCS32
	LDMFD R13!,{PC}
 |
	LDMFD R13!,{PC}^
 ]
	FUNCEND

	FUNC make_gainpow	; R0=i
				; On exit:
				; R0=tofpl(2^(-.25*(i+210)-gain_SH-in_SH))
	STMFD R13!,{R14}
	MOV R1,#gain_SH +in_SH
	ADD R0,R0,#210
	RSB R0,R0,#0
	FLTD F0,R0
	MVFD F1,#4
	DVFD F0,F0,F1
	FLTD F1,R1
	SUFD F0,F0,F1
	MVFD F1,#2
	POWD F0,F1,F0
	BL tofpl
 [ ATPCS32
	LDMFD R13!,{PC}
 |
	LDMFD R13!,{PC}^
 ]
	FUNCEND

	FUNC make_ispow	; R0=i
			; On exit:
			; R0=tofpl(i^(4/3)/2^ispow_SH)
	STMFD R13!,{R14}
	MOV R1,#1<<ispow_SH
	LDFD F1,mip_const
	FLTD F0,R0
	POWD F0,F0,F1
	FLTD F1,R1
	DVFD F0,F0,F1
	BL tofpl
 [ ATPCS32
	LDMFD R13!,{PC}
 |
	LDMFD R13!,{PC}^
 ]
	FUNCEND

mip_const DCD &3FF55555,&55555556	; 4/3

	FUNC make_aa_cs	; R0->Ci
			; On exit:
			; R0=tofpl(1/sqrt(1+Ci^2))
	STMFD R13!,{R14}
	LDFD F0,[R0]
	MUFD F0,F0,F0
	ADFD F0,F0,#1
	SQTD F0,F0
	RDFD F0,F0,#1
	BL tofpl
 [ ATPCS32
	LDMFD R13!,{PC}
 |
	LDMFD R13!,{PC}^
 ]
	FUNCEND

	FUNC make_aa_ca	; R0->Ci
			; On exit:
			; R0=tofpl(Ci/sqrt(1+Ci^2))
	STMFD R13!,{R14}
	LDFD F0,[R0]
	MUFD F1,F0,F0
	ADFD F1,F1,#1
	SQTD F1,F1
	DVFD F0,F0,F1
	BL tofpl
 [ ATPCS32
	LDMFD R13!,{PC}
 |
	LDMFD R13!,{PC}^
 ]
	FUNCEND

	FUNC make_win	; R0=i1
			; R1=i2
			; On exit:
			; R0=tofpl(.5*sin(i1*pi/72)/cos(i2*pi/72)/(2^win_SH))
	STMFD R13!,{R14}
	MOV R2,#1<<win_SH
	FLTD F0,R0
	FLTD F1,R1
	LDFD F2,pi72
	MUFD F0,F0,F2
	MUFD F1,F1,F2
	SIND F0,F0
	COSD F1,F1
	DVFD F0,F0,F1
	MUFD F0,F0,#0.5
	FLTD F1,R2
	DVFD F0,F0,F1
	BL tofpl
 [ ATPCS32
	LDMFD R13!,{PC}
 |
	LDMFD R13!,{PC}^
 ]
	FUNCEND

pi72	DCD &3FA65718,&4AE75511	; PI/72

	FUNC make_cos9	; R0=i
			; On exit:
			; R0=tofpl(cos(i*pi/18))
	STMFD R13!,{R14}
	FLTD F0,R0
	LDFD F1,pi18
	MUFD F0,F0,F1
	COSD F0,F0
	BL tofpl
 [ ATPCS32
	LDMFD R13!,{PC}
 |
	LDMFD R13!,{PC}^
 ]
	FUNCEND

pi18	DCD &3FC65718,&4AE75511	; PI/18

	FUNC make_cos36	; R0=i
			; R1=shift
			; On exit:
			; R0=tofpl(.5/cos(i*pi/36)/2^shift)
	STMFD R13!,{R14}
	MOV R2,#1
	MOV R1,R2,LSL R1
	FLTD F0,R0
	LDFD F1,pi36
	MUFD F0,F0,F1
	COSD F0,F0
	RDFD F0,F0,#0.5
	FLTD F1,R1
	DVFD F0,F0,F1
	BL tofpl
 [ ATPCS32
	LDMFD R13!,{PC}
 |
	LDMFD R13!,{PC}^
 ]
	FUNCEND

pi36	DCD &3FB65718,&4AE75511	; PI/36

	FUNC set_tan	; R0=i
			; R1=&tan
			; R2=&tans
			; set tan=tan(i*pi/12) and tans=2^pow_SH
	STMFD R13!,{R14}
	FLTD F0,R0
	LDFD F1,pi12
	MUFD F0,F0,F1
	TAND F0,F0
	STFD F0,[R1]
	MOV R0,#1<<pow_SH
	FLTD F0,R0
	STFD F0,[R2]
 [ ATPCS32
	LDMFD R13!,{PC}
 |
	LDMFD R13!,{PC}^
 ]

pi12	DCD  &3FD0C152,&382D6148	; PI/12

	FUNC make_tan11	; R0=&tan
			; R1=&tans
			; Returns: R0=tofpl(tan/(1+tan)/tans)
	STMFD R13!,{R14}
	LDFD F0,[R0]
	ADFD F1,F0,#1
	DVFD F0,F0,F1
	LDFD F1,[R1]
	DVFD F0,F0,F1
	BL tofpl
 [ ATPCS32
	LDMFD R13!,{PC}
 |
	LDMFD R13!,{PC}^
 ]
	FUNCEND

	FUNC make_tan21	; R0=&tan
			; R1=&tans
			; Returns R0=tofpl(1/(1+tan)/tans)
	STMFD R13!,{R14}
	LDFD F0,[R0]
	ADFD F0,F0,#1
	RDFD F0,F0,#1
	LDFD F1,[R1]
	DVFD F0,F0,F1
	BL tofpl
 [ ATPCS32
	LDMFD R13!,{PC}
 |
	LDMFD R13!,{PC}^
 ]
	FUNCEND

	FUNC make_tan12	; R0=&tan
			; R1=&tans
			; Returns R0=tofpl(sqr2*tan/(1+tan)/tans)
	STMFD R13!,{R14}
	LDFD F0,[R0]
	ADFD F1,F0,#1
	DVFD F0,F0,F1
	LDFD F1,sqr2
	MUFD F0,F0,F1
	LDFD F1,[R1]
	DVFD F0,F0,F1
	BL tofpl
 [ ATPCS32
	LDMFD R13!,{PC}
 |
	LDMFD R13!,{PC}^
 ]
	FUNCEND

	FUNC make_tan22	; R0=&tan
			; R1=&tans
			; Returns R0=tofpl(sqr2/(1+tan)/tans)
	STMFD R13!,{R14}
	LDFD F0,[R0]
	ADFD F0,F0,#1
	LDFD F1,sqr2
	DVFD F0,F1,F0
	LDFD F1,[R1]
	DVFD F0,F0,F1
	BL tofpl
 [ ATPCS32
	LDMFD R13!,{PC}
 |
	LDMFD R13!,{PC}^
 ]
	FUNCEND

sqr2	DCD &3FF6A09E,&667EFD46 ; SQR(2)

	FUNC make_pow	; R0=i
	                ; R1=x
			; R0=tofpl(2^(i/x-pow_SH))

	STMFD R13!,{R14}
	FLTD F0,R0
	FLTD F1,R1
	DVFD F0,F0,F1
	SUFD F0,F0,#pow_SH
	RPWD F0,F0,#2
	BL tofpl
 [ ATPCS32
	LDMFD R13!,{PC}
 |
	LDMFD R13!,{PC}^
 ]
	FUNCEND

	FUNC make_costab	; R0=i
				; R1=div
				; On exit:
				; R0=tofpl(.5/cos(i*pi/div)/2^pnts_SH)
	STMFD R13!,{R14}
	FLTD F0,R0
	LDFD F1,pi
	MUFD F0,F0,F1
	FLTD F1,R1
	DVFD F0,F0,F1
	COSD F0,F0
	RDFD F0,F0,#0.5
	MOV R0,#1<<pnts_SH
	FLTD F1,R0
	DVFD F0,F0,F1
	BL tofpl
 [ ATPCS32
	LDMFD R13!,{PC}
 |
	LDMFD R13!,{PC}^
 ]
	FUNCEND

pi	DCD &400921FB,&5444845D	; PI


	END
