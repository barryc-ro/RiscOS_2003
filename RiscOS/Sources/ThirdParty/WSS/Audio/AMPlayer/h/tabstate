#ifndef TABSTATE_H

#define TABSTATE_H

extern void CONST_st_active(void);
extern void CONST_st_playing(void);
extern void CONST_statename(void);

#define st_active(A)	((int)(((char *)CONST_st_active)[(A)]))
#define st_playing(A)	((int)(((char *)CONST_st_playing)[(A)]))
#define statename	((char **)CONST_statename)

#endif
