#ifndef TABLES2_H

#define TABLES2_H

/* First we define pointers to constant data as function pointers */
extern void CONST_table2(void);
extern void CONST_sblims(void);

/* Then some aliases to make them usable */
#define table2	((unsigned char **)CONST_table2)
#define sblims	((unsigned char *)CONST_sblims)

#endif
