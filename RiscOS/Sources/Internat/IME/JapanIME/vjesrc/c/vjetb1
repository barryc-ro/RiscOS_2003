CONST UCHAR	fzk_n[] ={
	__N, 0xc0, NN, SHUUSHIKEI,					/* n */
	__N, 0x81, NU, SHUU_TAI,						/* n */
	/*	_TYOUON, 0x80, TYOUON, SHUUSHIKEI,*/		/* - */
	0xff
};
CONST UCHAR	fzk_hy[] ={
	_KUTOUTEN, 0x80, KUTOUTEN, SHUUSHIKEI,			/**/
	0xff
};


CONST UCHAR	*fzk_ent[] ={
	/* _ a _ i _ u _ _ _ o 0x00-> 0x0b */
	0, 0, 0, fzk_a, 0, fzk_i, 0, fzk_u, 0, 0, 0, fzk_o,
	/* ka ga ki _ ku gu ke _ ko go 0x0c-> 0x15 */
	fzk_ka, fzk_ga, fzk_ki, 0, fzk_ku, fzk_gu, fzk_ke, 0, fzk_ko, fzk_go,
	/* sa za si zi su zu se ze so zo 0x16-> 0x1f */
	fzk_sa, fzk_za, fzk_si, fzk_zi, fzk_su, fzk_zu, fzk_se, fzk_ze, fzk_so, fzk_zo,
	/* ta da ti _ xtu tu du te de to do 0x20-> 0x2a */ /* xtu */
	fzk_ta, fzk_da, fzk_ti, 0, fzk_xtu, fzk_tu, fzk_du, fzk_te, fzk_de, fzk_to, fzk_do,
	/* na ni nu ne no 0x2b-> 0x2f */
	fzk_na, fzk_ni, fzk_nu, fzk_ne, fzk_no,
	/* ha ba _ _ _ _ _ _ _ he be _ ho _ _  0x30-> 0x3e */
	fzk_ha, fzk_ba, 0, 0, 0, 0, 0, 0, 0, fzk_he, fzk_be, 0, fzk_ho, 0, 0,
	/* ma mi _ _ mo 0x3f-> 0x43 */
	fzk_ma, fzk_mi, 0, 0, fzk_mo,
	/* _ ya _ yu_ yo 0x44-> 0x49 */
	0, fzk_ya, 0, fzk_yu, 0, fzk_yo,
	/* ra _ ru re _ 0x4a-> 0x4e */
	fzk_ra, 0, fzk_ru, fzk_re, 0,
	/* _ wa _ _ wo n _ - _ _ _ 0x4f-> 0x5f */
	0, fzk_wa, 0, 0, fzk_wo, fzk_n, 0, fzk_hy, 0, 0, 0, 0, 0, 0, 0, 0, 0
};

