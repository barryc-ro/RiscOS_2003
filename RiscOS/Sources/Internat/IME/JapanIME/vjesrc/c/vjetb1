/*
 *			comment
 *
 *	+-+-+-+-+-+-+-+-+
 *	+7|6|5|4|3|2|1|0|
 *	+-+-+-+-+-+-+-+-+
 * +- katuyou sinai           NO_FUZOKU
 * +-katuyou-+-gobi hissu -+- 
 *           |             +- RENYOU_TAIGEN
 *           +-nakutemo ok-+-meishi-- GOKAN_TAIGEN
 *                         +- ha X -- GOKANKEI
 *                                    GOKAN_IIKIRI
 *                                    GOKAN_MIZEN
 *                                    GOKAN_JIRITU
 *	7: NO_FUZOKU fuzokugo shori wo sinai 
 *     tankanji/settougo/rentaishi
 *	6: GOKAN_TAIGEN katuyousuruga gokan ga taigen ni naru (jhosuushi/setubi etc.)
 *	5: GOKAN_MIZEN gokan ga mizenkei/renyoukei ni naru (1dan doushi nomi ex. ukeru)
 *	4: HONSHOKU_NOKOSU honsoku mode de shoukyo sinai
 *	3: GOKANKEI gokan wo [gokan] toiu sikaku de fuzokugo setuzoku shori wo suru
 *	   (keiyoushi/keidou/fukushi (ex. utukusii : jhodoushi [souda] ni tunagaru)
 *	2: not used
 *     GOKAN_JIRITU fuzokugo ga nakutemo gokan nomi de siyou dekiru
 *	   (ex. fukushi : bit 6 tono tigai : bit 2 ha gokan nomi de shiyou
 *	    dekiruga kaku jhoshi ha setuzoku sinai)
 *	1: GOKAN_IIKIRI gokan nomi de iikiri ga dekiru (gokan no katati de bunmatu kurukotoga dekiru)
 *	   ex. keiyoudoushi/itibu no keiyoushi
 *	0: RENYOU_TAIGEN renyoukei ga meishi ni naru doushi
 */
/* 1f C1_MEISHI -> C1_JHOSUU hitori/sari 93/6/8 */
CONST JITBL jitb[] ={
/*  WORD_CNJ              FLAG   FZK_CNJ   GOBI_TBL  GOBI_GRP */
	C1_MEISHI, C2_SMEISHI,0x42,I_NSAHEN, G_NSAHEN, 0x01,	/* 0: GAKUSHUU */
	C1_KEIYOU, C2_KEIYOU, 0x08, I_KEIYOU, G_KEIYOU, 0x01,	/* 1: KEIYOUSHI SA */
