/*
 *			Comment
 *	              doushi keiyou keidou sahen kahen
 *	15: gokan
 *	14: taigen
 *	13: meireikei
 *	12: kateikei
 *	11: rentaikei        i/ki
 *	10: shuushikei
 *	 9: renyoukei        ku     de
 *	 8: mizenkei         karo   daro   si
 *	 7: miznekei2        kara   nara
 *	 6: rentaikei2       karu
 *	 5:
 *	 4:
 *	 3: o tuki doushi
 *	 2: soku_onbin   xtu                       henkei3
 *	 1: hatu_onbin   n   xyuu/u  ni            henkei2
 *	 0: i_onbin      i   kaxtu   daxtu  se     henkei1
 */
CONST USHORT	kata_tb[] ={
	0x0000, 0x0100, 0x0200, 0x0400, 0x0800, 0x1000, 0x0000, 0x4000, /*0-7*/
	0x8000, 0x0001, 0x0002, 0x0004, 0x0600, 0x8400, 0x0080, 0x0202, /*8-15*/
	0xffff, 0x0300, 0x1100, 0x2100, 0x1200, 0x2200, 0x0201, 0x0000, /*16-23*/
	0x0204, 0x4200, 0x1600, 0x1400, 0x2400, 0x0c00, 0x8800, 0x3000, /*24-31*/
	0x1001, 0x1004, 0x2000, 0x1002, 0x0101, 0x1080, 0x0040, 0x0840, /*32-39*/
	0x0440, 0x0401, 0x0402,	0x8200, 0x0500, 0x0008, 0x0e00, 0x1201, /* 40->47*/
	0x0108,	0x8202,	0x8600 /* 48 -> 53 */
};
/* chimei+chimei wo katensuruto "ienonaka"ga gohenkansareru 93/6/1*/
/*
 *		Comment
 *
 *	rentaishi             jhosuushi      suushi           settou(meishi)
 *	settou(sahen)         settou(kd)     1ji meishi nfzk  settou(chimei)
 *	settou(jinmei)        settou(o)      settou(go)       setubi
 *	3 ji meishi nfzk      chimei         sei              namae
 *	kansuu dai+nn         kansuu dai     kansuu(yaku etc) meishi fzk ha/no
 *	2ji meishi nfzk       doushi(renyou) yougen(renyou)   rentaikei
 *  4ji ijyou meishi nfzk kuten tuki     keiyou           keiyou doushi
 *  doushi                fukushi        meishi fzk       gakushuu
 */
CONST char	cnjtb_b[] ={
	/* 00 -> JHOSUUSHI */
	15 *0x10 + 1 , 5 *0x10 +15, /*RENTAI,     JHOSUU,    SUUSHI,     SET/MEISHI */  
	15 *0x10 +15 ,15 *0x10 +15, /*SET/SAHEN,  SET/KDOU,  MEISHI1     SET/CHI    */  
	15 *0x10 +15 ,15 *0x10 +15, /*SET/JIN,    SET/O,     SET/GO,     SETUBI     */  
	15 *0x10 +15 ,15 *0x10 +15, /*MEISHI3     CHIMEI,    SEI,        NAMAE      */  
	 5 *0x10 +15 ,15 *0x10 +15, /*KANSUU_O,   KANSUU_DAI KANSHUUSHI  MEISHI/FZK/HA */  
	15 *0x10 +15 ,15 *0x10 +15, /*MEISHI2,    DOU/YOU,   RENYOU,    , RENTAIKEI  */  
	15 *0x10 +15 ,15 *0x10 +15, /*MEISHI_NFZK,KUTEN,     KEIYOU,     KEIDOU     */  
	15 *0x10 +15 ,15 *0x10 +15, /*DOUSHI,     FUKUSHI,   MEISHI/FZK             */  
	/* 01 SUUSHI */
	 2 *0x10 + 1 , 3 *0x10 +15, /*RENTAI,     JHOSUU,    SUUSHI,     SET/MEISHI */    
	15 *0x10 +15 , 0 *0x10 +15, /*SET/SAHEN,  SET/KDOU,  MEISHI1     SET/CHI    */    
	 0 *0x10 + 0 , 0 *0x10 + 0, /*SET/JIN,    SET/O,     SET/GO,     SETUBI     */    
	 1 *0x10 + 0 , 0 *0x10 + 0, /*MEISHI3     CHIMEI,    SEI,        NAMAE      */    
	 4 *0x10 + 5 , 5 *0x10 + 1, /*KANSUU_O,   KANSUU_DAI KANSHUUSHI  MEISHI/FZK/HA */ 
	 1 *0x10 + 0 , 0 *0x10 + 1, /*MEISHI2,    DOU/YOU,   RENYOU,    , RENTAIKEI  */    
	 0 *0x10 + 0 , 0 *0x10 + 0, /*MEISHI_NFZK,KUTEN,     KEIYOU,     KEIDOU     */    
	 0 *0x10 + 2 , 0 *0x10 + 0, /*DOUSHI,     FUKUSHI,   MEISHI/FZK             */    
	/* 02 SETTOU */
	 2 *0x10 + 0 , 0 *0x10 + 0, /*RENTAI,     JHOSUU,    SUUSHI,     SET/MEISHI */   
	 0 *0x10 + 0 , 0 *0x10 + 0, /*SET/SAHEN,  SET/KDOU,  MEISHI1     SET/CHI    */   
	 0 *0x10 + 0 , 0 *0x10 + 0, /*SET/JIN,    SET/O,     SET/GO,     SETUBI     */   
	 0 *0x10 + 0 , 0 *0x10 + 0, /*MEISHI3     CHIMEI,    SEI,        NAMAE      */   
	 0 *0x10 + 0 , 0 *0x10 + 1, /*KANSUU_O,   KANSUU_DAI KANSHUUSHI  MEISHI/FZK/HA */
	 1 *0x10 + 0 , 0 *0x10 + 1, /*MEISHI2,    DOU/YOU,   RENYOU,    , RENTAIKEI  */   
	 0 *0x10 + 0 , 0 *0x10 + 0, /*MEISHI_NFZK,KUTEN,     KEIYOU,     KEIDOU     */   
	 0 *0x10 + 1 , 0 *0x10 + 0, /*DOUSHI,     FUKUSHI,   MEISHI/FZK             */   
	/* 03 SETUBI (JINMEI+) */
