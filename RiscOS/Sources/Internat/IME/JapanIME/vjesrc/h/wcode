#ifdef CODEINPUT
#if TAIWAN
#define CHANG_CODE_ORG		0xa140	/* Start cord BIG5 */
#define CHANG_CODE_SHI		0xa1	/* Start high code BIG5 */
#define CHANG_CODE_SLOW		0x40	/* Start low code BIG5 */
#define CHANG_CODE_ELOW		0xfe	/* End low code BIG5 */

#define CHANG_BREAK_CODE1	0x7e
#define CHANG_BREAK_CODE2	0xa1

#else
#if CHINA
#define CHANG_CODE_ORG		0xa1a1	/* Start cord GB */
#define CHANG_CODE_SHI		0xa1	/* Start high code GB */
#define CHANG_CODE_SLOW		0xa1	/* Start low code GB */
#define CHANG_CODE_ELOW		0xfe	/* End low code GB */
#else
#define CHANG_CODE_ORG		0x2121
#define CHANG_CODE_SHI		0x21
#define CHANG_CODE_SLOW		0x21
#define CHANG_CODE_ELOW		0x7e
#endif
#endif
#endif /* CODEINPUT */
