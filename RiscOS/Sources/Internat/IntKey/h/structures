#include "Unicode/iso10646.h"

#define MAXKEYS 0x80
#define MAXLAYERS 2

typedef struct KeyInfo
{
    UCS4 character[MAXLAYERS][8];
    unsigned caps[MAXLAYERS];
    char defined;
    char definedinlayer[MAXLAYERS];
    char easych[MAXLAYERS][8];
    char easylayer[MAXLAYERS];
    char easy;
}
KeyInfo;

typedef struct Keyboard
{
    int country;
    int layers;
    int numkeys;
    KeyInfo key[MAXKEYS];
    char keypad[2][17];
    char custompad;
    char needcodetable;
}
Keyboard;
