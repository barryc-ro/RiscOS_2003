; > Source.IntKeyMod

                GBLS    GetOther
 [ Keyboard <> "All" :LAND: Keyboard <> "Scandinavian" :LAND: Keyboard <> "UK"
GetOther        SETS    "GET    s.$Keyboard"
 |
GetOther        SETS    ""
 ]

 [ Keyboard = "Sweden" :LOR: Keyboard = "Finland"
GetOther        SETS    "GET    s.Swed-Finl"
 ]

        GET     Source.IntKeyBody
        GET     Source.Accents
        GET     Source.KeyStruct
        GET     s.UK
        $GetOther
 [ Keyboard = "Scandinavian"
        GET     s.Denmark
        GET     s.Norway
        GET     s.Swed-Finl
 ]

        GBLS    GetX
 [ Keyboard = "All"
GetX    SETS    "GET"
 |
GetX    SETS    ";"
 ]
        $GetX   s.Italy
        $GetX   s.Spain
        $GetX   s.France
        $GetX   s.Germany
        $GetX   s.Esperanto
        $GetX   s.Greece
        $GetX   s.Swed-Finl
        $GetX   s.Denmark
        $GetX   s.Norway
        $GetX   s.Iceland
        ; $GetX   s.Canada
        $GetX   s.Turkey
        $GetX   s.Russia
        ; $GetX   s.Russia2
        $GetX   s.Israel
        $GetX   s.LatinAm
        $GetX   s.Belgium
        $GetX   s.Netherlands
        $GetX   s.Japan
        $GetX   s.Switzerland
        $GetX   s.Wales
        $GetX   s.USA
        $GetX   s.DvorakUK
        $GetX   s.DvorakUSA

        END
