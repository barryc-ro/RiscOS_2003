/* File:        cistrcmp.h 
 * Purpose:     case-insensitive string comparison
 * Author:      Ian Johnson
 * History:     5-Aug-93: IDJ: created
 */

#ifndef __cistrcmp_h
#define __cistrcmp_h

extern int cistrcmp(const char *s1, const char *s2);

/*
 * Description:  compares 2 strings with no regard for case.
 *
 * Parameters:   const char *s1 - string number 1
 *               const char *s2 - string number 2
 *
 * Returns:      0 means same, > 0 means s1 > s2, < 0 means s1 < s2
 *
 */

#endif
