# Project:   wimplib


# Toolflags:
CCflags = -c -depend !Depend -throwback -IC:
FLEXflags = -c
C++flags = -c -depend !Depend -throwback -IC:
Pascalflags = -c -depend !Depend -throwback -IP:
Linkflags = -c++ -o $@
ObjAsmflags = -depend !Depend -ThrowBack -Stamp -quit -CloseExec
CMHGflags = 
LibFileflags = -c -o $@
Squeezeflags = -o $@
AAsmflags = -depend !Depend -quit -CloseExec -To $@ -From


# Final targets:
@.wimplib:   @.o.winit @.o.wcreatew @.o.wcreatei @.o.wdeletew @.o.wdeletei \
        @.o.wclosew @.o.wopenw @.o.wpoll @.o.wgetrec @.o.wredraww @.o.wupdatew @.o.wdragbox \
        @.o.wgeticst @.o.wgetpoi @.o.wgetwinf @.o.wgetwst @.o.wseticst @.o.wforcere @.o.wsetcarp \
        @.o.wgetcarp @.o.wcreatem @.o.wcreatesm @.o.wdecodem @.o.wwhichi @.o.wsetext @.o.wsetpos \
        @.o.wopente @.o.wclosete @.o.wloadte @.o.wclosedown @.o.wstartt @.o.wreporte @.o.wgetwino \
        @.o.wpollidle @.o.wplotic @.o.wsetmode @.o.wsetpal @.o.wreadpal @.o.wsetcol @.o.wsendm \
        @.o.wtextop @.o.wprocessk @.o.!!version @.o.wspriteop @.o.wbasesp @.o.wblockcpy \
        @.o.wslotsize @.o.wtextcol @.o.wcommw @.o.wtransbl @.o.wsysinfo @.o.wresizeic @.o.wsetfon \
        @.o.wgetmenu @.o.waddmsg @.o.wremmsg @.o.wsetcolm 
        LibFile $(LibFileflags) @.o.winit @.o.wcreatew @.o.wcreatei \
        @.o.wdeletew @.o.wdeletei @.o.wclosew @.o.wopenw @.o.wpoll @.o.wgetrec @.o.wredraww \
        @.o.wupdatew @.o.wdragbox @.o.wgeticst @.o.wgetpoi @.o.wgetwinf @.o.wgetwst @.o.wseticst \
        @.o.wforcere @.o.wsetcarp @.o.wgetcarp @.o.wcreatem @.o.wcreatesm @.o.wdecodem @.o.wwhichi \
        @.o.wsetext @.o.wsetpos @.o.wopente @.o.wclosete @.o.wloadte @.o.wclosedown @.o.wstartt \
        @.o.wreporte @.o.wgetwino @.o.wpollidle @.o.wplotic @.o.wsetmode @.o.wsetpal @.o.wreadpal \
        @.o.wsetcol @.o.wsendm @.o.wtextop @.o.wprocessk @.o.!!version @.o.wspriteop @.o.wbasesp \
        @.o.wblockcpy @.o.wslotsize @.o.wtextcol @.o.wcommw @.o.wtransbl @.o.wsysinfo @.o.wresizeic \
        @.o.wsetfon @.o.wgetmenu @.o.waddmsg @.o.wremmsg @.o.wsetcolm 


# User-editable dependencies:

clean:
        remove wimplib
        -wipe o.* ~c~v


