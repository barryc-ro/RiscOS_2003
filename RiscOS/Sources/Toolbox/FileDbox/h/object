#ifndef __object_h
#define __object_h

typedef struct filedboxinternal
{
  int flags;
  int object_id;
  int window;
  char *root_dir;
  char *dir;
  struct filedboxinternal *prev,
                          *next;

} FileDboxInternal;


#endif
