/*****************************************************************************
* MACROS
*****************************************************************************/
#ifndef IGNORE
#define IGNORE(x) ((x)=(x))
#endif

#ifndef MIN
#define MIN(x,y) ((x)<(y)?(x):(y))
#endif

#ifndef MAX
#define MAX(x,y) ((x)>(y)?(x):(y))
#endif

#ifndef NULL
#define NULL ((void *) 0)
#endif

#ifndef SKIP  /* for when a parameter has to be given, but will be ignored */
#define SKIP (0)
#endif


