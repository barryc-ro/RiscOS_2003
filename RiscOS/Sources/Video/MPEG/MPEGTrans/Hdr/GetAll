; > GetAll

        GBLL    Module
Module  SETL    {TRUE}          ; needed for ExitSWIHandler

        GBLL    SendFirstSCR
SendFirstSCR SETL {FALSE}       ; send first SCR in stream to video module

        GBLL    DataRateStats
DataRateStats SETL {FALSE}       ; collect statistics on number of bytes received each minute

; On ResetStream, junk audio/video packets until we get a TS packet on the video PID with an adaptation field and the
; random_access_indicator bit set, which means that the next video PES packet will have a sequence header in it.

	GBLL	SkipToSeqOnResetStream
SkipToSeqOnResetStream SETL {FALSE}

; On ResetStream, if bit 2 of r0 is set, then use r2 as an indicator of what trick mode data should follow,
; and discard all data until we get the correct mode.

	GBLL	SkipToCorrectTrickMode
SkipToCorrectTrickMode SETL {TRUE}

; Do new stuff with program element handlers

	GBLL	DoPEH
DoPEH	SETL	{TRUE}

        GBLL    international
international   SETL {FALSE}

        GET     Hdr:ListOpts
        GET     Hdr:Macros
        GET     Hdr:System
        GET     Hdr:CMOS
        GET     Hdr:ModHand
        GET     Hdr:Services
        GET     Hdr:Proc
        GET     Hdr:FSNumbers
        GET     Hdr:NewErrors
        GET     Hdr:MsgTrans
        GET     Hdr:Debug
        GET     Hdr:VduExt
        GET     Hdr:Podule
        GET     Hdr:DevNos

        GET     Hdr:MPEGCtrl
        GET     Hdr:MPEGVideo
        GET     Hdr:MPEGAudio
	GET	Hdr:DVBControl

        GET     s.Module

        END
