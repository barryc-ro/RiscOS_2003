%%BeginFont: RO_Trinity.Bold.Italic_ELatin1

% Modify font Times-BoldItalic to get font RO_Trinity.Bold.Italic_ELatin1
/Times-BoldItalic findfont  dup length dict  begin
  {1 index /FID ne {def} {pop pop} ifelse} forall
  /Encoding Acorn_Latin1Encoding def
currentdict end
/RO_Trinity.Bold.Italic_ELatin1 exch definefont pop

%%EndFont


