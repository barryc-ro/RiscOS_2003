; > GetAll

        GBLL    DataRateStats
DataRateStats SETL {FALSE}       ; collect statistics on number of bytes received each minute

	GBLL	DoPEH
DoPEH	SETL	{TRUE}

        GBLL    international
international   SETL {FALSE}

        GET     Hdr:ListOpts
        GET     Hdr:Macros
        GET     Hdr:System
        GET	Hdr:Machine.<Machine>
        GET     Hdr:CPU.FPA
        GET     Hdr:CMOS
        GET     Hdr:ModHand
        GET     Hdr:Services
        GET     Hdr:Proc
        GET     Hdr:FSNumbers
        GET     Hdr:NewErrors
        GET     Hdr:MsgTrans
        GET     Hdr:Debug
        GET     Hdr:NdrDebug
        GET     Hdr:VduExt
        GET     Hdr:Podule
        GET     Hdr:DevNos
        GET     Hdr:UpCall

        GET     Hdr:MPEGCtrl
        GET     Hdr:MPEGVideo
        GET     Hdr:MPEGAudio
	GET	Hdr:DVBControl
	GET     Hdr:UpCallMPEG

        GET     VersionASM
        GET     s.Module

        END
