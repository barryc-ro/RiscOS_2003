
        GET     Hdr:ListOpts
        GET     Hdr:Macros
        GET     Hdr:APCS.<APCS>
        GET     Hdr:System
        GET     Hdr:VduExt

        GET     hdr.Debug

 [ Debugging=1

        GBLL    Margin
Margin  SETL    {TRUE}

        AREA    |Asm$$Code|, CODE, READONLY

        ; extern void DWrite_Init (void);

        EXPORT  DWrite_Init
DWrite_Init
        FunctionEntry
        SUB     sp, sp, #4*4
        ADR     r0, %FT90
        MOV     r1, sp
        SWI     XOS_ReadVduVariables
        LDMIA   sp!, {a1-a4}
        StaticBaseFromSL ip
        ADRVar  lr, ip, ncols
        MOV     a2, a2, LSR #1 ; double height => half as many lines
      [ Margin
        SUB     a1, a1, #5
        SUB     a2, a2, #2
        STMIA   lr, {a1-a4}
        MOV     a1, #5
        MOV     a2, #2
      |
        STMIA   lr, {a1-a4}
        MOV     a1, #0
        MOV     a2, #0
      ]
        ADRVar  lr, ip, hpos
        STMIA   lr, {a1,a2}
        Return
90
        &       VduExt_ScrRCol, VduExt_ScrBRow, VduExt_ScreenStart, VduExt_LineLength, -1


        ; extern void DWrite_NewLine (void);

        EXPORT  DWrite_NewLine
DWrite_NewLine
        FunctionEntry
        MOV     a1, #10
        BL      DWrite_WriteC
        MOV     a1, #13
        BL      DWrite_WriteC
        Return


        ; extern void DWrite_Write0 (char *string);

        EXPORT  DWrite_Write0
DWrite_Write0
        FunctionEntry "v1"
        MOVS    v1, a1
        ADREQ   v1, %FT00
01      LDRB    a1, [v1], #1
        TEQ     a1, #0
        BEQ     %FT90
        BL      DWrite_WriteC
        B       %BT01
90
        Return  "v1"
00
        =       "*** NULL ***",  0
        ALIGN


        ; extern void DWrite_WriteCT (char *string);

        EXPORT  DWrite_WriteCT
DWrite_WriteCT
        FunctionEntry "v1"
        MOVS    v1, a1
        ADREQ   v1, %FT00
01      LDRB    a1, [v1], #1
        CMP     a1, #32
        BCC     %FT90
        BL      DWrite_WriteC
        B       %BT01
90
        Return  "v1"
00
        =       "*** NULL ***",  0
        ALIGN


        ; extern void DWrite_WriteHex8 (unsigned int value);

        EXPORT  DWrite_WriteHex8
DWrite_WriteHex8
        FunctionEntry "v1,v2"
        MOV     v1, a1
        MOV     a1, #' '
        BL      DWrite_WriteC
        MOV     v2, #8
01      MOV     a1, v1, LSR #28
        CMP     a1, #10
        ADDCC   a1, a1, #48
        ADDCS   a1, a1, #55
        BL      DWrite_WriteC
        MOV     v1, v1, LSL #4
        SUBS    v2, v2, #1
        BNE     %BT01
        Return  "v1,v2"


        ; extern void DWrite_WriteC (char character);

        EXPORT  DWrite_WriteC
DWrite_WriteC
        AND     r0, r0, #&FF
        TEQ     r0, #10
        BEQ     %FT10
        TEQ     r0, #13
        BEQ     %FT13
        TEQ     r0, #127
        BEQ     %FT99
        CMP     r0, #32
        Return  , LinkNotStacked, CC
        FunctionEntry "a1"
        StaticBaseFromSL ip
        ADRVar  lr, ip, screenbase
        LDMIA   lr, {a1-a4}
        MLA     lr, a2, a4, a3

        LDRVar  a4, ip, ncols
        ADD     a3, a3, #1
        CMP     a3, a4
      [ Margin
        MOVEQ   a3, #5
      |
        MOVEQ   a3, #0
      ]
        STRVar  a3, ip, hpos, a4

        LDRVar  a3, ip, vpos, EQ
        ADDEQ   a3, a3, #1
        STRVar  a3, ip, vpos, a4, EQ
        LDRVar  a4, ip, nrows, EQ
        CMPEQ   a3, a4
      [ Margin
        MOVEQ   a3, #2
      |
        MOVEQ   a3, #0
      ]
        STRVar  a3, ip, vpos, a4, EQ

        ADD     a3, a1, lr, LSL #4 ; screenbase + 16*hpos + 16*linelength*vpos
        LDR     a1, [sp], #4
        BL      PlotChar
        Return
10      ; LF
        StaticBaseFromSL ip
        LDRVar  a1, ip, vpos
        ADD     a1, a1, #1
        STRVar  a1, ip, vpos, a2
        Return  , LinkNotStacked
13      ; CR
        StaticBaseFromSL ip
      [ Margin
        MOV     a1, #5
      |
        MOV     a1, #0
      ]
        STRVar  a1, ip, hpos, a2
        Return  , LinkNotStacked
99      ; Delete
        FunctionEntry
        StaticBaseFromSL ip
        ADRVar  lr, ip, screenbase
        LDMIA   lr, {a1-a4}
      [ Margin
        SUB     a3, a3, #1
        CMP     a3, #5
        Return  ,, CC
      |
        SUBS    a3, a3, #1
        Return  ,, MI
      ]
        STRVar  a3, ip, hpos, lr
        MLA     lr, a2, a4, a3
        ADD     a3, a1, lr, LSL #4 ; screenbase + 16*hpos + 16*linelength*vpos
        MOV     a1, #' '
        BL      PlotChar
        Return


        ; static void PlotChar (char character, unsigned int linelength, unsigned char *address);
        ;
        ; Simple routine to plot a double-height character to the
        ; screen in white on black in a 16 bpp screen mode
        ;
        ; On entry:
        ;   R0 = character to output
        ;   R1 = VDU variable LineLength
        ;   R2 -> screen address to plot at

font    RN      1
screen  RN      2
bigfont RN      3
mask    RN      4
tophalf RN      5
bottomhalf RN   6
byte    RN      8
scrbyte RN      9
scrbyte2 RN     10
linelen RN      11

PlotChar
        FunctionEntry "r4-r11"
        MOV     linelen, r1
        ADR     font, HardFont-32*8
        ADD     font, font, r0, LSL #3
        ADRL    bigfont, WidthExpandTable
        LDMIA   font, {tophalf, bottomhalf}

        MOV     mask, #&FF000000
        SUB     linelen, linelen, #16

        AND     byte, mask, tophalf, LSL #24
        ADD     byte, bigfont, byte, LSR #20
        LDMIA   byte!, {scrbyte, scrbyte2}
        STMIA   screen!, {scrbyte, scrbyte2}
        LDMIA   byte, {scrbyte, scrbyte2}
        STMIA   screen!, {scrbyte, scrbyte2}
        ADD     screen, screen, linelen
        LDMDB   byte, {scrbyte, scrbyte2}
        STMIA   screen!, {scrbyte, scrbyte2}
        LDMIA   byte!, {scrbyte, scrbyte2}
        STMIA   screen!, {scrbyte, scrbyte2}
        ADD     screen, screen, linelen

        AND     byte, mask, tophalf, LSL #16
        ADD     byte, bigfont, byte, LSR #20
        LDMIA   byte!, {scrbyte, scrbyte2}
        STMIA   screen!, {scrbyte, scrbyte2}
        LDMIA   byte, {scrbyte, scrbyte2}
        STMIA   screen!, {scrbyte, scrbyte2}
        ADD     screen, screen, linelen
        LDMDB   byte, {scrbyte, scrbyte2}
        STMIA   screen!, {scrbyte, scrbyte2}
        LDMIA   byte!, {scrbyte, scrbyte2}
        STMIA   screen!, {scrbyte, scrbyte2}
        ADD     screen, screen, linelen

        AND     byte, mask, tophalf, LSL #8
        ADD     byte, bigfont, byte, LSR #20
        LDMIA   byte!, {scrbyte, scrbyte2}
        STMIA   screen!, {scrbyte, scrbyte2}
        LDMIA   byte, {scrbyte, scrbyte2}
        STMIA   screen!, {scrbyte, scrbyte2}
        ADD     screen, screen, linelen
        LDMDB   byte, {scrbyte, scrbyte2}
        STMIA   screen!, {scrbyte, scrbyte2}
        LDMIA   byte!, {scrbyte, scrbyte2}
        STMIA   screen!, {scrbyte, scrbyte2}
        ADD     screen, screen, linelen

        AND     byte, mask, tophalf
        ADD     byte, bigfont, byte, LSR #20
        LDMIA   byte!, {scrbyte, scrbyte2}
        STMIA   screen!, {scrbyte, scrbyte2}
        LDMIA   byte, {scrbyte, scrbyte2}
        STMIA   screen!, {scrbyte, scrbyte2}
        ADD     screen, screen, linelen
        LDMDB   byte, {scrbyte, scrbyte2}
        STMIA   screen!, {scrbyte, scrbyte2}
        LDMIA   byte!, {scrbyte, scrbyte2}
        STMIA   screen!, {scrbyte, scrbyte2}
        ADD     screen, screen, linelen

        AND     byte, mask, bottomhalf, LSL #24
        ADD     byte, bigfont, byte, LSR #20
        LDMIA   byte!, {scrbyte, scrbyte2}
        STMIA   screen!, {scrbyte, scrbyte2}
        LDMIA   byte, {scrbyte, scrbyte2}
        STMIA   screen!, {scrbyte, scrbyte2}
        ADD     screen, screen, linelen
        LDMDB   byte, {scrbyte, scrbyte2}
        STMIA   screen!, {scrbyte, scrbyte2}
        LDMIA   byte!, {scrbyte, scrbyte2}
        STMIA   screen!, {scrbyte, scrbyte2}
        ADD     screen, screen, linelen

        AND     byte, mask, bottomhalf, LSL #16
        ADD     byte, bigfont, byte, LSR #20
        LDMIA   byte!, {scrbyte, scrbyte2}
        STMIA   screen!, {scrbyte, scrbyte2}
        LDMIA   byte, {scrbyte, scrbyte2}
        STMIA   screen!, {scrbyte, scrbyte2}
        ADD     screen, screen, linelen
        LDMDB   byte, {scrbyte, scrbyte2}
        STMIA   screen!, {scrbyte, scrbyte2}
        LDMIA   byte!, {scrbyte, scrbyte2}
        STMIA   screen!, {scrbyte, scrbyte2}
        ADD     screen, screen, linelen

        AND     byte, mask, bottomhalf, LSL #8
        ADD     byte, bigfont, byte, LSR #20
        LDMIA   byte!, {scrbyte, scrbyte2}
        STMIA   screen!, {scrbyte, scrbyte2}
        LDMIA   byte, {scrbyte, scrbyte2}
        STMIA   screen!, {scrbyte, scrbyte2}
        ADD     screen, screen, linelen
        LDMDB   byte, {scrbyte, scrbyte2}
        STMIA   screen!, {scrbyte, scrbyte2}
        LDMIA   byte!, {scrbyte, scrbyte2}
        STMIA   screen!, {scrbyte, scrbyte2}
        ADD     screen, screen, linelen

        AND     byte, mask, bottomhalf
        ADD     byte, bigfont, byte, LSR #20
        LDMIA   byte!, {scrbyte, scrbyte2}
        STMIA   screen!, {scrbyte, scrbyte2}
        LDMIA   byte, {scrbyte, scrbyte2}
        STMIA   screen!, {scrbyte, scrbyte2}
        ADD     screen, screen, linelen
        LDMDB   byte, {scrbyte, scrbyte2}
        STMIA   screen!, {scrbyte, scrbyte2}
        LDMIA   byte!, {scrbyte, scrbyte2}
        STMIA   screen!, {scrbyte, scrbyte2}

        Return  "r4-r11"


