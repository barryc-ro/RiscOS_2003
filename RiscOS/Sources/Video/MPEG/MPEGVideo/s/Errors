; Error blocks
; Replace with proper error numbers eventually

NoSuchSWIError
        &       0
        =       "No such MPEGVideo SWI", 0
        ALIGN

BadCardControlReasonError
        &       0
        =       "Bad MPEGVideo_CardControl reason code", 0
        ALIGN

BadColourModeError
	&	0
	=	"Bad value for colour mode", 0
	ALIGN

BadReadParametersReasonError
        &       0
        =       "Bad MPEGVideo_ReadParameters reason code", 0
        ALIGN

NoPoduleError
        &       0
        =       NoPoduleErrorMessage, 0
        ALIGN

EscapeError
        &       17
        =       "Escape", 0
        ALIGN

HMEM15not1Error
        &       0
        =       "MPEG video decoder hardware is faulty", 0
        ALIGN

WrongLengthError
        &       0
        =       "Micro-code wrong length", 0
        ALIGN

BadMCodeError
        &       0
        =       "Bad micro-code", 0
        ALIGN

MisalignedIMEMError
        &       0
        =       "Mis-aligned IMEM segment", 0
        ALIGN

        END
