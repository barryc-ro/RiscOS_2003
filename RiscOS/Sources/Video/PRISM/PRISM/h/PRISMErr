#ifndef PRISM_PRISMErr__
  #define PRISM_PRISMErr__

  enum
  {
    prism_ERROR_INTERNAL_ERROR = PRISM_ERROR1_BASE + PRISM_PRIVATE_OFFSET,
    prism_ERROR_INVALID_ARRAY_BASE,
    prism_ERROR_NO_ZEROS_ALLOWED,
    prism_ERROR_ALL_HANDLES_USED,
    prism_ERROR_HANDLE_NOT_IN_USE
  };

#endif /* PRISM_PRISMErr__ */
