#ifndef PRISM_PRISMErr__
  #define PRISM_PRISMErr__

  enum
  {
    /* These must only be raised by the PRISM module itself */

    prism_ERROR_MESSAGE_NOT_FOUND     = PRISM_ERROR1_BASE + PRISM_PRIVATE_OFFSET,
    prism_ERROR_INTERNAL_ERROR
  };

#endif /* PRISM_PRISMErr__ */
