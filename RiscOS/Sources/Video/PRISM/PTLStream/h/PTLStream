#ifndef PTLStream_PTLStream__
  #define PTLStream_PTLStream__

  #include "PRISM.h"

  enum
  {
    ptlstream_ERROR_MESSAGE_NOT_FOUND = PRISM_ERROR1_BASE + PTL_STREAM_OFFSET,
    ptlstream_ERROR_METHOD_MISMATCH
  };

  #define PRISM_Parent_PTLStream      (0x001000)
  #define PTLStream_PRISMID           ((PRISM_Class_PTL) | (PRISM_Parent_PTLStream))

#endif /* PTLStream_PTLStream__ */

