	help-string:            IconHigh 0.25 (daytona)
#elif defined( iconhigh_DESKTOP)
	title-string:		IconHigh
	help-string:            IconHigh 0.23 (desktop)
#else
	title-string:		IconHigh
	help-string:            IconHigh 0.23
#endif
//date-string:		20 Nov 1997

initialisation-code:    Initialise
finalisation-code:      Finalise

swi-chunk-base-number:	0x4e700
swi-decoding-table:	IconHigh,
				DefaultEmulation GetDirection Start Stop Redrawer SetMomentum SetHighlightColour IgnoreNextCharacter SetHighlightWidth MoveHighlight HighlightIcon
swi-handler-code:	SWIHandler

#ifdef iconhigh_DAYTONA
generic-veneers:	PtrPreFilter, PtrPostFilter, ToggleHandler, ToggleHandler2, HighlightInsV2, CancelIRKeyDown, NextPoll, SetCaret, MoveHigh
#else
generic-veneers:	PtrPreFilter, PtrPostFilter, ToggleHandler, ToggleHandler2, HighlightInsV2, CancelIRKeyDown, NextPoll, HighlighCheck
#endif

#ifdef iconhigh_DAYTONA
vector-handlers:	Pointer_KeyV, HighlightInsV
#else
vector-handlers:	Pointer_KeyV, HighlightInsV, Pointer_PtrV, EventVHandler
#endif

service-call-handler:	ServiceHandler 0x46
// modechange.

command-keyword-table:	Command
			IconHigh_SetHighlightColour(	add-syntax:,
				help-text:	"Sets the colour used to highlight icons.\n",
				invalid-syntax:	"Syntax: IconHigh_SetHighlightColour [0xbbggrrxx]|[&bbggrrxx]|[colour in decimal] | <width> [colour1] [colour2]...\n",
				min-args:	1,
				max-args:	99),
			IconHigh_Start(	add-syntax:,
				help-text:	"Starts emulation.\n",
				invalid-syntax:	"Syntax: IconHigh_Start <emulation>\n emulation=0 for highlight mode, 1 for pointer mode.",
				min-args:	1,
				max-args:	1),
			IconHigh_Stop(	add-syntax:,
				help-text:	"Stops emulation.\n",
				invalid-syntax:	"Syntax: IconHigh_Stop",
				min-args:	0,
				max-args:	0)

