#ifndef __Desk_Window2_h
#define __Desk_Window2_h

#include "Desk.Wimp.h"


Desk_window_info*	Desk_Window_GetInfoAlloc( Desk_window_handle window);
/*
Returns a Desk_window_info followed by info on all icons in 'window'.

The memory returned should be freed by calling
Desk_Window_GetInfoFree().
 */

void			Desk_Window_GetInfoFree( Desk_window_info* info);
/*
Frees memory returned by Desk_Window_GetInfoAlloc().
 */

#endif
