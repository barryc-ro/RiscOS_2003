;============================================================================
;
        GET     Desk.sh.regdefs
        GET     h.swinos
        GET     Desk.sh.macros
;
;============================================================================
;
;  Code.
;
;============================================================================
;


        PREAMBLE
        STARTCODE Desk_Wimp_PlotIcon
        
        MOV     ip, lr
        MOV     a2, a1
        SWI     Desk_SWI_Wimp_PlotIcon + Desk_XOS_Bit
	Desk_Error2_CheckV
        MOVS    pc, ip
        
        END
