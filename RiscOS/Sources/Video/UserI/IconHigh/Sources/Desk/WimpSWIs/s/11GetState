        GET     Desk.sh.regdefs
        GET     h.SwiNos
        GET     Desk.sh.macros
;
;============================================================================
;
;  Code.
;
;============================================================================
;
        PREAMBLE
        STARTCODE Desk_Wimp_GetWindowState
;
;        STMFD   sp!, {lr}
        MOV     ip, lr

        STR     a1, [a2, #0]
        SWI     Desk_SWI_Wimp_GetWindowState + Desk_XOS_Bit
	Desk_Error2_CheckV

        MOVS    pc, ip
;        LDMFD   sp!, {pc}^
;
        END
