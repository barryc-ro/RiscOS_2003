
typedef struct
       {
         int area_number;
         int *area;
         int size;
         char name[12];
         int allocated;
         int type; /* 1 = sprite, 2 = jpeg */
       } dynamiclib_str;

void dynamiclib_init(void);
int dynamiclib_alloc(dynamiclib_str *in);
void dynamiclib_free(dynamiclib_str in);

extern dynamiclib_str graphics, temp_jpeg;
