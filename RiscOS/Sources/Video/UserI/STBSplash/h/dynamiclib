#include "kernel.h"

typedef struct
       {
         int area_number;
         int *area;
         int size;
         char name[16];
         int allocated;
         int type; /* 1 = sprite, 2 = jpeg */
       } dynamiclib_str;

_kernel_oserror *dynamiclib_init(void);
_kernel_oserror *dynamiclib_alloc(dynamiclib_str *in);
_kernel_oserror *dynamiclib_free(dynamiclib_str *in);

extern dynamiclib_str graphics, temp_jpeg;
