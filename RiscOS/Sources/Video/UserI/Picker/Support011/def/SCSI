TITLE SCSI;

NEEDS OS;

SWI SCSI_Version = (NUMBER 0x403C0*);
SWI SCSI_Initialise = (NUMBER 0x403C1*);
SWI SCSI_Control = (NUMBER 0x403C2*);
SWI SCSI_Op = (NUMBER 0x403C3*);
SWI SCSI_Status = (NUMBER 0x403C4*);
SWI SCSI_Reserve = (NUMBER 0x403C7*);
SWI SCSI_List = (NUMBER 0x403C8*);
SWI SCSI_LogVersion = (NUMBER 0x41080*);
SWI SCSI_LogList = (NUMBER 0x41081*)
