# > MakeFile

CC = cc -c

all:            !RunImage

o.curvefit:     c.curvefit h.curvefit h.includes
                $(CC) curvefit

o.scanspr3:     c.scanspr3 h.curvefit h.includes
                $(CC) scanspr3

!RunImage:      o.scanspr3 o.curvefit riscoslib:o.RISC_OSLib
                link -output !RunImage -via LinkFiles
                squeeze !RunImage

# end of MakeFile
