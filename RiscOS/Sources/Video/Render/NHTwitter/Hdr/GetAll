        GET     Hdr:CMOS
        GET     Hdr:ModHand
        GET     Hdr:Services
        GET     Hdr:Proc
        GET     Hdr:FSNumbers		; Jon Change (was File)
        GET     Hdr:NewErrors
        GET     Hdr:MsgTrans
        GET     Hdr:Debug
        GET     Hdr:VduExt
        GET     Hdr:Podule
	GET	Hdr:PoduleReg
        GET     Hdr:DevNos

        GET     s.Module		; Jon added s.

        END
