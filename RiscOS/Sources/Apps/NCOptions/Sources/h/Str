#ifndef __Str_h
#define __Str_h

int	CmpIdent( const char* a, int a_len, const char* b);
/*
Compares (case insensitively) the strings pointed to by a and b. b is
assumed to be 0-terminated. Only first 'a_len' characters of a are
considered.
 */

int	StrInitialCmp( const char* s, const char* initial);

#endif
