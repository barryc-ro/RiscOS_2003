/*  amd29f.h
 *
 *  Interface to AMD29F-type flash memory device.
 *
 *  04-Sep-1997 jfarrell - Created.
 */

#include "flashpod.h"

/*  Create a 'flash' object of the correct type for the AMD29F-type device
 *  that is memory mapped at 'address'.
 *  
 *  The returned object should be used with the flash_xxx macros in podflash.h.
 */
extern struct flash *amd29f_create (
    unsigned int *address
);


