#ifndef __fax__rccodes_h
#define __fax__rccodes_h

/* !Fax info/error codes */
enum ReturnCode {

   /* 0x000-0x0ff: Error codes (except for 0x000 and 0x00d/0x00e!) */

   /* 0x000-0x00f: Call placement/termination */
   Info_Normal_Termination           = 0x000,
   Error_No_Answer                   = 0x004,
   Error_No_Carrier                  = 0x005, /* Not found a fax machine */
   Error_No_Dialtone                 = 0x00a,
   Error_Busy                        = 0x00b,
   Error_Switchover_Protocol         = 0x00c,
   Info_Switchover_Originate         = 0x00d, /* Don't reset modem for this! */
   Info_Switchover_Answer            = 0x00e, /* Don't reset modem for this! */
   Error_Aborted                     = 0x00f, /* In response to abort request */
   /* 0x010-0x01f: Phase A, tx */
   Error_T1_Timeout                  = 0x011, /* Note this can also occur in rx */
   Error_Got_DCN_Atx                 = 0x01f,
   /* 0x020-0x03f: Phase B, tx */
   Error_Cannot_Receive              = 0x021, /* b10 of received DIS clear */
   Error_DCS_Thrice                  = 0x025,
   Error_Too_Slow                    = 0x027, /* FTT after 2400 bps TCF */
   Error_Got_DCN_Btx                 = 0x03f,
   /* 0x040-0x04f: Phase C, tx */
   /* 0x050-0x06e: Phase D, tx */
   Error_MPS_Thrice                  = 0x052,
   Error_EOP_Thrice                  = 0x054,
   Error_EOM_Thrice                  = 0x056,
   Error_RTN_Received_Thrice         = 0x06e,
   Error_Got_DCN_Dtx                 = 0x06f,
   /* 0x07f-0x07f: Phase A, rx (note breaks pattern) */
   /* 0x070-0x08f: Phase B, rx */
   Error_T2_Timeout                  = 0x073, /* Note this can also occur in D */
   Error_Got_DCN_Arx                 = 0x07f,
   Error_Unsupported_DCS             = 0x08d, /* Bad datarate bits */
   Error_Contradictory_DCS           = 0x08e, /* E.g. multiple resolutions */
   Error_Got_DCN_Brx                 = 0x08f,
   /* 0x090-0x09f: Phase C, rx */
   Error_Scan_Line_Too_Long          = 0x091, /* Using 1993 version (5 s) */
   /* 0x0a0-0x0bf: Phase D, rx */
   Error_RTN_Sent_Thrice             = 0x0be,
   Error_Got_DCN_Drx                 = 0x0bf,
   /* 0x0c0-0x0df: TIFF */
   Error_TIFF_Bad_ImageWidth         = 0x0c0,
   Error_TIFF_Bad_ImageLength        = 0x0c1,
   Error_TIFF_Bad_BitsPerSample      = 0x0c2,
   Error_TIFF_Bad_Compression        = 0x0c3,
   Error_TIFF_Bad_PhotometricInterpretation = 0x0c4,
   Error_TIFF_Bad_FillOrder          = 0x0c5,
   Error_TIFF_Bad_StripOffsets       = 0x0c6,
   Error_TIFF_Bad_SamplesPerPixel    = 0x0c7,
   Error_TIFF_Bad_StripByteCounts    = 0x0c8,
   Error_TIFF_Bad_XResolution        = 0x0c9,
   Error_TIFF_Bad_YResolution        = 0x0ca,
   Error_TIFF_Bad_T6Options          = 0x0cb,
   Error_TIFF_Bad_ResolutionUnit     = 0x0cc,
   Error_TIFF_Bad_Count              = 0x0dd,
   Error_TIFF_Write_Error            = 0x0de,
   Error_TIFF_Read_Error             = 0x0df,
   /* 0x0e0-0x0ff: Implementation-defined */
   Error_Ty_Timeout                  = 0x0f7, /* Disaster as per 5.2s/t/T.30 */
   Error_Page_Not_Found              = 0x0f8, /* At start or for close-ended tx */
   Error_Bad_Port_Number             = 0x0f9,
   Error_Port_Not_In_Use             = 0x0fa, /* In response to abort request */
   Error_Port_In_Use                 = 0x0fb,
   Error_HDLC_Overflow               = 0x0fc, /* HDLC frame too long */
   Error_Unexpected_Modem_Response   = 0x0fd,
   Error_Tx_Timeout                  = 0x0fe, /* Modem went silent */

   /* 0x100-0x1ff: Informational codes */

   /* 0x100-0x10f: Call placement/termination */
   Info_Connected                    = 0x100, /* Got first HDLC preamble */
   /* 0x110-0x11f: Phase A, tx */
   /* 0x120-0x13f: Phase B, tx */
   Info_CSI                          = 0x120, /* Called subscriber ID */
   /* 0x140-0x14f: Phase C, tx */
   Info_Sending_Page                 = 0x140, /* Not authoritative! */
   Info_Sent_Page                    = 0x141, /* Not authoritative! */
   /* 0x150-0x16f: Phase D, tx */
   /* 0x170-0x18f: Phase B, rx */
   Info_TSI                          = 0x170, /* Transmitting subscriber ID */
   Info_Fishy_DCS                    = 0x171, /* E.g. b25 set */
   /* 0x190-0x19f: Phase C, rx */
   Info_Receiving_Page               = 0x190, /* Not authoritative! */
   Info_Received_Page                = 0x191, /* Not authoritative! */
   /* 0x1a0-0x1bf: Phase D, rx */
   /* 0x1c0-0x1df: TIFF */
   /* 0x1e0-0x1ff: Implementation-defined */

   /* 0x200-     : !Fax/FaxSupport use only (not externally visible) */
   Info_Waiting_DCS                  = 0x201,
   Info_FCS_Error                    = 0x202,
   Info_Got_NSF                      = 0x203,
   Info_Got_CRP                      = 0x204,
   Info_Echoing_NSS                  = 0x205,
/* Normal_End_Of_Block               = 0x300, */
   Info_Got_CSI                      = 0x304,
   Info_Got_DIS                      = 0x305,
   Info_Got_NSS                      = 0x306,
   Info_Got_TSI                      = 0x307,
   Info_Got_DCS                      = 0x308,
   Info_Sending_NSF                  = 0x309,
   Info_Sending_CSI                  = 0x30a,
   Info_Sending_DIS                  = 0x30b,
   Info_Getting_T4                   = 0x30c,
   Info_Getting_TCF                  = 0x30d,
   Info_Sending_CFR                  = 0x30e,
   Info_Sending_FTT                  = 0x30f,
   Info_Got_MPS                      = 0x310,
   Info_Got_EOM                      = 0x311,
   Info_Got_EOP                      = 0x312,
   Info_Sending_NSS                  = 0x313,
   Info_Sending_TSI                  = 0x314,
   Info_Sending_DCS                  = 0x315,
   Info_Sending_TCF                  = 0x316,
   Info_Waiting_Pre                  = 0x317,
   Info_Got_CFR                      = 0x318,
   Info_Got_FTT                      = 0x319,
   Info_Sending_DCN                  = 0x31a,
   Info_Send_T4                      = 0x31b,
   Info_Sending_MPS                  = 0x31c,
   Info_Sending_EOM                  = 0x31d,
   Info_Sending_EOP                  = 0x31e,
   Info_Waiting_Post                 = 0x31f,
   Info_Got_MCF                      = 0x320,
   Info_Got_RTP                      = 0x321,
   Info_Got_RTN                      = 0x322,
   Info_Sent_Page_Internal           = 0x323,
   Status_Chopped                    = 0x400

};
typedef enum ReturnCode ReturnCode;

#endif

