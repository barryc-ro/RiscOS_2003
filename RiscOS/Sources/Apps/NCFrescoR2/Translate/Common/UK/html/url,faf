        function updateField(inc_or_dec)
        {
          index += inc_or_dec;
          document.urlform.url.value = urls[index];
        }

        function goBack()
        {
          if (index < count)
          {
            if ((index+1 < count) && (urls[index] == urls[index+1]))
            {
              updateField(+2);
            }
            else
            {
              updateField(+1);
            }
          }
        }
        function goForward()
        {
          if (index > 0)
          {
            if ((index-1 > 0) && (urls[index] == urls[index-1]))
            {
              updateField(-2);
            }
            else
            {
              updateField(-1);
            }
          }
