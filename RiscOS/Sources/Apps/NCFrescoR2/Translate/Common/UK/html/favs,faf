<SCRIPT>

function keypress(e)
{
  var pass_key_on = true;

  if (e.which == -2147483503) // Red key
  {
    location.href = "ncint:favsremove";
    pass_key_on = false;
  }

//  if (e.which == -2147483502) // Green key
//  {
//    location.href = "ncint:favmove";
//    pass_key_on = false;
//  }

  if (e.which == -2147483501) // Yellow key
  {
    location.href = "ncint:favrename";
    pass_key_on = false;
  }

  if (e.which == -2147483500) // Blue key
  {
    location.href = "ncint:favmove";
    pass_key_on = false;
  }

  return pass_key_on;
}

</SCRIPT>

</HEAD>

<BODY MARGINWIDTH=0 MARGINHEIGHT=0 ONKEYPRESS="javascript:return keypress(event)">

