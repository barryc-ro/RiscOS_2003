<META NAME="ncbrowsermode" CONTENT="noscale; nohistory; position=fullscreen; ensuretoolbar=1; selectbutton=0x2027">
<META HTTP-EQUIV=Content-Type CONTENT="text/html; charset=utf-8">
<LINK HREF="file:///NCFresco:html/global.css" REL="stylesheet" TITLE="default" TYPE="text/css">

<SCRIPT>

function keypress(e)
{
  var pass_key_on = true;
  var url = "";

  switch (e.which)
  {
    case 0x80000091: // Red key
      url = "ncint:favsremove";
      break;

//    case 0x80000092: // Green key
//      url = "ncint:favmove";
//      break;

    case 0x80000093: // Yellow key
      url = "ncint:favrename";
      break;

    case 0x80000094: // Blue key
      url = "ncint:favmove";
      break;
  }

  if (url != "")
  {
    pass_key_on = false;
    key.stopautorepeat();
    location.href = url;
  }

  return pass_key_on;
}

</SCRIPT>

</HEAD>

<BODY MARGINWIDTH=0 MARGINHEIGHT=0 ONKEYPRESS="javascript:return keypress(event)">

<p><p>

