#define  u_long  unsigned long
#define  u_short  unsigned short
#define  u_char  unsigned char

#ifndef NIL
#define  NIL    (void *)0
#endif

struct CNSDictType {
  u_long MaxSize;
  u_long realSize;
  u_long incrSize;
  u_char **buff;
  short (*compare)();
  u_char flags;
  u_char sep;
};

struct CNSDictType **CNSNewDict();
u_char **Dict2Handle();
long CNSSearch();
long CNSLookUp();
long CNSstrlen();
int CNSstrcmp();

#define  GrowSize  0x01
#define  ReplSame  0x02
#define  TryAtEnd  0x04
