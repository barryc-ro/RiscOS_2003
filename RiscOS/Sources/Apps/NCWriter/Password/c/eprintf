#include <stdio.h>

eputchar(ch)
{
#ifdef RISCOS
	if(ch == '\r')
		ch = '\n';
#else
	if(ch == '\n')
		ch = '\r';
#endif
	return(putc(ch,stderr));
}

eprintf(fmt,args)
char *fmt;
unsigned args;
{
	format(eputchar,fmt,&args);
}

eflush()
{
	fflush(stderr);
}
