u_char *PrSearch();

#define	PR_PREFIX(p)	((p)+1)
#define	PR_LENGTH(p)	((*(p) & 0x0f) - 1)
#define	PR_ORIGIN(p)	((*(p) & ~0x80) >> 4)
#define	PR_BOUNDARY(p)	(*(p) & 0x80)
#define	NEXT_PREFIX(p)	((p) + (*(p) & 0x0f))

#define	PrLatin		1
#define	PrGreek		2
#define	WBHier		0
#define	PrEnglish	3

struct PrIndexType {
	short maxCode;
	short maxHier;
	short list;
	short compTable;
	short index[1];
};
