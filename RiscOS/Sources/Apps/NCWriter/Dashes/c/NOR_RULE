	CONS|VCD|DNTL|STOP|PREJ,			/* d */
	VOWEL|LET_AE,						/* e */
	CONS|LABL|PREJ,						/* f */
	CONS|VCD|STOP|VELAR|PREJ,			/* g */
	CONS|VELAR|PREJ,					/* h */
	VOWEL|HIGH,							/* i */
	CONS|VCD|NONEND,					/* j */
	CONS|VELAR|STOP|PREJ,				/* k */
	CONS|VCD|SON|LIQ|DNTL|PREJ,			/* l */
	CONS|VCD|SON|LABL|PREJ,				/* m */
	CONS|VCD|SON|DNTL|PREJ,				/* n */
	VOWEL,								/* o */
	CONS|STOP|PREH|LABL,				/* p */
	CONS|VELAR|STOP|NONEND,				/* q */
	CONS|VCD|SON|LIQ|DNTL,				/* r */
	CONS|DNTL|PREJ,						/* s */
	CONS|STOP|DNTL,						/* t */
	VOWEL|HIGH,							/* u */
	CONS|VCD|LABL,						/* v */
	CONS|VCD|LABL|NONEND,				/* w */
	CONS|VELAR|NONEND,					/* x */
	VOWEL,								/* y */
	CONS|VCD|DNTL|NONEND,				/* z */
	VOWEL,								/* ¾ Nore */
	VOWEL,								/* ð Noro */
	VOWEL,								/* Œ Nora */
	VOWEL,								/* Š Norae - swe */
	VOWEL,								/* š Noroe - swe */
	VOWEL,								/* ‡ - ice */
	VOWEL,								/* Ž - ice */
	VOWEL,								/* ’ - ice */
	VOWEL,								/* — - ice */
	VOWEL,								/* œ - ice */
	VOWEL,								/* 2y - ice */
	CONS|NONEND,						/* ð - ice */
	CONS|DNTL							/* |o - ice */
};

