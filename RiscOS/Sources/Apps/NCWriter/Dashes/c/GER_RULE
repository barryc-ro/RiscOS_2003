RuleType GerFeTable[] = {
	VOWEL|LET_AE,					/* a */
	CONS|VCD|STOP|LABL,				/* b */
	CONS|STOP|VELAR|PREH|NONEND,	/* c */
	CONS|VCD|DNTL|STOP,				/* d */
	VOWEL|LET_AE,					/* e */
	CONS|LABL,						/* f */
	CONS|VCD|STOP|VELAR|LET_GQ,		/* g */
	CONS|NONEND|VELAR,				/* h */
	VOWEL|LET_IU,					/* i */
	CONS|VCD|NONEND,				/* j */
	CONS|VELAR|STOP,				/* k */
	CONS|VCD|SON|LIQ|DNTL,			/* l */
	CONS|VCD|SON|LABL,				/* m */
	CONS|VCD|SON|DNTL,				/* n */
	VOWEL,							/* o */
	CONS|STOP|PREH|LABL,			/* p */
	CONS|VELAR|STOP|LET_GQ|NONEND,	/* q */
	CONS|VCD|SON|LIQ|DNTL,			/* r */
	CONS|DNTL,						/* s */
	CONS|STOP|DNTL,					/* t */
	VOWEL|LET_IU,					/* u */
	CONS|VCD|LABL|NONEND,			/* v */
	CONS|VCD|LABL|NONEND,			/* w */
	CONS|VELAR|NONEND,				/* x */
	VOWEL,							/* y */
	CONS|VCD|DNTL,					/* z */
	VOWEL,							/* a" */
	VOWEL,							/* o" */
	CONS,							/* § */
	VOWEL,							/* u" */
	VOWEL|LABL,						/* i" */
	VOWEL|LABL,						/* e" */
	VOWEL,							/* a` */
	CONS|STOP,						/* c, */
	VOWEL|LET_AE,					/* e' */
	VOWEL|LET_AE,					/* e^ */
	VOWEL|LET_AE,					/* e` */
	VOWEL|LET_IU					/* i^ */
};

