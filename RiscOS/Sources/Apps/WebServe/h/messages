typedef enum {
        mtoken_FETCHING,
        mtoken_SENDING,
        mtoken_NO_HOST,
        mtoken_CACHEING,
        mtoken_INCOMING_FROM,
        mtoken_CENSORSHIP,
        mtoken_DIALLING_TO_FETCH,
        mtoken_FOR,
        mtoken_BUFFER,
        mtoken_LOOP,
        
        mtoken_MAX
} message_token;

extern MessagesFD mbl;
extern char *messages_read(size_t *size, const char *token);
extern char *messages_get(message_token);

extern _kernel_oserror *messages_init(void);
