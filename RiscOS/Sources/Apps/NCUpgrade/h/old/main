#ifndef __main_h
#define __main_h

#define FALSE 0
#define TRUE 1

typedef struct
{
  char *phone_num;
  char *uname;
  char *passwd;
} ISP;

typedef struct
{
  char *url;
  char *uname;
  char *passwd;
} FTP;

typedef struct
{
  char *primary;
  char *secondary;
} DNS;

typedef struct
{
  ISP isp_info;
  FTP ftp_info;
  DNS dns_servers;
} ServerAccess;

#define NVRAM_Read 0x4EE00
#define NVRAM_Write 0x4EE01

#endif

