#include "kernel.h"
#include "swis.h"
#include "wimp.h"

#include "GCManager.h"
#include "task.h"
#include "utils.h"

#include "stdio.h"

#ifndef XNH_AntiTwitter
#define XNH_AntiTwitter (0x838c0 + XOS_Bit)
#endif

void plot_antitwitter_rect(GC *gc, Graphics_op *op)
{
    _swix(XNH_AntiTwitter, _INR(0,3), gc->ox + op->data.point2.x1,
			gc->oy + op->data.point2.y1 - op->data.point2.y2,
			op->data.point2.x2,
			op->data.point2.y2);
}
