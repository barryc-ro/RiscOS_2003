Dir <Obey$Dir>
Set LazarusUI$Dir <Obey$Dir>
amu_machine THROWBACK=-throwback RESDIR=Translated.dummy resources
| And setup some useful paths
Set MAScr$Path <LazarusUI$Dir>.Translated.!Boot.HTML.MA.
Set ISPCScr$Path <LazarusUI$Dir>.Translated.!Boot.HTML.ISPCapture.
Set OptScr$Path <LazarusUI$Dir>.Translated.!Boot.HTML.Setup.
Set OtherScr$Path <LazarusUI$Dir>.Translated.!Boot.HTML.Other.
Set DialScr$Path <LazarusUI$Dir>.Translated.!Boot.HTML.Dialling.
Unset LazarusUI$Dir
