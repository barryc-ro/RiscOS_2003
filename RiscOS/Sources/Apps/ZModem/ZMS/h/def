/**************************************************************************************************/
/* File:    def.h                                                                                 */
/* Purpose: ZModem ASCII character definitions                                                    */
/*                                                                                                */
/* Author:  Chuck Forsberg (original author)                                                      */
/* History: Unknown                                                                               */
/**************************************************************************************************/


#define NUL  0x0      /* Null */
#define SOH  0x1      /* Start of header */
#define STX  0x2      /* Ctrl-B */
#define ETX  0x3
#define EOT  0x4
#define ENQ  0x5      /* ENQ */
#define ACK  0x6      /* ACK */
#define BEL  0x7      /* Bell (Beep) */
#define ITB  0x7
#define BS   0x8      /* Backspace */
#define HT   0x9      /* Horizontal Tab */
#define LF   0xA      /* Linefeed */
#define NL   0xA      /* Newline */
#define VT   0xB      /* Vertical Tab */
#define FF   0xC      /* Formfeed */
#define CR   0xD      /* Carriage Return */
#define SO   0xE      /* Shift Out */
#define SI   0xF      /* Shift In */
#define DLE  0x10     /* Datalink Escape */
#define XON  0x11     /* XON flow control, resume communication */
#define DC1  0x11
#define DC2  0x12
#define XOFF 0x13     /* XOFF flow control, suspend communication */
#define DC3  0x13
#define DC4  0x14
#define NAK  0x15     /* NAK */
#define SYN  0x16     /* SYN, Ctrl-V */
#define ETB  0x17
#define CAN  0x18
#define EM   0x19
#define SUB  0x1A     /* SUB */
#define ESC  0x1B     /* Escape */
#define FS   0x1C     /* Field Separator,  Ctrl-Backslash */
#define GS   0x1D     /* Group Separator,  Ctrl-Rightbracket */
#define RS   0x1E     /* Record Separator, Ctrl-Circumflex */
#define US   0x1F     /* Unit Separator,   Ctrl-Underscore */
#define SPC  0x20
#define SP   0x20
#define DEL  0x7F     /* Delete (Rubout) */
#define RUB  0x7F     /* Delete (Rubout) */




#define ESC_@  0x80   /* ESC @ */
#define ESC_A  0x81   /* ESC A */
#define ESC_B  0x82   /* ESC B */
#define ESC_C  0x83   /* ESC C */
#define IND    0x84   /* ESC D */
#define NEL    0x85   /* ESC E */
#define SSA    0x86   /* ESC F */
#define ESA    0x87   /* ESC G */
#define HTS    0x88   /* ESC H */
#define HTJ    0x89   /* ESC I */
#define VTS    0x8A   /* ESC J */
#define PLD    0x8B   /* ESC K */
#define PLU    0x8C   /* ESC L */
#define RI     0x8D   /* ESC M */
#define SS2    0x8E   /* ESC N */
#define SS3    0x8F   /* ESC O */
#define DCS    0x90   /* ESC P */
#define PU1    0x91   /* ESC Q */
#define PU2    0x92   /* ESC R */
#define STS    0x93   /* ESC S */
#define CCH    0x94   /* ESC T */
#define MW     0x95   /* ESC U */
#define SPA    0x96   /* ESC V */
#define EPA    0x97   /* ESC W */
#define ESC_X  0x98   /* ESC X */
#define ESC_Y  0x99   /* ESC Y */
#define ESC_Z  0x9A   /* ESC Z */
#define CSI    0x9B   /* ESC [ */
#define ST     0x9C   /* ESC \ */
#define OSC    0x9D   /* ESC ] */
#define PM     0x9E   /* ESC ^ */
#define APC    0x9F   /* ESC _ */


/* End of def.h */
