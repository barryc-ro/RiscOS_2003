/* Function prototypes */

_kernel_oserror * unifont_start_redraw (void);
_kernel_oserror * unifont_write0       (const char *s, int x, int y, int xs, int ys);
_kernel_oserror * unifont_end_redraw   (void);
int               unifont_widechar     (UCS4 c);
