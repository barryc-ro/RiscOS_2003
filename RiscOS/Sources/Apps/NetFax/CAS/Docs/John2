typedef struct FormRecord
{
  /* The number of label/field pairs */
  char number_of_fields;

  /* Optional preset fax number */
  char *fax_recipient;

  /* Optional preset email address */
  char *email_recipient;

  /* Form ID */
  char *id;

  /* Form title */
  char *title;

  /* The label/field pairs */
  char **labels;
  char **fields;
}FormRecord;
