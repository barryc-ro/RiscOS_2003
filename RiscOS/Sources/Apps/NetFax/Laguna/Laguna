# Makefile for Laguna
# ===================
#

# -----------------------------------------------------------------------------
# Change List
#

# Date          Name    Description
# ====          ====    ===========
# 1997-10-06    BAL     Created
# 1998-03-10    NCE     modified for Laguna build

# -----------------------------------------------------------------------------
# Program specific options
#

COMPONENT       = Laguna
APP             = !Laguna
MODULE          = rm.!Laguna
ROM_MODULE      = aof.Laguna
LDIR            = Resources.${LOCALE}.${Machine}

# -----------------------------------------------------------------------------
# Export Paths for Messages module
#

RESDIR          = ${RESDIR}.${COMPONENT}
RESAPP          = <Resource$Dir>.Apps.${APP}


# -----------------------------------------------------------------------------
# Generic options
#

MKDIR           = cdir
AS              = objasm
CC              = cc
C++             = c++
CMHG            = cmhg
CP              = copy
LD              = link
RM              = remove
SQUEEZE         = squeeze
WIPE            = -wipe

AFLAGS          = ${THROWBACK} -depend !Depend -nocache -stamp -quit
CFLAGS          = ${THROWBACK} -depend !Depend -ffa ${INCLUDES} -wp
C++FLAGS        = ${THROWBACK} -depend !Depend -ffa ${INCLUDES} -wp
CPFLAGS         = ~cfr~v~n
SQFLAGS         = -f
WFLAGS          = ~cf~vr


# -----------------------------------------------------------------------------
# Flags
#

# These are for the standard Desktop build (plus debug)

DFLAGS          = 
DDFLAGS         = ${DFLAGS} -fn
DZFLAGS         = 
DDZFLAGS        = ${DZFLAGS} -fn


# -----------------------------------------------------------------------------
# Libraries
#

LIBDIR          = <Lib$Dir>

CLIB            = CLib:o.stubs
C++LIB          = CPP:o.c++lib
ROMCSTUBS       = RISC_OSLib:o.romcstubs
ABSSYM          = RISC_OSLib:o.c_abssym
WRAPPER         = RISC_OSLib:s.ModuleWrap

SOCKLIB         = TCPIPLibs:o.socklib5

EVENTLIB        = tbox:o.eventlib
TOOLBOXLIB      = tbox:o.toolboxlib
WIMPLIB         = tbox:o.wimplib
TEXTGADGETLIB   = tbox:o.textgadget

NFELIB          = <Lib$Dir>.NetFax.NFELib.o.NFELib
EMAILLIB	= <Lib$Dir>.Email.EmailLib.o.EmailLib
UILIB		= <Lib$Dir>.NetFax.UILib.o.UILib
ERRORLIB	= ${LIBDIR}.ErrorLib.o.ErrorLib
MSGLIB		= ${LIBDIR}.MsgLib.o.MsgLib
NUMCONVERT	= ${LIBDIR}.NumConvert.o.numconv


LIBSM = \
 ${UILIB}zm \
 ${NFELIB}zm \
 ${EMAILLIB}zm\
 ${MSGLIB}zm \
 ${ERRORLIB} \
 ${TEXTGADGETLIB} \
 ${WIMPLIB} \
 ${TOOLBOXLIB} \
 ${EVENTLIB}m \
 ${SOCKLIB}zm \
 ${NUMCONVERT}zm \
 ${C++LIB}


LIBS = \
 ${UILIB} \
 ${NFELIB} \
 ${EMAILLIB}\
 ${MSGLIB} \
 ${ERRORLIB} \
 ${ACORNPOP3LIB} \
 ${ACORNSMTPLIB} \
 ${TEXTGADGETLIB} \
 ${WIMPLIB} \
 ${TOOLBOXLIB} \
 ${EVENTLIB} \
 ${SOCKLIB} \
 ${NUMCONVERT} \
 ${C++LIB} \
 ${CLIB}


LIBSD = \
 ${UILIB} \
 ${NFELIB} \
 ${EMAILLIB}\
 ${MSGLIB} \
 ${ERRORLIB} \
 ${TEXTGADGETLIB} \
 ${WIMPLIB} \
 ${TOOLBOXLIB} \
 ${EVENTLIB} \
 ${SOCKLIB} \
 ${C++LIB} \
 ${NUMCONVERT} \
 ${CLIB}


# -----------------------------------------------------------------------------
# Include files (libraries and resources)
#

INCLUDES        =\
-Itbox:,\
C:,\
TCPIPLibs:,\
<Lib$Dir>.NetFax.NFELib,\
<Lib$Dir>.Email.EmailLib,\
<Lib$Dir>.NetFax.UILib,\
<Lib$Dir>.MsgLib,\
<Lib$Dir>.ErrorLib,\
<Lib$Dir>.DebugLib

FILES = \
 ${LDIR}.!Boot \
 ${LDIR}.!Run \
 ${LDIR}.!Sprites \
 ${LDIR}.Messages \
 ${TARGET}

# Include these in the ROM module
RESFILES = \
 ${LDIR}.!Sprites 


# -----------------------------------------------------------------------------
# Include files (objects)
#

OBJS = \
 o.AddrBookIF \
 o.AddrFile \
