/* Wimp messages to/from !Schedule */
#define	Dayt_Setup_Message      0x50fc0     /* block 0x50FC0 allocated to setup */

#define Dayt_Schedule_Message   0x50fc4     /* part of block 0x50FC0 allocated to setup */
#define Dayt_Sched_Ack          0x50fc5


/* The Scheduler start message */
typedef struct dayt_schedblock
{
  struct
  {
    int size;
    int sender;
    int my_ref;
    int your_ref;
    int action_code;
  } Hdr;

  struct dayt_schedblockmessagedata
  {
    int  message_code; /* Currently a meaningless number */
  } data;
} Dayt_SchedBlock;


