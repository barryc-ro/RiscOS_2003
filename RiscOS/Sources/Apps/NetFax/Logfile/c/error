#include "include.h"
#include "tbox.h"
#include "miscwimp.h"
#include "error.h"

void error_wimp_error (_kernel_oserror *er)
{
   wimp_report_error (er, 0, miscwimp_lookup_message_token ("_TaskName"), 
                      0, 0, 0);
}

void error_check_error (_kernel_oserror *er)
{
  /* If an error has occurred */
  if (er != NULL)
  {
    /* Report it */
    error_wimp_error (er);
  }
}
