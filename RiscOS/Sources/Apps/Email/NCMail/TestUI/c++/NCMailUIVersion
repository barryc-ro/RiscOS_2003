// =============================================================================
// Include header files

#include "Version.h"
#include "VersionNum"


// =============================================================================
// Constants

const char lib_name[] = "NCMail Test UI component";
NCMailUIVersion* NCMailUIVersion::_instance = 0;


// =============================================================================
// Define member functions

/* -----------------------------------------------------------------------------
 */

NCMailUIVersion::NCMailUIVersion()
                :Version(lib_name,
                         Module_MajorVersion,
                         Module_MinorVersion,
                         Module_Version,
                         Module_Date)
{
}


Version* NCMailUIVersion::Instance(void)
{
  if (_instance == 0)
    _instance = new NCMailUIVersion();

  return _instance;
}
