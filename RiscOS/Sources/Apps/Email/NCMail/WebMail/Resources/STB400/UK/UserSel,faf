<SCRIPT LANGUAGE="JAVASCRIPT">

function find_public_path()
{
  var server_path = "http://"+getEnvVariable("inet_bootserver") + "/cgi-bin/email/";
  var public_path = getEnvVariable("public_dir");

  document.location.href = server_path+"accounts.pl?path="+public_path;
}

</SCRIPT>

