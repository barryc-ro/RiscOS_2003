        GET     Desk.sh.regdefs
        GET     h.swinos
        GET     Desk.sh.macros

; Desk_os_error *Desk_OS_Word(Desk_osword_number reason, void *Desk_param_block);

        PREAMBLE
        STARTCODE Desk_OS_Word
;
        STMFD   sp!, {lr}
        SWI     Desk_SWI_OS_Word + Desk_XOS_Bit
        Desk_Error2_CheckV
        ;MOVVC   r0, #0
        LDMFD   sp!, {pc}^
;
        END
