#!/bin/sh
TO=${FRESCO}/http
TO1=${FRESCO}
FROM=..
cp -p $FROM/c/certs $TO/certs.c
cp -p $FROM/c/csfs $TO/csfs.c
cp -p $FROM/h/csfs $TO/csfs.h
cp -p $FROM/c/http $TO/http.c
cp -p $FROM/h/http $TO/http.h
cp -p $FROM/c/mimemap $TO/mimemap.c
cp -p $FROM/h/mimemap $TO/mimemap.h
cp -p $FROM/c/module $TO/module.c
cp -p $FROM/h/notlayer $TO/notlayer.h
cp -p $FROM/c/predirect $TO/predirect.c
#
cp -p $FROM/h/httppub $TO/httppub.h
cp -p $FROM/h/pads $TO/pads.h
#
cp -p $FROM/h/status $TO1/commonsrc/status.h
#
cp -p $FROM/c/files $TO1/layers/riscos/files.c
cp -p $FROM/h/files $TO1/layers/files.h
cp -p $FROM/c/file $TO1/layers/riscos/file.c
cp -p $FROM/h/file $TO1/layers/file.h
#cp -p $FROM/c/layers $TO1/layers/riscos/layers.c
#cp -p $FROM/h/layers $TO1/layers/layers.h
cp -p $FROM/h/platforms $TO1/layers/platforms.h
#
cp -p $FROM/h/types $TO1/libs/riscos_lib/types.h
#
echo put files into Master source tree
