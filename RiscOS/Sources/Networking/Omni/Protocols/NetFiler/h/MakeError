  err_NONET,
  err_BADMOUNT,
  err_BADPRINT,
  err_NOMEM,
  err_NOOMNI,
  err_NOSERVER,
  err_CONNECTED,

  err_UNKNOWN
} err_number_type;

extern _kernel_oserror *MakeError(int n);
