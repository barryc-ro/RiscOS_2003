help-string: 		OmniNFS 0.64

irq-handlers: inet_entry/inet_handler
irq-handlers: timer_entry/timer_handler
irq-handlers: callback_entry/callback_handler

swi-chunk-base-number: 	0x4A480

swi-handler-code: 	onfs_swi

