        GET     Hdr:ListOpts
        GET     Hdr:Macros
        GET     Hdr:System
        GET     Hdr:EnvNumbers
        GET     Hdr:ModHand

        AREA    handlink,PIC,CODE,REL,READONLY

	EXPORT callproto
        ALIGN

callproto	MOV R12,R3
;
; Make sure we don't change the fucking mode!
;
		MOV r3, pc
		AND r3, r3, #&fc000003
		ORR r2, r2, r3

		MOV pc, R2

	END
