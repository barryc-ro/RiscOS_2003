/* -*-c-*- */
/* pea.config.h */

#ifndef included_pea_config_h
#define included_pea_config_h

#define CARD_WIDTH		8
#define HANDLE_8005		1
#define HANDLE_8004		1
#define HANDLE_8BIT		1
#define HANDLE_POCKET		1
#define REQUIRE_MEMC1a		1
#define SUPPORT_8005		0		/* @@@@ */
#define SUPPORT_8004		1

#define MEMORY_MAPPED		0

#define HAS_CMOS		0

#define IRQS_DEVICE		1		/* IRQs via the device sources */
#define DEVICE_VECTOR_NUMBER	0		/* Parallel port device number */

#define FLAVOUR_STRING		"Parallel port"	/* Features we require */

#define MAXSWI                  10
#define MAXCLI                  1

/* Imported/exported official numbers and strings */

#define SWI_BASE                0x44F80         /* Official SWI chunk */
#define CARDSTR                 "ep"            /* Official ethernet card type string */
#define CARDSTR_CAP		"Ep"            /* Used for the count variable */
#define EA_TYPE                 ""              /* Official product type code */
#define EA_ERROR_BASE           0x00800900      /* Official Borris error base 16 for ethera */
#define TITLE_STRING            "EtherP"

#endif /* included_pea_config_h */
