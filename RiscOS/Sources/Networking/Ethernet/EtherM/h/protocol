/*1 protocol.h */

#ifndef included_protocol_h
#define included_protocol_h

typedef struct client
{
    struct client          *next;
    unsigned long           flags;
    unsigned int            frmlvl;
    unsigned int            addrlvl;
    unsigned int            errlvl;
    unsigned int            protocol_handle;
    void		   *rcv_fn;
} client;


#endif /* included_protocol_h */
