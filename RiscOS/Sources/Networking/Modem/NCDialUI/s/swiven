        GET     h.regdefs
        GET     h.swinos
        GET     h.macros
;
        PREAMBLE

        STARTCODE  MAM_Enq

        STMFD   sp!, {a2-a3, lr}
        SWI     SWI_NCAccessManager_Enquiry + XOS_Bit
        LDMFD   sp!, {a2-a3, pc}
        
        STARTCODE  ncregenq

        STMFD   sp!, {a2-a3, lr}
        SWI     SWI_NCRegistry_Enquiry + XOS_Bit
        LDMFD   sp!, {a2-a3, pc}
        
        STARTCODE  callback

        STMFD   sp!, {a1-a2, lr}
        SWI     SWI_OS_AddCallBack + XOS_Bit
        LDMFD   sp!, {a1-a2, pc}
        
        STARTCODE  callafter

        STMFD   sp!, {a1-a3, lr}
        SWI     SWI_OS_CallAfter + XOS_Bit
        LDMFD   sp!, {a1-a3, pc}
        
        STARTCODE  removeticker

        STMFD   sp!, {a1-a2, lr}
        SWI     SWI_OS_RemoveTickerEvent + XOS_Bit
        LDMFD   sp!, {a1-a2, pc}
        
        STARTCODE  monotonic_time

        STMFD   sp!, {lr}
        SWI     SWI_OS_ReadMonotonicTime + XOS_Bit
        LDMFD   sp!, {pc}
        
        STARTCODE  oscli

        STMFD   sp!, {a1, lr}
        SWI     SWI_OS_CLI + XOS_Bit
        LDMFD   sp!, {a1, pc}
        
        END
