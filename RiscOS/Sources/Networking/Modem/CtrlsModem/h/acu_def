    ubyte reserved5:           1;      /* bit 5 */
    ubyte reserved4:           1;      /* bit 4 */
    ubyte half_duplex:         1;      /* bit 3 */
    ubyte asym_symbols:        1;      /* bit 2 */
    ubyte asym_rates:          1;      /* bit 1 */
