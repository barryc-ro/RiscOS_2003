#ifndef _icd_msgs_h
#define _icd_msgs_h

#define PACKET_AUDIO_CONTROL	0x01
#define PACKET_CHANNEL_CONTROL	0x02
#define PACKET_MODE_CONTROL	0x03
#define PACKET_PA_POWER_LEVEL_CONTROL	0x04
#define PACKET_RADIO_POWER_CONTROL	0x05
#define PACKET_DTMF_CONTROL	0x06
#define PACKET_MODE_MANAGER_SPARE0	0x07
#define PACKET_RUN_DSP	0x08
#define PACKET_DSP_PROGRAM_MEMORY_READ	0x09
#define PACKET_DSP_DATA_MEMORY_READ_B0	0x0A
#define PACKET_DSP_DATA_MEMORY_READ_B1	0x0B
#define PACKET_DSP_PROGRAM_MEMORY_WRITE	0x0C
#define PACKET_DSP_DATA_MEMORY_WRITE_B0	0x0D
#define PACKET_DSP_DATA_MEMORY_WRITE_B1	0x0E
#define PACKET_ENTER_MON	0x0F
#define PACKET_SLEEP_REQUEST	0x10
#define PACKET_SLEEP_RESPONSE_ACK	0x11
#define PACKET_MODE_REQUEST	0x10
#define PACKET_MODE_RESPONSE	0x11
#define PACKET_SLEEP_RESPONSE	0x12
#define PACKET_RSSI_XOVER	0x12
#define PACKET_WAKEUP	0x13
#define PACKET_PROGRAM_MEMORY_READBACK	0x14
#define PACKET_DATA_MEMORY_READBACK	0x15
#define PACKET_BREAK_RSP	0x16
#define PACKET_BADINT	0x17
#define PACKET_BADINT2	0x18
#define PACKET_BADRST	0x19
#define PACKET_DIAGNOSTICS_COMMAND	0x30
#define PACKET_DIAGNOSTICS_RESULT	0x31
#define PACKET_AMPS_MODE_CONTROL	0x40
#define PACKET_RECC_MESSAGE	0x41
#define PACKET_RVC_MESSAGE	0x42
#define PACKET_ST_CONTROL	0x43
#define PACKET_TRANSMISSION_CONTROL	0x44
#define PACKET_FVC_MESSAGE	0x50
#define PACKET_FOCC_MESSAGE	0x51
#define PACKET_SAT	0x52
#define PACKET_SETUP_ACK	0x53
#define PACKET_SYNC_FAILURE_STATUS	0x54
#define PACKET_TRANSMISSION_STATUS	0x55
#define PACKET_MODEM_INIT	0x60
#define PACKET_MODEM_INIT_USART	0x61
#define PACKET_MODEM_GUARD_TONE	0x62
#define PACKET_MODEM_SET_PARAMS	0x63
#define PACKET_MODEM_SET_RATE_SEQ	0x64
#define PACKET_MODEM_INIT_CALL_PROGRESS	0x65
#define PACKET_MODEM_CONFIG_DIAL	0x66
#define PACKET_MODEM_DIAL_DIGIT	0x67
#define PACKET_MODEM_CONNECT	0x68
#define PACKET_MODEM_RETRAIN	0x69
#define PACKET_MODEM_SET_EXT_SYSDEL    0x6A
#define PACKET_MODEM_RENEGOTIATE	0x6B
#define PACKET_MODEM_RDL_ENABLE	0x6C
#define PACKET_MODEM_SEND_RDL_REQUEST	0x6D
#define PACKET_MODEM_ALB	0x6E
#define PACKET_MODEM_SEND_RDL_RELEASE	0x6F
#define PACKET_MODEM_GET_BAUD_RATE	0x70
#define PACKET_MODEM_HOOK_WAKEUP_RESET	0x71
#define PACKET_MODEM_GET_MODE	0x72
#define PACKET_MODEM_FAX_CONFIGURE	0x73
#define PACKET_MODEM_FAX_RTS_OFF	0x74
#define PACKET_MODEM_FAX_RTS_ON	0x75
#define PACKET_MODEM_FAX_TEST_ENABLE	0x76
#define PACKET_MODEM_TX_FLOW_CTL	0x77
#define PACKET_MODEM_TX_BREAK	0x78
#define PACKET_MODEM_INIT_RX_FLOW_CTL	0x79
#define PACKET_MODEM_DIGITAL_LOOPBACK	0x7A

/* jeb 6//27/95 changes for MNP10 and ETC */
#define  PACKET_MODEM_SET_TX_LEVEL     0x7B
#define  PACKET_MODEM_SEND_TONE        0x7C
#define  PACKET_MODEM_DETECT_TONE      0x7D
#define  PACKET_MODEM_CONNECT_NOAT     0x7E
#define  PACKET_MODEM_ETC_DEEMPHASIS   0x7F

/*  These old ones are not supported anymore */
/*#define PACKET_MODEM_DP10_SET_TX_LEVEL	0x7B
#define PACKET_MODEM_DP10_INIT_ANSWER_TONE_DETECT	0x7C
#define PACKET_MODEM_DP10_SEND_ANSWER_TONE	0x7D
#define PACKET_MODEM_GET_EQM10_VALUE	0x7E
#define PACKET_MODEM_GET_AGC10_VALUE	0x7F */

#define PACKET_MODEM_HOOK_RELAY	0x80
#define PACKET_MODEM_SPEAKER	0x81
#define PACKET_MODEM_READ_DSP_RAM   0x82
#define PACKET_MODEM_WRITE_DSP_RAM                      0x83
#define PACKET_MODEM_DISPLAY_DIAGNOSTICS                0x84
/* start change 10/11/95 - jeb */
#define PACKET_MODEM_SET_MAX_LINE_RATE    0x85
/* end change 10/11/95 - jeb */
#define PACKET_MODEM_UN_TEST_FLAGS                      0x87
#define PACKET_MODEM_SET_DTMF_LEVELS                    0x88
#define PACKET_MODEM_GET_ADDRESS                        0x89
#define PACKET_MODEM_GET_LINK_INFO                      0x8A
#define PACKET_MODEM_CLEAR_DATA_BUFFER                  0x8B
#define PACKET_MODEM_SET_BLANK_AND_BURST                0x8C
#define PACKET_MODEM_GET_NECO                           0x8D
#define PACKET_MODEM_CHECK_V21_ON_FAX                   0x8E
#define PACKET_MODEM_SET_ETC                            0x8F
#define PACKET_MODEM_GET_V34_INFO                       0x91
#define PACKET_MODEM_V34_CONFIGURE                      0x90
#define PACKET_MODEM_CID_RELAY	0x91
#define PACKET_MODEM_VOICE_RELAY	0x92
#define PACKET_MODEM_GET_VERSION    0x94
#define PACKET_MODEM_VCE_INIT	0x97
#define PACKET_MODEM_VCE_SET_LEVEL	0x98
#define PACKET_MODEM_VCE_DETECT_SILENCE	0x99
#define PACKET_MODEM_VCE_COMPRESSION_INIT	0x9A
#define PACKET_MODEM_VCE_RETURN_TO_IDLE	0x9B
#define PACKET_MODEM_VCE_MICROPHONE	0x9C
#define PACKET_MODEM_VCE_PGC_INIT   0x9C
#define PACKET_MODEM_VCE_GPIO_OPERATION                 0x9D

#define PACKET_MODEM_SET_MNP10_MASK 0xA7

#define PACKET_DP_ACK	0xC0
#define PACKET_DP_DATA_RATE	0xC1
#define PACKET_DP_MODE	0xC2
#define PACKET_DP_VCE_GPIO_STATUS                       0xC3
#define PACKET_DP_DSP_RAM  0xC5
#define PACKET_DP_V34_INFO                              0xC6
#define PACKET_DP_ACQ10_STATUS	0xC6
#define PACKET_DP_VERSION_INFO  0xCB
#define PACKET_TEI_VALUE	0xE0
#define PACKET_XID_PARAMETERS	0xE1
#define PACKET_LOOK_FOR_SYNC	0xE2
#define PACKET_ABORT_LOOK_FOR_SYNC	0xE3
#define PACKET_MAC_FLUSH_REQUEST	0xE4
#define PACKET_MAC_DATA_REQUEST_START	0xE5
#define PACKET_MAC_DATA_REQUEST_CONTINUE	0xE6
#define PACKET_DSP_TEST_MODE	0xE7
#define PACKET_START_TEST	0xE8
#define PACKET_STOP_TEST	0xE9
#define PACKET_COLOR_CODE	0xF0
#define PACKET_FORWARD_CHANNEL_USAGE_STATISTICS	0xF1
#define PACKET_MAC_DATA_CONFIRMATION	0xF2
#define PACKET_MAC_ERROR_INDICATION	0xF3
#define PACKET_REVERSE_CHANNEL_USAGE_STATISTICS	0xF4
#define PACKET_SYSTEM_TIME_TICK	0xF5
#define PACKET_SYSTEM_TIME	0xF6
#define PACKET_MAC_DATA_INDICATION_START	0xF7
#define PACKET_MAC_DATA_INDICATION_CONTINUE	0xF8
#define PACKET_MAC_FLUSH_CONFIRMATION	0xF9
#define PACKET_SYNC_STATUS	0xFA
#define PACKET_RSSI_VALID	0xFB
#define PACKET_SYNC_NOT_FOUND	0xFC
#define PACKET_TRANSMIT_DONE	0xFD

#endif
