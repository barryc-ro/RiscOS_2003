         ubyte d, ubyte e, ubyte f,\
         ubyte g, ubyte h, ubyte i,\
         ubyte j, ubyte k, ubyte l,\
         ubyte m, ubyte n)
{
   a = b = c = d = e = f = g = h = i = j = k = l = m = n;
}
