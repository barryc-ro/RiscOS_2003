
#define  REPORT  470
/*max ring off time 450mSec, min time before callerid mesg, 500 */

#define   CID_RELAY_ON   0x0000                  /* low active */
#define   CID_RELAY_OFF  0x0200
#define   CID_MASK_BIT   0x2000

#define  PACKET_MODEM_CID       0x93
#define  PACKET_MODEM_CID_LEN   1


#define  ACK                    0
#define  BUFLEN                 128


/* cid relay on gpio5 */
#ifndef __riscos
#define  GPCR0          0xffc210   /* config reg, 0 input, 1 output */
#define  GPR0           0xffc204   /* if as output,value reg */
#endif

#define   OK             0
#define   NO_CID_INFO    100
#define   CID_ERROR      4
