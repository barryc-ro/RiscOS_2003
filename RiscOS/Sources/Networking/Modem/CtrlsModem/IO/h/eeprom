#ifndef __riscos
/* 3-2096 */
#define   GPR0	  	0xffc204
#define   GPCR0	0xffc210
#endif

#define   READ         0xc0000000
#define   WRITE        0xa0000000  /*Write command with a leading zero*/
#define   EWEN         0x98000000  /*Write enable comd with a leading zero*/
#define   EWDS         0x80000000  /*Write disable with a leading zero*/
#define   ERASE        0xe0000000
#define   WRAL         0x88000000
#define   ERAL         0x90000000

#define   MASK_bit_31  0x80000000  /*Mask only bit 31 for the serial data*/

#define   WRITE_NO_OF_BITS    20   /*testing No. left shift for the write command*/
#define   SHIFT_OFFSET_L_VAL  20   /*EEprom addr left shift for write comd */
#define   SHIFT_DATA_L_VAL    12   /*EEprom data left shift for write comd */

#define   READ_NO_BITS        8					/*8*/
#define   WRITE_THE_READ_SEQ  12
#define   SER_COUNT_EN_DS     13   /*left shifts for Enable and Disable comds*/
											  /* may not be necessary 20, try 9+4=13? */
#define  DO_MASK    0x01		/* 1-2-96 0x40 */

#define  DATA_OUT   0x11
#define  DATA_IN    0x10

#define  DO_HI      0x11
#define  DO_LO      0x10

#define  CS_HI      0x22
#define  CS_LO      0x20

