PNIAR    EQU   &00FFC530 ;pnp interface addr reg	   8 bits
PNIDR    EQU   &00FFC534 ;pnp interface data reg	   8 bits
PNSR	  EQU   &00FFC53C ;pnp status &00 control reg 16 bits

PRPCBR0  EQU   &00FFC520 ;pnp RES Base reg, low 16 bits
PRPCBR1  EQU   &00FFC524 ;pnp RES Base reg, upper 8 bits

IDMS     EQU   &FFC540   ; IRQ and DMA mode select
IRQMAP0  EQU   &FFC544   ;host int and map reg 0  
IRQMAP1  EQU   &FFC548   ;host int and map reg 1  

;PRPCBR		EQU	&00FFC538 ;pnp RES Base reg 		  24 bits

PNP_RDS_ADDR	EQU	&FF7E00	;pnp RDS start addr in RAM
       END
