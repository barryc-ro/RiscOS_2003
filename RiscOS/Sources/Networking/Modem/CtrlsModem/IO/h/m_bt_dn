START_OF_BOOT_CODE			EQU 	&40100
BOOT_CODE_INFO					EQU	&40004

UPLD_DNLD_REGIS_F8 			EQU	&f8
UPLD_DNLD_REGIS_F9 			EQU	&f9
UPLD_DNLD_DATA_REGIS			EQU	&fa
CP_CONTROL_REGISTER 			EQU	&fc
CP_DNLD_ENABLE_FLAG 			EQU	&0a
CP_RESET_AND_RUN_BOOT_CODE EQU	&09
INT_RESET_VECTOR				EQU	&0800
WORD_INC_OFFSET				EQU	4
COM_RAM							EQU &90000

	END
