;equals for download main code to cutlass 12-20-95

START_OF_BOOT_CODE				EQU 	&40100
BOOT_CODE_INFO						EQU	&40004
MAIN_CODE_INFO						EQU	BOOT_CODE_INFO + &0C
WORD_INC_OFFSET					EQU	4
EXT_RESET_VECTOR 					EQU	&FFFF

CP_DNLD_ENABLE_FLAG 				EQU	&0a

DOWNLOAD_REG_E8					EQU	&e8 ;added by ramn on 1/18/96
DOWNLOAD_REG_E9					EQU	&e9 ; needed for page switching
DOWNLOAD_REG_EA					EQU	&ea
DOWNLOAD_REG_EB					EQU	&eb
DOWNLOAD_REG_EC					EQU	&ec
DOWNLOAD_REG_ED					EQU	&ed
DOWNLOAD_REG_EE					EQU	&ee
DOWNLOAD_REG_EF					EQU	&ef

DNLD_WORD_READY					EQU	&0055		;x 
CUTLASS_DONE						EQU	&00AA		;u  
VERIFY_ADDR_READY					EQU	&0088
CP_CONTROL_REGISTER 				EQU	&fc
CP_RESET_AND_RUN_BOOT_CODE 	EQU	&09
COM_RAM								EQU   &90000

SELECT_PLL_CLK_VAL				EQU	&00
SELECT_PLL_CLK_LOC				EQU	&FD

PAGING_MASK							EQU 	&8000

	END
