/   -------------------------------------------------------------
/
/   Author & Date: RSA
/   Description:
/   Reason:
/
*******************************************************************************/
void hy_SVV(ubyte *parm_ptr)
{
    /* if on hook.. */
    /* RK 2/16/96 check handset also */
    if (hook && (modem_handset_hook()== HANDSET_ON_HOOK) )
         acu_vcv_command_error();
   else
       acu_vcv_command(parm_ptr, VCV_SVV_COMMAND);

}
