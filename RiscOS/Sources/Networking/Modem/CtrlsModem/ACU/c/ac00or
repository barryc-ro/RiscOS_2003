#include "sys_def.h"
#include "acu_def.h"
#include "acu_mem.h"
#include "acu_pro.h"
#include "ll_pro.h"
#include "v25_pro.h"
/* start change 1/25/95 keh => add amps */
#include "wl_pro.h"
/* end change 1/25/95 keh => add amps */


void orig_bg(void);
void proc_orig(void);

/*******************************************************************************
/
/	Routine: orig_bg
/
/	Author : RSA
/
/	Creation Date: 06/17/91
/
/	Description : This Routine will process the background functions for
/		      the ORIG state.
/
/	Input Parameters: None
/
/	Output Parameters: none
/
/	Global Variables Changed:  None
/
/	Users: acu_bg
/
/	-------------------------------------------------------------
/	-			Modifications			    -
/	-------------------------------------------------------------
/
/	Author & Date: RSA
/	Description:
/	Reason:
/
*******************************************************************************/
void orig_bg(void)
{
   switch (st_orig.type)
   {
/* start change 1/25/95 keh => add amps */
       case PSTN_PHONE:
           /* Intentionally fall through to AT_PSTN */
/* end change 1/25/95 keh => add amps */
       case AT_PSTN:
           orig_pstn_bg();
           break;

       case V25_PSTN:
           /*orig_v25_pstn_bg();  */    /* ramn 9/27/96 */
           break;

       case LEASED_LINE:
           /*orig_ll_bg();*/  /* ramn 9/23/96 */
           break;


       case VOICE:
           orig_voice_bg();
           break;


/* start change keh 8/21/95 => Add VV to ACU */
       case VOICE_VIEW:
           orig_vv_bg();
           break;
/* end change keh 8/21/95 => Add VV to ACU */


/* start change 1/25/95 keh => add amps */
       case WIRELESS_DATA:
       case WIRELESS_FAX_ONLINE:
       case WIRELESS_VOICE:
       case WIRELESS_PHONE:
           /* wl_orig_bg();*/ /* ramn 9/26/96 */
           break;
/* end change 1/25/95 keh => add amps */
   }
}

/*******************************************************************************
/
/	Routine: proc_orig
/
/	Author : RSA
/
/	Creation Date: 06/17/91
/
/	Description : This Routine will process the events for the ORIG state.
/
/	Input Parameters: None
/
/	Output Parameters: none
/
/	Global Variables Changed:  None
/
/	Users: auto_dialer
/
/	-------------------------------------------------------------
/	-			Modifications			    -
/	-------------------------------------------------------------
/
/	Author & Date: RSA
/	Description:
/	Reason:
/
*******************************************************************************/
void proc_orig(void)
{
   switch (st_orig.type)
   {
       case AT_PSTN:
           proc_orig_at();
           break;

       case V25_PSTN:
           /* proc_orig_v25();  */    /* ramn 9/27/96 */
           break;
       
       case LEASED_LINE:
           /*proc_orig_ll();*/  /* ramn 9/23/96 */
           break;

/* start change keh 8/21/95 => Add VV to ACU */
       case VOICE_VIEW:
           proc_orig_vv();
           break;
/* end change keh 8/21/95 => Add VV to ACU */


       case VOICE:
           proc_orig_voice();
           break;

/* start change 1/25/95 keh => add amps */
       case PSTN_PHONE:
		 /*
           proc_orig_pstn_phone();
           break;
		 */ /* ramn 9/26/96 */

       case WIRELESS_DATA:
       case WIRELESS_FAX_ONLINE:
       case WIRELESS_VOICE:
		 /*
           proc_wl_orig();
           break;
		 */ /* ramn 9/26/96 */

       case WIRELESS_PHONE:
		 /*	
		     proc_wl_orig_phone();   */ /* ramn 9/26/96 */
           break;
/* end change 1/25/95 keh => add amps */
   }
}

