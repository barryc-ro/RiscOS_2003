#include "sys_def.h"
#include "par_def.h"

PARSE_CMD_TABLE_STRUCT *parse_tbls_array[MAX_PARSE_TABLES];
PARSE_CMD_TABLE_STRUCT *parse_tbls_suspended[MAX_PARSE_TABLES];
ubyte parse_tbls_count;
ubyte parse_tbls_susp_cnt;

PARSE_CMD_TABLE_STRUCT *parse_table_ptr;
ubyte *parse_cmd_ptr;
ubyte parse_current_table;
ubyte parse_state;
ubyte pcs_parsed;
ubyte parse_p_cnt;

/* Define buffer as a long array to force even alignment required for some
    machines.  This parameter buffer is used to hold uwords or ubytes */
/* aka Parser AT Command Parameter Buffer */
/* aka Parse Parameter Buffer */
/* keh + */
PARSE_PARAM_BUFFER parse_p_buffer;
#if 0
union 
{
/* keh + */
   ubyte empty[0];
/* keh - */
   ubyte bdata[PARM_BUFF_SIZE];
   uword wdata[PARM_BUFF_SIZE/2];
   ulong ldata[PARM_BUFF_SIZE/2];
} parse_p_buffer;
#endif
/* keh - */

/* Null AT Parser Command Table used for error conditions */
PARSE_CMD_TABLE_STRUCT null_commands []=
{
    END_OF_PARSE_TABLE
};

