#include "sys_def.h"
#include "acu_def.h"
#include "acu_mem.h"
#include "acu_pro.h"
#include "v25_pro.h"
#include "ll_pro.h"
/* start change 1/26/95 keh => add amps */
#include "wl_pro.h"
/* end change 1/26/95 keh => add amps */


void parse_bg(void);
void proc_parse(void);

/*******************************************************************************
/
/    Routine: parse_bg
/
/    Author : RSA
/
/    Creation Date: 06/17/91
/
/    Description : This routine will process the background functions for
/              the PARSE state.
/
/    Input Parameters: None
/
/    Output Parameters: none
/
/    Global Variables Changed:  None
/
/    Users: acu_bg
/
/    -------------------------------------------------------------
/    -            Modifications                -
/    -------------------------------------------------------------
/
/       Author & Date: RSA
/       Description:
/       Reason:
/
*******************************************************************************/
void parse_bg(void)
{
   switch (st_parse.type)
   {
       case TEST:
           /* Keep the test running while parsing AT commands.  This
               allows for propper test time-out and self test data
               transfer */
           idle_test_bg();

           /* Intentionally fall through to parse the AT commands */

       case AT_OFFLINE:
       case AT_ONLINE:
       case VOICE:
       case FAX_ONLINE:
/* start change 1/26/95 keh => add amps */
       case AT_PSTN_PHONE:
       case AT_WIRELESS_PHONE:
       case AT_WIRELESS_DATA:
       case AT_WIRELESS_FAX:
/* end change 1/26/95 keh => add amps */
/* start change keh 8/21/95 => Add VV to ACU */
       case VOICE_VIEW:
/* end change keh 8/21/95 => Add VV to ACU */
           parse_at_bg();
           break;

       case LL_ONLINE:
           /*parse_ll_bg();*/  /* ramn 9/23/96 */
           break;

       case V25BIS:
           /* parse_v25_bg(); */    /* ramn 9/27/96 */
           break;
   }
}


/*******************************************************************************
/
/    Routine: proc_parse
/
/    Author : RSA
/
/    Creation Date: 06/17/91
/
/    Description : This routine will process the events for the PARSE
/              state.
/
/    Input Parameters: None
/
/    Output Parameters: none
/
/    Global Variables Changed:  None
/
/    Users: auto_dialer
/
/    -------------------------------------------------------------
/    -            Modifications                -
/    -------------------------------------------------------------
/
/       Author & Date: RSA
/       Description:
/       Reason:
/
*******************************************************************************/
void proc_parse(void)
{
   switch (st_parse.type)
   {
       case AT_OFFLINE:
           proc_parse_offline();
           break;

       case AT_ONLINE:
           proc_parse_online();
           break;
        
       case LL_ONLINE:
          /* proc_parse_ll();*/  /* ramn 9/23/96 */
           break;

       case V25BIS:
           /* proc_parse_v25();*/    /* ramn 9/27/96 */
           break;

       case TEST:
           proc_parse_test();
           break;

       case VOICE:
           proc_parse_voice();
           break;

       case FAX_ONLINE:
           proc_parse_fax_on();
           break;

/* start change keh 8/21/95 => Add VV to ACU */
       case VOICE_VIEW:
           proc_parse_vv();
           break;
/* end change keh 8/21/95 => Add VV to ACU */

/* start change 1/26/95 keh => add amps */
       case AT_PSTN_PHONE:
         /*
			  proc_parse_pstn_phone(); 
           break;	
		   */ /* ramn 9/26/96 */

       case AT_WIRELESS_FAX:
         /* 
			  proc_wl_parse_fax_on();	  
           break;
			*/ /* ramn 9/26/96 */

       case AT_WIRELESS_PHONE:
       case AT_WIRELESS_DATA:
           /*
			  proc_wl_parse();*/ /* ramn 9/26/96 */
           break;
/* end change 1/26/95 keh => add amps */
   }
}

