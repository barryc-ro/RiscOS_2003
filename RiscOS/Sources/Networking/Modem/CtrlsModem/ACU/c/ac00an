#include "sys_def.h"
#include "acu_def.h"
#include "acu_mem.h"
#include "acu_pro.h"
#include "v25_pro.h"
#include "ll_pro.h"
/* start change 1/26/95 keh => add amps */
#include "wl_pro.h"
/* end change 1/26/95 keh => add amps */

void answ_bg(void);
void proc_answ(void);

/*******************************************************************************
/
/    Routine: answ_bg
/
/    Author : RSA
/
/    Creation Date: 06/17/91
/
/    Description : This Routine will process the background functions for
/              the ANSW state.
/
/    Input Parameters: None
/
/    Output Parameters: none
/
/    Global Variables Changed:  None
/
/    Users: acu_bg
/
/    -------------------------------------------------------------
/    -            Modifications                -
/    -------------------------------------------------------------
/
/    Author & Date:    RSA
/    Description:
/    Reason:
/
*******************************************************************************/
void answ_bg(void)
{
   switch (st_answ.type)
   {
/* start change 1/25/95 keh => add amps */
       case PSTN_PHONE:
           /* Intentionally fall through to AT_PSTN */
/* end change 1/25/95 keh => add amps */
       case AT_PSTN:
       case V25_PSTN:
           answ_pstn_bg();
           break;

       case LEASED_LINE:
           /*answ_ll_bg();*/  /* ramn 9/23/96 */
           break;

/* start change keh 8/21/95 => Add VV to ACU */
       case VOICE_VIEW:
           answ_vv_bg();
           break;
/* end change keh 8/21/95 => Add VV to ACU */

/* start change 2/1/95 keh => add amps */
       case WIRELESS_DATA:
       case WIRELESS_FAX_ONLINE:
       case WIRELESS_VOICE:
       case WIRELESS_PHONE:
           /*wl_answ_bg();*/ /* ramn 9/26/96 */
           break;
/* end change 2/1/95 keh => add amps */
   }
}


/*******************************************************************************
/
/    Routine: proc_answ
/
/    Author : RSA
/
/    Creation Date: 06/17/91
/
/    Description : This Routine will process the events for the ANSW state.
/
/    Input Parameters: None
/
/    Output Parameters: none
/
/    Global Variables Changed:  None
/
/    Users: auto_dialer
/
/    -------------------------------------------------------------
/    -            Modifications                -
/    -------------------------------------------------------------
/
/    Author & Date:    RSA
/    Description:
/    Reason:
/
*******************************************************************************/
void proc_answ(void)
{
   switch (st_answ.type)
   {
       case AT_PSTN:
           proc_answ_at();
           break;

       case V25_PSTN:
           /*proc_answ_v25();  */    /* ramn 9/27/96 */
           break;

       case LEASED_LINE:
           /*proc_answ_ll();*/  /* ramn 9/23/96 */
           break;

/* start change keh 8/21/95 => Add VV to ACU */
       case VOICE_VIEW:
           proc_answ_vv();
           break;
/* end change keh 8/21/95 => Add VV to ACU */

/* start change 2/1/95 keh => add amps */
       case PSTN_PHONE:
           /* ANSWER and ORIG share the same event Table for Phone mode */
       /*
              proc_answ_pstn_phone();
           break;
         */ /* ramn 9/26/96 */

       case WIRELESS_DATA:
       case WIRELESS_FAX_ONLINE:
       case WIRELESS_VOICE:
       /*
             proc_wl_answ();
           break;
         */ /* ramn 9/26/96 */

       case WIRELESS_PHONE:
       /*
             proc_wl_answ_phone();  */ /* ramn 9/26/96 */
           break;
/* end change 2/1/95 keh => add amps */
   }
}

