/******************************************************************************
/
/              Parser Structure for ROCKWELL Voice AT Commands
/              (only accepted when #CLS=8, per specification) 
/
/  -------------------------------------------------------------
/  -                       Modifications                       -
/  -------------------------------------------------------------
/
/  Author & Date: RSA
/  Description:
/  Reason:
/
*******************************************************************************/
/* Start Change 8/24/1995. */
PARSE_CMD_TABLE_STRUCT voice_rockwell_commands []=
{
    (ubyte *) ";",      rw_semicolon, NO_PARAM,       NO_FLAGS, 
    (ubyte *) "#VBS=?", pnd_VBS_eq, NO_PARAM,       NO_FLAGS, 
    (ubyte *) "#VBS=",  pnd_VBS_e,  BYTE_NUMERIC,   NO_FLAGS, 
    (ubyte *) "#VBS?",  pnd_VBS_q,  NO_PARAM,       NO_FLAGS,
    (ubyte *) "#VBT=?", pnd_VBT_eq, NO_PARAM,       NO_FLAGS, 
    (ubyte *) "#VBT=",  pnd_VBT_e,  BYTE_NUMERIC,   NO_FLAGS, 
    (ubyte *) "#VBT?",  pnd_VBT_q,  NO_PARAM,       NO_FLAGS,
    (ubyte *) "#VBQ?",  pnd_VBQ_q,  NO_PARAM,       NO_FLAGS,
    (ubyte *) "#VCI?",  pnd_VCI_q,  NO_PARAM,       NO_FLAGS, 
    (ubyte *) "#VGR=?", pnd_VGR_eq, NO_PARAM,       NO_FLAGS, 
    (ubyte *) "#VGR=",  pnd_VGR_e,  BYTE_NUMERIC,   NO_FLAGS, 
    (ubyte *) "#VGR?",  pnd_VGR_q,  NO_PARAM,       NO_FLAGS,
    (ubyte *) "#VGT=?", pnd_VGT_eq, NO_PARAM,       NO_FLAGS, 
    (ubyte *) "#VGT=",  pnd_VGT_e,  BYTE_NUMERIC,   NO_FLAGS, 
    (ubyte *) "#VGT?",  pnd_VGT_q,  NO_PARAM,       NO_FLAGS,
    (ubyte *) "#VLS=?", pnd_VLS_eq, NO_PARAM,       NO_FLAGS, 
    (ubyte *) "#VLS=",  pnd_VLS_e,  BYTE_NUMERIC,   NO_FLAGS, 
    (ubyte *) "#VLS?",  pnd_VLS_q,  NO_PARAM,       NO_FLAGS,
    (ubyte *) "#VRA=?", pnd_VRA_eq, NO_PARAM,       NO_FLAGS, 
    (ubyte *) "#VRA=",  pnd_VRA_e,  BYTE_NUMERIC,   NO_FLAGS, 
    (ubyte *) "#VRA?",  pnd_VRA_q,  NO_PARAM,       NO_FLAGS,
    (ubyte *) "#VRN=?", pnd_VRN_eq, NO_PARAM,       NO_FLAGS, 
    (ubyte *) "#VRN=",  pnd_VRN_e,  BYTE_NUMERIC,   NO_FLAGS, 
    (ubyte *) "#VRN?",  pnd_VRN_q,  NO_PARAM,       NO_FLAGS,
    (ubyte *) "#VRX",   pnd_VRX_e,  NO_PARAM,       NO_FLAGS,
    (ubyte *) "#VSD=?", pnd_VSD_eq, NO_PARAM,       NO_FLAGS, 
    (ubyte *) "#VSD=",  pnd_VSD_e,  BYTE_NUMERIC,   NO_FLAGS, 
    (ubyte *) "#VSD?",  pnd_VSD_q,  NO_PARAM,       NO_FLAGS,
    (ubyte *) "#VSK=?", pnd_VSK_eq, NO_PARAM,       NO_FLAGS, 
    (ubyte *) "#VSK=",  pnd_VSK_e,  BYTE_NUMERIC,   NO_FLAGS, 
    (ubyte *) "#VSK?",  pnd_VSK_q,  NO_PARAM,       NO_FLAGS,
    (ubyte *) "#VSP=?", pnd_VSP_eq, NO_PARAM,       NO_FLAGS, 
    (ubyte *) "#VSP=",  pnd_VSP_e,  BYTE_NUMERIC,   NO_FLAGS, 
    (ubyte *) "#VSP?",  pnd_VSP_q,  NO_PARAM,       NO_FLAGS,
    (ubyte *) "#VSR=?", pnd_VSR_eq, NO_PARAM,       NO_FLAGS, 
    (ubyte *) "#VSR=",  pnd_VSR_e,  WORD_NUMERIC,   NO_FLAGS, 
    (ubyte *) "#VSR?",  pnd_VSR_q,  NO_PARAM,       NO_FLAGS,
    (ubyte *) "#VSS=?", pnd_VSS_eq, NO_PARAM,       NO_FLAGS, 
    (ubyte *) "#VSS=",  pnd_VSS_e,  BYTE_NUMERIC,   NO_FLAGS, 
    (ubyte *) "#VSS?",  pnd_VSS_q,  NO_PARAM,       NO_FLAGS,
    (ubyte *) "#VTD=?", pnd_VTD_eq, NO_PARAM,       NO_FLAGS, 
    (ubyte *) "#VTD=",  pnd_VTD_e,  COMPOUND_BYTE_HEX,  NO_FLAGS, 
    (ubyte *) "#VTD?",  pnd_VTD_q,  NO_PARAM,       NO_FLAGS,
    (ubyte *) "#VTS=?", pnd_VTS_eq, NO_PARAM,       NO_FLAGS, 
    (ubyte *) "#VTS=",  pnd_VTS_e,  COMPOUND_DTMF,  NO_FLAGS, 
    (ubyte *) "#VTX",   pnd_VTX_e,  NO_PARAM,       NO_FLAGS,
    (ubyte *) "#VTM=?", pnd_VTM_eq, NO_PARAM,       NO_FLAGS, 
    (ubyte *) "#VTM=",  pnd_VTM_e,  BYTE_NUMERIC,   NO_FLAGS, 
    (ubyte *) "#VTM?",  pnd_VTM_q,  NO_PARAM,       NO_FLAGS,
    END_OF_PARSE_TABLE
};
/* End Change 8/24/1995. */


/*******************************************************************************
/
/      Routine:        set_voice_rock_at_cmds
/
/      Author:         RSA
/
/      Creation Date:  12/16/94
/
/      Description:    This Routine enables parsing of VOICE Rockwell AT
/                      commands.  These are commands defined by the
/                      Rockwell Voice specification.
/
/      Input Parameters:   state - OFF (0) to disable, ON (otherwise) to enable.
/
/      Output Parameters: None
/
/      Global Variables Changed:  None
/
/      Users:
/
/      -------------------------------------------------------------
/      -                        Modifications                        -
/      -------------------------------------------------------------
/
/      Author & Date:    RSA
/      Description:
/      Reason:
/
*******************************************************************************/
void set_voice_rock_at_cmds(ubyte state)
{
   if (state == OFF)
       at_parser_tables(DISABLE_TABLE,voice_rockwell_commands);
   else
       at_parser_tables(ENABLE_TABLE,voice_rockwell_commands);
}

/******************************************************************************
/
/   Routine:        acu_rvce_command
/
/   Author:         RSA
/
/   Creation Date:  12/16/95
/
/   Description:    This routine is called to exercise Rockwell VOICE AT
/                   commands parsed by the ACU.  This routine serves as the
/                   ACU to VOICE interface for executable Rockwell specified
/                   VOICE commands.
/
/   Input Parameters: which_command - Which VOICE command to execute
/
/   Output Parameters: none
/
/   Global Variables Changed: none
/
/   Users: All commands in this file.
/
/   -------------------------------------------------------------
/   -                       Modifications                       -
/   -------------------------------------------------------------
/
/   Author & Date: RSA
/   Description:
/   Reason:
/
/  Author & Date: RSA 6/2/1995.
/  Description: Added the assignment send_ok = FALSE.
/  Reason: When going online in voice mode, we need to send CONNECT but NOT
/           OK.
/
*******************************************************************************/
void acu_rvce_command(uword which_command,ubyte *param_ptr)
{
    ubyte *response_ptr,result;

    response_ptr = 0;
    result = 0;

    /* Update VOICE w/the latest configuration before every command */
    acu_config_voice();

    /* Pass the command up to voice */
    switch (rockwell_execute_command(which_command, param_ptr, &response_ptr))
    {
        case VOICE_CONTINUE_PARSING:
            break;

        case VOICE_GOTO_ONLINE_COMMAND:
            on_line_command = TRUE;

            /* Stop the OK result code from going back to the terminal.  The
                voice code has already send back a response */
            send_ok = FALSE;
            break;

        case VOICE_GOTO_OFFLINE_COMMAND:
            on_line_command = FALSE;
            break;

        case VOICE_GO_ONLINE:
            build_event(E_CONNECT,CONN_VOICE);
            if (response_ptr)
                output_info(response_ptr, UNFORMATTED);
            return;

        case VOICE_ERROR:
            acu_vce_command_error();
            break;
     }
}

/******************************************************************************
/
/   Routine:        acu_rvce_query
/
/   Author:         RSA
/
/   Creation Date:  12/16/95
/
/   Description:    This routine is called to exercise Rockwell VOICE AT
/                   Request commands parsed by the ACU.  This routine serves
/                   as the ACU to VOICE interface for the Rockwell VOICE
/                   request commands.
/
/   Input Parameters: which_command - Which VOICE command to execute
/
/   Output Parameters: none
/
/   Global Variables Changed: none
/
/   Users: All commands in this file.
/
/   -------------------------------------------------------------
/   -                       Modifications                       -
/   -------------------------------------------------------------
/
/   Author & Date: RSA
/   Description:
/   Reason:
/
*******************************************************************************/
void acu_rvce_query(uword which_command)
{
    ubyte *response_ptr,result;

    response_ptr = 0;
    result = 0;

    /* Update VOICE w/the latest configuration before every command */
    acu_config_voice();

    /* Pass the command up to VOICE */
    result = rockwell_execute_command(which_command, UNUSED, &response_ptr);

    /* Display any command result */
    if (result == VOICE_ERROR)
        acu_vce_command_error();
    else
        output_info(response_ptr, UNFORMATTED);
}



/******************************************************************************
/
/   Routine:        pnd_VBS_eq
/
/   Author:         RSA
/
/   Creation Date:  2/16/95
/
/   Description:    This routine is called to implement the Rockwell Voice command:
/                   "AT#VBS=?"
/                   The #VBS command is used to set the Bits per Sample.
/                   This routine responds to the terminal with
/                   the vaild command input.
/
/   Input Parameters: parm_ptr - Pointer to the AT command's parameter:
/                                 (Unused).
/
/   Output Parameters: none
/
/   Global Variables Changed: none
/
/   Users: PARSE_CMD_TABLE_STRUCT voice_rockwell_commands[]
/
/   -------------------------------------------------------------
/   -                       Modifications                       -
/   -------------------------------------------------------------
/
/   Author & Date: RSA
/   Description:
/   Reason:
/
*******************************************************************************/
void pnd_VBS_eq(void)
{
    acu_rvce_query(RW_VBS_QUERY);
}

