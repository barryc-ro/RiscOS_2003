const ubyte S7_ptr[]   = {'S','0','7',':',3,' ',7};
const ubyte S8_ptr[]   = {'S','0','8',':',3,' ',7};
const ubyte S9_ptr[]   = {'S','0','9',':',3,' ',7};
const ubyte S10_ptr[]  = {'S','1','0',':',3,' ',7};
