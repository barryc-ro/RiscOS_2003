#include "sys_def.h"

void fax2_init(ubyte powerup)
{
    ubyte temp;
    temp = powerup;
}

void fax2_configure(ubyte fclass, ubyte response_type, ubyte dte_baud,
                   ubyte flow_control)
{
    ubyte temp;

    temp = fclass;
    temp = response_type;
    temp = dte_baud;
    temp = flow_control;
}

void fax2_hangup(void)
{
}

void fax2_timer(void)
{
}

ubyte *fax2_tx(void)
{
    return(0);
}

ubyte *fax2_rx(void)
{
    return(0);
}

ubyte fax2_bg(void)
{
    return(1);  /* stay in parse state */
}

ubyte fax2_execute_command(uword command, ubyte *command_parm_ptr,
                           ubyte **response_ptr_ptr)
{
    uword temp;
    temp = command;
    temp = *command_parm_ptr;
    temp = **response_ptr_ptr;
    return(2);  /* ERROR */
}

ubyte fax2_parse(ubyte **command_ptr_ptr, ubyte **response_ptr_ptr)
{
    ubyte temp;
    temp = **command_ptr_ptr;
    temp = **response_ptr_ptr;
    return(3);  /* NOTA_COMMAND */
}

ubyte fax20_execute_command(uword command, ubyte *command_parm_ptr,
                           ubyte **response_ptr_ptr)
{
    uword temp;
    temp = command;
    temp = *command_parm_ptr;
    temp = **response_ptr_ptr;
    return(2);  /* ERROR */
}

ubyte fax20_parse(ubyte **command_ptr_ptr, ubyte **response_ptr_ptr)
{
    ubyte temp;
    temp = **command_ptr_ptr;
    temp = **response_ptr_ptr;
    return(3);  /* NOTA_COMMAND */
}

void fax2_connect(ubyte direction)
{
    ubyte temp;
    temp = direction;
}

