/   -------------------------------------------------------------
/
/   Author & Date: RSA
/   Descritpion:
/   Reason:
/
*******************************************************************************/
void fax2_store_configuration_page(T30_parms_type *parms_struct_ptr, ubyte **string_ptr_ptr)
{
    mmu_open_fax_pg
    fax2_store_configuration(parms_struct_ptr, string_ptr_ptr);
    mmu_restore_stored_pg
}

