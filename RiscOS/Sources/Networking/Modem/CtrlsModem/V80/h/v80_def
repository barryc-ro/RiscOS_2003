#include "sys_def.h"

#define V80_7BIT_MASK           0x7F
#define EM                      0x19
#define EM_99                   0x99
#define FLAG                    0xB1
#define ABORT                   0xB2
#define MARK                    0xB0

#define V80_CMD_IND_T1          0x5C
#define V80_CMD_IND_T2          0x76
#define V80_CMD_IND_T3          0xA0
#define V80_CMD_IND_T4          0xA1
#define V80_CMD_IND_T5          0x5D
#define V80_CMD_IND_T6          0x77
#define V80_CMD_IND_T7          0xA2
#define V80_CMD_IND_T8          0xA3
#define V80_CMD_IND_T9          0xA4
#define V80_CMD_IND_T10         0xA5
#define V80_CMD_IND_T11         0xA6
#define V80_CMD_IND_T12         0xA7
#define V80_CMD_IND_T13         0xA8
#define V80_CMD_IND_T14         0xA9
#define V80_CMD_IND_T15         0xAA
#define V80_CMD_IND_T16         0xAB
#define V80_CMD_IND_T17         0xAC
#define V80_CMD_IND_T18         0xAD
#define V80_CMD_IND_T19         0xAE
#define V80_CMD_IND_T20         0xAF

#define EM_DTE_MARK             0xB0
#define EM_DTE_FLAG             0xB1
#define EM_DTE_ERR              0xB2
#define EM_DTE_HUNT             0xB3
#define EM_DTE_RESUME           0xB7
#define EM_DTE_EOT              0xBA
#define EM_DTE_ECS              0xBB
#define EM_DTE_RRN              0xBC
#define EM_DTE_RTN              0xBD
#define EM_DTE_RATE             0xBE

#define EM_DTE_CTL              0xBF
#define EM_DTE_RTNC             0xC0

#define EM_P12                  0x20
#define EM_P24                  0x21
#define EM_P48                  0x22
#define EM_P72                  0x23
#define EM_P96                  0x24
#define EM_P120                 0x25
#define EM_P144                 0x26
#define EM_P168                 0x27
#define EM_P192                 0x28
#define EM_P216                 0x29
#define EM_P240                 0x2A
#define EM_P264                 0x2B
#define EM_P288                 0x2C
#define EM_P312                 0x2D
#define EM_P336                 0x2E

#define DC1                     0x11
#define DC3                     0x13

#define V80_EM_19                   0
#define V80_EM_99                   1

#define V80_MAX_DCE_XLATE_BUF       2
#define V80_MAX_DTE_XLATE_BUF       2
#define V80_RATE_DATA_SPACE_REQD    3

#define HI_BIT                      0x80

#define TSM                         0
#define FSM                         1

/* V80 parameters for V80_modem_state_ind() */

/* Modem Task status values */
#define V80_RATE_MASK         0x0F

#define V80_XLATE_RATE1       0x1D  /* xlate constant between modem task and V80 */
#define V80_XLATE_RATE2       0x19  /* xlate constant between modem task and V80 */


