#include "sys_def.h"

#include "f2x_def.h"

/* table used to determine the minimum number of bytes needed at a specific */
/* line speed to meet the negotiated minimum scan line time */
const ubyte fax2_minimum_scan_table[6][5] =
{
{0, 2, 3, 6, 12}, {0, 3, 6, 12, 24}, {0, 5, 9, 18, 36},
{0, 6, 12, 24, 48}, {0, 8, 15, 30, 60}, {0, 9, 18, 36, 72}
};

/* table that determines the number of leading (from right) zero bit in a */
/* byte. the byte is used as an index into the table */
const ubyte fax2_leading_zeros_table[256] =
{
