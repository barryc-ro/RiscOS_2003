#include "sys_def.h"

void fax1_init(ubyte powerup)
{
    ubyte temp;
    temp = powerup;
}

void fax1_configure(ubyte fclass, ubyte response_type, ubyte dte_baud,
                   ubyte flow_control)
{
    ubyte temp;

    temp = fclass;
    temp = response_type;
    temp = dte_baud;
    temp = flow_control;
}

void fax1_hangup(void)
{
}

void fax1_timer(void)
{
}

ubyte *fax1_tx(void)
{
    return(0);
}

ubyte *fax1_rx(void)
{
    return(0);
}

ubyte fax1_bg(void)
{
    return(1);  /* stay in parse state */
}

ubyte fax1_execute_command(uword command,ubyte *command_parm_ptr,
                           ubyte **response_ptr_ptr)
{
    uword temp;
    temp = command;
    temp = *command_parm_ptr;
    temp = **response_ptr_ptr;
    return(2);  /* ERROR */
}


void fax1_connect(ubyte direction)
{
    ubyte temp;
    temp = direction;
}

