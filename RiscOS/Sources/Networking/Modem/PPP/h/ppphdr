#ifndef __cmhg_defs_h
#define __cmhg_defs_h

#define  PPP_00                0x04b600
#define  PPP_DCIVersion        0x04b600
#define  PPP_Inquire           0x04b601
#define  PPP_GetNetworkMTU     0x04b602
#define  PPP_SetNetworkMTU     0x04b603
#define  PPP_Transmit          0x04b604
#define  PPP_Filter            0x04b605
#define  PPP_Stats             0x04b606
#define  PPP_07                0x04b607
#define  PPP_08                0x04b608
#define  PPP_09                0x04b609
#define  PPP_0A                0x04b60a
#define  PPP_0B                0x04b60b
#define  PPP_0C                0x04b60c
#define  PPP_0D                0x04b60d
#define  PPP_0E                0x04b60e
#define  PPP_0F                0x04b60f
#define  PPP_10                0x04b610
#define  PPP_11                0x04b611
#define  PPP_12                0x04b612
#define  PPP_13                0x04b613
#define  PPP_14                0x04b614
#define  PPP_15                0x04b615
#define  PPP_16                0x04b616
#define  PPP_17                0x04b617
#define  PPP_18                0x04b618
#define  PPP_19                0x04b619
#define  PPP_1A                0x04b61a
#define  PPP_1B                0x04b61b
#define  PPP_1C                0x04b61c
#define  PPP_1D                0x04b61d
#define  PPP_1E                0x04b61e
#define  PPP_1F                0x04b61f
#define  PPP_AutoDial          0x04b620
#define  PPP_Answering         0x04b621

#define  CMD_PPPInfo           0
#define  CMD_PPPConnect        1
#define  CMD_PPPDisconnect     2
#define  CMD_PPPLog            3

#endif
