  <SCRIPT LANGUAGE="JavaScript1.1">
    urls[0] = "SendMessage: -m \"this is a test\"";
    urls[1] = "SendMessage: -TCP -d 136.170.128.60:12345 -q -m \"Hello world!\"";
    urls[2] = "SendMessage: -UDP -d 136.170.128.60:54321 -q -m \"Hello again...\"";

    function SendMessage(num)
    {
      location.href=urls[num];
    }
  </SCRIPT>

