#include "VersionNum"

title-string: TFTP

help-string: TFTP_Client Module_MajorVersion_CMHG Module_MinorVersion_CMHG

date-string: Module_Date_CMHG

command-keyword-table: tftp_command

        TFTPRead(min-args:3, max-args:4, add-syntax:, gstrans-map:255,
                help-text: "*TFTPRead downloads a file from a TFTP server.\r"
                           "Options:\r"
                           "-e\tSet system variable Inet$Error rather than "
                           "returning an error.\r",
                invalid-syntax: "Syntax: *TFTPRead [-e] <server> <server filename> <local filename>" ),

	TFTPLoadModule(min-args:2, max-args:3, add-syntax: gstrans-map:255,
                help-text: "*TFTPLoadModule loads a relocate module from a TFTP server.\r"
                           "Options:\r"
                           "-e\tSet system variable Inet$Error rather than "
                           "returning an error.\r",
                invalid-syntax: "Syntax: *TFTPLoadModule [-e] <server> <server filename>" )
