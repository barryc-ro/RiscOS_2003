  _kernel_oserror *e = 0;
  _kernel_swi_regs r;
  
  r.r[0] = (int)*flags;
  r.r[1] = (int)pollword_addr;
  r.r[2] = (int)msg_id;
  r.r[3] = timeout_UIDL;
  
  e = _kernel_swi(AcornPOP3_GetUniqueID, &r, &r);
  
