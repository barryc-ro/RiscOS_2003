        GET     h.regdefs
        GET     h.swinos
        GET     h.macros
;
        PREAMBLE

        STARTCODE  render

        STMFD   sp!, {a1-v1, lr}
        MOV     a4, a3
        MOV     a3, a2
        MOV     a2, a1
        MOV     a1, #(1 << 11)
        MOV     v1, #0
        SWI     SWI_DrawFile_Render + XOS_Bit
        LDMFD   sp!, {a1-v1, pc}
        
        STARTCODE  osmodget

        STMFD   sp!, {a2-a4, lr}
        MOV     a4, a1
        MOV     a1, #6
        SWI     SWI_OS_Module + XOS_Bit
        MOVVS   a1, #0
        MOVVC   a1, a3
        LDMFD   sp!, {a2-a4, pc}
        
        STARTCODE  osmodfree

        STMFD   sp!, {a1-a3, lr}
        MOV     a3, a1
        MOV     a1, #7
        SWI     SWI_OS_Module + XOS_Bit
        LDMFD   sp!, {a1-a3, pc}
        
        STARTCODE  ncregenq

        STMFD   sp!, {a2-a3, lr}
        SWI     SWI_NCRegistry_Enquiry + XOS_Bit
        LDMFD   sp!, {a2-a3, pc}
        
        STARTCODE  callback

        STMFD   sp!, {a1-a2, lr}
        SWI     SWI_OS_AddCallBack + XOS_Bit
        LDMFD   sp!, {a1-a2, pc}
        
        STARTCODE  callafter

        STMFD   sp!, {a1-a3, lr}
        SWI     SWI_OS_CallAfter + XOS_Bit
        LDMFD   sp!, {a1-a3, pc}
        
        STARTCODE  removeticker

        STMFD   sp!, {a1-a2, lr}
        SWI     SWI_OS_RemoveTickerEvent + XOS_Bit
        LDMFD   sp!, {a1-a2, pc}
        
        STARTCODE  monotonic_time

        STMFD   sp!, {lr}
        SWI     SWI_OS_ReadMonotonicTime + XOS_Bit
        LDMFD   sp!, {pc}
        
        STARTCODE  oscli

        STMFD   sp!, {a1, lr}
        SWI     SWI_OS_CLI + XOS_Bit
        LDMFD   sp!, {a1, pc}
        
        END
