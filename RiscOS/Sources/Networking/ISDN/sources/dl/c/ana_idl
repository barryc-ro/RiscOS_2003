#define Pr_msg_ti_dl fprintf (f_out, "from = %c to = %c code = %s nai = %-1u sapi = %-u ces = %-1u sn = %-u",\
	L_msg_from, L_msg_to, ti_dl_prim[strchar (ti_dl_prim, L_msg_code)].p_text, L_msg_nai, L_msg_sapi, L_msg_conn_id, L_msg_inf0)

