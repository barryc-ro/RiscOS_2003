#if NS_SIM_ANALYSER == ON

/*--------------------------------------------------------------------------*/

		/* Flag external */

extern	uint	ns_pr_object;

		/* Printing formats */

#define Pr_nsna_a		fprintf (f_out, "\n\nNS NA  nai = %1d (%s) type = %s",\
								 Nai, Network, Code_to_text (t_na_type, Na_type))

#define Pr_nsdl_a		fprintf (f_out, "   lc_state = %s first = %lp last = %lp",\
								Code_to_text (t_lc_state, Lc_state), Dl_first, Dl_last)

#define Pr_nsna_a_2		fprintf (f_out, "\n\nNS NA  nai = %1d (%s) type = %s",\
								Nai, Network, Code_to_text (t_na_type, Na_type))

#define Pr_nsdl_a_2		fprintf (f_out, "   lc_state = %s", \
								Code_to_text (t_lc_state, Lc_state))

#define Pr_nsdl_ei		fprintf (f_out, "\n   EI state = %s", \
								Code_to_text (t_ei_state, Dl_ei_state))


#define Pr_nsna_b		fprintf (f_out, "\n        outgoing = %-d  mixed = %-d  incoming = %-d",\
								Outgoing_call_nb, Mixed_call_nb, Incoming_call_nb)

#define Pr_call_b	fprintf (f_out,  "\nCALL   nai = %1d (%s) conn_id = %-3d state = %-3d sub-state = %d\n\t\ttype = %s call_ref = %-d n308 = %-1d",\
								Nai, Network, Call_conn_id, Call_state, Call_sub_state, Code_to_text (t_call_type, Call_type), Call_ref, N308)


#define Pr_call		fprintf (f_out,  "\nCALL   nai = %1d (%s) conn_id = %-3d state = %-3d type = %s call_ref = %-d n308 = %-1d",\
								Nai, Network, Call_conn_id, Call_state, Code_to_text (t_call_type, Call_type), Call_ref, N308)

#define	Pr_timer(xx)	fprintf (f_out, "\n       timer : %s  ID = %Fp value = %-u state = %-u  sequence nber = %-u",\
								Code_to_text (Ti_ref, Ti_idx(xx)), (uchar FAR *)Ti_id (xx), (uint)(Ti_val (Ti_idx(xx)) / (SECOND)), (uint)Ti_state (xx), (uint)Ti_sn (xx))

#define	Pr_timer_2(xx)	fprintf (f_out, "\n       timer : %s           value = %-u state = %-u  sequence nber = %-u",\
								Code_to_text (Ti_ref, Ti_idx(xx)),							 (uint)(Ti_val (Ti_idx(xx)) / (SECOND)), (uint)Ti_state (xx), (uint)Ti_sn (xx))

#define	Pr_int_timer(xx)	fprintf (f_out, "\nces %d, timer : %s  ID = %Fp value = %-u state = %-u  sequence nber = %-u",\
								Int_ces (xx), Code_to_text (TI_INT_REF, Int_timer_idx(xx)), (uchar FAR *)Int_timer_id (xx), (uint)(Ti_int_val (Int_timer_idx(xx)) / (SECOND)), (uint)Int_timer_state (xx), (uint)Int_timer_sn (xx))

#define	Pr_int_timer_2(xx)	fprintf (f_out, "\nces %d, timer : %s           value = %-u state = %-u  sequence nber = %-u",\
								Int_ces (xx), Code_to_text (TI_INT_REF, Int_timer_idx(xx)),							      (uint)(Ti_int_val (Int_timer_idx(xx)) / (SECOND)), (uint)Int_timer_state (xx), (uint)Int_timer_sn (xx))

#define	Pr_dl_timer(xx) fprintf (f_out, "\n       timer : %s  ID = %Fp value = %-u state = %-u  sequence nber = %-u",\
								Code_to_text (Ti_dl_ref, i), (uchar FAR *)Dl_timer_id(i),	(uint)(Ti_dl_val(xx) / (SECOND)), (uint)Dl_timer_state(xx), (uint)Dl_timer_sn(xx))

#define	Pr_dl_timer_2(xx) fprintf (f_out, "\n       timer : %s          value = %-u state = %-u  sequence nber = %-u",\
								Code_to_text (Ti_dl_ref, i),								(uint)(Ti_dl_val(xx) / (SECOND)), (uint)Dl_timer_state(xx), (uint)Dl_timer_sn(xx))

#define Pr_nfas(x) fprintf (f_out, "\nPRI Group # %x\
								   \n       Active D chan state       : %s\
								   \n       Backup D chan state       : %s",x, Code_to_text (nfas_state,Nfas_active_d_chan_status), Code_to_text (nfas_state,Nfas_backup_d_chan_status)); \
				  fprintf (f_out, "\n       Nai bearing Active D chan : %x\
								   \n       Nai bearing Backup D chan : %x",Nfas_nai_active_d_chan, Nfas_nai_backup_d_chan)

/*------------------------------------------------------------------------*/

			/* Print all the objects of the NS configuration */

void pr_ns_object ()
{
	uchar	i;			/* loop index */

	if ((ns_pr_object == ON) && (print_flag == ON)){

		for (i = 0; ;i++) {

			if (ns_access_na (i) == NOK) {
				return;
			}

			p_tab_ti_ref        = (struct code_text *)tab_ti_ref    [Na_network];
			p_tab_ti_dl_ref     = (struct code_text *)tab_ti_dl_ref [Na_network];
			p_tab_ti_ns_prim    = (struct code_text *)tab_ti_ns_prim[Na_network];
			p_tab_ns_prim       = (struct code_text *)tab_ns_prim   [Na_network];

			Pr_nsna_a_2;
			if (Dl_current_exist) {
				Pr_nsdl_a_2;

#				if NS_SPID == ON

					if ((Na_network == ni2) || (Na_network == at9) || (Na_network == at5)) {
						Pr_nsdl_ei;
					}

#				endif
			}

			Pr_nsna_b;

			ns_access_first_call ();
			while (Call_current_exist) {

#			if (F_AT5 == ON && SUPPLEMENTARY_SERVICES == ON) || \
				(F_NTT == ON) || (F_AT9 == ON) || (F_NI2 == ON) ||\
				(EQUIPMENT == NT2)

					if ((Na_network == at5) || (Na_network == ntt) || (Na_network == at9) || (Na_network == ni2)) {
						Pr_call_b;

					} else {

						Pr_call;
					}
#			else
					Pr_call;
#			endif

				ns_access_next_call ();
			}

		}

	}

}

/*--------------------------------------------------------------------------*/

		/*
		   Printing of the current na and call objects pointed by
		   p_nsna, p_nsdl and p_call.
		*/

void pr_ns_state ()
{
	uchar i;

#	if NS_MAINTENANCE_CAPABILITY == ON
		uchar j,k;
#	endif

	if ((print_flag == ON) && (Na_current_exist)) {

		p_tab_ti_ref        = (struct code_text *)tab_ti_ref    [Na_network];
		p_tab_ti_dl_ref     = (struct code_text *)tab_ti_dl_ref [Na_network];
		p_tab_ti_ns_prim    = (struct code_text *)tab_ti_ns_prim[Na_network];
		p_tab_ns_prim       = (struct code_text *)tab_ns_prim   [Na_network];

#		if PR_POINTER == ON
			Pr_nsna_a;

			if (Dl_current_exist) {
				Pr_nsdl_a;

				for (i = 0; i < MX_DLCI_TIMER; i++) {

					if (State_dl_timer (i) == ON) {
						Pr_dl_timer (i);
					}

				}

			}

#		else
			Pr_nsna_a_2;

			if (Dl_current_exist) {
				Pr_nsdl_a_2;

#				if NS_SPID == ON

					if ((Na_network == at9) || (Na_network == at5) || (Na_network == ni2)) {
						Pr_nsdl_ei;
					}

#				endif

				for (i = 0; i < MX_DLCI_TIMER; i++) {

					if (State_dl_timer (i) == ON) {
						Pr_dl_timer_2 (i);
					}

				}

#               if NS_D_BACKUP == ON

					for (i = 0; i < ns_nfas_group_nb; i++) {

						Ns_access_d_chan_ctx (i);

						if ((Nfas_nai_active_d_chan == Nai) ||
							(Nfas_nai_backup_d_chan == Nai)) {
							Pr_nfas (i);
							break;
						}

					}

#               endif

#				if NS_MAINTENANCE_CAPABILITY == ON /* { */

					if (Dl_p_chan_mgt != P_NIL) {

						k = Nai;

						if (is_ie_i (CODESET_0, CHAN_ID) == PRESENT) {

							if (I_(chan_id,chan_sel) != NO_CHAN) {

								if (I_(chan_id, int_id) == INT_ID_EXPLICIT) {
									k = I_(chan_id, int_id_val);
								}
							}
						}

						fprintf (f_out, "\n===> Nai = %d (B_channel, state) :\n", k);

						for (i = j = 0; i < NS_MX_B_CHAN; i++) {

							if ((++j % 5) == 0) {
								fprintf (f_out, "\n");
							}

							fprintf (f_out, "(%d, %s) ", i, Code_to_text (maint_state, Dl_b_chan_mode_int (k, i)));
						}
                    }

#				endif /* } */
			}

#		endif

		if (Call_current_exist) {

#			if (F_AT5 == ON && SUPPLEMENTARY_SERVICES == ON) || \
				(F_NTT == ON) || (F_AT9 == ON) || (F_NI2 == ON) ||\
				(EQUIPMENT == NT2)

					if ((Na_network == at5) || (Na_network == ntt) || (Na_network == at9) || (Na_network == ni2)) {
						Pr_call_b;

					} else {

						Pr_call;
					}
#			else
					Pr_call;
#			endif

			for (i = 0; i < NB_CALL_TIMER; i++) {

				if (Ti_state (i) == ON) {

#					if PR_POINTER == ON
						Pr_timer (i);
#					else
						Pr_timer_2 (i);
#					endif

				}

			}

#			if (EQUIPMENT == NT2) && ((F_VN6 == ON) || (F_ETS == ON))

				if ((Na_network != ets) && (Na_network != vn6)) return;

				for (i = 0; i < MX_INT_PROC; i++) {

					if (Int_timer_state (i) == ON) {

#						if PR_POINTER == ON
							Pr_int_timer (i);
#						else
							Pr_int_timer_2 (i);
#						endif

					}

				}

#			endif

		}

	} /* Endif ns_pr_state */

}

