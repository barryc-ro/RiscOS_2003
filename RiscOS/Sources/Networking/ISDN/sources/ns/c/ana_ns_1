			case NS_INFO_IN :
				return (sizeof (off_ns_a17_info_in) / sizeof (uint));

			case NS_HOLD_RQ :

#				if MESSAGE_HEADER_IE != ON
					return (0);
#				else
					return (sizeof (off_ns_a17_hold_rq) / sizeof (uint));
#				endif

			case NS_HOLD_IN :

#				if MESSAGE_HEADER_IE != ON
					return (0);
#				else
					return (sizeof (off_ns_a17_hold_in) / sizeof (uint));
#				endif

			case NS_HOLD_RS :
				return (sizeof (off_ns_a17_hold_rs) / sizeof (uint));

			case NS_HOLD_CO :
				return (sizeof (off_ns_a17_hold_co) / sizeof (uint));

			case NS_RET_RQ :

#				if MESSAGE_HEADER_IE != ON
					return (0);
#				else
					return (sizeof (off_ns_a17_ret_rq) / sizeof (uint));
#				endif

			case NS_RET_IN :

#				if MESSAGE_HEADER_IE != ON
					return (0);
#				else
					return (sizeof (off_ns_a17_ret_in) / sizeof (uint));
#				endif

			case NS_RET_CO :
				return (sizeof (off_ns_a17_ret_co) / sizeof (uint));

			case NS_RET_RS :
				return (sizeof (off_ns_a17_ret_rs) / sizeof (uint));

	}

	return (BYTE_VAL_NIL);
}

