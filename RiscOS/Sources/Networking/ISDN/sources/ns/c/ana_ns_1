			case NS_SETUP_ACK_IN :
				return (sizeof (off_ns_tr6_setup_ack_in) / sizeof (uint));

			case NS_SETUP_ACK_RQ :
				return (sizeof (off_ns_tr6_setup_ack_rq) / sizeof (uint));

			case NS_SUSPEND_RQ :
				return (sizeof (off_ns_tr6_suspend_rq) / sizeof (uint));

			case NS_SUSPEND_CO :
				return (sizeof (off_ns_tr6_suspend_co) / sizeof (uint));

			case NS_RESUME_RQ :
				return (sizeof (off_ns_tr6_resume_rq) / sizeof (uint));

			case NS_RESUME_CO :
				return (sizeof (off_ns_tr6_resume_co) / sizeof (uint));

#			if NS_SPF_FACILITY == ON

				case NS_SPF_FACILITY_RQ :
					return (sizeof (off_ns_tr6_spf_facility_rq) / sizeof (uint));

				case NS_SPF_FACILITY_IN :
					return (sizeof (off_ns_tr6_spf_facility_in) / sizeof (uint));

