				case NS_RET_RQ :
					return (sizeof (off_ns_ets_ret_rq) / sizeof (uint));

				case NS_RET_IN :
					return (sizeof (off_ns_ets_ret_in) / sizeof (uint));

				case NS_RET_RS :
					return (sizeof (off_ns_ets_ret_rs) / sizeof (uint));

				case NS_RET_CO :
					return (sizeof (off_ns_ets_ret_co) / sizeof (uint));
#			endif

#			if NS_SUPP_SERVICES == ON
				case NS_SPF_REGISTRATION_RQ :
					return (sizeof (off_ns_ets_reg_rq) / sizeof (uint));

				case NS_SPF_REGISTRATION_IN :
					return (sizeof (off_ns_ets_reg_in) / sizeof (uint));
#			endif
	}

	return (BYTE_VAL_NIL);
}

