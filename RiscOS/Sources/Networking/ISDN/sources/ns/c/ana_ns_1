					return (sizeof (off_ns_tr6_spf_facility_rs) / sizeof (uint));

				case NS_SPF_FACILITY_CO :
					return (sizeof (off_ns_tr6_spf_facility_co) / sizeof (uint));
#			endif

			case NS_USER_INFO_RQ :
				return (sizeof (off_ns_tr6_user_info_rq) / sizeof (uint));

			case NS_USER_INFO_IN :
				return (sizeof (off_ns_tr6_user_info_in) / sizeof (uint));

			case NS_DETACH_RQ :
				break;

			case NS_CONGEST_CONTROL_IN :
				return (sizeof (off_ns_tr6_con_con_in) / sizeof (uint));

			case NS_STATUS_ENQ_RQ :
				return (sizeof (off_ns_tr6_status_enq_rq) / sizeof (uint));


			case NS_INFO_RQ :
				return (sizeof (off_ns_tr6_info_rq) / sizeof (uint));

			case NS_INFO_IN :
				return (sizeof (off_ns_tr6_info_in) / sizeof (uint));
	}
#else

	switch (msg_code) {

			case NS_CONN_RQ :
				return (sizeof (off_ns_tr6_net_conn_rq) / sizeof (uint));

			case NS_CONN_IN :
				return (sizeof (off_ns_tr6_net_conn_in) / sizeof (uint));

			case NS_CONN_RS :
				return (sizeof (off_ns_tr6_net_conn_rs) / sizeof (uint));

			case NS_CONN_CO :
				return (sizeof (off_ns_tr6_net_conn_co) / sizeof (uint));

			case NS_CLEAR_RQ :
				return (sizeof (off_ns_tr6_net_clear_rq) / sizeof (uint));

			case NS_CLEAR_IN :
				return (sizeof (off_ns_tr6_net_clear_in) / sizeof (uint));

			case NS_CLEAR_RS :
				return (sizeof (off_ns_tr6_net_clear_rs) / sizeof (uint));

			case NS_CLEAR_CO :
				return (sizeof (off_ns_tr6_net_clear_co) / sizeof (uint));

			case NS_ALERT_RQ :
				return (sizeof (off_ns_tr6_net_alert_rq) / sizeof (uint));

			case NS_ALERT_IN :
				return (sizeof (off_ns_tr6_net_alert_in) / sizeof (uint));

			case NS_CALL_PROC_RQ :
				return (sizeof (off_ns_tr6_net_call_proc_rq) / sizeof (uint));

			case NS_CALL_PROC_IN :
				return (sizeof (off_ns_tr6_net_call_proc_in) / sizeof (uint));

			case NS_SETUP_ACK_IN :
				return (sizeof (off_ns_tr6_net_setup_ack_in) / sizeof (uint));

			case NS_SETUP_ACK_RQ :
				return (sizeof (off_ns_tr6_net_setup_ack_rq) / sizeof (uint));


#			if NS_SPF_FACILITY == ON

				case NS_SPF_FACILITY_RQ :
					return (sizeof (off_ns_tr6_net_spf_facility_rq) / sizeof (uint));

				case NS_SPF_FACILITY_IN :
					return (sizeof (off_ns_tr6_net_spf_facility_in) / sizeof (uint));

				case NS_SPF_FACILITY_RS :
					return (sizeof (off_ns_tr6_net_spf_facility_rs) / sizeof (uint));

				case NS_SPF_FACILITY_CO :
					return (sizeof (off_ns_tr6_net_spf_facility_co) / sizeof (uint));
#			endif

			case NS_USER_INFO_RQ :
				return (sizeof (off_ns_tr6_net_user_info_rq) / sizeof (uint));

			case NS_USER_INFO_IN :
				return (sizeof (off_ns_tr6_net_user_info_in) / sizeof (uint));

			case NS_DETACH_RQ :
				break;

			case NS_INFO_RQ :
				return (sizeof (off_ns_tr6_net_info_rq) / sizeof (uint));

			case NS_INFO_IN :
				return (sizeof (off_ns_tr6_net_info_in) / sizeof (uint));
	}

#endif

	return (BYTE_VAL_NIL);
}

#endif

#if F_ETS == ON

uchar ets_get_offset_nb (msg_code)
	code_t msg_code;
{
	switch (msg_code) {

			case NS_CONN_RQ :
				return (sizeof (off_ns_ets_conn_rq) / sizeof (uint));

			case NS_CONN_IN :
				return (sizeof (off_ns_ets_conn_in) / sizeof (uint));

			case NS_CONN_RS :
				return (sizeof (off_ns_ets_conn_rs) / sizeof (uint));

			case NS_CONN_CO :
				return (sizeof (off_ns_ets_conn_co) / sizeof (uint));

			case NS_CLEAR_RQ :
				return (sizeof (off_ns_ets_clear_rq) / sizeof (uint));

			case NS_CLEAR_IN :
				return (sizeof (off_ns_ets_clear_in) / sizeof (uint));

			case NS_CLEAR_RS :
				return (sizeof (off_ns_ets_clear_rs) / sizeof (uint));

			case NS_CLEAR_CO :
				return (sizeof (off_ns_ets_clear_co) / sizeof (uint));

			case NS_ALERT_RQ :
				return (sizeof (off_ns_ets_alert_rq) / sizeof (uint));

			case NS_ALERT_IN :
				return (sizeof (off_ns_ets_alert_in) / sizeof (uint));

			case NS_CALL_PROC_RQ :
				return (sizeof (off_ns_ets_call_proc_rq) / sizeof (uint));

			case NS_CALL_PROC_IN :
				return (sizeof (off_ns_ets_call_proc_in) / sizeof (uint));

			case NS_CONGEST_CONTROL_IN :
				return (sizeof (off_ns_ets_con_con_in) / sizeof (uint));

#			if EQUIPMENT == NT2

				case NS_CONGEST_CONTROL_RQ :
					return (sizeof (off_ns_ets_net_con_con_rq) / sizeof (uint));

				case NS_SUSPEND_IN :
					return (sizeof (off_ns_ets_net_suspend_in) / sizeof (uint));

				case NS_SUSPEND_RS :
					return (sizeof (off_ns_ets_net_suspend_rs) / sizeof (uint));

				case NS_RESUME_IN :
					return (sizeof (off_ns_ets_net_resume_in) / sizeof (uint));

				case NS_RESUME_RS :
					return (sizeof (off_ns_ets_net_resume_rs) / sizeof (uint));

				case NS_FAC_RS :
					return (sizeof (off_ns_ets_net_fac_rs) / sizeof (uint));

#			endif

			case NS_PROGRESS_RQ :
				return (sizeof (off_ns_ets_progress_rq) / sizeof (uint));

			case NS_PROGRESS_IN :
				return (sizeof (off_ns_ets_progress_in) / sizeof (uint));

			case NS_SUSPEND_RQ :
				return (sizeof (off_ns_ets_suspend_rq) / sizeof (uint));

			case NS_SUSPEND_CO :
				return (sizeof (off_ns_ets_suspend_co) / sizeof (uint));

			case NS_RESUME_RQ :
				return (sizeof (off_ns_ets_resume_rq) / sizeof (uint));

			case NS_RESUME_CO :
				return (sizeof (off_ns_ets_resume_co) / sizeof (uint));

			case NS_NOTIFY_RQ :
				return (sizeof (off_ns_ets_notify_rq) / sizeof (uint));

			case NS_NOTIFY_IN :
				return (sizeof (off_ns_ets_notify_in) / sizeof (uint));

			case NS_USER_INFO_RQ :
				return (sizeof (off_ns_ets_user_info_rq) / sizeof (uint));

			case NS_USER_INFO_IN :
				return (sizeof (off_ns_ets_user_info_in) / sizeof (uint));

			case NS_RESTART_RQ :
				return (1);

			case NS_RESTART_IN :
				return (sizeof (off_ns_ets_restart_in) / sizeof (uint));

			case NS_RESTART_RS :
				return (sizeof (off_ns_ets_restart_rs) / sizeof (uint));

			case NS_FAC_RQ :
				return (sizeof (off_ns_ets_fac_rq) / sizeof (uint));

			case NS_FAC_IN :
				return (sizeof (off_ns_ets_fac_in) / sizeof (uint));

			case NS_FAC_CO :
				return (sizeof (off_ns_ets_fac_co) / sizeof (uint));

			case NS_SETUP_ACK_RQ :
				return (sizeof (off_ns_ets_setup_ack_rq) / sizeof (uint));

			case NS_SETUP_ACK_IN :
				return (sizeof (off_ns_ets_setup_ack_in) / sizeof (uint));

			case NS_STATUS_ENQ_RQ :
				break;

			case NS_INFO_RQ :
				return (sizeof (off_ns_ets_info_rq) / sizeof (uint));

			case NS_INFO_IN :
				return (sizeof (off_ns_ets_info_in) / sizeof (uint));

#			if NS_HOLD_RETRIEVE == ON
				case NS_HOLD_RQ :
					return (sizeof (off_ns_ets_hold_rq) / sizeof (uint));

				case NS_HOLD_IN :
					return (sizeof (off_ns_ets_hold_in) / sizeof (uint));

				case NS_HOLD_RS :
					return (sizeof (off_ns_ets_hold_rs) / sizeof (uint));

				case NS_HOLD_CO :
