			case NS_INFO_RQ :
				return (sizeof (off_ns_at9_info_rq) / sizeof (uint));

			case NS_INFO_IN :
				return (sizeof (off_ns_at9_info_in) / sizeof (uint));

			case NS_K_SETUP_IN :
				return (sizeof (off_ns_at9_key_setup_in) / sizeof (uint));

			case NS_K_REL_IN :
				return (sizeof (off_ns_at9_key_rel_in) / sizeof (uint));

			case NS_K_HOLD_IN :
				return (sizeof (off_ns_at9_key_hold_in) / sizeof (uint));

			case NS_RESTART_IN :
				return (sizeof (off_ns_at9_restart_in) / sizeof (uint));

			case NS_RESTART_RS :
				return (sizeof (off_ns_at9_restart_rs) / sizeof (uint));
	}

	return (BYTE_VAL_NIL);
