							/* NS interface */

#define Pr_msg_ns fprintf (f_out, "from = %c to = %c code = %s nai = %-1u (%s) conn_id = %-1u",\
	L_msg_from, L_msg_to, Code_to_text (Ns_prim, L_msg_code), L_msg_nai, Network, L_msg_conn_id)

#define Pr_msg_mns fprintf (f_out, "from = %c to = %c code = %s nai = %-1u (%s) conn_id = %-1u",\
	L_msg_from, L_msg_to, Code_to_text (mns_prim, L_msg_code), L_msg_nai, Network, L_msg_conn_id)

#define Pr_msg_mns_2 fprintf (f_out, "from = %c to = %c code = %s nai = %-1u conn_id = %-1u",\
	L_msg_from, L_msg_to, Code_to_text (mns_prim, L_msg_code), L_msg_nai, L_msg_conn_id)

#define Pr_msg_ti_ns fprintf (f_out, "from = %c to = %c code = %s nai = %-1u (%s) conn_id = %-1u sn  = %-u",\
	L_msg_from, L_msg_to, Code_to_text (Ti_ns_prim, L_msg_code), L_msg_nai, Network, L_msg_chani, L_msg_inf0)

