extern uint ns_pr_cc_sdu_ie;

#if NS_SUPP_SERVICES == ON
	extern uint	ns_pr_sdu_ss;
#endif

#if NEW_FCT_STYLE == ON
	void	pr_sdu_ns_cc_data	(uchar FAR *p_data, uchar data_size, uchar offset_nb);
#	if NS_SUPP_SERVICES == ON
		void	ns_pr_ss_data_in_sdu	(uchar FAR *p_data, ushort data_size, code_t msg_code, uchar net);
#	endif
	uchar	get_offset_nb		(code_t msg_code, uchar net);
	uchar	at4_get_offset_nb	(code_t msg_code);
	uchar	at5_get_offset_nb	(code_t msg_code);
	uchar	at9_get_offset_nb	(code_t msg_code);
	uchar	bt2_get_offset_nb	(code_t msg_code);
	uchar	bip_get_offset_nb	(code_t msg_code);
	uchar	dms_get_offset_nb	(code_t msg_code);
	uchar	kdd_get_offset_nb	(code_t msg_code);
	uchar	ni1_get_offset_nb	(code_t msg_code);
	uchar	ni2_get_offset_nb	(code_t msg_code);
	uchar	ntt_get_offset_nb	(code_t msg_code);
	uchar	swd_get_offset_nb	(code_t msg_code);
	uchar	bv1_get_offset_nb	(code_t msg_code);
	uchar	au1_get_offset_nb	(code_t msg_code);
	uchar	tr6_get_offset_nb	(code_t msg_code);
	uchar	vn2_get_offset_nb	(code_t msg_code);
	uchar	vn3_get_offset_nb	(code_t msg_code);
	uchar	vn6_get_offset_nb	(code_t msg_code);
	uchar	ets_get_offset_nb	(code_t msg_code);
	uchar	qsi_get_offset_nb	(code_t msg_code);
	uchar	hkt_get_offset_nb	(code_t msg_code);
	uchar	a17_get_offset_nb	(code_t msg_code);
	uchar	a11_get_offset_nb	(code_t msg_code);
	uchar	e10_get_offset_nb	(code_t msg_code);
#else
	void	pr_sdu_ns_cc_data	();
#	if NS_SUPP_SERVICES == ON
		void	ns_pr_ss_data_in_sdu	();
#	endif
	uchar	get_offset_nb		();
	uchar	at4_get_offset_nb	();
	uchar	at5_get_offset_nb	();
	uchar	at9_get_offset_nb	();
	uchar	bip_get_offset_nb	();
	uchar	bt2_get_offset_nb	();
	uchar	dms_get_offset_nb	();
	uchar	kdd_get_offset_nb	();
	uchar	ni1_get_offset_nb	();
	uchar	ni2_get_offset_nb	();
	uchar	ntt_get_offset_nb	();
	uchar	swd_get_offset_nb	();
	uchar	bv1_get_offset_nb	();
	uchar	au1_get_offset_nb	();
	uchar	tr6_get_offset_nb	();
	uchar	vn2_get_offset_nb	();
	uchar	vn3_get_offset_nb	();
	uchar	vn6_get_offset_nb	();
	uchar	ets_get_offset_nb	();
	uchar	qsi_get_offset_nb	();
	uchar	hkt_get_offset_nb	();
	uchar	a17_get_offset_nb	();
	uchar	a11_get_offset_nb	();
	uchar	e10_get_offset_nb	();
#endif

uchar ana_sdu_ns (p_msg)
	struct message FAR *p_msg;
{
	uchar	offset_nb;
	uchar	data_size;


	if (ns_access_na (Rcv_nai) == NOK) return (NOK);

#	if ONE_NETWORK == OFF
		ns_build_p_table ();
		p_tab_ti_ref        = (struct code_text *)tab_ti_ref    [Na_network];
		p_tab_ti_dl_ref     = (struct code_text *)tab_ti_dl_ref [Na_network];
		p_tab_ti_ns_prim    = (struct code_text *)tab_ti_ns_prim[Na_network];
		p_tab_ns_prim       = (struct code_text *)tab_ns_prim   [Na_network];
#	endif

	Pr_msg_ns;

	if (ns_pr_cc_sdu_ie	== ON) {

		/*
		 * IE in the sdu have to be displayed
		 * one by one on seperate lines, in order
		 * to make the buffer comprehension easier.
		 */

		p_buffer = L_msg_p_buffer;

		offset_nb = get_offset_nb (L_msg_code, Na_network);

			/* when the message has no IE, display raw data */

		if ((offset_nb == BYTE_VAL_NIL) || (offset_nb == 0)) return (CONTINUE);

			/* Message data */

		if (p_buffer == P_NIL) {

			data_size = Message_lgth - L_msg_data_offset;

			if (data_size > 0) {
				fprintf (f_out, "\nmessage header :   ");
				fprintf (f_out, "offset = %2x  size = %x\n",
				L_msg_data_offset, data_size);
				pr_sdu_ns_cc_data (L_msg_a_start_data, data_size, offset_nb);

#				if NS_SUPP_SERVICES == ON
					if (ns_pr_sdu_ss == ON) {	/* print Supplementary Services data contained in SDU */

						ns_pr_ss_data_in_sdu (L_msg_a_start_data, data_size, L_msg_code, where_is_net (Na_network));
					}
#				endif
			}

		}

#		if PR_POINTER == ON
			fprintf (f_out, "\n           &B = %Fp",p_buffer);
#		endif

		while (p_buffer != P_NIL) {

				/* Buffer data */

			fprintf (f_out, "\nbuffer header :   ");

#			if PR_POINTER == ON
				fprintf (f_out, "offset = %2x  size = %x  p_next = %Fp  (%d msg)\n",
					Buffer_data_offset, Buffer_data_size,
					(char *)Buffer_p_next, Buffer_link_count );
#			else
				fprintf (f_out, "offset = %2x  size = %x  (%d msg)\n",
					Buffer_data_offset, Buffer_data_size, Buffer_link_count );
#			endif

			if (Buffer_data_size > Buffer_data_lgth) {
				data_size = Buffer_data_lgth;

			} else {

				data_size = Buffer_data_size;
			}

			pr_sdu_ns_cc_data (Buffer_a_start_data, data_size, offset_nb);

#			if NS_SUPP_SERVICES == ON
				if (ns_pr_sdu_ss == ON) {	/* print Supplementary Services data contained in SDU */

					ns_pr_ss_data_in_sdu (Buffer_a_start_data, data_size, L_msg_code, where_is_net (Na_network));
				}
#			endif

			p_buffer = Buffer_p_next;

#			if NS_IE_TSP_IN == ON

				if (p_buffer != P_NIL) {
					fprintf (f_out, "\n\n#### Second buffer contents (Q931 format)");
					pr_data (Buffer_a_first_data, Buffer_data_size, Buffer_data_lgth);
					return (STOP);
				}

#			endif

		}

		return (STOP);

	} else {

		/*
		 * Display raw data.
		 */

		return (CONTINUE);
	}

}

/*-------------------------------------------------------------------------*/

void pr_sdu_ns_cc_data (p_data, data_size, offset_nb)
	uchar FAR			*p_data;		/* address of the data to print */
	uchar				data_size;		/* size of data in the buffer */
	uchar 				offset_nb;		/* nb of offset in the message */
{
	uchar FAR *p_current_offset;		/* pointer to the offset where to find the next data to be displayed. */
	uchar FAR *p_current_data;          /* pointer to the current data of the current IE. */
	uchar next_offset; 					/* smallest offset greater than the current one. */
	uchar nb_data;						/* nb of data of the current IE. */
	uchar nb_data_displayed;			/* nb of data displayed on the current line. */
	uchar nb;							/* nb of line of 'NB_BYTE_PER_LINE' bytes already displayed */
	uchar i,j,k;						/* dummy */
	uchar warn_flag;					/* flag showing equal offsets */
	uchar nb_data_s;					/* backup for nb_data */
	uchar nb_data_ie_r;					/* nb of data dipslayed for a repeated IE */
	uchar r_ie_of;						/* next offset of a repeated IE */
	uchar end_of_ie_r;					/* boolean to test end of a repeated IE */
	uchar ii;							/* dummy */

	for (i = 0; i < offset_nb; i++) {

		p_current_offset = p_data + i;
		fprintf (f_out, "\n  OF.%-2x(%2x): ", i, *p_current_offset);

		if ((*p_current_offset == OFFSET_NIL) || (*p_current_offset == BYTE_VAL_NIL)) {
			continue;			          /* IE is missing. */
		}

		if (*p_current_offset > data_size) {

				/* Oops ! We missed the end ??? */

			break;

		}

		p_current_data	= p_data + *p_current_offset;
		next_offset		= BYTE_VAL_NIL;

			/*
			 * Find the smallest offset greater than the
			 * current one.
			 */

		warn_flag = OFF;

		for (j = 0; j < offset_nb; j++) {

			if (i == j) continue;		/* skip the current offset */

			if ((*p_current_offset == p_data[j]) && (i < j)) {
				warn_flag = ON;

			} else {

				if ((*p_current_offset	< p_data[j]) &&
					(next_offset 		> p_data[j])) {
					next_offset = p_data[j];
				}
			}
		}

			/* display nb_data from p_current_data. */

		if (warn_flag == OFF) {

			if ((next_offset == BYTE_VAL_NIL) || (next_offset > data_size)) {
			   nb_data = data_size	- *p_current_offset;

			} else {

			   nb_data	= next_offset - *p_current_offset;
			}

			nb				= 0;
			j				= 0;
			ii				= 0;
			r_ie_of			= 0;
			nb_data_ie_r	= 0;
			nb_data_s		= nb_data;
			end_of_ie_r		= FALSE;

			if (p_current_data[0] != 0) {
				r_ie_of		= p_current_data[0];
				nb_data		= r_ie_of - p_current_offset[0];
			}

			while (!end_of_ie_r) {


				while (j < nb_data) {

					fprintf (f_out, "%2x", p_current_data[j]);
					fprintf (f_out, (j == nb_data-1) ? "." : " ");

					if (pr_text == ON) {

							/*
							 * We start displaing data in text (ASCII) when
							 * 1) Next data has to be displayed on the next line
							 *			OR
							 * 2) All data were displayed.
							 */

						if (((j+1) % NB_BYTE_PER_LINE == 0) || (j == nb_data - 1)) {

							/* Compute how many data were displayed */

							nb_data_displayed = NB_BYTE_PER_LINE;

							if (j == nb_data - 1) {

								nb_data_displayed = nb_data - nb*NB_BYTE_PER_LINE;

								  /* Add ' ' to reach the end of line. */

								for (k = 0; k < NB_BYTE_PER_LINE - nb_data_displayed; k++) {
									fprintf (f_out, "   ");
								}
							}

							fprintf (f_out, " ");

								/* Display data in text */

							for (k = 0; k < nb_data_displayed; k++) {

								if ((char)p_current_data[nb*NB_BYTE_PER_LINE + k] < ' ') {
									fprintf (f_out,  "");

								} else {

									fprintf (f_out, "%c", (char)p_current_data[nb*NB_BYTE_PER_LINE + k]);
								}
							}

							nb++;    	/* increment the set of 'NB_BYTE_PER_LINE octets'. */
						}
					}

					j++;

					if (j % NB_BYTE_PER_LINE == 0 && (j != nb_data)) {
						fprintf (f_out, "\n             ");
					}

				}	/* end of while data */


				ii++;						/* dummy, just to know how many times the IE is repeated */
				nb_data_ie_r += nb_data;

					/* Were all instances displayed ? */

				if (nb_data_ie_r < nb_data_s) {
					r_ie_of			= p_current_data[j];
					p_current_data += j;

					if (r_ie_of == 0) {		/* Last time this IE is repeated */
						nb_data = nb_data_s - nb_data_ie_r;

					} else {

						nb_data	= r_ie_of - (*p_current_offset + nb_data_ie_r);
					}

					fprintf (f_out, "\n  OF.%-2x(%2x): ", i+ii, *p_current_offset);
					j		= 0;
					nb		= 0;

				} else {

					end_of_ie_r = TRUE;
				}

			}	/* end of while (not end of current IE) */

		}

	}

}

/*-------------------------------------------------------------------------*

	For each primitive, except those containing no IE, this function
	returns the number of IE in the SSDU.

 *-------------------------------------------------------------------------*/

uchar get_offset_nb (msg_code, net)
	code_t msg_code;
	uchar  net;
{
	switch (where_is_net (net)) {

#if F_AT4 == ON

		case ATT_4ESS :
			return (at4_get_offset_nb (msg_code));
#endif

#if F_AT5 == ON

		case ATT_5E5 :
			return (at5_get_offset_nb (msg_code));
#endif

#if F_AT9 == ON

		case ATT_5E9 :
			return (at9_get_offset_nb (msg_code));
#endif

#if F_AU1 == ON

		case AUSTEL_1 :
			return (au1_get_offset_nb (msg_code));
#endif

#if F_BIP == ON

		case BELLCORE_PRI :
			return (bip_get_offset_nb (msg_code));
#endif

#if F_BV1 == ON

		case BG_V1 :
			return (bv1_get_offset_nb (msg_code));
#endif

#if F_BT2 == ON

		case BT_ISDN2 :
			return (bt2_get_offset_nb (msg_code));
#endif

#if F_TR6 == ON

		case DT_1TR6 :
			return (tr6_get_offset_nb (msg_code));
#endif

#if F_ETS == ON

		case ETSI :
			return (ets_get_offset_nb (msg_code));
#endif

#if F_A17 == ON

		case CTU_CDS :
			return (a17_get_offset_nb (msg_code));
#endif

#if F_A11 == ON

		case CTU_BS :
			return (a11_get_offset_nb (msg_code));
#endif

#if F_E10 == ON

		case ATT_5E10 :
			return (e10_get_offset_nb (msg_code));
#endif

#if F_VN2 == ON

		case FT_VN2 :
			return (vn2_get_offset_nb (msg_code));
#endif

#if F_VN3 == ON

		case FT_VN3 :
			return (vn3_get_offset_nb (msg_code));
#endif

#if F_VN6 == ON

		case FT_VN6 :
			return (vn6_get_offset_nb (msg_code));
#endif

#if F_KDD == ON

		case KDD :
			return (kdd_get_offset_nb (msg_code));
#endif

#if F_NI1 == ON

		case N_ISDN1 :
			return (ni1_get_offset_nb (msg_code));
#endif

#if F_NI2 == ON

		case N_ISDN2 :
			return (ni2_get_offset_nb (msg_code));
#endif

#if F_DMS == ON

		case NT_DMS100 :
			return (dms_get_offset_nb (msg_code));
#endif

#if F_NTT == ON

		case NTT :
			return (ntt_get_offset_nb (msg_code));
#endif

#if F_SWD == ON

		case SWD_TVKT :
			return (swd_get_offset_nb (msg_code));
#endif

#if F_QSI == ON

		case ECMA_QSIG :
			return (qsi_get_offset_nb (msg_code));
#endif

#if F_HKT == ON

		case HK_TEL :
			return (hkt_get_offset_nb (msg_code));
#endif

	}

	return (BYTE_VAL_NIL);

}

#if F_AT4 == ON

uchar at4_get_offset_nb (msg_code)
	code_t msg_code;
{
	switch (msg_code) {

			case NS_CONN_RQ :
				return (sizeof (off_ns_at4_conn_rq) / sizeof (uint));

			case NS_CONN_IN :
				return (sizeof (off_ns_at4_conn_in) / sizeof (uint));

			case NS_CONN_RS :
				return (sizeof (off_ns_at4_conn_rs) / sizeof (uint));

			case NS_CONN_CO :
				return (sizeof (off_ns_at4_conn_co) / sizeof (uint));

			case NS_CLEAR_RQ :
				return (sizeof (off_ns_at4_clear_rq) / sizeof (uint));

			case NS_CLEAR_IN :
				return (sizeof (off_ns_at4_clear_in) / sizeof (uint));

			case NS_CLEAR_RS :
				return (sizeof (off_ns_at4_clear_rs) / sizeof (uint));

			case NS_CLEAR_CO :
				return (sizeof (off_ns_at4_clear_co) / sizeof (uint));

			case NS_ALERT_RQ :
				return (sizeof (off_ns_at4_alert_rq) / sizeof (uint));

			case NS_ALERT_IN :
				return (sizeof (off_ns_at4_alert_in) / sizeof (uint));

			case NS_CALL_PROC_RQ :
				return (sizeof (off_ns_at4_call_proc_rq) / sizeof (uint));

			case NS_CALL_PROC_IN :
				return (sizeof (off_ns_at4_call_proc_in) / sizeof (uint));

			case NS_CONGEST_CONTROL_IN :
				return (sizeof (off_ns_at4_con_con_in) / sizeof (uint));

			case NS_PROGRESS_RQ :
				return (sizeof (off_ns_at4_progress_rq) / sizeof (uint));

			case NS_PROGRESS_IN :
				return (sizeof (off_ns_at4_progress_in) / sizeof (uint));

			case NS_USER_INFO_RQ :
				return (sizeof (off_ns_at4_user_info_rq) / sizeof (uint));

			case NS_USER_INFO_IN :
				return (sizeof (off_ns_at4_user_info_in) / sizeof (uint));

			case NS_RESTART_RQ :
				return (1);

			case NS_RESTART_IN :
				return (sizeof (off_ns_at4_restart_in) / sizeof (uint));

			case NS_RESTART_RS :
				return (sizeof (off_ns_at4_restart_rs) / sizeof (uint));

			case NS_FAC_RQ :
				return (sizeof (off_ns_at4_fac_rq) / sizeof (uint));

			case NS_FAC_CO :
				return (sizeof (off_ns_at4_fac_co) / sizeof (uint));

			case NS_STATUS_ENQ_RQ :
				break;
	}

	return (BYTE_VAL_NIL);
}

#endif	/* AT4 */

