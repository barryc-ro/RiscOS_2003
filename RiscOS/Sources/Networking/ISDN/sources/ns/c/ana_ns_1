#if F_AT9 == ON

uchar at9_get_offset_nb (msg_code)
	code_t msg_code;
{
	switch (msg_code) {


			case NS_CONN_RQ :
				return (sizeof (off_ns_at9_conn_rq) / sizeof (uint));

			case NS_CONN_IN :
