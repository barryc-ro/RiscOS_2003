/*-------------------------------------------------------------------------*/

uchar ana_sdu_mns (p_msg)
	struct message FAR *p_msg;
{
	uchar FAR *p_data;

	if (Rcv_nai == 255) {

		Pr_msg_mns_2;

	} else {

		if (ns_access_na (Rcv_nai) == NOK) return (NOK);

		Pr_msg_mns;

	}

		/* Decode the MNS_ERR_IN primitive */

	p_data = L_a_data;

	switch (L_msg_code) {

		case MNS_ERR_IN :
			fprintf (f_out, "\n\t\t\tERROR    : %s, diag : %d", Code_to_text (t_mns_err_code, Mns_err_cause), Mns_err_diagnostic);
			fprintf (f_out, "\n\t\t\tCALL_REF : l=%d, v=%d - TEI %d", Mns_err_call_ref_lgth, (Mns_err_call_ref & Mask (16)), Mns_err_tei);
			fprintf (f_out, "\n\t\t\tSPDU_ID  : 0x%02x, PD : %d, SSDU_ID : %s", Mns_err_spdu_id, Mns_err_protocol_discr, Code_to_text (Ns_prim, Mns_err_ssdu_id));
			fprintf (f_out, "\n\t\t\tIE       : codeset %d, id 0x%02x, rep %d", Mns_err_ie_codeset, Mns_err_ie_id, Mns_err_ie_instance);
			return (STOP);

		case MNS_EVENT_IN :

			fprintf (f_out, "\n\t\t\tEVENT : %s", Code_to_text (t_mns_event_type, Mns_event_type));

			switch (Mns_event_type) {

				case NS_EVENT_TIMER :
					fprintf (f_out, " %s", Code_to_text (Ti_ns_prim, Mns_event_code));
					break;

				case NS_EVENT_PROTOCOL_ID :
					fprintf (f_out, " : %02xh", Mns_event_code);
					break;

#				if NS_SPID == ON
					case NS_EVENT_SPID_OK :
						fprintf (f_out, " (USID,TID) = (%x,%x)", Mns_event_usid, Mns_event_tid);
						break;
#				endif

			}

			return (STOP);

		case MNS_STATUS_IN :
			fprintf (f_out, "\n\t\t\tTei %d, Call_ref : %d", Mns_status_tei, (Mns_status_call_ref & Mask(16)));
			fprintf (f_out, "\n\t\t\tCause : location %02xh, value %02xh, diag %d, Call state %d", Mns_status_location, C_v (Mns_status_cause_class, Mns_status_cause_val), Mns_status_diagnostic, Mns_status_call_state);
			return (STOP);

#		if NS_MAINTENANCE_CAPABILITY == ON

			case MNS_SERVICE_CO :
			case MNS_SERVICE_RQ :
				fprintf (f_out, "\n\t\t\tPreference   : %s", Code_to_text(maint_pref, Mns_service_pref));
				fprintf (f_out, "\n\t\t\tInterface ID : %x", Mns_service_int_id);
				fprintf (f_out, "\n\t\t\tChannel      : %x", Mns_service_b_chan);
				fprintf (f_out, "\n\t\t\tStatus       : %s", Code_to_text(maint_b_state, Mns_service_status));
				fprintf (f_out, "\n\t\t\tError        : %x", Mns_service_error);
				fprintf (f_out, "\n\t\t\tAction       : %s", Code_to_text(maint_action, Mns_service_action_type));
				return (STOP);
#		endif

	}

	return (CONTINUE);
}

/*-------------------------------------------------------------------------*/

uchar ana_sdu_ti_ns (p_msg)
	struct message FAR *p_msg;
{

	if (ns_access_na (Rcv_nai) == NOK) return (NOK);

	Pr_msg_ti_ns;

	return (CONTINUE);
}

#endif	/* SIMULATION ANALYSER */


/*-------------------------------------------------------------------------*/

void ns_build_simul_table (network)
	uchar network;
{

#if F_AT4 == ON
	if (at4 != NIL) {
		tab_ti_ref		[at4] 		= (struct code_text *)&AT4_TI_REF;
		tab_ti_dl_ref	[at4] 		= (struct code_text *)&AT4_TI_DL_REF;
		tab_ti_ns_prim	[at4] 		= (struct code_text *)&AT4_TI_NS_PRIM;
		tab_ns_prim		[at4] 		= (struct code_text *)&AT4_NS_PRIM;
	}
#endif

#if F_AT5 == ON
	if (at5 != NIL) {
		tab_ti_ref		[at5] 		= (struct code_text *)&AT5_TI_REF;
		tab_ti_dl_ref	[at5] 		= (struct code_text *)&AT5_TI_DL_REF;
		tab_ti_ns_prim	[at5] 		= (struct code_text *)&AT5_TI_NS_PRIM;
		tab_ns_prim		[at5] 		= (struct code_text *)&AT5_NS_PRIM;
	}
#endif

#if F_AT9	== ON
	if (at9 != NIL) {
		tab_ti_ref		[at9] 		= (struct code_text *)&AT9_TI_REF;
		tab_ti_dl_ref	[at9] 		= (struct code_text *)&AT9_TI_DL_REF;
		tab_ti_ns_prim	[at9] 		= (struct code_text *)&AT9_TI_NS_PRIM;
		tab_ns_prim		[at9] 		= (struct code_text *)&AT9_NS_PRIM;
	}
#endif

#if F_BIP	== ON
	if (bip != NIL) {
		tab_ti_ref		[bip] 		= (struct code_text *)&BIP_TI_REF;
		tab_ti_dl_ref	[bip] 		= (struct code_text *)&BIP_TI_DL_REF;
		tab_ti_ns_prim	[bip] 		= (struct code_text *)&BIP_TI_NS_PRIM;
		tab_ns_prim		[bip] 		= (struct code_text *)&BIP_NS_PRIM;
