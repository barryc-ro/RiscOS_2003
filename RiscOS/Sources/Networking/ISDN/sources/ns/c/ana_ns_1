			case NS_CONN_CO :
				return (sizeof (off_ns_at9_conn_co) / sizeof (uint));

			case NS_CLEAR_RQ :
				return (sizeof (off_ns_at9_clear_rq) / sizeof (uint));

			case NS_CLEAR_IN :
				return (sizeof (off_ns_at9_clear_in) / sizeof (uint));

			case NS_CLEAR_RS :
				return (sizeof (off_ns_at9_clear_rs) / sizeof (uint));

			case NS_CLEAR_CO :
				return (sizeof (off_ns_at9_clear_co) / sizeof (uint));

			case NS_ALERT_RQ :
				return (sizeof (off_ns_at9_alert_rq) / sizeof (uint));

			case NS_ALERT_IN :
				return (sizeof (off_ns_at9_alert_in) / sizeof (uint));

			case NS_CALL_PROC_RQ :
				return (sizeof (off_ns_at9_call_proc_rq) / sizeof (uint));

			case NS_CALL_PROC_IN :
				return (sizeof (off_ns_at9_call_proc_in) / sizeof (uint));

			case NS_SETUP_ACK_IN :
				return (sizeof (off_ns_at9_setup_ack_in) / sizeof (uint));

			case NS_PROGRESS_IN :
				return (sizeof (off_ns_at9_progress_in) / sizeof (uint));

