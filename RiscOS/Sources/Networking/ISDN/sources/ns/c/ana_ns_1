		tab_ti_ns_prim	[vn6] 		= (struct code_text *)&VN6_TI_NS_PRIM;
		tab_ns_prim		[vn6] 		= (struct code_text *)&VN6_NS_PRIM;
	}
#endif


	p_tab_ti_ref		= (struct code_text	*)tab_ti_ref	[network];
	p_tab_ti_dl_ref		= (struct code_text	*)tab_ti_dl_ref	[network];
	p_tab_ti_ns_prim	= (struct code_text	*)tab_ti_ns_prim[network];
	p_tab_ns_prim		= (struct code_text	*)tab_ns_prim	[network];

}
/*--------------------------------------------------------------------------*/

extern char	com_log_net_op[5][5];

static struct {
	char FAR *str;
	uchar     ns_operator;
	ushort    country_code;

} CONST_ARRAY t_country_strings[] = {

	{"au1", AUSTEL_1,     AUSTRALIA},
	{"bt2", BT_ISDN2,     UK},
	{"bv1", BG_V1,        BELGIUM},
	{"dms", NT_DMS100,    USA},
	{"ets", ETSI,         EUROPE},
	{"a17", CTU_CDS,      COUNTRY_NIL},
	{"a11", CTU_BS,       COUNTRY_NIL},
	{"e10", ATT_5E10,     USA},
	{"kdd", KDD,          JAPAN},
	{"ni1", N_ISDN1,      USA},
	{"ni2", N_ISDN2,      USA},
	{"ntt", NTT,          JAPAN},
	{"qsi", ECMA_QSIG,    EUROPE},
	{"hkt", HK_TEL,       HONG_KONG},
	{"swd", SWD_TVKT,     SWEDEN},
	{"tr6", DT_1TR6,      GERMANY},
	{"vn2", FT_VN2,       FRANCE},
	{"vn3", FT_VN3,       FRANCE},
	{"vn6", FT_VN6,       FRANCE},
	{"at4", ATT_4ESS,     USA},
	{"at5", ATT_5E5,      USA},
	{"at9", ATT_5E9,      USA},
	{"bip", BELLCORE_PRI, USA},
};

void get_simul_net_op (nai, na_country, na_op)
	uchar nai;
	ushort FAR *na_country;
	uchar  FAR *na_op;
{
	char *p_net_op;
	uchar i;

		/* look for the NAI value in the "n=..." command line strings */

	for (i = 0; i < 5; i++) {

		if ((com_log_net_op[i][0] == '*') ||
			(com_log_net_op[i][0] == (nai + '0'))) {
			break;	/* found nai on the command line, or '*'=all nais */
		}
	}

	if (i == 5) return;	/* not found: return */

		/* Found one: determine the country-code and the network-operator */

	p_net_op = com_log_net_op[i];
	p_net_op++;

			/* force lower case letters for the operator string */

	for (i = 0; i < 3; i++) {
		p_net_op[i] = Tolower (p_net_op[i]);
	}

			/* determine the country-code and the network-operator */

	for (i = 0;
		 i < Ns_tab_item_nb (t_country_strings);
		 i++) {

		if (Strcmp_far (p_net_op, t_country_strings[i].str) == 0) {
			*na_op      = t_country_strings[i].ns_operator;
			*na_country = t_country_strings[i].country_code;
			break;
		}
	}

}

/*-------------------------------------------------------------------------*
					 F U N C T I O N   H E A D E R

Name            where_is_net -	Find the idx according the network idx

Usage           uchar where_is_net (uchar net);

*--------------------------------------------------------------------------*/
uchar where_is_net (net)
	uchar net;
{

	uchar i;

	i = 0;

	do {

		if (Net_table_idx (i) == net) {
			return (i);
		}

		i++;

	} while (i < MX_OPERATOR_NB);

	return (i);


}

#endif	/* ANY SIMULATION */

