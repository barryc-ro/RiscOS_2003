	Ns_ntt_tab_tabl_entry (NTT_T_CHAN_NUMBER,			NTT_CHAN_NUMBER_S)
	Ns_ntt_tab_tabl_entry (NTT_T_CHAN_PREF_EXCL,		NTT_CHAN_PREF_EXCL_S)
