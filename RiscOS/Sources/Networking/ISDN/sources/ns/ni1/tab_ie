	Ns_ni1_tab_tabx_entry (T_NIL,					P_NIL)
	Ns_ni1_tab_tabx_entry (NI1_TX_C_V_CCITT,      	NI1_TAB_C_V_CCITT_S)
	Ns_ni1_tab_tabx_entry (NI1_TX_C_V_SPF,        	NI1_TAB_C_V_SPF_S)
