	Ns_ni1_tab_tabx_entry (NI1_TX_TRF_RATE,       	NI1_TAB_TRF_RATE_S)
	Ns_ni1_tab_tabx_entry (NI1_TX_CHAN_SEL,       	NI1_TAB_CHAN_SEL_S)
Ns_ni1_end_table_or_function /* } */

/*-------------------------------------------------------------------------*

IE description structures :
-------------------------

					ie        tab_ie
	===============================================================
	| codeset | id | p_prog | size | p_struc | p_init| repetition |
	|---------|----|--------|------|---------|-------|------------|  meta-prog
	|         |    |  X----------------------------------------------> START
	|---------|----|--------|------|---------|-------|------------|     XXXXX
	|         |    |        |      |         |       |            |     YYYYY
	|---------|----|--------|------|---------|-------|------------| END
    |         |    |        |      |         |       |            |
    |---------|----|--------|------|---------|-------|------------|
    |         |    |        |  X   |    X------------------------------> i_xxxx
    |---------|----|--------|--|---|---------|-------|------------|         Œ internal
    |         |    |        |  ---------------------------------------------| data
    |---------|----|--------|------|---------|-------|------------|         v structure
    |         |    |        |      |         |       |            |
    |---------|----|--------|------|---------|-------|------------|
    |         |    |        |      |         |  X--------------------> df_spdu_ni1_i_xxxx
    |---------|----|--------|------|---------|-------|------------|     Œ implicit  and df_ssdu_ni1_i_xxx
    |         |    |        |      |         |       |            |     | value or
    |---------|----|--------|------|---------|-------|------------|
    v  P_NIL

         Information Element (IE) table of item :
                    - codeset,
                    - ie identifier;
                    - program sequence address,
                    - internal structure size,
                    - internal structure address,
                    - initialization structure address,
                    - maximum number of repeated IE.

*/

Ns_ni1_table_or_function (ie, NI1_TAB_IE)	/* { */

Ns_ni1_tab_ie_entry (CODESET_0,BC,                  &NI1_BC_P,                i_bc,                 &df_spdu_ni1_i_bc,                &df_ssdu_ni1_i_bc,                NI1_MX_BC)
Ns_ni1_tab_ie_entry (CODESET_0,CALL_STATE,          &NI1_CALL_STATE_P,        i_call_state,         &df_spdu_ni1_i_call_state,        &df_ssdu_ni1_i_call_state,        NI1_MX_CALL_STATE)
Ns_ni1_tab_ie_entry (CODESET_0,CALLED_NB,           &NI1_CALLED_NB_P,         i_called_nb,          &df_spdu_ni1_i_called_nb,         &df_ssdu_ni1_i_called_nb,         NI1_MX_CALLED_NB)
Ns_ni1_tab_ie_entry (CODESET_0,CALLED_SUB_ADD,      &NI1_CALLED_SUB_ADD_P,    i_called_sub_add,     &df_spdu_ni1_i_called_sub_add,    &df_ssdu_ni1_i_called_sub_add,    NI1_MX_CALLED_SUB_ADD)
Ns_ni1_tab_ie_entry (CODESET_0,CALLING_NB,          &NI1_CALLING_NB_P,        i_calling_nb,         &df_spdu_ni1_i_calling_nb,        &df_ssdu_ni1_i_calling_nb,        NI1_MX_CALLING_NB)
Ns_ni1_tab_ie_entry (CODESET_0,CALLING_SUB_ADD,     &NI1_CALLING_SUB_ADD_P,   i_calling_sub_add,    &df_spdu_ni1_i_calling_sub_add,   &df_ssdu_ni1_i_calling_sub_add,   NI1_MX_CALLING_SUB_ADD)
Ns_ni1_tab_ie_entry (CODESET_0,CAUSE,               &NI1_CAUSE_P,             i_cause,              &df_spdu_ni1_i_cause,             &df_ssdu_ni1_i_cause,             NI1_MX_CAUSE)
Ns_ni1_tab_ie_entry (CODESET_0,CHAN_ID,             &NI1_CHAN_ID_P,           i_chan_id,            &df_spdu_ni1_i_chan_id,           &df_ssdu_ni1_i_chan_id,           NI1_MX_CHAN_ID)
Ns_ni1_tab_ie_entry (CODESET_0,CONN_NB,             &NI1_CONN_NB_P,           i_conn_nb,            &df_spdu_ni1_i_conn_nb,           &df_ssdu_ni1_i_conn_nb,           NI1_MX_CONN_NB)
Ns_ni1_tab_ie_entry (CODESET_0,ENDPOINT_ID,         &NI1_ENDPOINT_ID_P,       i_endpoint_id,        &df_spdu_ni1_i_endpoint_id,       A_NIL,                            NI1_MX_ENDPOINT_ID)
Ns_ni1_tab_ie_entry (CODESET_0,FEATURE_ACT,         &NI1_FEATURE_ACT_P,       i_feature_act,        &df_spdu_ni1_i_feature_act,       &df_ssdu_ni1_i_feature_act,       NI1_MX_FEATURE_ACT)
Ns_ni1_tab_ie_entry (CODESET_0,FEATURE_IND,         &NI1_FEATURE_IND_P,       i_feature_ind,        &df_spdu_ni1_i_feature_ind,       &df_ssdu_ni1_i_feature_ind,       NI1_MX_FEATURE_IND)
Ns_ni1_tab_ie_entry (CODESET_0,HLC,                 &NI1_HLC_P,               i_hlc,                &df_spdu_ni1_i_hlc,               &df_ssdu_ni1_i_hlc,               NI1_MX_HLC)
Ns_ni1_tab_ie_entry (CODESET_0,INFORMATION_RQ,      &NI1_INFORMATION_RQ_P,    i_information_rq,     &df_spdu_ni1_i_information_rq,    &df_ssdu_ni1_i_information_rq,    NI1_MX_INFORMATION_RQ)
Ns_ni1_tab_ie_entry (CODESET_0,KEYPAD,              &NI1_KEYPAD_P,            i_keypad,             &df_spdu_ni1_i_keypad,            &df_ssdu_ni1_i_keypad,            NI1_MX_KEYPAD)
Ns_ni1_tab_ie_entry (CODESET_0,LLC,                 &NI1_LLC_P,               i_llc,                &df_spdu_ni1_i_llc,               &df_ssdu_ni1_i_llc,               NI1_MX_LLC)
Ns_ni1_tab_ie_entry (CODESET_0,NOTIF_IND,           &NI1_NOTIF_IND_P,         i_notif_ind,          &df_spdu_ni1_i_notif_ind,         &df_ssdu_ni1_i_notif_ind,         NI1_MX_NOTIF_IND)
Ns_ni1_tab_ie_entry (CODESET_0,O_CALLED_NB,         &NI1_O_CALLED_NB_P,       i_o_called_nb,        &df_spdu_ni1_i_o_called_nb,       &df_ssdu_ni1_i_o_called_nb,       NI1_MX_O_CALLED_NB)
Ns_ni1_tab_ie_entry (CODESET_0,PROGRESS_IND,        &NI1_PROGRESS_IND_P,      i_progress_ind,       &df_spdu_ni1_i_progress_ind,      &df_ssdu_ni1_i_progress_ind,      NI1_MX_PROGRESS_IND)
Ns_ni1_tab_ie_entry (CODESET_0,REDIR_NB,            &NI1_REDIR_NB_P,          i_redir_nb,           &df_spdu_ni1_i_redir_nb,          &df_ssdu_ni1_i_redir_nb,          NI1_MX_REDIR_NB)
Ns_ni1_tab_ie_entry (CODESET_0,REDIRECTION_NB,      &NI1_REDIRECTION_NB_P,    i_redirection_nb,     &df_spdu_ni1_i_redirection_nb,    &df_ssdu_ni1_i_redirection_nb,    NI1_MX_REDIRECTION_NB)
Ns_ni1_tab_ie_entry (CODESET_0,RESTART_IND,         &NI1_RESTART_IND_P,       i_restart_ind,        &df_spdu_ni1_i_restart_ind,       &df_ssdu_ni1_i_restart_ind,       NI1_MX_RESTART_IND)
