Ns_ni1_tab_ie_entry (CODESET_0,SPID,                &NI1_SPID_P,              i_spid,               &df_spdu_ni1_i_spid,              &df_ssdu_ni1_i_spid,              NI1_MX_SPID)

Ns_ni1_tab_ie_entry (CODESET_0,TRANSIT_NET_SEL,     &NI1_TRANSIT_NET_SEL_P,   i_transit_net_sel,    &df_spdu_ni1_i_transit_net_sel,   &df_ssdu_ni1_i_transit_net_sel,   NI1_MX_TRANSIT_NET_SEL)

Ns_ni1_tab_ie_entry (CODESET_5,OS_ACCSS,            &NI1_OS_ACCSS_P,          i_os_accss,           &df_spdu_ni1_i_os_accss,          &df_ssdu_ni1_i_os_accss,          NI1_MX_OS_ACCSS)
