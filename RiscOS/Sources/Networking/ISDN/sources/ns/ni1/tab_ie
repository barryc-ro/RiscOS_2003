Ns_ni1_tab_ie_entry (CODESET_6,CALL_APPEAR,         &NI1_CALL_APPEAR_P,       i_call_appear,        &df_spdu_ni1_i_call_appear,       &df_ssdu_ni1_i_call_appear,       NI1_MX_CALL_APPEAR)
