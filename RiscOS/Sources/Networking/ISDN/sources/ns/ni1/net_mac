/*====================		MISCELLANEOUS  MESSAGES	=======================*/

		/* NS_INFO_RQ */

struct off_ns_ni1_te_info_rq {
	Message_header_offset_ni1				/* message header parameters */
	ns_offset_t ni1_keypad;					/* keypad */
	ns_offset_t ni1_feature_act;			/* feature activation */
	ns_offset_t ni1_spid;					/* spid */
	ns_offset_t ni1_called_nb;				/* called number */
	ns_offset_t ni1_prot_ver_ctrl;			/* protocol version control */
};

typedef struct off_ns_ni1_te_info_rq		off_ns_ni1_info_rq;
typedef struct off_ns_ni1_te_info_rq		off_ns_ni1_net_info_in;

		/* NS_INFO_IN */

struct off_ns_ni1_te_info_in {
	Message_header_offset_ni1				/* message header parameters */
	ns_offset_t ni1_cause;					/* cause */
	ns_offset_t ni1_conn_nb;				/* connected number */
	ns_offset_t ni1_progress_ind;			/* progress indication */
	ns_offset_t ni1_information_rq;			/* information request */
	ns_offset_t ni1_signal;					/* signal */
	ns_offset_t ni1_feature_ind;			/* feature indication */
	ns_offset_t ni1_redirection_nb;			/* redirection number */
	ns_offset_t ni1_prot_ver_ctrl;			/* protocol version control */
};

typedef struct off_ns_ni1_te_info_in		off_ns_ni1_net_info_rq;
typedef struct off_ns_ni1_te_info_in		off_ns_ni1_info_in;

		/* NS_NOTIFY_RQ */

struct off_ns_ni1_te_notify_rq {
	Message_header_offset_ni1				/* message header parameters */
	ns_offset_t ni1_cause;					/* cause */
	ns_offset_t ni1_conn_nb;				/* connected number */
	ns_offset_t ni1_progress_ind;			/* progress indicator */
	ns_offset_t ni1_notif_ind;				/* notify indicator */
	ns_offset_t ni1_information_rq;			/* information request */
	ns_offset_t ni1_calling_nb;				/* calling nb */
	ns_offset_t ni1_redirection_nb;			/* redirection number */
	ns_offset_t ni1_display_txt;			/* display */
};

typedef struct off_ns_ni1_te_notify_rq		off_ns_ni1_net_notify_in;
typedef struct off_ns_ni1_te_notify_rq		off_ns_ni1_notify_rq;

		/* NS_NOTIFY_IN */

struct off_ns_ni1_te_notify_in {
	Message_header_offset_ni1				/* message header parameters */
	ns_offset_t ni1_bc;						/* bearer capability */
	ns_offset_t ni1_cause;					/* cause */
	ns_offset_t ni1_conn_nb;				/* connected number */
	ns_offset_t ni1_progress_ind;			/* progress indicator */
	ns_offset_t ni1_notif_ind;				/* notify indicator */
	ns_offset_t ni1_signal;					/* signal */
	ns_offset_t ni1_feature_ind;			/* feature indication */
	ns_offset_t ni1_information_rq;			/* information request */
	ns_offset_t ni1_calling_nb;				/* calling nb */
	ns_offset_t ni1_called_nb;				/* called nb */
	ns_offset_t ni1_redirection_nb;			/* redirection number */
	ns_offset_t ni1_display_txt;			/* display txt */
};

typedef struct off_ns_ni1_te_notify_in		off_ns_ni1_net_notify_rq;
typedef struct off_ns_ni1_te_notify_in		off_ns_ni1_notify_in;

		/* NS_RESTART_RQ */

struct off_ns_ni1_te_restart_rq {
	Message_header_offset_ni1				/* message header parameters */

#	if MESSAGE_HEADER_IE != ON
		ns_offset_t ie_nil;					/* or no IE */
#	endif

};

typedef struct off_ns_ni1_te_restart_rq		off_ns_ni1_restart_rq;
typedef struct off_ns_ni1_te_restart_rq		off_ns_ni1_net_restart_in;

		/* NS_RESTART_RS */

struct off_ns_ni1_te_restart_rs {
	Message_header_offset_ni1				/* message header parameters */
	ns_offset_t ni1_chan_id;					/* channel Id */
	ns_offset_t ni1_restart_ind;				/* restart indicator */
};

typedef struct off_ns_ni1_te_restart_rs		off_ns_ni1_restart_rs;
typedef struct off_ns_ni1_te_restart_rs		off_ns_ni1_net_restart_co;

		/* NS_RESTART_IN */

struct off_ns_ni1_te_restart_in {
	Message_header_offset_ni1				/* message header parameters */
	ns_offset_t ni1_chan_id;				/* channel Id */
	ns_offset_t ni1_restart_ind;			/* restart indicator */
};

typedef struct off_ns_ni1_te_restart_in		off_ns_ni1_net_restart_rq;
typedef struct off_ns_ni1_te_restart_in		off_ns_ni1_restart_in;

		/*****************************************************
		**	DESCRIPTION of the value of an IE.				**
		**	---------------------------------				**
		**	EACH ENTRY SHALL BE IN ACCORDANCE WITH THE		**
		**	CORRESPONDING SSDU IE META-PROGRAM				**
		**													**
		*****************************************************/

		/* Bearer Capability (BC) */

struct val_ns_ni1_bc {
	Next_instance_offset
	ie_val		trf_cap;				/* information transfer capability */
	ie_val		protocol_id_1;			/* user information layer 1 protocol */
};

		/* Called party number (CALLED_NB) */

struct val_ns_ni1_called_nb {
	Next_instance_offset
	ie_val		nb_type;				/* numbering type */
	ie_val	 	plan_id;				/* numbering plan ID */
	ie_array 	nb[NI1_MX_SZ_CALLED_NB+2];	/* called number digits */
};

		/* Called party subaddress (CALLED_SUB_ADD) */

struct val_ns_ni1_called_sub_add {
	Next_instance_offset
	ie_val		sub_add_type;			/* called subaddress type */
	ie_val		odd_even_ind;			/* odd/even indicator */
	ie_array 	sub_add[NI1_MX_SZ_CALLED_SUB_ADD+2];	/* sub-add information */
};

		/* Calling party number (CALLING_NB) */

struct val_ns_ni1_calling_nb {
	Next_instance_offset
	ie_val		nb_type;				/* numbering type */
	ie_val	 	plan_id;				/* numbering plan ID */
	ie_val		pres;					/* presentation indicator */
	ie_val		screen_ind;				/* screening indicator */
	ie_array 	nb[NI1_MX_SZ_CALLING_NB+2];	/* calling number digits */
};

		/* Calling party subaddress (CALLING_SUB_ADD) */

struct val_ns_ni1_calling_sub_add {
	Next_instance_offset
	ie_val  	sub_add_type;			/* calling subaddress type */
	ie_val		odd_even_ind;			/* odd/even indicator */
	ie_array 	sub_add[NI1_MX_SZ_CALLING_SUB_ADD+2];	/* sub-add information */
};

		/* Cause (CAUSE) */

struct val_ns_ni1_cause {
	Next_instance_offset
	ie_val		coding_std;				/* coding standard */
	ie_val		cause_loc;				/* location */
	ie_val 		cause_class;			/* cause class field */
	ie_val		cause_val;				/* cause value field */
	ie_array	cause_diag[NI1_MX_SZ_CAUSE_DIAG+2];	/* diagnostics */
};

		/* Channel ID (CHAN_ID) */

struct val_ns_ni1_chan_id {
	Next_instance_offset
	ie_val 		int_type;				/* interface type */
	ie_val 		int_id;					/* interface id */
	ie_val 		pref_excl;				/* preferred or exclusive channel */
	ie_val		d_chan;					/* D-channel indicator */
	ie_val		int_id_val;				/* interface id value */
	ie_val 		chan_sel;				/* information channel selection */
	ie_val		chan_nb;				/* channel number */
};

		/* Connected number (CONN_NB) */

struct val_ns_ni1_conn_nb {
	Next_instance_offset
	ie_val		nb_type;				/* numbering type */
	ie_val	 	plan_id;				/* numbering plan ID */
	ie_val		pres;					/* presentation indicator */
	ie_val		screen_ind;				/* screening indicator */
	ie_array 	nb[NI1_MX_SZ_CONN_NB+2];	/* connected number digits */
};


		/* Endpoint identifier (ENDPOINT_ID) */

struct val_ns_ni1_endpoint_id {
	Next_instance_offset
	ie_val		usid;					/* user id */
	ie_val		tid;					/* terminal id */
	ie_val		interp;					/* interpreter flag */
};


		/* Feature activation (FEATURE_ACT) */

struct val_ns_ni1_feature_act {
	Next_instance_offset
	ie_val		feature_id;					/* feature identifier */
	ie_val		feature_id_2;				/* feature identifier (continuation) */
};

		/* Feature indication (FEATURE_IND) */

struct val_ns_ni1_feature_ind {
	Next_instance_offset
	ie_val		feature_id;					/* feature identifier */
	ie_val		feature_id_2;				/* feature identifier (continuation) */
};

		/* High Layer Compatibility (HLC) */

struct val_ns_ni1_hlc {
	Next_instance_offset
	ie_val		coding_std;					/* coding standard */
	ie_val		teleservice;				/* high layer characteristics identification */
	ie_val		ext_teleservice;			/* extended high layer characteristics identification */
};

		/* Information request (INFORMATION_RQ) */

struct val_ns_ni1_information_rq {
	Next_instance_offset
	ie_val		info_type;				/* information type */
	ie_val		info_ind;				/* information indicator */
	ie_val		info_specific;			/* specific information */
};

		/* Keypad */

struct val_ns_ni1_keypad {
	Next_instance_offset
	ie_array 	text[NI1_MX_SZ_KEYPAD+2];			/* keypad information */
};

		/* Low Layer Compatibility (LLC) */

struct val_ns_ni1_llc {
	Next_instance_offset
	ie_val		coding_std;				/* coding standard */
	ie_val		trf_cap;				/* information transfer capability */
	ie_val		out_band_neg;			/* out-band negotiation */
	ie_val		trf_mode;				/* transfer mode */
	ie_val		trf_rate;				/* information transfer rate */
	ie_val		structure;				/* structure */
	ie_val		dest_rate;				/* destination rate */
	ie_val		protocol_id_1;			/* user information layer 1 protocol */
	ie_val		syn_asyn;				/* synchronous/asynchronous */
	ie_val		negotiation;			/* negotiation */
	ie_val		user_rate;				/* user rate */
	ie_val		inter_rate;				/* intermediate rate */
	ie_val		nic_tx;					/* network independent clock on transmission (Tx) */
	ie_val		nic_rx;					/* network independent clock on reception (Rx) */
	ie_val		fc_tx;					/* flow control on transmission (Tx) */
	ie_val		fc_rx;					/* flow control on reception (Rx) */
	ie_val		hdr;					/* rate adaption header included or not */
	ie_val		multi_frame;			/* multiple frame establishement support in DL */
	ie_val		op_mode;				/* mode of operation */
	ie_val		lli_negot;				/* logical link identifier negotiation */
	ie_val		assign;					/* assignor/assignee */
	ie_val		band_negot;				/* in-band/out-band negotiation */
	ie_val		stop_bit;				/* number of stop bits */
	ie_val		data_bit;				/* number of data bits excluding parity if present */
	ie_val		parity;					/* parity information */
	ie_val		duplex;					/* duplex mode */
	ie_val		modem;					/* modem type */
	ie_val		protocol_id_2;			/* user layer 2 protocol information */
	ie_val		option_2;				/* optional layer 2 protocol information */
	ie_val		protocol_id_3;			/* user information layer 3 protocol */
	ie_val		option_3;				/* optional layer 3 protocol information */
};

		/* Notification indicator (NOTIF_IND) */

struct val_ns_ni1_notif_ind {
	Next_instance_offset
	ie_val		notif_descr;			/* notification description*/
};

		/* Original called number (O_CALLED_NB) */

struct val_ns_ni1_o_called_nb {
	Next_instance_offset
	ie_val		nb_type;				/* numbering type */
	ie_val	 	plan_id;				/* numbering plan ID */
	ie_val		pres;					/* presentation indicator */
	ie_val		screen_ind;				/* screening indicator */
	ie_val		reason;					/* reason */
	ie_val		cfnr;					/* cfnr*/
	ie_val		redir_counter;			/* counter */
	ie_array 	nb[NI1_MX_SZ_O_CALLED_NB+2];	/* original called number digits */
};

		/* Progress indicator (PROGRESS_IND) */

struct val_ns_ni1_progress_ind {
	Next_instance_offset
	ie_val		coding_std;				/* coding standard */
	ie_val		progress_loc;			/* progress location */
	ie_val		progress_descr;			/* progress description */
};

		/* Redirecting number (REDIR_NB) */

struct val_ns_ni1_redir_nb {
	Next_instance_offset
	ie_val		nb_type;				/* numbering type */
	ie_val	 	plan_id;				/* numbering plan ID */
	ie_val		pres;					/* presentation indicator */
	ie_val		screen_ind;				/* screening indicator */
	ie_val		reason;					/* reason for redirection */
	ie_array 	nb[NI1_MX_SZ_REDIR_NB+2];	/* number digits */
};

		/* Redirection number (REDIRECTION_NB) */

struct val_ns_ni1_redirection_nb {
	Next_instance_offset
	ie_val		nb_type;				/* numbering type */
	ie_val	 	plan_id;				/* numbering plan ID */
	ie_val		pres;					/* presentation indicator */
	ie_val		screen_ind;				/* screening indicator */
	ie_val		reason;					/* reason for redirection */
	ie_array 	nb[NI1_MX_SZ_REDIRECTION_NB+2];	/* number digits */
};

		/* Signal (SIGNAL) */

struct val_ns_ni1_signal {
	Next_instance_offset
	ie_val		signal_val;				/* signal value */
};

		/* Spid (SPID) */

struct val_ns_ni1_spid {
	Next_instance_offset
	ie_array 	p_spid[NI1_MX_SZ_SPID+2];	/* spid information */
};

		/* Transit network selection (TRANSIT_NET_SEL) */

struct val_ns_ni1_transit_net_sel {
	Next_instance_offset
	ie_val		net_id_type;				/* numbering type */
	ie_val	 	net_id_plan;				/* numbering plan ID */
	ie_array	net_id[NI1_MX_SZ_TRANSIT_NET_SEL+2];	/* address of network identification information */
};

		/* Display text (DISPLAY_TXT) */

struct val_ns_ni1_display_txt {
	Next_instance_offset
	ie_array 	text[NI1_MX_SZ_DISPLAY+2];		/* display information */
};

		/* Operator system access (OS_ACCSS) */

struct val_ns_ni1_os_accss {
	Next_instance_offset
	ie_val		access_type;				/* type of access */
};

		/* Call appearance (CALL_APPEAR) */

struct val_ns_ni1_call_appear {
	Next_instance_offset
	ie_val		call_appear_id;				/* call appearance identifier */
	ie_val		call_appear_id_2;			/* call appearance identifier (continuation) */
};

		/* Protocol version control (PROT_VER_CTRL) */

struct val_ns_ni1_prot_ver_ctrl {
	Next_instance_offset
	ie_val		version;					/* version identifier */
	ie_val		issue;						/* issue code */
};

/*=========================================================================*/

							/* Information Elements (IE) */

			/*
				Isdn_ni1_store_bc : store the bearer capability information
									element :

					INPUT :
						- pr			= primitive ID,
						- trf_cap_		= information transfer capability,
						- protocol_id_1_= user information layer 1 protocol,
			*/

#define Isdn_ni1_store_bc(pr, trf_cap_, protocol_id_1_)							\
				Set_offset_ni1	(pr,	bc);										\
				Set_val_ni1		(bc,	trf_cap,		trf_cap_);					\
				Set_val_ni1		(bc,	protocol_id_1,	protocol_id_1_);			\
				Set_next_ie_ni1	(bc)

			/*------------------------------------------------------*/

			/*
				Isdn_ni1_store_bc_V110 : store the bearer capability information
										element for V110 rate adaption :

					INPUT :
						- pr			= primitive ID.
			*/

#define Isdn_ni1_store_bc_V110(pr) 												\
		Isdn_ni1_store_bc	(pr,		TRF_CAP_UNRESTRICTED,	PROTOCOL_ID_1_V110)

/*-------------------------------------------------------------------------*/

			/*
				Isdn_ni1_store_called_nb : called number IE

					INPUT :
						- pr				= primitive ID,
						- nb_type_			= numbering type,
						- plan_id_			= numbering plan,
						- nb_				= address of the literal string.
			*/

#define Isdn_ni1_store_called_nb(pr, nb_type_, plan_id_, nb_)					\
		Set_offset_ni1	(pr,		called_nb);										\
		Set_val_ni1		(called_nb,	nb_type,	nb_type_);							\
		Set_val_ni1		(called_nb,	plan_id,	plan_id_);							\
		Set_str_ni1		(called_nb,	nb,			nb_,		NI1_MX_SZ_CALLED_NB+1)

/*-------------------------------------------------------------------------*/

			/*
				Isdn_ni1_store_called_sub_add : called sub-address IE

					INPUT :
						- pr			= primitive ID,
						- sub_add_type_	= subaddress type,
						- odd_even_ind_	= odd/even indicator,
						- sub_add_		= address of the literal string.

			*/

#define Isdn_ni1_store_called_sub_add(pr, sub_add_type_, odd_even_ind_,	sub_add_) \
		Set_offset_ni1	(pr,				called_sub_add);						\
		Set_val_ni1		(called_sub_add,	sub_add_type,	sub_add_type_);			\
		Set_val_ni1		(called_sub_add,	odd_even_ind,	odd_even_ind_);			\
		Set_str_ni1 	(called_sub_add,	sub_add,		sub_add_, NI1_MX_SZ_CALLED_SUB_ADD+1)

/*-------------------------------------------------------------------------*/

			/*
				Isdn_ni1_store_calling_nb : store calling number IE

					INPUT :
						- pr 				= primitive ID,
						- nb_type_			= numbering type,
						- plan_id_			= numbering plan,
						- pres_				= presentation indicator,
						- screen_ind_		= screening indicator,
						- nb_				= pointer on the calling number literal string.
			*/

#define Isdn_ni1_store_calling_nb(pr, nb_type_, plan_id_, pres_, screen_ind_, nb_)					\
		Set_offset_ni1	(pr,			calling_nb);								\
		Set_val_ni1		(calling_nb,	nb_type,		nb_type_);					\
		Set_val_ni1		(calling_nb,	plan_id,		plan_id_);					\
		Set_val_ni1		(calling_nb,	pres,			pres_);						\
		Set_val_ni1		(calling_nb,	screen_ind, 	screen_ind_);				\
		Set_str_ni1		(calling_nb,	nb, 			nb_, NI1_MX_SZ_CALLING_NB+1)

/*-------------------------------------------------------------------------*/

			/*
				  Isdn_ni1_calling_sub_add : calling sub-address IE

						INPUT :
						- pr			= primitive ID,
						- sub_add_type_	= subaddress type,
						- odd_even_ind_	= odd/even indicator,
						- sub_add_		= address of the literal string.

			*/

#define Isdn_ni1_store_calling_sub_add(pr, sub_add_type_, odd_even_ind_, sub_add_) \
		Set_offset_ni1	(pr,				calling_sub_add);						\
		Set_val_ni1		(calling_sub_add,	sub_add_type, 	sub_add_type_);			\
		Set_val_ni1		(calling_sub_add,	odd_even_ind,	odd_even_ind_);			\
		Set_str_ni1		(calling_sub_add,	sub_add,		sub_add_, NI1_MX_SZ_CALLING_SUB_ADD+1)

/*-------------------------------------------------------------------------*/

			/*
				Isdn_ni1_store_cause : cause IE

					INPUT :
						- pr			= primitive ID,
						- coding_std_	= coding standard,
						- cause_loc_	= cause location,
						- cause_class_	= cause class,
						- cause_val_	= cause value,
						- cause_diag_	= cause diagnostic.

			*/

#define Isdn_ni1_store_cause(pr, coding_std_, cause_loc_, cause_class_, cause_val_, cause_diag_) \
		Set_offset_ni1	(pr,	cause);												\
		Set_val_ni1		(cause,	coding_std,	coding_std_);							\
		Set_val_ni1		(cause,	cause_loc,	cause_loc_);							\
		Set_val_ni1		(cause,	cause_class,cause_class_);							\
		Set_val_ni1		(cause,	cause_val,	cause_val_);							\
		Set_str_ni1		(cause,	cause_diag,	cause_diag_, NI1_MX_SZ_CAUSE_DIAG+1)

/*-------------------------------------------------------------------------*/

			/*
				 Isdn_ni1_store_chan_id_BRI : store channel ID IE

					INPUT :
						- pr 			= primitive ID,
						- pref_excl_	= preferred channel,
						- chan_sel_		= channel selection,
			*/

#define Isdn_ni1_store_chan_id_BRI(pr, pref_excl_, chan_sel_)		\
		Set_offset_ni1	(pr,		chan_id);							\
        Set_val_nil_ni1	(chan_id);										\
		Set_val_ni1		(chan_id,	int_type,	INT_TYPE_BRI);			\
		Set_val_ni1		(chan_id,	pref_excl,	pref_excl_);			\
		Set_val_ni1		(chan_id,	chan_sel,	chan_sel_);				\
		Set_next_ie_ni1	(chan_id)

			/*

				 Isdn_ni1_store_chan_id_PRI : store channel ID IE

					INPUT :
						- pr 			= primitive ID,
						- int_id_		= interface ID,
						- pref_excl_	= preferred channel,
						- d_chan_		= D-channel indicator,
						- int_id_val_	= interface ID value,
						- chan_sel_		= channel selection,
						- chan_nb_		= channel number.

			*/

#define Isdn_ni1_store_chan_id_PRI(pr, int_id_,pref_excl_, d_chan_, chan_sel_, int_id_val_,chan_nb_) \
		Set_offset_ni1	(pr,		chan_id);							\
		Set_val_ni1		(chan_id,	int_type,	INT_TYPE_PRI);			\
		Set_val_ni1		(chan_id,	int_id,		int_id_);				\
		Set_val_ni1		(chan_id,	pref_excl,	pref_excl_);			\
		Set_val_ni1		(chan_id,	d_chan,		d_chan_);				\
		Set_val_ni1		(chan_id,	int_id_val,	int_id_val_);			\
		Set_val_ni1		(chan_id,	chan_sel,	chan_sel_);				\
		Set_val_ni1		(chan_id,	chan_nb,	chan_nb_);				\
		Set_next_ie_ni1	(chan_id)

/*-------------------------------------------------------------------------*/

			/*
				 Isdn_ni1_store_endpoint_id : store endpoint identifier

					INPUT :
						- pr 		= primitive ID,
						- us_id		= user id,
						- t_id		= terminal id,
						- interp_	= interpreter flag
			*/

#define Isdn_ni1_store_endpoint_id(pr, us_id, t_id, interp_)		\
				Set_offset_ni1	(pr,			endpoint_id);			\
				Set_val_ni1		(endpoint_id,	usid,		us_id);		\
				Set_val_ni1		(endpoint_id,	tid,		t_id);		\
				Set_val_ni1		(endpoint_id,	interp,		interp_);	\
				Set_next_ie_ni1	(endpoint_id)

/*-------------------------------------------------------------------------*/

			/*
				Isdn_ni1_store_conn_nb : store connected number IE

					INPUT :
						- pr 				= primitive ID,
						- nb_type_			= numbering type,
						- plan_id_			= numbering plan,
						- pres_				= presentation indicator,
						- screen_ind_		= screening indicator,
						- nb_				= pointer on the conn number literal string.
			*/

#define Isdn_ni1_store_conn_nb(pr, nb_type_, plan_id_, pres_, screen_ind_, nb_)				\
		Set_offset_ni1	(pr,		conn_nb);								\
		Set_val_ni1		(conn_nb,	nb_type,		nb_type_);				\
		Set_val_ni1		(conn_nb,	plan_id,		plan_id_);				\
		Set_val_ni1		(conn_nb,	pres,			pres_);					\
		Set_val_ni1		(conn_nb,	screen_ind, 	screen_ind_);			\
		Set_str_ni1		(conn_nb,	nb, 			nb_, NI1_MX_SZ_CONN_NB+1)

/*-------------------------------------------------------------------------*/

			/*
				Isdn_ni1_store_feature_act : store feature activation IE

					INPUT :
						- pr 				= primitive ID,
						- feature_id_		= feature identifier,
						- feature_id_2_		= feature identifier (continuation).
			*/

#define Isdn_ni1_store_feature_act(pr, feature_id_,	feature_id_2_)	\
		Set_offset_ni1	(pr,			feature_act);					\
		Set_val_ni1		(feature_act,	feature_id,		feature_id_);	\
		Set_val_ni1		(feature_act,	feature_id_2,	feature_id_2_);	\
		Set_next_ie_ni1	(feature_act)

/*-------------------------------------------------------------------------*/

			/*
				Isdn_ni1_store_feature_ind : store feature indication IE

					INPUT :
						- pr 				= primitive ID,
						- feature_id_		= feature identifier,
						- feature_id_2_		= feature identifier (continuation)
						- status_ind_		= status indicator.
			*/

#define Isdn_ni1_store_feature_ind(pr, feature_id_,	feature_id_2_, status_ind_)	\
		Set_offset_ni1	(pr,			feature_ind);								\
		Set_val_ni1		(feature_ind,	feature_id,		feature_id_);				\
		Set_val_ni1		(feature_ind,	feature_id_2,	feature_id_2_);				\
		Set_val_ni1		(feature_ind,	status_ind,		status_ind_);				\
		Set_next_ie_ni1	(feature_ind)

/*-------------------------------------------------------------------------*/

			/*
				Isdn_ni1_store_hlc : high layer compatibility for
										non-CCITT coding standard

					INPUT :
						- pr		= primitive ID,
						- std		= coding standard,
						- tele	  = teleservice,
						- ext_tele	= extended teleservice.
			*/

#define Isdn_ni1_store_hlc(pr, std, tele, ext_tele)			\
		Set_offset_ni1	(pr,	hlc);							\
		Set_val_ni1		(hlc,	coding_std,			std);		\
		Set_val_ni1		(hlc,	teleservice,		tele);		\
		Set_val_ni1		(hlc,	ext_teleservice,	ext_tele);	\
		Set_next_ie_ni1	(hlc)

			/*
				Isdn_ni1_store_hlc_CCITT : high layer compatibility for
										CCITT coding standard

					INPUT :
						- pr		= primitive ID,
						- tele	  = teleservice,
						- ext_tele	= extended teleservice.
			*/

#define Isdn_ni1_store_hlc_CCITT(pr, tele, ext_tele)				\
		Isdn_ni1_store_hlc	(pr, CODING_STD_CCITT, tele, ext_tele)

/*-------------------------------------------------------------------------*/

			/*
				Isdn_ni1_store_information_rq : information_rq IE

					INPUT :
						- pr			= primitive ID,
						- info_type_	= information type
						- info_ind_		= information indicator
						- specific_info_= specific information

			*/

#define Isdn_ni1_store_information_rq(pr, info_type_, info_ind_, specif_)	\
		Set_offset_ni1	(pr,				information_rq);				\
		Set_val_ni1		(information_rq,	info_type,		info_type_);	\
		Set_val_ni1		(information_rq,	info_ind,		info_ind);		\
		Set_val_ni1		(information_rq,	specific_info,	specif_);		\
		Set_next_ie_ni1	(information_rq)

/*-------------------------------------------------------------------------*/

			/*
				Isdn_ni1_store_keypad : keypad IE

					INPUT :
						- pr	 = primitive ID,
						- p_text = address of the literal string.

			*/

#define Isdn_ni1_store_keypad(pr,p_text)							\
		Set_offset_ni1	(pr,		keypad);							\
		Set_str_ni1		(keypad,	text,	p_text,	NI1_MX_SZ_KEYPAD+1)

/*-------------------------------------------------------------------------*/

			/*
				Isdn_ni1_store_llc : low layer compatibility

				First definition : in one part :

					INPUT :
						- pr			= primitive ID,
						- coding_std_	= coding standard,
						- trf_cap_		= information transfer capability,
						- out_neg		= out band negotiation,
						- trf_mode_		= transfer mode,
						- trf_rate_		= information transfer rate,
						- structure_	= structure,
						- dest_rate_	= destination rate,
						- protocol_id_1_= user information layer 1 protocol,
						- syn_asyn_		= synchronous/asynchronous,
						- negotiation_	= negotiation,
						- user_rate_	= user rate,
						- inter_rate_	= intermediate rate,
						- nic_tx_		= network independent clock (NIC) on Transmission (Tx),
						- nic_rx_		= network independent clock (NIC) on Reception (Rx),
						- fc_tx_		= flow control on Transmission (Tx),
						- fc_rx_		= flow control on Reception (Rx),
						- hdr_			= rate adaption header included or not,
						- multi_frame_	= multiple frame establishement support in DL,
						- op_mode_		= mode of operation,
						- lli_negot_	= logical link identifier negotiation,
						- assign_		= assignor/assignee,
						- band_negot_	= in-band/out-band negotiation,
						- stop_bit_		= number of stop bits,
						- data_bit_		= number of data bits,
						- parity_		= number of parity bit,
						- duplex_		= duplex mode,
						- modem_		= modem type,
						- protocol_id_2_= user information layer 2 protocol,
						- option_2_		= optional layer 2 protocol information,
						- protocol_id_3_= user information layer 3 protocol,
						- option_3_		= optional layer 3 protocol information.
			*/

#define Isdn_ni1_store_llc(pr,coding_std_, trf_cap_, out_neg, trf_mode_, trf_rate_, structure_, dest_rate_,	protocol_id_1_,	syn_asyn_, negotiation_, user_rate_, inter_rate_, nic_tx_, nic_rx_, fc_tx_,	fc_rx_,	hdr_, multi_frame_,	op_mode_, lli_negot_, assign_, band_negot_, stop_bit_, data_bit_, parity_, duplex_,	modem_,	protocol_id_2_,	option_2_, protocol_id_3_, option_3_) \
		Set_offset_ni1	(pr,	llc);										\
		Set_val_ni1		(llc,	coding_std,		coding_std_);				\
		Set_val_ni1		(llc,	trf_cap,		trf_cap_);					\
		Set_val_ni1		(llc,	out_band_neg,	out_neg);					\
		Set_val_ni1		(llc,	trf_mode,		trf_mode_);					\
		Set_val_ni1		(llc,	trf_rate,		trf_rate_);					\
		Set_val_ni1		(llc,	structure,		structure_);				\
		Set_val_ni1		(llc,	dest_rate,		dest_rate_);				\
		Set_val_ni1		(llc,	protocol_id_1,	protocol_id_1_);			\
		Set_val_ni1		(llc,	syn_asyn,		syn_asyn_);					\
		Set_val_ni1		(llc,	negotiation,	negotiation_);				\
		Set_val_ni1		(llc,	user_rate,		user_rate_);				\
		Set_val_ni1		(llc,	inter_rate,		inter_rate_);				\
		Set_val_ni1		(llc,	nic_tx,			nic_tx_);					\
		Set_val_ni1		(llc,	nic_rx,			nic_rx_);					\
		Set_val_ni1		(llc,	fc_tx,			fc_tx_);					\
		Set_val_ni1		(llc,	fc_rx,			fc_rx_);					\
		Set_val_ni1		(llc,	hdr,			hdr_);						\
		Set_val_ni1		(llc,	multi_frame,	multi_frame_);				\
		Set_val_ni1		(llc,	op_mode,		op_mode_);					\
		Set_val_ni1		(llc,	lli_negot,		lli_negot_);				\
		Set_val_ni1		(llc,	assign,			assign_);					\
		Set_val_ni1		(llc,	band_negot,		band_negot_);				\
		Set_val_ni1		(llc,	stop_bit,		stop_bit_);					\
		Set_val_ni1		(llc,	data_bit,		data_bit_);					\
		Set_val_ni1		(llc,	parity,			parity_);					\
		Set_val_ni1		(llc,	duplex,			duplex_);					\
		Set_val_ni1		(llc,	modem,			modem_);					\
		Set_val_ni1		(llc,	protocol_id_2,	protocol_id_2_);			\
		Set_val_ni1		(llc,	option_2,		option_2_);					\
		Set_val_ni1		(llc,	protocol_id_3,	protocol_id_3_);			\
		Set_val_ni1		(llc,	option_3,		option_3_);					\
		Set_next_ie_ni1	(llc)

			/*--------------------------------------------*/

			/*
				Isdn_ni1_store_llc : low layer compatibility

				Second definition : DIVIDED IN 2 PARTS for compilers which
				do not accept so many parameters in macros :

					Isdn_ni1_store_llc_1_4 : octet 1 to octet 4 included,

						INPUT :
							- pr			= primitive ID,
							- coding_std_	= coding standard,
							- trf_cap_		= information transfer capability,
							- out_neg		= out band negotiation,
							- trf_mode_		= transfer mode,
							- trf_rate_		= information transfer rate,
							- structure_	= structure,
							- dest_rate_	= destination rate,

					Isdn_ni1_store_llc_5_7 : octet 5 to octet 7 included,

						- protocol_id_1_= user information layer 1 protocol,
						- syn_asyn_		= synchronous/asynchronous,
						- negotiation_	= negotiation,
						- user_rate_	= user rate,
						- inter_rate_	= intermediate rate,
						- nic_tx_		= network independent clock (NIC) on Transmission (Tx),
						- nic_rx_		= network independent clock (NIC) on Reception (Rx),
						- fc_tx_		= flow control on Transmission (Tx),
						- fc_rx_		= flow control on Reception (Rx),
						- hdr_			= rate adaption header included or not,
						- multi_frame_	= multiple frame establishement support in DL,
						- op_mode_		= mode of operation,
						- lli_negot_	= logical link identifier negotiation,
						- assign_		= assignor/assignee,
						- band_negot_	= in-band/out-band negotiation,
						- stop_bit_		= number of stop bits,
						- data_bit_		= number of data bits,
						- parity_		= number of parity bit,
						- duplex_		= duplex mode,
						- modem_		= modem type,
						- protocol_id_2_= user information layer 2 protocol,
						- option_2_		= optional layer 2 protocol information,
						- protocol_id_3_= user information layer 3 protocol,
						- option_3_		= optional layer 3 protocol information.
			*/

#define Isdn_ni1_store_llc_1_4(pr,coding_std_, trf_cap_, out_neg, trf_mode_, trf_rate_, structure_, dest_rate_)\
		Set_offset_ni1	(pr,	llc);										\
		Set_val_ni1		(llc,	coding_std,		coding_std_);				\
		Set_val_ni1		(llc,	trf_cap,		trf_cap_);					\
		Set_val_ni1		(llc,	out_band_neg,	out_neg);					\
		Set_val_ni1		(llc,	trf_mode,		trf_mode_);					\
		Set_val_ni1		(llc,	trf_rate,		trf_rate_);					\
		Set_val_ni1		(llc,	structure,		structure_);				\
		Set_val_ni1		(llc,	dest_rate,		dest_rate_)

#define Isdn_ni1_store_llc_5_7(protocol_id_1_,	syn_asyn_, negotiation_, user_rate_, inter_rate_, nic_tx_, nic_rx_, fc_tx_,	fc_rx_,	hdr_, multi_frame_,	op_mode_, lli_negot_, assign_, band_negot_, stop_bit_, data_bit_, parity_, duplex_,	modem_,	protocol_id_2_,	option_2_, protocol_id_3_, option_3_) \
		Set_val_ni1		(llc,	protocol_id_1,	protocol_id_1_);			\
		Set_val_ni1		(llc,	syn_asyn,		syn_asyn_);					\
		Set_val_ni1		(llc,	negotiation,	negotiation_);				\
		Set_val_ni1		(llc,	user_rate,		user_rate_);				\
		Set_val_ni1		(llc,	inter_rate,		inter_rate_);				\
		Set_val_ni1		(llc,	nic_tx,			nic_tx_);					\
		Set_val_ni1		(llc,	nic_rx,			nic_rx_);					\
		Set_val_ni1		(llc,	fc_tx,			fc_tx_);					\
		Set_val_ni1		(llc,	fc_rx,			fc_rx_);					\
		Set_val_ni1		(llc,	hdr,			hdr_);						\
		Set_val_ni1		(llc,	multi_frame,	multi_frame_);				\
		Set_val_ni1		(llc,	op_mode,		op_mode_);					\
		Set_val_ni1		(llc,	lli_negot,		lli_negot_);				\
		Set_val_ni1		(llc,	assign,			assign_);					\
		Set_val_ni1		(llc,	band_negot,		band_negot_);				\
		Set_val_ni1		(llc,	stop_bit,		stop_bit_);					\
		Set_val_ni1		(llc,	data_bit,		data_bit_);					\
		Set_val_ni1		(llc,	parity,			parity_);					\
		Set_val_ni1		(llc,	duplex,			duplex_);					\
		Set_val_ni1		(llc,	modem,			modem_);					\
		Set_val_ni1		(llc,	protocol_id_2,	protocol_id_2_);			\
		Set_val_ni1		(llc,	option_2,		option_2_);					\
		Set_val_ni1		(llc,	protocol_id_3,	protocol_id_3_);			\
		Set_val_ni1		(llc,	option_3,		option_3_);					\
		Set_next_ie_ni1	(llc)

			/*--------------------------------------------*/

			/*
				Isdn_ni1_store_llc_V110 : low layer compatibility for
											V110 rate adaption :

					INPUT :
						- pr			= primitive ID,
						- coding_std_	= coding standard,
						- trf_cap_		= information transfer capability,
						- out_neg		= out band negotiation,
						- trf_mode_		= transfer mode,
						- trf_rate_		= information transfer rate,
						- structure_	= structure,
						- dest_rate_	= destination rate,
						- syn_asyn_		= synchronous/asynchronous,
						- negotiation_	= negotiation,
						- user_rate_	= user rate,
						- inter_rate_	= intermediate rate,
						- nic_tx_		= network independent clock (NIC) on Transmission (Tx),
						- nic_rx_		= network independent clock (NIC) on Reception (Rx),
						- fc_tx_		= flow control on Transmission (Tx),
						- fc_rx_		= flow control on Reception (Rx),
						- stop_bit_		= number of stop bits,
						- data_bit_		= number of data bits,
						- parity_		= number of parity bit,
						- duplex_		= duplex mode,
						- modem_		= modem type,
						- protocol_id_2_= user information layer 2 protocol,
						- protocol_id_3_= user information layer 3 protocol,

			*/

#define Isdn_ni1_store_llc_V110(pr, coding_std_, trf_cap_, out_neg, trf_mode_, trf_rate_, structure_, dest_rate_, syn_asyn_, negotiation_, user_rate_, inter_rate_, nic_tx_, nic_rx_, fc_tx_, fc_rx_, stop_bit_, data_bit_, parity_, duplex_, modem_, protocol_id_2_, protocol_id_3_) \
		Isdn_ni1_store_llc_1_4(pr,			coding_std_,	trf_cap_,			\
							out_neg,		trf_mode_,		trf_rate_,			\
							structure_,		dest_rate_);						\
		Isdn_ni1_store_llc_5_7(PROTOCOL_ID_1_V110,	syn_asyn_,	negotiation_,	\
							user_rate_,		inter_rate_,	nic_tx_,			\
							nic_rx_,		fc_tx_,			fc_rx_,				\
							BYTE_VAL_NIL,	BYTE_VAL_NIL,	BYTE_VAL_NIL,		\
							BYTE_VAL_NIL,	BYTE_VAL_NIL,	BYTE_VAL_NIL,		\
							stop_bit_,		data_bit_,		parity_,			\
							duplex_,		modem_,			protocol_id_2_,		\
							BYTE_VAL_NIL,	protocol_id_3_,	BYTE_VAL_NIL)

			/*--------------------------------------------*/

			/*
				Isdn_ni1_store_llc_V120 : low layer compatibility for
											V120 rate adaption :

					INPUT :
						- pr			= primitive ID,
						- coding_std_	= coding standard,
						- trf_cap_		= information transfer capability,
						- out_neg		= out band negotiation,
						- trf_mode_		= transfer mode,
						- trf_rate_		= information transfer rate,
						- structure_	= structure,
						- dest_rate_	= destination rate,
						- syn_asyn_		= synchronous/asynchronous,
						- negotiation_	= negotiation,
						- user_rate_	= user rate,
						- hdr_			= rate adaption header included or not,
						- multi_frame_	= multiple frame establishement support in DL,
						- op_mode_		= mode of operation,
						- lli_negot_	= logical link identifier negotiation,
						- assign_		= assignor/assignee,
						- band_negot_	= in-band/out-band negotiation,
						- stop_bit_		= number of stop bits,
						- data_bit_		= number of data bits,
						- parity_		= number of parity bit,
						- duplex_		= duplex mode,
						- modem_		= modem type,
						- protocol_id_2_= user information layer 2 protocol,
						- protocol_id_3_= user information layer 3 protocol,

			*/

#define Isdn_ni1_store_llc_V120(pr, coding_std_, trf_cap_, out_neg, trf_mode_, trf_rate_, structure_, dest_rate_, syn_asyn_, negotiation_, user_rate_, hdr_, multi_frame_, op_mode_, lli_negot_, assign_, band_negot_, stop_bit_, data_bit_, parity_, duplex_, modem_, protocol_id_2_, protocol_id_3_) \
		Isdn_ni1_store_llc_1_4(pr,			coding_std_,	trf_cap_,			\
							out_neg,		trf_mode_,		trf_rate_,			\
							structure_,		dest_rate_);						\
		Isdn_ni1_store_llc_5_7(PROTOCOL_ID_1_V120,								\
							syn_asyn_,		negotiation_,	user_rate_,			\
							BYTE_VAL_NIL,	BYTE_VAL_NIL,	BYTE_VAL_NIL,		\
							BYTE_VAL_NIL,	BYTE_VAL_NIL,	hdr_,				\
							multi_frame_,	op_mode_,		lli_negot_,			\
							assign_,		band_negot_,	stop_bit_,			\
							data_bit_,		parity_,		duplex_,			\
							modem_,			protocol_id_2_, BYTE_VAL_NIL,		\
							protocol_id_3_,	BYTE_VAL_NIL)

/*-------------------------------------------------------------------------*/

			/*
				Isdn_ni1_store_notif_ind : notification indicator

					INPUT :
						- pr	= primitive ID,
						- descr	= description.

			*/

#define Isdn_ni1_store_notif_ind(pr, descr)				\
		Set_offset_ni1	(pr,		notif_ind);				\
		Set_val_ni1		(notif_ind,	notif_descr,	descr);	\
		Set_next_ie_ni1	(notif_ind)

/*-------------------------------------------------------------------------*/

			/*
				Isdn_ni1_store_o_called_nb : store connected number IE

					INPUT :
						- pr 				= primitive ID,
						- nb_type_			= numbering type,
						- plan_id_			= numbering plan,
						- pres_				= presentation indicator,
						- screen_ind_		= screening indicator,
						- reason_			= reason
						- cfnr_				= cfnr
						- redir_counter_	= redir counter
						- nb_				= pointer on the conn number literal string.
			*/

#define Isdn_ni1_store_o_called_nb(pr, nb_type_, plan_id_, pres_, screen_ind_, reason_, cfnr_, redir_counter_, nb_)\
		Set_offset_ni1	(pr,		o_called_nb);							\
		Set_val_ni1		(o_called_nb,	nb_type,		nb_type_);			\
		Set_val_ni1		(o_called_nb,	plan_id,		plan_id_);			\
		Set_val_ni1		(o_called_nb,	pres,			pres_);				\
		Set_val_ni1		(o_called_nb,	screen_ind, 	screen_ind_);		\
		Set_val_ni1		(o_called_nb,	reason, 		reason_);			\
		Set_val_ni1		(o_called_nb,	cfnr, 			cfnr_);				\
		Set_val_ni1		(o_called_nb,	redir_counter,	redir_counter_);	\
		Set_str_ni1		(o_called_nb,	nb, 			nb_, NI1_MX_SZ_CONN_NB+1)

/*-------------------------------------------------------------------------*/

			/*
				Isdn_ni1_store_progress_ind : progress indicator

					INPUT :
						- pr				= primitive ID,
						- coding_std_		= coding standard,
						- progress_loc_		= location.
						- progress_descr_	= description.

			*/

#define Isdn_ni1_store_progress_ind(pr, coding_std_, progress_loc_,progress_descr_)	\
		Set_offset_ni1	(pr,			progress_ind);						\
		Set_val_ni1		(progress_ind,	coding_std,		coding_std_);		\
		Set_val_ni1		(progress_ind,	progress_loc,	progress_loc_);		\
		Set_val_ni1		(progress_ind,	progress_descr,	progress_descr_);	\
		Set_next_ie_ni1	(progress_ind)

/*-------------------------------------------------------------------------*/

			/*
				Isdn_ni1_store_redir_nb : store connected number IE

					INPUT :
						- pr 				= primitive ID,
						- nb_type_			= numbering type,
						- plan_id_			= numbering plan,
						- pres_				= presentation indicator,
						- screen_ind_		= screening indicator,
						- reason_			= reason
						- nb_				= pointer on the conn number literal string.
			*/

#define Isdn_ni1_store_redir_nb(pr, nb_type_, plan_id_, pres_, screen_ind_, reason_, nb_)\
		Set_offset_ni1	(pr,		redir_nb);							\
		Set_val_ni1		(redir_nb,	nb_type,		nb_type_);			\
		Set_val_ni1		(redir_nb,	plan_id,		plan_id_);			\
		Set_val_ni1		(redir_nb,	pres,			pres_);				\
		Set_val_ni1		(redir_nb,	screen_ind, 	screen_ind_);		\
		Set_val_ni1		(redir_nb,	reason, 		reason_);			\
		Set_str_ni1		(redir_nb,	nb, 			nb_, NI1_MX_SZ_REDIR+1)

/*-------------------------------------------------------------------------*/

			/*
				Isdn_ni1_store_redirection_nb : store connected number IE

					INPUT :
						- pr 				= primitive ID,
						- nb_type_			= numbering type,
						- plan_id_			= numbering plan,
						- pres_				= presentation indicator,
						- screen_ind_		= screening indicator,
						- reason_			= reason
						- nb_				= pointer on the conn number literal string.
			*/

#define Isdn_ni1_store_redirection_nb(pr, nb_type_, plan_id_, pres_, screen_ind_, reason_, nb_)\
