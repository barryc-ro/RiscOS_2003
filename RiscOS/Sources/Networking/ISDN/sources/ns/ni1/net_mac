		Set_val_ni1		(redirection_nb,	plan_id,		plan_id_);			\
		Set_val_ni1		(redirection_nb,	pres,			pres_);				\
		Set_val_ni1		(redirection_nb,	screen_ind, 	screen_ind_);		\
		Set_val_ni1		(redirection_nb,	reason, 		reason_);			\
		Set_str_ni1		(redirection_nb,	nb, 			nb_, NI1_MX_SZ_REDIR+1)

/*-------------------------------------------------------------------------*/

			/*
				Isdn_ni1_store_signal : signal message

					INPUT :
						- pr			= primitive ID,
						- signal_val	= signal value.

			*/

#define Isdn_ni1_store_signal(pr, signal_val_)				\
		Set_offset_ni1	(pr,		signal);					\
		Set_val_ni1		(signal,	signal_val,	signal_val_);	\
		Set_next_ie_ni1	(signal)

/*-------------------------------------------------------------------------*/

			/*
				Isdn_ni1_store_spid

					INPUT :
						- pr		= primitive ID,
						- spid_p	= spid.

			*/

#define Isdn_ni1_store_spid(pr, spid_p)								\
		Set_offset_ni1	(pr,		spid);								\
		Set_str_ni1		(spid,		p_spid,		spid_p,	NI1_MX_SZ_SPID+1)

/*-------------------------------------------------------------------------*/

			/*
				Isdn_ni1_store_transit_net_sel : transit network selection

					INPUT :
						- pr 			= primitive ID,
						- nb_type_		= numbering type,
						- plan_id_		= numbering plan,
						- net_id_		= address of network identification.
			*/

#define Isdn_ni1_store_transit_net_sel(pr, nb_type_, nb_plan_, net_id_)								\
		Set_offset_ni1	(pr,				transit_net_sel);						\
		Set_val_ni1		(transit_net_sel,	net_id_type,	nb_type_);	\
		Set_val_ni1		(transit_net_sel,	net_id_plan,	nb_plan_);	\
		Set_str_ni1		(transit_net_sel,	net_id,			net_id_,	NI1_MX_SZ_TRANSIT_NET_SEL+1)

/*-------------------------------------------------------------------------*/

			/*
				Isdn_ni1_display_txt : transit network selection

					INPUT :
						- pr 			= primitive ID,
						- text			= text
			*/

#define Isdn_ni1_display_txt(pr, text_)								\
		Set_offset_ni1	(pr,				display_txt);						\
		Set_str_ni1		(display_txt,		text,	text_,	NI1_MX_SZ_DISPLAY+1)

/*-------------------------------------------------------------------------*/

			/*
				Isdn_ni1_store_os_accss : Operator system access (OS_ACCSS)

					INPUT :
						- pr 			= primitive ID,
						- accss_type_	= type of access
			*/

#define Isdn_ni1_store_os_accss(pr, access_type_)				\
		Set_offset_ni1	(pr,		os_accss);						\
		Set_val_ni1		(os_accss,	access_type,	access_type_);	\
		Set_next_ie_ni1	(os_accss)

/*-------------------------------------------------------------------------*/

			/*
				Isdn_ni1_store_call_appear : Call appearance (CALL_APPEAR)

					INPUT :
						- pr 				= primitive ID,
						- call_appear_id_	= call appearance identifier
						- call_appear_id_2_	= call appearance identifier (cont.)
			*/

#define Isdn_ni1_store_call_appear(pr, call_appear_id_, call_appear_id_2_)	\
		Set_offset_ni1	(pr,			call_appear);							\
		Set_val_ni1		(call_appear,	call_appear_id,		call_appear_id_);	\
		Set_val_ni1		(call_appear,	call_appear_id_2,	call_appear_id_2_);	\
		Set_next_ie_ni1	(call_appear)

/*-------------------------------------------------------------------------*/

			/*
				Isdn_ni1_store_prot_ver_ctrl : Protocol version control (PROT_VER_CTRL)

					INPUT :
						- pr 		= primitive ID,
						- version_	= version identifier
						- issue_	= issue code
			*/

#define Isdn_ni1_store_prot_ver_ctrl(pr, version_, issue_)	\
		Set_offset_ni1	(pr,			prot_ver_ctrl);			\
		Set_val_ni1		(prot_ver_ctrl,	version,	version_);	\
		Set_val_ni1		(prot_ver_ctrl,	issue,		issue_);	\
		Set_next_ie_ni1	(prot_ver_ctrl)

