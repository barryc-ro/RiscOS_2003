				/*
					MACROs and DECLARATIONS used to access the Network
					Signalling entity (NS) from the user Layer 4 Signalling
					entity (L4S).
				*/
/*-------------------------------------------------------------------------*/


			/*
				Isdn_init_ni1_ctx_???? = initialization of the common context
									 used by a primitive macro :

					INPUT :
						- pr 	 = primitive ID,
						- p_off  = pointer to the offset area,

			*/

#define Isdn_init_ni1_ctx_rq_rs(pr, p_off)	Isdn_init_ctx_rq_rs(Paste(ni1_,pr), p_off)
#define Isdn_init_ni1_ctx_in_co(p_off)		Isdn_init_ctx_in_co(p_off)

/*-------------------------------------------------------------------------*/

#define	Get_ie_ni1(pr,ie)					Get_ie 			(Paste(ni1_,pr), Paste (ni1_, ie))
#define	Isdn_get_val_ni1(pr,ie,fld)			Isdn_get_val	(Paste(ni1_,pr), Paste (ni1_, ie), fld)
#define Isdn_p_str_ni1(pr,ie,fld)			Isdn_p_str		(Paste(ni1_,pr), Paste (ni1_, ie), fld)
#define	Isdn_lgth_str_ni1(pr,ie,fld)		Isdn_lgth_str	(Paste(ni1_,pr), Paste (ni1_, ie), fld)
#define	Isdn_mx_lgth_str_ni1(pr,ie,fld)		Isdn_mx_lgth_str(Paste(ni1_,pr), Paste (ni1_, ie), fld)
#define	P_start_val_ns_ni1(pr,ie)			P_start_val_ns	(Paste(ni1_,pr), Paste (ni1_, ie))
#define	P_val_ns_ni1(ie)					P_val_ns		(Paste(ni1_,ie))
#define Set_offset_ni1(pr,ie)				Set_offset		(Paste(ni1_,pr), Paste(ni1_,ie))
#define	Set_next_ie_ni1(ie)					Set_next_ie		(Paste(ni1_,ie))
#define	Set_val_ni1(ie,fld,val)				Set_val			(Paste(ni1_,ie), fld, val)
#define Set_str_ni1(ie, lv, p_str, mx_sz)	Set_str			(Paste(ni1_,ie), lv, p_str, mx_sz)
#define Set_mem_ni1(ie, lv, p_mem, sz)		Set_mem			(Paste(ni1_,ie), lv, p_mem, sz)
#define Set_to_next_ie_instance_ni1(pr,ie)	Set_to_next_ie_instance (Paste(ni1_,pr), Paste (ni1_, ie))
#define Get_remaining_ie_instance_nb_ni1(pr,ie,p_ie)	Get_remaining_ie_instance_nb (Paste(ni1_,pr), Paste (ni1_, ie), p_ie)
#define	Set_val_nil_ni1(ie)					Set_val_nil		(Paste(ni1_,ie))

#if MESSAGE_HEADER_IE == ON
#	define Message_header_offset_ni1	ns_offset_t ni1_message_header;
#else
#	define Message_header_offset_ni1
#endif

/*-------------------------------------------------------------------------*/
/*-------------------------------------------------------------------------*/

		/* Structures of the offset area of each primitive interfaces */

		/*****************************************************
		**	 Each offset is a relative pointer on the	  	**
		**	 data of an IE element						 	**
		******************************************************/

/*====================	CALL  ESTABLISHMENT  MESSAGES	=====================*/

		/* NS_ALERT_RQ */

struct off_ns_ni1_te_alert_rq {
	Message_header_offset_ni1				/* message header parameters */
	ns_offset_t ni1_chan_id;				/* channel ID */
	ns_offset_t ni1_progress_ind;			/* progress indicator */
};

typedef struct off_ns_ni1_te_alert_rq		off_ns_ni1_alert_rq;
typedef struct off_ns_ni1_te_alert_rq		off_ns_ni1_net_alert_in;

		/* NS_ALERT_IN */

struct off_ns_ni1_te_alert_in {
	Message_header_offset_ni1				/* message header parameters */
	ns_offset_t ni1_chan_id;				/* channel ID */
	ns_offset_t ni1_progress_ind;			/* progress indicator */
	ns_offset_t ni1_signal;					/* signal */
	ns_offset_t ni1_redir_nb;				/* redirecting number */
	ns_offset_t ni1_redirection_nb;			/* redirection number */
	ns_offset_t ni1_display_txt;			/* display text */
};

typedef struct off_ns_ni1_te_alert_in		off_ns_ni1_net_alert_rq;
typedef struct off_ns_ni1_te_alert_in		off_ns_ni1_alert_in;

		/* NS_CALL_PROC_RQ */

struct off_ns_ni1_te_call_proc_rq {
	Message_header_offset_ni1				/* message header parameters */
	ns_offset_t ni1_chan_id;				/* channel ID */
	ns_offset_t ni1_progress_ind;			/* progress indicator */
};

typedef struct off_ns_ni1_te_call_proc_rq	off_ns_ni1_call_proc_rq;
typedef struct off_ns_ni1_te_call_proc_rq	off_ns_ni1_net_call_proc_in;

		/* NS_CALL_PROC_IN */

struct off_ns_ni1_te_call_proc_in {
	Message_header_offset_ni1				/* message header parameters */
	ns_offset_t ni1_chan_id;				/* channel ID */
	ns_offset_t ni1_progress_ind;			/* progress indicator */
	ns_offset_t ni1_notif_ind;				/* notify indicator */
	ns_offset_t ni1_information_rq;			/* information request */
	ns_offset_t ni1_signal;					/* signal */
	ns_offset_t ni1_feature_ind;			/* feature indication */
	ns_offset_t ni1_called_nb;				/* called number */
	ns_offset_t ni1_called_sub_add;			/* called sub address */
	ns_offset_t ni1_display_txt;			/* display text */
};

typedef struct off_ns_ni1_te_call_proc_in	off_ns_ni1_net_call_proc_rq;
typedef struct off_ns_ni1_te_call_proc_in	off_ns_ni1_call_proc_in;

		/* NS_CONN_RQ */

struct off_ns_ni1_te_conn_rq {
	Message_header_offset_ni1				/* message header parameters */
	ns_offset_t ni1_bc;						/* bearer capability */
	ns_offset_t ni1_chan_id;				/* channel ID */
	ns_offset_t ni1_progress_ind;			/* progress indicator */
	ns_offset_t ni1_keypad;					/* keypad */
	ns_offset_t ni1_feature_act;			/* feature activation */
	ns_offset_t ni1_calling_nb;				/* calling number */
	ns_offset_t ni1_calling_sub_add;		/* calling sub-address */
	ns_offset_t ni1_called_nb;				/* called number */
	ns_offset_t ni1_called_sub_add;			/* called sub-address */
	ns_offset_t ni1_o_called_nb;			/* original called number */
	ns_offset_t ni1_transit_net_sel;		/* transit network selection */
	ns_offset_t ni1_llc;					/* low layer compatibility */
	ns_offset_t ni1_hlc;					/* high layer compability */
	ns_offset_t ni1_os_accss;				/* operator system access */
	ns_offset_t ni1_call_appear;			/* call appearance */
};

typedef struct off_ns_ni1_te_conn_rq		off_ns_ni1_conn_rq;
typedef struct off_ns_ni1_te_conn_rq		off_ns_ni1_net_conn_in;

		/* NS_CONN_IN */

struct off_ns_ni1_te_conn_in {
	Message_header_offset_ni1				/* message header parameters */
	ns_offset_t ni1_bc;						/* bearer capability */
	ns_offset_t ni1_chan_id;				/* channel ID */
	ns_offset_t ni1_progress_ind;			/* progress indicator */
	ns_offset_t ni1_signal;					/* signal */
	ns_offset_t ni1_endpoint_id;			/* endpoint identifier */
	ns_offset_t ni1_calling_nb;				/* calling number (from network) */
	ns_offset_t ni1_calling_sub_add;		/* calling subaddress */
	ns_offset_t ni1_called_nb;				/* called number */
	ns_offset_t ni1_called_sub_add;			/* called subaddress */
	ns_offset_t ni1_o_called_nb;			/* original called number */
	ns_offset_t ni1_redir_nb;				/* redirecting number */
	ns_offset_t ni1_llc;					/* low layer capability */
	ns_offset_t ni1_hlc;					/* high layer compatibility */
	ns_offset_t ni1_display_txt;			/* display text */
	ns_offset_t ni1_call_appear;			/* call appearance */
};

typedef struct off_ns_ni1_te_conn_in		off_ns_ni1_net_conn_rq;
typedef struct off_ns_ni1_te_conn_in		off_ns_ni1_conn_in;

		/* NS_CONN_RS (CONN) */

struct off_ns_ni1_te_conn_rs {
	Message_header_offset_ni1				/* message header parameters */
	ns_offset_t ni1_chan_id;				/* channel ID included for support of Annex D */
};

typedef struct off_ns_ni1_te_conn_rs		off_ns_ni1_conn_rs;
typedef struct off_ns_ni1_te_conn_rs		off_ns_ni1_net_conn_co;

		/* NS_CONN_CO (CONN or CONN_ACK) */

struct off_ns_ni1_te_conn_co {
	Message_header_offset_ni1				/* message header parameters */
	ns_offset_t ni1_conn_nb;				/* connected number */
	ns_offset_t ni1_chan_id;				/* channel ID */
	ns_offset_t ni1_progress_ind;			/* progress indicator */
	ns_offset_t ni1_notif_ind;				/* notify indicator */
	ns_offset_t ni1_signal;					/* signal */
	ns_offset_t ni1_display_txt;			/* display text */
};

typedef struct off_ns_ni1_te_conn_co		off_ns_ni1_net_conn_rs;
typedef struct off_ns_ni1_te_conn_co		off_ns_ni1_conn_co;

		/* NS_PROGRESS_RQ */

struct off_ns_ni1_te_progress_rq {
	Message_header_offset_ni1				/* message header parameters */
	ns_offset_t ni1_cause;					/* cause */
	ns_offset_t ni1_progress_ind;			/* progress indicator */
};

typedef struct off_ns_ni1_te_progress_rq	off_ns_ni1_net_progress_in;
typedef struct off_ns_ni1_te_progress_rq	off_ns_ni1_progress_rq;


		/* NS_PROGRESS_IN */

struct off_ns_ni1_te_progress_in {
	Message_header_offset_ni1				/* message header parameters */
	ns_offset_t ni1_cause;					/* cause */
	ns_offset_t ni1_progress_ind;			/* progress indicator */
	ns_offset_t ni1_notif_ind;				/* notify indicator */
	ns_offset_t ni1_signal;					/* signal */
};

typedef struct off_ns_ni1_te_progress_in	off_ns_ni1_net_progress_rq;
typedef struct off_ns_ni1_te_progress_in	off_ns_ni1_progress_in;

		/* NS_SETUP_ACK_IN */

struct off_ns_ni1_te_setup_ack_in {
	Message_header_offset_ni1				/* message header parameters */
	ns_offset_t ni1_chan_id;				/* channel ID */
	ns_offset_t ni1_progress_ind;			/* progress indicator */
	ns_offset_t ni1_notif_ind;				/* notify indicator */
	ns_offset_t ni1_information_rq;			/* information request */
	ns_offset_t ni1_signal;					/* signal */
	ns_offset_t ni1_feature_ind;			/* feature indication */
};

typedef struct off_ns_ni1_te_setup_ack_in	off_ns_ni1_net_setup_ack_rq;
typedef struct off_ns_ni1_te_setup_ack_in	off_ns_ni1_setup_ack_in;

/*===============	CALL  INFORMATION  PHASE  MESSAGES	====================*/

		/* NS_RET_RQ */

struct off_ns_ni1_te_ret_rq {
