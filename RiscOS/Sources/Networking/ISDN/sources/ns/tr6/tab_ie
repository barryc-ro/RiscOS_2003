	Ns_tr6_tabx_list_entry (C_V_101_TR6_S)
	Ns_tr6_tabx_list_entry (C_V_111_TR6_S)
Ns_tr6_end_table_or_function /* } */

Ns_tr6_table_or_function (tabx_list, TAB_CHAN_SEL_TR6_S)	/* { */
	Ns_tr6_tabx_list_entry (CHAN_SEL_BRI_TR6_S)
	Ns_tr6_tabx_list_entry (CHAN_SEL_PRI_TR6_S)
Ns_tr6_end_table_or_function /* } */

