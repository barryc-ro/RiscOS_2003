		/*
			Table of table items - the items are ordered according to the
			table ID number : table ID = index of the table.
		*/

Ns_tr6_table_or_function (tabl, TR6_TAB_TABL)	/* { */
	Ns_tr6_tab_tabl_entry (T_NIL,							P_NIL)
	Ns_tr6_tab_tabl_entry (TR6_T_C_C,                		C_C_TR6_S)
	Ns_tr6_tab_tabl_entry (TR6_T_CHAN_PREF_EXCL,     		CHAN_PREF_EXCL_TR6_S)
	Ns_tr6_tab_tabl_entry (TR6_T_INT_TYPE,           		INT_TYPE_TR6_S)
	Ns_tr6_tab_tabl_entry (TR6_T_CHAN_NUMBER,         		CHAN_NUMBER_TR6_S)
	Ns_tr6_tab_tabl_entry (TR6_T_SPF_FAC_CODE,	        	SPF_FAC_CODE_TR6_S)
	Ns_tr6_tab_tabl_entry (TR6_T_LOCATION,            		LOCATION_TR6_S)
	Ns_tr6_tab_tabl_entry (TR6_T_CONN_NB_TYPE,        		CONN_NB_TYPE_TR6_S)
	Ns_tr6_tab_tabl_entry (TR6_T_CONN_NB_PLAN,        		CONN_NB_PLAN_TR6_S)
	Ns_tr6_tab_tabl_entry (TR6_T_CONGESTION_TYPE,     		CONGESTION_TYPE_TR6_S)
	Ns_tr6_tab_tabl_entry (TR6_T_SPF_FAC_B_CHAN,		    SPF_FAC_B_CHAN_TR6_S)
	Ns_tr6_tab_tabl_entry (TR6_T_SERVICE_IND,         		SERVICE_IND_TR6_S)
	Ns_tr6_tab_tabl_entry (TR6_T_FAC_STAT_INFO,  		    FAC_STAT_INFO_TR6_S)
	Ns_tr6_tab_tabl_entry (TR6_T_CALLED_STATUS, 		    CALLED_STATUS_TR6_S)
	Ns_tr6_tab_tabl_entry (TR6_T_RQ_IN_FLAG,          		RQ_IN_FLAG_TR6_S)
	Ns_tr6_tab_tabl_entry (TR6_T_ADD_ATTRIB,          		ADD_ATTRIB_TR6_S)
	Ns_tr6_tab_tabl_entry (TR6_T_ADD_INFO_SYN_ASYN,			ADD_INFO_SYN_ASYN_TR6_S)
	Ns_tr6_tab_tabl_entry (TR6_T_ADD_INFO_PROTOCOL_ID_1,	ADD_INFO_PROTOCOL_ID_1_TR6_S)
	Ns_tr6_tab_tabl_entry (TR6_T_ADD_INFO_STOP_BIT,			ADD_INFO_STOP_BIT_TR6_S)
	Ns_tr6_tab_tabl_entry (TR6_T_ADD_INFO_DATA_BIT,			ADD_INFO_DATA_BIT_TR6_S)
	Ns_tr6_tab_tabl_entry (TR6_T_ADD_INFO_PARITY,			ADD_INFO_PARITY_TR6_S)
	Ns_tr6_tab_tabl_entry (TR6_T_ADD_INFO_USER_RATE_SYN,	ADD_INFO_USER_RATE_SYN_TR6_S)
	Ns_tr6_tab_tabl_entry (TR6_T_ADD_INFO_USER_RATE_ASYN,	ADD_INFO_USER_RATE_ASYN_TR6_S)
	Ns_tr6_tab_tabl_entry (TR6_T_DIGIT,               		DIGIT_TR6_S)
	Ns_tr6_tab_tabl_entry (TR6_T_CONGEST_LEVEL,				CONGEST_LEVEL_TR6_S)
Ns_tr6_end_table_or_function /* } */

/*=========================================================================*/

		/* Table of list of tables */

