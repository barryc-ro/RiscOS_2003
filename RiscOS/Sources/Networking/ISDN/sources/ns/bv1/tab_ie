	Ns_bv1_tab_tabx_entry (BV1_TX_TRF_RATE,			BV1_TAB_TRF_RATE_S)
Ns_bv1_end_table_or_function /* } */

/*-------------------------------------------------------------------------*

IE description structures :
-------------------------

					ie		  tab_ie
	===============================================================
	| codeset | id | p_prog | size | p_struc | p_init| repetition |
	|---------|----|--------|------|---------|-------|------------|  meta-prog
	|		  |	   |  X----------------------------------------------> START
	|---------|----|--------|------|---------|-------|------------|	   XXXXX
	|		  |	   |		|	   |		 |	     |			  |	   YYYYY
	|---------|----|--------|------|---------|-------|------------|	END
	|		  |	   |		|	   |		 |	     |			  |
	|---------|----|--------|------|---------|-------|------------|
	|		  |	   |		|  X   |   X------------------------------> i_xxxx
	|---------|----|--------|--|---|---------|-------|------------|		 Œ internal
	|		  |	   |		|  ---------------------------------------------| data
	|---------|----|--------|------|---------|-------|------------|		 v structure
	|		  |	   |		|	   |		 |	     |			  |
	|---------|----|--------|------|---------|-------|------------|
	|		  |	   |		|	   |		 |   X--------------------> df_spdu_bv1_i_xxxx
	|---------|----|--------|------|---------|-------|------------|	   Œ implicit   and df_ssdu_bv1_i_xxx
	|		  |	   |		|	   |		 |	     |			  |	   | value or
	|---------|----|--------|------|---------|-------|------------|	   v  P_NIL


		 Information Element (IE) table of item :
					- codeset,
					- ie identifier;
					- program sequence address,
					- internal structure size,
					- internal structure address,
					- initialization structure address,
					- maximum number of repeated IE.

*/

Ns_bv1_table_or_function (ie, BV1_TAB_IE)	/* { */

Ns_bv1_tab_ie_entry (CODESET_SO,CONGEST_LEVEL,		&BV1_CONGEST_LEVEL_P,		i_congest_level,	&df_spdu_bv1_i_congest_level,	&df_ssdu_bv1_i_congest_level,	BV1_MX_CONGEST_LEVEL)

Ns_bv1_tab_ie_entry (CODESET_0,BC,					&BV1_BC_P,					i_bc,				&df_spdu_bv1_i_bc,				&df_ssdu_bv1_i_bc,				BV1_MX_BC)
Ns_bv1_tab_ie_entry (CODESET_0,CAUSE,				&BV1_CAUSE_P,				i_cause,			&df_spdu_bv1_i_cause,			&df_ssdu_bv1_i_cause,			BV1_MX_CAUSE)
Ns_bv1_tab_ie_entry (CODESET_0,CALL_ID,				&BV1_CALL_ID_P,				i_call_id,			&df_spdu_bv1_i_call_id,			&df_ssdu_bv1_i_call_id,			BV1_MX_CALL_ID)
Ns_bv1_tab_ie_entry (CODESET_0,CALL_STATE,			&BV1_CALL_STATE_P,			i_call_state,		&df_spdu_bv1_i_call_state,		&df_ssdu_bv1_i_call_state,		BV1_MX_CALL_STATE)
Ns_bv1_tab_ie_entry (CODESET_0,CHAN_ID,				&BV1_CHAN_ID_P,				i_chan_id,			&df_spdu_bv1_i_chan_id,			&df_ssdu_bv1_i_chan_id,			BV1_MX_CHAN_ID)
Ns_bv1_tab_ie_entry (CODESET_0,PROGRESS_IND,		&BV1_PROGRESS_IND_P,		i_progress_ind,		&df_spdu_bv1_i_progress_ind,	&df_ssdu_bv1_i_progress_ind,	BV1_MX_PROGRESS_IND)

Ns_bv1_tab_ie_entry (CODESET_0,NOTIF_IND,			&BV1_NOTIF_IND_P,			i_notif_ind,		&df_spdu_bv1_i_notif_ind,		&df_ssdu_bv1_i_notif_ind,		BV1_MX_NOTIF_IND)
Ns_bv1_tab_ie_entry (CODESET_0,DISPLAY,				&BV1_DISPLAY_P,				i_display,			&df_spdu_bv1_i_display,			&df_ssdu_bv1_i_display,			BV1_MX_DISPLAY)
Ns_bv1_tab_ie_entry (CODESET_0,KEYPAD,				&BV1_KEYPAD_P,				i_keypad,			&df_spdu_bv1_i_keypad,			&df_ssdu_bv1_i_keypad,			BV1_MX_KEYPAD)

Ns_bv1_tab_ie_entry (CODESET_0,CALLING_NB,			&BV1_CALLING_NB_P,			i_calling_nb,		&df_spdu_bv1_i_calling_nb,		&df_ssdu_bv1_i_calling_nb,		BV1_MX_CALLING_NB)
Ns_bv1_tab_ie_entry (CODESET_0,CALLING_SUB_ADD,		&BV1_CALLING_SUB_ADD_P,		i_calling_sub_add,	&df_spdu_bv1_i_calling_sub_add,	&df_ssdu_bv1_i_calling_sub_add,	BV1_MX_CALLING_SUB_ADD)
Ns_bv1_tab_ie_entry (CODESET_0,CALLED_NB,			&BV1_CALLED_NB_P,			i_called_nb,		&df_spdu_bv1_i_called_nb,		&df_ssdu_bv1_i_called_nb,		BV1_MX_CALLED_NB)
Ns_bv1_tab_ie_entry (CODESET_0,CALLED_SUB_ADD,		&BV1_CALLED_SUB_ADD_P,		i_called_sub_add,	&df_spdu_bv1_i_called_sub_add,	&df_ssdu_bv1_i_called_sub_add,	BV1_MX_CALLED_SUB_ADD)

Ns_bv1_tab_ie_entry (CODESET_0,RESTART_IND,			&BV1_RESTART_IND_P,			i_restart_ind,		&df_spdu_bv1_i_restart_ind,		&df_ssdu_bv1_i_restart_ind,		BV1_MX_RESTART_IND)
Ns_bv1_tab_ie_entry (CODESET_0,LLC,					&BV1_LLC_P,					i_llc,				&df_spdu_bv1_i_llc,				&df_ssdu_bv1_i_llc,				BV1_MX_LLC)
Ns_bv1_tab_ie_entry (CODESET_0,HLC,					&BV1_HLC_P,					i_hlc,				&df_spdu_bv1_i_hlc,				&df_ssdu_bv1_i_hlc,				BV1_MX_HLC)
Ns_bv1_tab_ie_entry (CODESET_0,UUI,					&BV1_UUI_P,					i_uui,				&df_spdu_bv1_i_uui,				&df_ssdu_bv1_i_uui,				BV1_MX_UUI)

Ns_bv1_tab_ie_entry (CODESET_5,CONN_NB,				&BV1_CONNECTED_NB_P,		i_conn_nb,			&df_spdu_bv1_i_conn_nb,			&df_ssdu_bv1_i_conn_nb,			BV1_MX_CONN_NB)
Ns_bv1_tab_ie_entry (CODESET_5,CONN_SUB_ADD,		&BV1_CONNECTED_SUB_ADD_P,	i_conn_sub_add,		&df_spdu_bv1_i_conn_sub_add,	&df_ssdu_bv1_i_conn_sub_add,	BV1_MX_CONN_SUB_ADD)
Ns_bv1_tab_ie_entry (CODESET_5,ADVICE_CHARGE,		&BV1_ADVICE_CHARGE_P,		i_advice_charge,	&df_spdu_bv1_i_advice_charge,	&df_ssdu_bv1_i_advice_charge,	BV1_MX_ADVICE_CHARGE)

Ns_bv1_tab_ie_entry (CODESET_6,REDIR_NB,			&BV1_REDIR_NB_P,			i_redir_nb,			&df_spdu_bv1_i_redir_nb,		&df_ssdu_bv1_i_redir_nb,		BV1_MX_REDIR_NB)

#if MESSAGE_HEADER_IE == ON
		Ns_bv1_tab_ie_msg_header
#endif

Ns_bv1_end_table_or_function /* } */

