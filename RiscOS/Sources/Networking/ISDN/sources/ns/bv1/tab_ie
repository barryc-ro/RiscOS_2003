Ns_bv1_table_or_function (tabx, BV1_TAB_TABX)	/* { */
	Ns_bv1_tab_tabx_entry (T_NIL,					P_NIL)
	Ns_bv1_tab_tabx_entry (BV1_TX_C_V_CCITT,		BV1_TAB_C_V_CCITT_S)
	Ns_bv1_tab_tabx_entry (BV1_TX_C_V_NATIONAL,		BV1_TAB_C_V_NATIONAL_S)
