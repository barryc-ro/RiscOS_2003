					/* Assignor/Assignee (ie LLC) */

CONST	uchar HKT_ASSIGN_S[] = {
	DEFAULT_ASSIGNEE,
	ASSIGNOR_ONLY,
	};

					/* Cause class (ie CAUSE) */

CONST	uchar HKT_C_C_S[]={
	C_C_NORMAL_EVENT_0,
