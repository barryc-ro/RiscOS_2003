	ie_val 		chan_sel;				/* information channel selection */
	ie_val		coding_std;				/* coding standard */
	ie_val		chan_type;				/* channel type */
	ie_array	chan_nb[ETS_MX_SZ_CHAN_ID+2];	/* channel number */
};

