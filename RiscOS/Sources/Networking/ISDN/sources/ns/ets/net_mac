#if NS_IE_TSP_OUT == ON

#	define	Isdn_ets_store_advice_charge_tsp(pr, p_data_)				\
		Set_ns_offset_tsp_ets	(pr, advice_charge);						\
		Set_mem_eob_ets	(advice_charge, tsp_data, p_data_,(p_data_[0] > ETS_MX_SZ_ADVICE_CHARGE_TSP) ? 1 + ETS_MX_SZ_ADVICE_CHARGE_TSP : 1 + p_data_[0]);\
		Set_next_ie_ets	(advice_charge)

#	define	Isdn_ets_store_bc_tsp(pr, p_data_)				\
		Set_ns_offset_tsp_ets	(pr, bc);						\
		Set_mem_eob_ets	(bc, tsp_data, p_data_,(p_data_[0] > ETS_MX_SZ_BC_TSP) ? 1 + ETS_MX_SZ_BC_TSP : 1 + p_data_[0]);\
		Set_next_ie_ets	(bc)

#	define	Isdn_ets_store_call_id_tsp(pr, p_data_)				\
		Set_ns_offset_tsp_ets	(pr, call_id);						\
		Set_mem_eob_ets	(call_id, tsp_data, p_data_,(p_data_[0] > ETS_MX_SZ_CALL_ID_TSP) ? 1 + ETS_MX_SZ_CALL_ID_TSP : 1 + p_data_[0]);\
		Set_next_ie_ets	(call_id)

#	define	Isdn_ets_store_called_nb_tsp(pr, p_data_)				\
		Set_ns_offset_tsp_ets	(pr, called_nb);						\
		Set_mem_eob_ets	(called_nb, tsp_data, p_data_,(p_data_[0] > ETS_MX_SZ_CALLED_NB_TSP) ? 1 + ETS_MX_SZ_CALLED_NB_TSP : 1 + p_data_[0]);\
		Set_next_ie_ets	(called_nb)

#	define	Isdn_ets_store_calling_nb_tsp(pr, p_data_)				\
		Set_ns_offset_tsp_ets	(pr, calling_nb);						\
		Set_mem_eob_ets	(calling_nb, tsp_data, p_data_,(p_data_[0] > ETS_MX_SZ_CALLING_NB_TSP) ? 1 + ETS_MX_SZ_CALLING_NB_TSP : 1 + p_data_[0]);\
		Set_next_ie_ets	(calling_nb)

#	define	Isdn_ets_store_called_sub_add_tsp(pr, p_data_)				\
		Set_ns_offset_tsp_ets	(pr, called_sub_add);						\
		Set_mem_eob_ets	(called_sub_add, tsp_data, p_data_,(p_data_[0] > ETS_MX_SZ_CALLED_SUB_ADD_TSP) ? 1 + ETS_MX_SZ_CALLED_SUB_ADD_TSP : 1 + p_data_[0]);\
		Set_next_ie_ets	(called_sub_add)

#	define	Isdn_ets_store_calling_sub_add_tsp(pr, p_data_)				\
		Set_ns_offset_tsp_ets	(pr, calling_sub_add);						\
		Set_mem_eob_ets	(calling_sub_add, tsp_data, p_data_,(p_data_[0] > ETS_MX_SZ_CALLING_SUB_ADD_TSP) ? 1 + ETS_MX_SZ_CALLING_SUB_ADD_TSP : 1 + p_data_[0]);\
		Set_next_ie_ets	(calling_sub_add)

#	define	Isdn_ets_store_cause_tsp(pr, p_data_)				\
		Set_ns_offset_tsp_ets	(pr, cause);						\
		Set_mem_eob_ets	(cause, tsp_data, p_data_,(p_data_[0] > ETS_MX_SZ_CAUSE_TSP) ? 1 + ETS_MX_SZ_CAUSE_TSP : 1 + p_data_[0]);\
		Set_next_ie_ets	(cause)

#	define	Isdn_ets_store_chan_id_tsp(pr, p_data_)				\
		Set_ns_offset_tsp_ets	(pr, chan_id);						\
		Set_mem_eob_ets	(chan_id, tsp_data, p_data_,(p_data_[0] > ETS_MX_SZ_CHAN_ID_TSP) ? 1 + ETS_MX_SZ_CHAN_ID_TSP : 1 + p_data_[0]);\
		Set_next_ie_ets	(chan_id)

#	define	Isdn_ets_store_congest_level_tsp(pr, p_data_)				\
		Set_ns_offset_tsp_ets	(pr, congest_level);						\
		Set_mem_eob_ets	(congest_level, tsp_data, p_data_,(p_data_[0] > ETS_MX_SZ_CONGEST_LEVEL_TSP) ? 1 + ETS_MX_SZ_CONGEST_LEVEL_TSP : 1 + p_data_[0]);\
		Set_next_ie_ets	(congest_level)

