	ushort					component_nb;	/* number of components in IE */
    ushort					size;			/* total size of all components in IE */
	struct ns_ss_fac_comp	component;		/* first component : data common to all facility components */
		/*	Here follows data specific to each component as described in ns_ssint.h.
			It starts at location indicated by "data_offset" in component structure */
};

#endif

		/* High Layer Compatibility (HLC) */

struct val_ns_ets_hlc {
