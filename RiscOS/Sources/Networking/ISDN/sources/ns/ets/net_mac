/*=========================================================================*/

							/* Information Elements (IE) */

			/*
			   Isdn_ets_store_advice_charge : store the advice of charge information
								   element :
			   Isdn_ets_store_advice_charg_5 : store the advice of charge information
								   element in codeset 5 :

					INPUT :
						- pr				= primitive ID,
						- coding_std		= coding standard
						- cept_std			= CEPT standard
						- charge_type		= indication type
						- charge_period		= charging period
						- charge_period_m	= charging period multiplier
						- value[ETS_MX_SZ_ADVICE_CHARGE+1];		= value

			*/

#define Isdn_ets_store_advice_charge(pr, coding, cept, type, p ,p_m, val, sz)	\
				Set_offset_ets	(pr,	advice_charge);							\
				Set_val_ets		(advice_charge,	coding_std,		coding);		\
				Set_val_ets		(advice_charge,	cept_std,		cept);			\
				Set_val_ets		(advice_charge,	charge_type,	type);			\
				Set_val_ets		(advice_charge,	charge_period,	p);				\
				Set_val_ets		(advice_charge,	charge_period_m,p_m);			\
				Set_mem_ets		(advice_charge,	value, val, sz)

#define Isdn_ets_store_advice_charg_5(pr, coding, cept, type, p ,p_m, val, sz)	\
				Set_offset_ets	(pr,	advice_charge_5);						\
				Set_val_ets		(advice_charge_5,	coding_std,		coding);	\
				Set_val_ets		(advice_charge_5,	cept_std,		cept);		\
				Set_val_ets		(advice_charge_5,	charge_type,	type);		\
				Set_val_ets		(advice_charge_5,	charge_period,	p);			\
				Set_val_ets		(advice_charge_5,	charge_period_m,p_m);		\
				Set_mem_ets		(advice_charge_5,	value, val, sz)

			/*
			   Isdn_ets_store_bc : store the bearer capability information
								   element :

					INPUT :
						- pr			= primitive ID,
						- trf_cap_		= information transfer capability,
						- trf_mode_		= transfer mode,
						- trf_rate_		= information transfer rate,
						- structure_	= structure,
						- dest_rate_	= destination rate,
						- protocol_id_1_= user information layer 1 protocol,
						- syn_asyn_		= synchronous/asynchronous,
						- negotiation_	= negotiation,
						- user_rate_	= user rate,
						- inter_rate_	= intermediate rate,
						- nic_tx_		= network independent clock (NIC) on Transmission (Tx),
						- nic_rx_		= network independent clock (NIC) on Reception (Rx),
						- fc_tx_		= flow control on Transmission (Tx),
						- fc_rx_		= flow control on Reception (Rx),
						- stop_bit_		= number of stop bits,
						- data_bit_		= number of data bits,
						- parity_		= number of parity bit,
						- duplex_		= duplex mode,
						- modem_		= modem type,
						- protocol_id_2_= user information layer 2 protocol,
						- protocol_id_3_= user information layer 3 protocol,

			*/

#define Isdn_ets_store_bc(pr, trf_cap_, trf_mode_, trf_rate_, structure_, dest_rate_, protocol_id_1_, syn_asyn_, negotiation_, user_rate_, inter_rate_, nic_tx_, nic_rx_, fc_tx_, fc_rx_, stop_bit_, data_bit_, parity_, duplex_, modem_, protocol_id_2_, protocol_id_3_) \
				Set_offset_ets	(pr,	bc);										\
				Set_val_ets		(bc,	coding_std,		CODING_STD_CCITT);			\
				Set_val_ets		(bc,	trf_cap,		trf_cap_);					\
				Set_val_ets		(bc,	trf_mode,		trf_mode_);					\
				Set_val_ets		(bc,	trf_rate,		trf_rate_);					\
				Set_val_ets		(bc,	structure,		structure_);				\
				Set_val_ets		(bc,	dest_rate,		dest_rate_);				\
				Set_val_ets		(bc,	protocol_id_1,	protocol_id_1_);			\
				Set_val_ets		(bc,	syn_asyn,		syn_asyn_);					\
				Set_val_ets		(bc,	negotiation,	negotiation_);				\
				Set_val_ets		(bc,	user_rate,		user_rate_);				\
				Set_val_ets		(bc,	inter_rate,		inter_rate_);				\
				Set_val_ets		(bc,	nic_tx,			nic_tx_);					\
				Set_val_ets		(bc,	nic_rx,			nic_rx_);					\
				Set_val_ets		(bc,	fc_tx,			fc_tx_);					\
				Set_val_ets		(bc,	fc_rx,			fc_rx_);					\
				Set_val_ets		(bc,	stop_bit,		stop_bit_);					\
				Set_val_ets		(bc,	data_bit,		data_bit_);					\
				Set_val_ets		(bc,	parity,			parity_);					\
				Set_val_ets		(bc,	duplex,			duplex_);					\
				Set_val_ets		(bc,	modem,			modem_);					\
				Set_val_ets		(bc,	protocol_id_2,	protocol_id_2_);			\
				Set_val_ets		(bc,	protocol_id_3,	protocol_id_3_);			\
				Set_next_ie_ets	(bc)

/*-------------------------------------------------------------------------*/

			/*
			   Isdn_ets_store_bc_V110 : store the bearer capability information
										element for V110 rate adaption :

					INPUT :
						- pr			= primitive ID,
						- trf_cap_		= information transfer capability,
						- trf_mode_		= transfer mode,
						- trf_rate_		= information transfer rate,
						- structure_	= structure,
						- dest_rate_	= destination rate,
						- syn_asyn_		= synchronous/asynchronous,
						- negotiation_	= negotiation,
						- user_rate_	= user rate,
						- inter_rate_	= intermediate rate,
						- nic_tx_		= network independent clock (NIC) on Transmission (Tx),
						- nic_rx_		= network independent clock (NIC) on Reception (Rx),
						- fc_tx_		= flow control on Transmission (Tx),
						- fc_rx_		= flow control on Reception (Rx),
						- stop_bit_		= number of stop bits,
						- data_bit_		= number of data bits,
						- parity_		= number of parity bit,
						- duplex_		= duplex mode,
						- modem_		= modem type,
						- protocol_id_2_= user information layer 2 protocol,
						- protocol_id_3_= user information layer 3 protocol,

			*/

#define Isdn_ets_store_bc_V110(pr, trf_cap_, trf_mode_,	trf_rate_, structure_, dest_rate_, syn_asyn_, negotiation_,	user_rate_,	inter_rate_, nic_tx_, nic_rx_, fc_tx_, fc_rx_, stop_bit_, data_bit_, parity_, duplex_, modem_, protocol_id_2_, protocol_id_3_) \
		Isdn_ets_store_bc	(pr,			trf_cap_,		trf_mode_,			\
							trf_rate_,		structure_,		dest_rate_,			\
							PROTOCOL_ID_1_V110,syn_asyn_,	negotiation_,		\
							user_rate_,		inter_rate_,	nic_tx_,			\
							nic_rx_,		fc_tx_,			fc_rx_,				\
							stop_bit_,		data_bit_,		parity_,			\
							duplex_,		modem_,			protocol_id_2_,		\
							protocol_id_3_)

			/*
			   Isdn_ets_store_bc_not_V110 : store the bearer capability information
											element for non V110 rate adaption :

					INPUT :
						- pr			= primitive ID,
						- trf_cap_		= information transfer capability,
						- trf_mode_		= transfer mode,
						- trf_rate_		= information transfer rate,
						- structure_	= structure,
						- dest_rate_	= destination rate,
						- protocol_id_1_= protocol id 1
						- protocol_id_2_= user information layer 2 protocol,
						- protocol_id_3_= user information layer 3 protocol,

			*/

#define Isdn_ets_store_bc_not_V110(pr, trf_cap_, trf_mode_,	trf_rate_, structure_, dest_rate_, protocol_id_1_, protocol_id_2_, protocol_id_3_) \
		Isdn_ets_store_bc	(pr,			trf_cap_,		trf_mode_,			\
							trf_rate_,		structure_,		dest_rate_,			\
							protocol_id_1_,	BYTE_VAL_NIL,	BYTE_VAL_NIL,		\
							BYTE_VAL_NIL,	BYTE_VAL_NIL,	BYTE_VAL_NIL,       \
							BYTE_VAL_NIL,	BYTE_VAL_NIL,	BYTE_VAL_NIL,		\
							BYTE_VAL_NIL,	BYTE_VAL_NIL,	BYTE_VAL_NIL,		\
							BYTE_VAL_NIL,	BYTE_VAL_NIL,	protocol_id_2_,		\
							protocol_id_3_)

/*-------------------------------------------------------------------------*/

			/*
				Isdn_ets_store_called_nb : called number IE

					INPUT :
						- pr				= primitive ID,
						- nb_type_			= numbering type,
						- plan_id_			= numbering plan,
						- nb_				= address of the literal string.
			*/

#define Isdn_ets_store_called_nb(pr, nb_type_, plan_id_, nb_)						\
		Set_offset_ets	(pr,		called_nb);										\
		Set_val_ets		(called_nb,	nb_type,	nb_type_);							\
		Set_val_ets		(called_nb,	plan_id,	plan_id_);							\
		Set_str_ets		(called_nb,	nb,			nb_,		ETS_MX_SZ_CALLED_NB+1)

/*-------------------------------------------------------------------------*/

			/*
				Isdn_ets_store_called_sub_add   : called sub-address IE
				Isdn_ets_store_called_sub_add_u : called sub-address IE
												  for SUB_ADD_USER type.

					INPUT :
						- pr			= primitive ID,
						- sub_add_type_	= subaddress type,
						- odd_even_ind_	= odd/even indicator,
						- sub_add_		= address of the literal string.
						- sz_            = size of the block for SUB_ADD_USER.

			*/

#define Isdn_ets_store_called_sub_add(pr, sub_add_type_, odd_even_ind_,	sub_add_)	\
		Set_offset_ets	(pr,				called_sub_add);						\
		Set_val_ets		(called_sub_add,	sub_add_type,	sub_add_type_);			\
		Set_val_ets		(called_sub_add,	odd_even_ind,	odd_even_ind_);			\
		Set_str_ets 	(called_sub_add,	sub_add,		sub_add_, ETS_MX_SZ_CALLED_SUB_ADD+1)

#define Isdn_ets_store_called_sub_add_u(pr, odd_even_ind_,	sub_add_, sz_)			\
		Set_offset_ets	(pr,				called_sub_add);						\
		Set_val_ets		(called_sub_add,	sub_add_type,	SUB_ADD_USER);			\
		Set_val_ets		(called_sub_add,	odd_even_ind,	odd_even_ind_);			\
		Set_mem_ets 	(called_sub_add,	sub_add,		sub_add_, sz_)

/*-------------------------------------------------------------------------*/

			/*
				Isdn_ets_store_calling_nb : store calling number IE

					INPUT :
						- pr 				= primitive ID,
						- nb_type_		   	= numbering type,
						- plan_id_			= numbering plan ID,
						- pres_				= presentation indicator,
						- screen_ind_    	= screening indicator,
						- nb_				= pointer on the calling number literal string.
			*/

#define Isdn_ets_store_calling_nb(pr, nb_type_, plan_id_, pres_, screen_ind_, nb_)	\
		Set_offset_ets	(pr,			calling_nb);								\
		Set_val_ets		(calling_nb,	nb_type, 		nb_type_);					\
		Set_val_ets		(calling_nb,	plan_id, 		plan_id_);					\
		Set_val_ets		(calling_nb,	pres,			pres_);						\
		Set_val_ets		(calling_nb,	screen_ind, 	screen_ind_);				\
		Set_str_ets		(calling_nb,	nb, 			nb_, ETS_MX_SZ_CALLING_NB+1)

/*-------------------------------------------------------------------------*/

			/*
				  Isdn_ets_calling_sub_add : calling sub-address IE

						INPUT :
						- pr			= primitive ID,
						- sub_add_type_	= subaddress type,
						- odd_even_ind_	= odd/even indicator,
						- sub_add_		= address of the literal string.


			*/

#define Isdn_ets_store_calling_sub_add(pr, sub_add_type_, odd_even_ind_, sub_add_) \
		Set_offset_ets	(pr,				calling_sub_add);						\
		Set_val_ets		(calling_sub_add,	sub_add_type, 	sub_add_type_);			\
		Set_val_ets		(calling_sub_add,	odd_even_ind,	odd_even_ind_);			\
		Set_str_ets		(calling_sub_add,	sub_add,		sub_add_, ETS_MX_SZ_CALLING_SUB_ADD+1)

#define Isdn_ets_store_calling_sub_add_u(pr, odd_even_ind_, sub_add_, sz_) \
		Set_offset_ets	(pr,				calling_sub_add);						\
		Set_val_ets		(calling_sub_add,	sub_add_type, 	SUB_ADD_USER);			\
		Set_val_ets		(calling_sub_add,	odd_even_ind,	odd_even_ind_);			\
		Set_mem_ets		(calling_sub_add,	sub_add,		sub_add_, sz_)

/*-------------------------------------------------------------------------*/

			/*
				Isdn_ets_store_call_id : call ID

					INPUT :
						- pr	= primitive ID,
						- value_= call ID.

			*/


#define Isdn_ets_store_call_id(pr, value_)										\
		Set_offset_ets	(pr,		call_id);										\
		Set_str_ets		(call_id,	value,	value_,	ETS_MX_SZ_CALL_ID+1)

/*-------------------------------------------------------------------------*/

			/*
				Isdn_ets_store_cause : cause IE

					INPUT :
						- pr			= primitive ID,
						- coding_std_	= coding standard,
						- cause_loc_	= cause location,
						- recommend_	= recommendation,
						- cause_class_	= cause class,
						- cause_val_	= cause value,
						- cause_diag_	= cause diagnostic.

			*/

#define Isdn_ets_store_cause(pr, coding_std_, cause_loc_, recommend_, cause_class_, cause_val_, cause_diag_) \
		Set_offset_ets	(pr,	cause);												\
		Set_val_ets		(cause,	coding_std,	coding_std_);							\
		Set_val_ets		(cause,	cause_loc,	cause_loc_);							\
		Set_val_ets		(cause,	recommend,	recommend_);							\
		Set_val_ets		(cause,	cause_class,cause_class_);							\
		Set_val_ets		(cause,	cause_val,	cause_val_);							\
		Set_str_ets		(cause,	cause_diag,	cause_diag_, ETS_MX_SZ_CAUSE_DIAG+1)

/*-------------------------------------------------------------------------*/

			/*
				 Isdn_ets_store_chan_id_BRI : store channel ID IE

					INPUT :
						- pr 			= primitive ID,
						- pref_excl_	= preferred channel,
						- d_chan_		= D-channel indicator,
						- chan_sel_		= channel selection,
			*/

#define Isdn_ets_store_chan_id_BRI(pr, pref_excl_, d_chan_, chan_sel_) \
		Set_offset_ets	(pr,		chan_id);							\
		Set_val_ets		(chan_id,	int_type,	INT_TYPE_BRI);			\
		Set_val_ets		(chan_id,	pref_excl,	pref_excl_);			\
		Set_val_ets		(chan_id,	d_chan,		d_chan_);				\
		Set_val_ets		(chan_id,	chan_sel,	chan_sel_);				\
		Set_str_ets		(chan_id,	chan_nb,	P_NIL,		1)

			/*
				 Isdn_ets_store_chan_id_PRI : store channel ID IE

					INPUT :
						- pr 			= primitive ID,
						- pref_excl_	= preferred channel,
						- d_chan_		= D-channel indicator,
						- chan_sel_		= channel selection,
						- coding_std_	= coding standard,
						- chan_type_	= channel type,
						- p_chan_nb_	= channel number.
			*/

#define Isdn_ets_store_chan_id_PRI(pr, pref_excl_, d_chan_, chan_sel_, coding_std_, chan_type_, p_chan_nb_) \
		Set_offset_ets	(pr,		chan_id);							\
		Set_val_ets		(chan_id,	int_type,	INT_TYPE_PRI);			\
		Set_val_ets		(chan_id,	pref_excl,	pref_excl_);			\
		Set_val_ets		(chan_id,	d_chan,		d_chan_);				\
		Set_val_ets		(chan_id,	chan_sel,	chan_sel_);				\
		Set_val_ets		(chan_id,	coding_std,	coding_std_);			\
		Set_val_ets		(chan_id,	chan_type,	chan_type_);			\
		Set_str_ets		(chan_id,	chan_nb,	p_chan_nb_,		ETS_MX_SZ_CHAN_ID+1)

/*-------------------------------------------------------------------------*/

#if NS_COLP_COLR == ON /* COLP and COLR Supp Services supported{ */

