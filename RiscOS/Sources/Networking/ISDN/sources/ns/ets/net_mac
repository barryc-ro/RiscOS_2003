		Set_next_ie_ets	(congest_level)

/*-------------------------------------------------------------------------*/

			/*
				Isdn_ets_store_date_time : date / time

					INPUT :
						- pr= primitive ID,
						- y	= year,
						- mo= month,
						- d = day,
						- h = hour,
						- mi= minute.

				Isdn_ets_store_date_t_with_se : the same with 1 more argument : second. 
			*/

#define Isdn_ets_store_date_t_with_se(pr, y, mo, d, h, mi, s)						\
		Set_offset_ets	(pr,		date_time);										\
		Set_val_ets		(date_time,	year,	y);										\
		Set_val_ets		(date_time,	month,	mo);									\
		Set_val_ets		(date_time,	day,	d);										\
		Set_val_ets		(date_time,	hour,	h);										\
		Set_val_ets		(date_time,	minute,	mi);									\
		Set_val_ets		(date_time,	second,	s);										\
		Set_next_ie_ets	(date_time)

#define Isdn_ets_store_date_time(pr, y, mo, d, h, mi)							\
			Isdn_ets_store_date_t_with_se (pr, y, mo, d, h, mi, BYTE_VAL_NIL)

/*-------------------------------------------------------------------------*/

			/*
				Isdn_ets_store_display : display IE

					INPUT :
						- pr	 = primitive ID,
						- p_text = address of the literal string.

			*/


#define Isdn_ets_store_display(pr,	p_text)										\
		Set_offset_ets	(pr,		display);										\
		Set_str_ets		(display,	text,	p_text,	ETS_MX_SZ_DISPLAY+1)

/*-------------------------------------------------------------------------*/

			/*
				  Isdn_ets_store_escape : escape IE

						INPUT :
						- pr			= primitive ID,
						- id			= IE id,
						- p_ie			= address of the IE.


			*/

#define Isdn_ets_store_escape(pr, ie_id_, p_ie_)								\
		Set_offset_ets	(pr,		escape);										\
		Set_val_ets		(escape,	ie_id, 		ie_id_);							\
		Set_str_ets		(escape,	p_ie,		p_ie_, ETS_MX_SZ_ESCAPE+1)

/*-------------------------------------------------------------------------*/

#if NS_SUPP_SERVICES == OFF
			/*
				Isdn_ets_store_t_facility : transparent facility IE

					INPUT :
						- pr 			= primitive ID,
						- p_info_		= facility string
						- size_			= facility string size
			*/

#define Isdn_ets_store_t_facility(pr, p_info_, size_) \
		Set_offset_ets	(pr,		facility);										\
		Set_mem_ets		(facility,	invoke_id,	p_info_,	size_)

			/*
				Isdn_ets_store_facility : facility IE

					INPUT :
						- pr 			= primitive ID,
						- comp_tag		= component tag
						- service		= service
						- pref			= preferred
						- error_val		= error value
						- p_invoke_id	= invoke id
			*/

#define Isdn_ets_store_facility(pr, comp_tag_, service_, pref_, error_val_, p_invoke_id) \
		Set_offset_ets	(pr,		facility);										\
		Set_val_ets		(facility,	comp_tag,	comp_tag_);							\
		Set_val_ets		(facility,	service,	service_);							\
		Set_val_ets		(facility,	pref,		pref_);								\
		Set_val_ets		(facility,	error_val,	error_val_);						\
		Set_str_ets		(facility,	invoke_id,	p_invoke_id,	ETS_MX_SZ_FACILITY+1)

			/*
				Isdn_ets_store_facility_INVOKE

					INPUT :
						- pr 			= primitive ID,
						- pref			= preferred
						- service		= service
						- invoke_id		= invoke id
			*/

#define Isdn_ets_store_facility_INVOKE(pr, service_, pref_, p_invoke_id)	\
		Isdn_ets_store_facility(pr,			FAC_INVOKE,	service_,			\
								pref_,		BYTE_VAL_NIL,p_invoke_id)

			/*
				Isdn_ets_store_facility_RESULT

					INPUT :
						- pr 			= primitive ID,
						- invoke_id		= invoke id
			*/

#define Isdn_ets_store_facility_RESULT(pr, p_invoke_id)						\
		Isdn_ets_store_facility(pr,			FAC_RETURN_RESULT,	BYTE_VAL_NIL,\
								BYTE_VAL_NIL,BYTE_VAL_NIL,		p_invoke_id)

			/*
				Isdn_ets_store_facility_ERROR

					INPUT :
						- pr 			= primitive ID,
						- error_val		= error value
						- invoke_id		= invoke id
			*/

#define Isdn_ets_store_facility_ERROR(pr, error_val_, p_invoke_id)			\
		Isdn_ets_store_facility(pr,			FAC_RETURN_ERROR,	BYTE_VAL_NIL,\
								BYTE_VAL_NIL,error_val_,		p_invoke_id)

#else /* NS_SUPP_SERVICES == ON */

			/*
				Isdn_ets_store_facility : facility IE

					INPUT :
						- pr 			= primitive ID.
			*/

#define Isdn_ets_store_facility(pr) \
		Set_offset_ets	(pr,		facility);							\
		ns_int_ctx.p__ss_data = (uchar FAR *)&(((P_val_ns_ets (facility))ns_int_ctx.p__val) -> component) 
#endif

/*-------------------------------------------------------------------------*/

			/*
				Isdn_ets_store_hlc : high layer compatibility for
										non-CCITT coding standard

					INPUT :
						- pr		= primitive ID,
						- std		= coding standard,
						- tele      = teleservice,
						- ext_tele	= extended teleservice.
			*/

#define Isdn_ets_store_hlc(pr, std, tele, ext_tele)								\
		Set_offset_ets	(pr,	hlc);												\
		Set_val_ets		(hlc,	coding_std,			std);							\
		Set_val_ets		(hlc,	teleservice,		tele);							\
		Set_val_ets		(hlc,	ext_teleservice,	ext_tele);						\
		Set_next_ie_ets	(hlc)

			/*
				Isdn_ets_store_hlc_CCITT : high layer compatibility for
										CCITT coding standard

					INPUT :
						- pr		= primitive ID,
						- tele      = teleservice,
						- ext_tele	= extended teleservice.
			*/

#define Isdn_ets_store_hlc_CCITT(pr, tele, ext_tele)							\
		Isdn_ets_store_hlc	(pr, CODING_STD_CCITT, tele, ext_tele)

/*-------------------------------------------------------------------------*/

			/*
				Isdn_ets_store_keypad : keypad IE

					INPUT :
						- pr	 = primitive ID,
						- p_text = address of the literal string.

			*/


#define Isdn_ets_store_keypad(pr,p_text)										\
		Set_offset_ets	(pr,		keypad);										\
		Set_str_ets		(keypad,	text,	p_text,	ETS_MX_SZ_KEYPAD+1)

/*-------------------------------------------------------------------------*/

			/*
			   Isdn_ets_store_llc : low layer compatibility

			   First definition : in one part :

					INPUT :
						- pr			= primitive ID,
						- coding_std_	= coding standard,
						- trf_cap_		= information transfer capability,
						- out_neg		= out band negotiation,
						- trf_mode_		= transfer mode,
						- trf_rate_		= information transfer rate,
						- structure_	= structure,
						- dest_rate_	= destination rate,
						- protocol_id_1_= user information layer 1 protocol,
						- syn_asyn_		= synchronous/asynchronous,
						- negotiation_	= negotiation,
						- user_rate_	= user rate,
						- inter_rate_	= intermediate rate,
						- nic_tx_		= network independent clock (NIC) on Transmission (Tx),
						- nic_rx_		= network independent clock (NIC) on Reception (Rx),
						- fc_tx_		= flow control on Transmission (Tx),
						- fc_rx_		= flow control on Reception (Rx),
						- hdr_			= rate adaption header included or not,
						- multi_frame_	= multiple frame establishement support in DL,
						- op_mode_		= mode of operation,
						- lli_negot_	= logical link identifier negotiation,
						- assign_		= assignor/assignee,
						- band_negot_	= in-band/out-band negotiation,
						- stop_bit_		= number of stop bits,
						- data_bit_		= number of data bits,
						- parity_		= number of parity bit,
						- duplex_		= duplex mode,
						- modem_		= modem type,
						- protocol_id_2_= user information layer 2 protocol,
						- option_2_		= optional layer 2 protocol information,
						- protocol_id_3_= user information layer 3 protocol,
						- option_3_		= optional layer 3 protocol information.
			*/

#define Isdn_ets_store_llc(pr,coding_std_, trf_cap_, out_neg, trf_mode_, trf_rate_, structure_, dest_rate_,	protocol_id_1_,	syn_asyn_, negotiation_, user_rate_, inter_rate_, nic_tx_, nic_rx_, fc_tx_,	fc_rx_,	hdr_, multi_frame_,	op_mode_, lli_negot_, assign_, band_negot_, stop_bit_, data_bit_, parity_, duplex_,	modem_,	protocol_id_2_,	option_2_, protocol_id_3_, option_3_) \
		Set_offset_ets	(pr,	llc);										\
		Set_val_ets		(llc,	coding_std,		coding_std_);				\
		Set_val_ets		(llc,	trf_cap,		trf_cap_);					\
		Set_val_ets		(llc,	out_band_neg,	out_neg);					\
		Set_val_ets		(llc,	trf_mode,		trf_mode_);					\
		Set_val_ets		(llc,	trf_rate,		trf_rate_);					\
		Set_val_ets		(llc,	structure,		structure_);				\
		Set_val_ets		(llc,	dest_rate,		dest_rate_);				\
		Set_val_ets		(llc,	protocol_id_1,	protocol_id_1_);			\
		Set_val_ets		(llc,	syn_asyn,		syn_asyn_);					\
		Set_val_ets		(llc,	negotiation,	negotiation_);				\
		Set_val_ets		(llc,	user_rate,		user_rate_);				\
		Set_val_ets		(llc,	inter_rate,		inter_rate_);				\
		Set_val_ets		(llc,	nic_tx,			nic_tx_);					\
		Set_val_ets		(llc,	nic_rx,			nic_rx_);					\
		Set_val_ets		(llc,	fc_tx,			fc_tx_);					\
		Set_val_ets		(llc,	fc_rx,			fc_rx_);					\
		Set_val_ets		(llc,	hdr,			hdr_);						\
		Set_val_ets		(llc,	multi_frame,	multi_frame_);				\
		Set_val_ets		(llc,	op_mode,		op_mode_);					\
		Set_val_ets		(llc,	lli_negot,		lli_negot_);				\
		Set_val_ets		(llc,	assign,			assign_);					\
		Set_val_ets		(llc,	band_negot,		band_negot_);				\
		Set_val_ets		(llc,	stop_bit,		stop_bit_);					\
		Set_val_ets		(llc,	data_bit,		data_bit_);					\
		Set_val_ets		(llc,	parity,			parity_);					\
		Set_val_ets		(llc,	duplex,			duplex_);					\
		Set_val_ets		(llc,	modem,			modem_);					\
		Set_val_ets		(llc,	protocol_id_2,	protocol_id_2_);			\
		Set_val_ets		(llc,	option_2,		option_2_);					\
		Set_val_ets		(llc,	protocol_id_3,	protocol_id_3_);			\
		Set_val_ets		(llc,	option_3,		option_3_);					\
		Set_next_ie_ets	(llc)



			/*--------------------------------------------*/


			/*
			   Isdn_ets_store_llc : low layer compatibility

			   Second definition : DIVIDED IN 2 PARTS for compilers which
			   do not accept so many parameters in macros :

					Isdn_ets_store_llc_1_4 : octet 1 to octet 4 included,

						INPUT :
							- pr			= primitive ID,
							- coding_std_	= coding standard,
							- trf_cap_		= information transfer capability,
							- out_neg		= out band negotiation,
							- trf_mode_		= transfer mode,
							- trf_rate_		= information transfer rate,
							- structure_	= structure,
							- dest_rate_	= destination rate,

					Isdn_ets_store_llc_5_7 : octet 5 to octet 7 included,

						- protocol_id_1_= user information layer 1 protocol,
						- syn_asyn_		= synchronous/asynchronous,
						- negotiation_	= negotiation,
						- user_rate_	= user rate,
						- inter_rate_	= intermediate rate,
						- nic_tx_		= network independent clock (NIC) on Transmission (Tx),
						- nic_rx_		= network independent clock (NIC) on Reception (Rx),
						- fc_tx_		= flow control on Transmission (Tx),
						- fc_rx_		= flow control on Reception (Rx),
						- hdr_			= rate adaption header included or not,
						- multi_frame_	= multiple frame establishement support in DL,
						- op_mode_		= mode of operation,
						- lli_negot_	= logical link identifier negotiation,
						- assign_		= assignor/assignee,
						- band_negot_	= in-band/out-band negotiation,
						- stop_bit_		= number of stop bits,
						- data_bit_		= number of data bits,
						- parity_		= number of parity bit,
						- duplex_		= duplex mode,
						- modem_		= modem type,
						- protocol_id_2_= user information layer 2 protocol,
						- option_2_		= optional layer 2 protocol information,
						- protocol_id_3_= user information layer 3 protocol,
						- option_3_		= optional layer 3 protocol information.
			*/

#define Isdn_ets_store_llc_1_4(pr,coding_std_, trf_cap_, out_neg, trf_mode_, trf_rate_, structure_, dest_rate_)\
		Set_offset_ets	(pr,	llc);										\
		Set_val_ets		(llc,	coding_std,		coding_std_);				\
		Set_val_ets		(llc,	trf_cap,		trf_cap_);					\
		Set_val_ets		(llc,	out_band_neg,	out_neg);					\
		Set_val_ets		(llc,	trf_mode,		trf_mode_);					\
		Set_val_ets		(llc,	trf_rate,		trf_rate_);					\
		Set_val_ets		(llc,	structure,		structure_);				\
		Set_val_ets		(llc,	dest_rate,		dest_rate_)

#define Isdn_ets_store_llc_5_7(protocol_id_1_,	syn_asyn_, negotiation_, user_rate_, inter_rate_, nic_tx_, nic_rx_, fc_tx_,	fc_rx_,	hdr_, multi_frame_,	op_mode_, lli_negot_, assign_, band_negot_, stop_bit_, data_bit_, parity_, duplex_,	modem_,	protocol_id_2_,	option_2_, protocol_id_3_, option_3_) \
		Set_val_ets		(llc,	protocol_id_1,	protocol_id_1_);			\
		Set_val_ets		(llc,	syn_asyn,		syn_asyn_);					\
		Set_val_ets		(llc,	negotiation,	negotiation_);				\
		Set_val_ets		(llc,	user_rate,		user_rate_);				\
		Set_val_ets		(llc,	inter_rate,		inter_rate_);				\
		Set_val_ets		(llc,	nic_tx,			nic_tx_);					\
		Set_val_ets		(llc,	nic_rx,			nic_rx_);					\
		Set_val_ets		(llc,	fc_tx,			fc_tx_);					\
		Set_val_ets		(llc,	fc_rx,			fc_rx_);					\
		Set_val_ets		(llc,	hdr,			hdr_);						\
		Set_val_ets		(llc,	multi_frame,	multi_frame_);				\
		Set_val_ets		(llc,	op_mode,		op_mode_);					\
		Set_val_ets		(llc,	lli_negot,		lli_negot_);				\
		Set_val_ets		(llc,	assign,			assign_);					\
		Set_val_ets		(llc,	band_negot,		band_negot_);				\
		Set_val_ets		(llc,	stop_bit,		stop_bit_);					\
		Set_val_ets		(llc,	data_bit,		data_bit_);					\
		Set_val_ets		(llc,	parity,			parity_);					\
		Set_val_ets		(llc,	duplex,			duplex_);					\
		Set_val_ets		(llc,	modem,			modem_);					\
		Set_val_ets		(llc,	protocol_id_2,	protocol_id_2_);			\
		Set_val_ets		(llc,	option_2,		option_2_);					\
		Set_val_ets		(llc,	protocol_id_3,	protocol_id_3_);			\
		Set_val_ets		(llc,	option_3,		option_3_);					\
		Set_next_ie_ets	(llc)


			/*--------------------------------------------*/


			/*
			   Isdn_ets_store_llc_V110 : low layer compatibility for
											V110 rate adaption :

					INPUT :
						- pr			= primitive ID,
						- coding_std_	= coding standard,
						- trf_cap_		= information transfer capability,
						- out_neg		= out band negotiation,
						- trf_mode_		= transfer mode,
						- trf_rate_		= information transfer rate,
						- structure_	= structure,
						- dest_rate_	= destination rate,
						- syn_asyn_		= synchronous/asynchronous,
						- negotiation_	= negotiation,
						- user_rate_	= user rate,
						- inter_rate_	= intermediate rate,
						- nic_tx_		= network independent clock (NIC) on Transmission (Tx),
						- nic_rx_		= network independent clock (NIC) on Reception (Rx),
						- fc_tx_		= flow control on Transmission (Tx),
						- fc_rx_		= flow control on Reception (Rx),
						- stop_bit_		= number of stop bits,
						- data_bit_		= number of data bits,
						- parity_		= number of parity bit,
						- duplex_		= duplex mode,
						- modem_		= modem type,
						- protocol_id_2_= user information layer 2 protocol,
						- protocol_id_3_= user information layer 3 protocol,

			*/

#define Isdn_ets_store_llc_V110(pr, coding_std_, trf_cap_, out_neg, trf_mode_, trf_rate_, structure_, dest_rate_, syn_asyn_, negotiation_, user_rate_, inter_rate_, nic_tx_, nic_rx_, fc_tx_, fc_rx_, stop_bit_, data_bit_, parity_, duplex_, modem_, protocol_id_2_, protocol_id_3_) \
		Isdn_ets_store_llc_1_4(pr,			coding_std_,	trf_cap_,			\
							out_neg,		trf_mode_,		trf_rate_,			\
							structure_,		dest_rate_);						\
		Isdn_ets_store_llc_5_7(PROTOCOL_ID_1_V110,	syn_asyn_,	negotiation_,	\
							user_rate_,		inter_rate_,	nic_tx_,			\
							nic_rx_,		fc_tx_,			fc_rx_,				\
							BYTE_VAL_NIL,	BYTE_VAL_NIL,	BYTE_VAL_NIL,		\
							BYTE_VAL_NIL,	BYTE_VAL_NIL,	BYTE_VAL_NIL,		\
							stop_bit_,		data_bit_,		parity_,			\
							duplex_,		modem_,			protocol_id_2_,		\
							BYTE_VAL_NIL,	protocol_id_3_,	BYTE_VAL_NIL)


			/*--------------------------------------------*/


			/*
			   Isdn_ets_store_llc_V120 : low layer compatibility for
											V120 rate adaption :

					INPUT :
						- pr			= primitive ID,
						- coding_std_	= coding standard,
						- trf_cap_		= information transfer capability,
						- out_neg		= out band negotiation,
						- trf_mode_		= transfer mode,
						- trf_rate_		= information transfer rate,
						- structure_	= structure,
						- dest_rate_	= destination rate,
						- syn_asyn_		= synchronous/asynchronous,
						- negotiation_	= negotiation,
						- user_rate_	= user rate,
						- hdr_			= rate adaption header included or not,
						- multi_frame_	= multiple frame establishement support in DL,
						- op_mode_		= mode of operation,
						- lli_negot_	= logical link identifier negotiation,
						- assign_		= assignor/assignee,
						- band_negot_	= in-band/out-band negotiation,
						- stop_bit_		= number of stop bits,
						- data_bit_		= number of data bits,
						- parity_		= number of parity bit,
						- duplex_		= duplex mode,
						- modem_		= modem type,
						- protocol_id_2_= user information layer 2 protocol,
						- protocol_id_3_= user information layer 3 protocol,

			*/

#define Isdn_ets_store_llc_V120(pr, coding_std_, trf_cap_, out_neg, trf_mode_, trf_rate_, structure_, dest_rate_, syn_asyn_, negotiation_, user_rate_, hdr_, multi_frame_, op_mode_, lli_negot_, assign_, band_negot_, stop_bit_, data_bit_, parity_, duplex_, modem_, protocol_id_2_, protocol_id_3_) \
		Isdn_ets_store_llc_1_4(pr,			coding_std_,	trf_cap_,			\
							out_neg,		trf_mode_,		trf_rate_,			\
							structure_,		dest_rate_);						\
		Isdn_ets_store_llc_5_7(PROTOCOL_ID_1_V120,								\
							syn_asyn_,		negotiation_,	user_rate_,			\
							BYTE_VAL_NIL,	BYTE_VAL_NIL,	BYTE_VAL_NIL,		\
							BYTE_VAL_NIL,	BYTE_VAL_NIL,	hdr_,				\
							multi_frame_,	op_mode_,		lli_negot_,			\
							assign_,		band_negot_,	stop_bit_,			\
							data_bit_,		parity_,		duplex_,			\
							modem_,			protocol_id_2_, BYTE_VAL_NIL,		\
							protocol_id_3_,	BYTE_VAL_NIL)

/*-------------------------------------------------------------------------*/

			/*
				Isdn_ets_store_more_data : more data

					INPUT :
						- pr		= primitive ID,

			*/

#define Isdn_ets_store_more_data(pr) \
		Set_offset_ets	(pr,		more_data)

/*-------------------------------------------------------------------------*/

			/*
				Isdn_ets_store_notif_ind : notification indicator

					INPUT :
						- pr	= primitive ID,
						- descr	= description.

			*/

#define Isdn_ets_store_notif_ind(pr, descr)										\
		Set_offset_ets	(pr,		notif_ind);										\
		Set_val_ets		(notif_ind,	notif_descr,	descr);							\
		Set_next_ie_ets	(notif_ind)

/*-------------------------------------------------------------------------*/

			/*
				Isdn_ets_store_progress_ind : progress indicator

					INPUT :
						- pr				= primitive ID,
						- coding_std_		= coding standard,
						- progress_loc_		= location,
						- progress_descr_	= description.

			*/

#define Isdn_ets_store_progress_ind(pr, coding_std_, progress_loc_, progress_descr_) \
		Set_offset_ets	(pr,			progress_ind);								\
		Set_val_ets		(progress_ind,	coding_std,		coding_std_);				\
		Set_val_ets		(progress_ind,	progress_loc,	progress_loc_);				\
		Set_val_ets		(progress_ind,	progress_descr,	progress_descr_);			\
		Set_next_ie_ets	(progress_ind)

/*-------------------------------------------------------------------------*/

#if NS_SS_DIVERSION == ON /* Diversion Supplementary Services supported{ */

			/*
				Isdn_ets_store_redir_nb : store redirecting number IE

					INPUT :
						- pr 				= primitive ID,
						- nb_type_		   	= numbering type,
						- plan_id_			= numbering plan ID,
						- pres_				= presentation indicator,
                        - reason_			= reason for diversion,
						- nb_				= pointer on the redirecting number literal string.
			*/

#define Isdn_ets_store_redir_nb(pr, nb_type_, plan_id_, pres_, reason_, nb_)	\
		Set_offset_ets	(pr,			redir_nb);								\
		Set_val_ets		(redir_nb,	nb_type, 		nb_type_);					\
		Set_val_ets		(redir_nb,	plan_id, 		plan_id_);					\
		Set_val_ets		(redir_nb,	pres,			pres_);						\
		Set_val_ets		(redir_nb,	reason, 		reason_);					\
		Set_str_ets		(redir_nb,	nb, 			nb_, ETS_MX_SZ_REDIR_NB+1)

			/*
				Isdn_ets_store_redirection_nb : store redirection number IE

					INPUT :
						- pr 				= primitive ID,
						- nb_type_		   	= numbering type,
						- plan_id_			= numbering plan ID,
						- pres_				= presentation indicator,
						- nb_				= pointer on the redirecting number literal string.
			*/

#define Isdn_ets_store_redirection_nb(pr, nb_type_, plan_id_, pres_, nb_)	\
		Set_offset_ets	(pr,			redirection_nb);					\
		Set_val_ets		(redirection_nb,	nb_type, 		nb_type_);		\
		Set_val_ets		(redirection_nb,	plan_id, 		plan_id_);		\
		Set_val_ets		(redirection_nb,	pres,			pres_);			\
		Set_str_ets		(redirection_nb,	nb, 			nb_, ETS_MX_SZ_REDIRECTION_NB+1)

#endif /* end of NS_SS_DIVERSION = ON} */

/*-------------------------------------------------------------------------*/

			/*
				Isdn_ets_store_restart_ind : restart indicator

					INPUT :
						- pr	= primitive ID,
						- class_= class.

			*/

#define Isdn_ets_store_restart_ind(pr, class_)									\
		Set_offset_ets	(pr,			restart_ind);								\
		Set_val_ets		(restart_ind,	restart_class,	class_);					\
		Set_next_ie_ets	(restart_ind)

/*-------------------------------------------------------------------------*/

			/*
				Isdn_ets_store_sending_complete : sending complete

					INPUT :
						- pr		= primitive ID,

			*/

#define Isdn_ets_store_sending_complete(pr)	\
		Set_offset_ets	(pr,				sending_complete)

/*-------------------------------------------------------------------------*/

			/*
				Isdn_ets_store_signal : signal message

					INPUT :
						- pr			= primitive ID,
						- signal_val	= signal value.

			*/

#define Isdn_ets_store_signal(pr, signal_val_)									\
		Set_offset_ets	(pr,		signal);										\
		Set_val_ets		(signal,	signal_val,	signal_val_);						\
		Set_next_ie_ets	(signal)

/*-------------------------------------------------------------------------*/

#if NS_SPF_FACILITY == ON

			/*
				Isdn_ets_store_spf_fac : network specific facility

					INPUT :
						- pr 			= primitive ID,
						- net_id_lgth_	= length of network identification,
						- net_id_type_	= type of network identification,
						- net_id_plan_	= network identification plan,
						- p_net_id		= network identification array
						- p_fac_info	= network-specific facility specification
			*/

#define Isdn_ets_store_spf_fac(pr, net_id_lgth_, net_id_type_, net_id_plan_, p_net_id, p_fac_info)	\
		Set_offset_ets	(pr,		spf_fac);										\
		Set_val_ets		(spf_fac,	net_id_lgth,	net_id_lgth_);					\
		Set_val_ets		(spf_fac,	net_id_type,	net_id_type_);					\
		Set_val_ets		(spf_fac,	net_id_plan,	net_id_plan_);					\
		Set_mem_ets		(spf_fac,	net_id,			p_net_id,	(net_id_lgth_ > ETS_MX_SZ_NET_ID) ? ETS_MX_SZ_NET_ID : net_id_lgth_); \
		Set_str_ets		(spf_fac,	fac_info,		p_fac_info, ETS_MX_SZ_SPF_FAC+1)

			/*
				Isdn_ntt_get_spf_fac_p_info
				Isdn_ntt_get_spf_fac_info_lgth : network specific facility

					INPUT :
						- pr 			= primitive ID,
			*/

#define Isdn_ets_get_spf_fac_p_info(pr)	\
		Isdn_p_str (pr, net_id) + Isdn_lgth_str (pr, net_id) + 1

#define Isdn_ets_get_spf_fac_info_lgth(pr)	\
		*(Isdn_ets_get_spf_fac_p_info(pr) - 1)

#endif

/*-------------------------------------------------------------------------*/

			/*
				Isdn_ets_store_transit_net_sel : transit network selection

					INPUT :
						- pr 			= primitive ID,
						- net_id_type_	= type of network identification,
						- net_id_plan_	= network identification plan,
						- net_id_		= address of network identification.
			*/

#define Isdn_ets_store_transit_net_sel(pr, net_id_type_, net_id_plan_, net_id_)	\
		Set_offset_ets	(pr,				transit_net_sel);						\
		Set_val_ets		(transit_net_sel,	net_id_type,	net_id_type_);			\
		Set_val_ets		(transit_net_sel,	net_id_plan,	net_id_plan_);			\
		Set_str_ets		(transit_net_sel,	net_id,			net_id_,	ETS_MX_SZ_TRANSIT_NET_SEL+1)

/*-------------------------------------------------------------------------*/

			/*
				Isdn_ets_store_uui : user to user information IE

					INPUT :
						- pr				= primitive ID,
						- protocol_discr_	= protocol discriminator,
						- p_text 			= address of the user info.
			*/

#define Isdn_ets_store_uui(pr, protocol_discr_, p_text)							\
		Set_offset_ets	(pr,	uui);												\
		Set_val_ets		(uui,	protocol_discr,	protocol_discr_);					\
		Set_str_ets		(uui, 	text,	p_text,	ETS_MX_SZ_UUI+1)


/*-------------------------------------------------------------------------*/
