				/*
				   MACROs and DECLARATIONS used to access the Network
				   Signalling entity (NS) from the user Layer 4 Signalling
				   entity (L4S).
				*/

/*-------------------------------------------------------------------------*/


			/*
				Isdn_init_ets_ctx_???? = initialization of the common context
									 used by a primitive macro :

					INPUT :
						- pr 	 = primitive ID,
						- p_off  = pointer to the offset area,

			*/


#if NS_IE_TSP_OUT == ON
#	define Isdn_init_ets_ctx_rq_rs_tsp(pr, p_off, data_lg)	Isdn_init_ctx_rq_rs_tsp(Paste(ets_,pr), p_off, data_lg)
#endif

#define Isdn_init_ets_ctx_rq_rs(pr, p_off)	Isdn_init_ctx_rq_rs(Paste(ets_,pr), p_off)
#define Isdn_init_ets_ctx_in_co(p_off)		Isdn_init_ctx_in_co(p_off)

/*-------------------------------------------------------------------------*/

#define	Get_ie_ets(pr,ie)					Get_ie 			(Paste(ets_,pr), Paste (ets_, ie))
#define	Isdn_get_val_ets(pr,ie,fld)			Isdn_get_val	(Paste(ets_,pr), Paste (ets_, ie), fld)
#define Isdn_p_str_ets(pr,ie,fld)			Isdn_p_str		(Paste(ets_,pr), Paste (ets_, ie), fld)
#define	Isdn_lgth_str_ets(pr,ie,fld)		Isdn_lgth_str	(Paste(ets_,pr), Paste (ets_, ie), fld)
#define	Isdn_mx_lgth_str_ets(pr,ie,fld)		Isdn_mx_lgth_str(Paste(ets_,pr), Paste (ets_, ie), fld)
#define	P_start_val_ns_ets(pr,ie)			P_start_val_ns	(Paste(ets_,pr), Paste (ets_, ie))
#define	P_val_ns_ets(ie)					P_val_ns		(Paste(ets_,ie))
#define Set_offset_ets(pr,ie)				Set_offset		(Paste(ets_,pr),Paste(ets_,ie))
#define	Set_next_ie_ets(ie)					Set_next_ie		(Paste(ets_,ie))
#define	Set_val_ets(ie,fld,val)				Set_val			(Paste(ets_,ie), fld, val)
#define Set_str_ets(ie, lv, p_str, mx_sz)	Set_str			(Paste(ets_,ie), lv, p_str, mx_sz)
#define Set_mem_ets(ie, lv, p_mem, sz)		Set_mem			(Paste(ets_,ie), lv, p_mem, sz)
#define Set_to_next_ie_instance_ets(pr,ie)	Set_to_next_ie_instance (Paste(ets_,pr), Paste (ets_, ie))
#define Get_remaining_ie_instance_nb_ets(pr,ie,p_ie)	Get_remaining_ie_instance_nb (Paste(ets_,pr), Paste (ets_, ie), p_ie)

#if NS_SUPP_SERVICES == ON
#	define Isdn_get_ss_first_comp_ets(pr,ie)	Isdn_get_ss_first_component	(Paste(ets_,pr), Paste(ets_,ie))
#endif

#if MESSAGE_HEADER_IE == ON
#	define Message_header_offset_ets		ns_offset_t ets_message_header;
#else
#	define Message_header_offset_ets
#endif


#define Set_ns_offset_tsp_ets(pr,ie)		Set_ns_offset_tsp	(Paste(ets_,pr),Paste(ets_,ie))
#define Set_mem_eob_ets(ie,lv,p_mem,sz)		Set_mem_eob		(Paste(ets_,ie), lv, p_mem, sz)

/*-------------------------------------------------------------------------*/
		/* Structures of the offset area of each primitive interfaces */

		/******************************************************
		**     Each offset is a relative pointer on the      **
		**     data of an IE element                         **
		******************************************************/

/*====================   CALL  ESTABLISHMENT  MESSAGES    =====================*/

		/* NS_ALERT_RQ/IN */

struct off_ns_ets_te_alert_in {
	Message_header_offset_ets				/* message header parameters */
	ns_offset_t ets_chan_id;				/* channel ID */
	ns_offset_t ets_facility;				/* facility */
	ns_offset_t ets_progress_ind;			/* progress indicator */
	ns_offset_t ets_display;				/* display */
	ns_offset_t ets_uui;					/* user to user information */
#	if NS_SUPP_SERVICES == ON
		ns_offset_t ets_notif_ind;			/* notification indicator */
#		if NS_SS_DIVERSION == ON
			ns_offset_t ets_redirection_nb;		/* redirection number */
#		endif
#	endif
};

typedef struct off_ns_ets_te_alert_in		off_ns_ets_alert_rq;
typedef struct off_ns_ets_te_alert_in		off_ns_ets_alert_in;

		/* NS_CALL_PROC_RQ/IN */

struct off_ns_ets_te_call_proc_in {
	Message_header_offset_ets				/* message header parameters */
	ns_offset_t ets_chan_id;				/* channel ID */
	ns_offset_t ets_progress_ind;			/* progress indicator */
	ns_offset_t ets_display;				/* display */
#	if NS_SUPP_SERVICES == ON
		ns_offset_t ets_facility;			/* facility */
		ns_offset_t ets_notif_ind;			/* notification indicator */
#		if NS_SS_DIVERSION == ON
			ns_offset_t ets_redirection_nb;		/* redirection number */
#		endif
#	endif
};

typedef struct off_ns_ets_te_call_proc_in	off_ns_ets_call_proc_rq;
typedef struct off_ns_ets_te_call_proc_in	off_ns_ets_call_proc_in;

		/* NS_CONN_RQ/IN */

struct off_ns_ets_te_conn_in {
	Message_header_offset_ets				/* message header parameters */
	ns_offset_t ets_sending_complete;		/* sending complete */
	ns_offset_t ets_bc;						/* bearer capability */
	ns_offset_t ets_chan_id;				/* channel ID */
	ns_offset_t ets_facility;				/* facility */
	ns_offset_t ets_progress_ind;			/* progress indicator */
#if NS_SPF_FACILITY == ON
	ns_offset_t ets_spf_fac;				/* network specific facility */
#endif
	ns_offset_t ets_display;				/* display */
	ns_offset_t ets_keypad;					/* keypad */
	ns_offset_t ets_calling_nb;				/* calling number (from network) */
	ns_offset_t ets_calling_sub_add;		/* calling subaddress */
	ns_offset_t ets_called_nb;				/* called number */
	ns_offset_t ets_called_sub_add;			/* called subaddress */
	ns_offset_t ets_llc;					/* low layer capability */
	ns_offset_t ets_hlc;					/* high layer compatibility */
	ns_offset_t ets_uui;					/* user to user information */
#	if NS_SUPP_SERVICES == ON
		ns_offset_t ets_notif_ind;			/* notification indicator */
#		if NS_SS_DIVERSION == ON
			ns_offset_t ets_redir_nb;		/* redirecting number */
#		endif
#	endif
	ns_offset_t ets_transit_net_sel;		/* transit network selection */
};

typedef struct off_ns_ets_te_conn_in		off_ns_ets_conn_rq;
typedef struct off_ns_ets_te_conn_in		off_ns_ets_conn_in;

		/* NS_CONN_RS/CO */

struct off_ns_ets_te_conn_co {
	Message_header_offset_ets				/* message header parameters */
	ns_offset_t ets_chan_id;				/* channel ID */
	ns_offset_t ets_facility;				/* facility */
	ns_offset_t ets_progress_ind;			/* progress indicator */
	ns_offset_t ets_display;				/* display */
	ns_offset_t ets_date_time;				/* date and time */
	ns_offset_t ets_llc;					/* low layer capability */
	ns_offset_t ets_uui;					/* user to user information */
	ns_offset_t ets_advice_charge_5;		/* advice of charge (codeset 5) */
	ns_offset_t ets_advice_charge;			/* advice of charge */
#	if NS_COLP_COLR == ON
		ns_offset_t ets_conn_nb;			/* connected number */
		ns_offset_t ets_conn_sub_add;		/* connected subaddress */
#	endif

#	if NS_SUPP_SERVICES == ON
		ns_offset_t ets_notif_ind;			/* notification indicator */
#		if NS_SS_DIVERSION == ON
			ns_offset_t ets_redirection_nb;		/* redirection number */
#		endif
#	endif
};

typedef struct off_ns_ets_te_conn_co		off_ns_ets_conn_rs;
typedef struct off_ns_ets_te_conn_co		off_ns_ets_conn_co;

		/* NS_PROGRESS_RQ_IN */

struct off_ns_ets_te_progress_in {
	Message_header_offset_ets				/* message header parameters */
	ns_offset_t ets_cause;					/* cause */
	ns_offset_t ets_progress_ind;			/* progress indicator */
	ns_offset_t ets_display;				/* display */
	ns_offset_t ets_uui;					/* user to user information */
#	if NS_SUPP_SERVICES == ON
		ns_offset_t ets_facility;			/* facility */
		ns_offset_t ets_notif_ind;			/* notification indicator */
#		if NS_SS_DIVERSION == ON
			ns_offset_t ets_redirection_nb;		/* redirection number */
#		endif
#	endif
};

typedef struct off_ns_ets_te_progress_in	off_ns_ets_progress_rq;
typedef struct off_ns_ets_te_progress_in	off_ns_ets_progress_in;

		/* NS_SETUP_ACK_RQ/IN */

struct off_ns_ets_te_setup_ack_in {
	Message_header_offset_ets				/* message header parameters */
	ns_offset_t ets_chan_id;				/* channel ID */
	ns_offset_t ets_progress_ind;			/* progress indicator */
	ns_offset_t ets_display;				/* display */
#	if NS_SUPP_SERVICES == ON
		ns_offset_t ets_facility;			/* facility */
		ns_offset_t ets_notif_ind;			/* notification indicator */
#	endif
};

typedef struct off_ns_ets_te_setup_ack_in	off_ns_ets_setup_ack_rq;
typedef struct off_ns_ets_te_setup_ack_in	off_ns_ets_setup_ack_in;

#if NS_SUPP_SERVICES == ON /*{*/

		/* NS_SPF_REGISTRATION_RQ/IN */

struct off_ns_ets_te_reg_rq {
	Message_header_offset_ets				/* message header parameters */
