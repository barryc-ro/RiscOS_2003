		/* Table of table items (indexed tables )*/

Ns_dms_table_or_function (tabx, DMS_TAB_TABX)	/* { */
	Ns_dms_tab_tabx_entry (T_NIL,					P_NIL)
	Ns_dms_tab_tabx_entry (DMS_TX_C_V_CCITT,		DMS_TAB_C_V_CCITT_S)
	Ns_dms_tab_tabx_entry (DMS_TX_C_V_SPF,        	DMS_TAB_C_V_SPF_S)
