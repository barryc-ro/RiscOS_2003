#define Isdn_swd_store_display(pr,	p_text)										\
		Set_offset_swd	(pr,		display);										\
		Set_str_swd		(display,	text,	p_text,	SWD_MX_SZ_DISPLAY+1)

/*-------------------------------------------------------------------------*/

			/*
				Isdn_swd_store_hlc : high layer compatibility for
										non-CCITT coding standard

					INPUT :
						- pr		= primitive ID,
						- std		= coding standard,
						- tele      = teleservice,
			*/

#define Isdn_swd_store_hlc(pr, std, tele)										\
		Set_offset_swd	(pr,	hlc);												\
		Set_val_swd		(hlc,	coding_std,			std);							\
		Set_val_swd		(hlc,	teleservice,		tele);							\
		Set_next_ie_swd	(hlc)

			/*
				Isdn_swd_store_hlc_CCITT : high layer compatibility for
										CCITT coding standard

					INPUT :
						- pr		= primitive ID,
						- tele      = teleservice,
			*/

#define Isdn_swd_store_hlc_CCITT(pr, tele)										\
		Isdn_swd_store_hlc	(pr, CODING_STD_CCITT, tele)

/*-------------------------------------------------------------------------*/

			/*
				Isdn_swd_store_keypad : keypad IE

					INPUT :
						- pr	 = primitive ID,
						- p_text = address of the literal string.

			*/

#define Isdn_swd_store_keypad(pr,p_text)										\
		Set_offset_swd	(pr,		keypad);										\
		Set_str_swd		(keypad,	text,	p_text,	SWD_MX_SZ_KEYPAD+1)

/*-------------------------------------------------------------------------*/

			/*
			   Isdn_swd_store_llc : low layer compatibility

			   First definition : in one part :

					INPUT :
						- pr			= primitive ID,
						- coding_std_	= coding standard,
						- trf_cap_		= information transfer capability,
						- out_neg		= out band negotiation,
						- trf_mode_		= transfer mode,
						- trf_rate_		= information transfer rate,
						- structure_	= structure,
						- dest_rate_	= destination rate,
						- protocol_id_1_= user information layer 1 protocol,
						- syn_asyn_		= synchronous/asynchronous,
						- negotiation_	= negotiation,
						- user_rate_	= user rate,
						- inter_rate_	= intermediate rate,
						- nic_tx_		= network independent clock (NIC) on Transmission (Tx),
						- nic_rx_		= network independent clock (NIC) on Reception (Rx),
						- fc_tx_		= flow control on Transmission (Tx),
						- fc_rx_		= flow control on Reception (Rx),
						- hdr_			= rate adaption header included or not,
						- multi_frame_	= multiple frame establishement support in DL,
						- op_mode_		= mode of operation,
						- lli_negot_	= logical link identifier negotiation,
						- assign_		= assignor/assignee,
						- band_negot_	= in-band/out-band negotiation,
						- stop_bit_		= number of stop bits,
						- data_bit_		= number of data bits,
						- parity_		= number of parity bit,
						- duplex_		= duplex mode,
						- modem_		= modem type,
						- protocol_id_2_= user information layer 2 protocol,
						- option_2_		= optional layer 2 protocol information,
						- protocol_id_3_= user information layer 3 protocol,
						- option_3_		= optional layer 3 protocol information.
			*/

#define Isdn_swd_store_llc(pr,coding_std_, trf_cap_, out_neg, trf_mode_, trf_rate_, structure_, dest_rate_,	protocol_id_1_,	syn_asyn_, negotiation_, user_rate_, inter_rate_, nic_tx_, nic_rx_, fc_tx_,	fc_rx_,	hdr_, multi_frame_,	op_mode_, lli_negot_, assign_, band_negot_, stop_bit_, data_bit_, parity_, duplex_,	modem_,	protocol_id_2_,	option_2_, protocol_id_3_, option_3_) \
		Set_offset_swd	(pr,	llc);										\
		Set_val_swd		(llc,	coding_std,		coding_std_);				\
		Set_val_swd		(llc,	trf_cap,		trf_cap_);					\
		Set_val_swd		(llc,	out_band_neg,	out_neg);					\
		Set_val_swd		(llc,	trf_mode,		trf_mode_);					\
		Set_val_swd		(llc,	trf_rate,		trf_rate_);					\
		Set_val_swd		(llc,	structure,		structure_);				\
		Set_val_swd		(llc,	dest_rate,		dest_rate_);				\
		Set_val_swd		(llc,	protocol_id_1,	protocol_id_1_);			\
		Set_val_swd		(llc,	syn_asyn,		syn_asyn_);					\
		Set_val_swd		(llc,	negotiation,	negotiation_);				\
		Set_val_swd		(llc,	user_rate,		user_rate_);				\
		Set_val_swd		(llc,	inter_rate,		inter_rate_);				\
		Set_val_swd		(llc,	nic_tx,			nic_tx_);					\
		Set_val_swd		(llc,	nic_rx,			nic_rx_);					\
		Set_val_swd		(llc,	fc_tx,			fc_tx_);					\
		Set_val_swd		(llc,	fc_rx,			fc_rx_);					\
		Set_val_swd		(llc,	hdr,			hdr_);						\
		Set_val_swd		(llc,	multi_frame,	multi_frame_);				\
		Set_val_swd		(llc,	op_mode,		op_mode_);					\
		Set_val_swd		(llc,	lli_negot,		lli_negot_);				\
		Set_val_swd		(llc,	assign,			assign_);					\
		Set_val_swd		(llc,	band_negot,		band_negot_);				\
		Set_val_swd		(llc,	stop_bit,		stop_bit_);					\
		Set_val_swd		(llc,	data_bit,		data_bit_);					\
		Set_val_swd		(llc,	parity,			parity_);					\
		Set_val_swd		(llc,	duplex,			duplex_);					\
		Set_val_swd		(llc,	modem,			modem_);					\
		Set_val_swd		(llc,	protocol_id_2,	protocol_id_2_);			\
		Set_val_swd		(llc,	option_2,		option_2_);					\
		Set_val_swd		(llc,	protocol_id_3,	protocol_id_3_);			\
		Set_val_swd		(llc,	option_3,		option_3_);					\
		Set_next_ie_swd	(llc)

			/*--------------------------------------------*/

			/*
			   Isdn_swd_store_llc : low layer compatibility

			   Second definition : DIVIDED IN 2 PARTS for compilers which
			   do not accept so many parameters in macros :

					Isdn_swd_store_llc_1_4 : octet 1 to octet 4 included,

						INPUT :
							- pr			= primitive ID,
							- coding_std_	= coding standard,
							- trf_cap_		= information transfer capability,
							- out_neg		= out band negotiation,
							- trf_mode_		= transfer mode,
							- trf_rate_		= information transfer rate,
							- structure_	= structure,
							- dest_rate_	= destination rate,

					Isdn_swd_store_llc_5_7 : octet 5 to octet 7 included,

						- protocol_id_1_= user information layer 1 protocol,
						- syn_asyn_		= synchronous/asynchronous,
						- negotiation_	= negotiation,
						- user_rate_	= user rate,
						- inter_rate_	= intermediate rate,
						- nic_tx_		= network independent clock (NIC) on Transmission (Tx),
						- nic_rx_		= network independent clock (NIC) on Reception (Rx),
						- fc_tx_		= flow control on Transmission (Tx),
						- fc_rx_		= flow control on Reception (Rx),
						- hdr_			= rate adaption header included or not,
						- multi_frame_	= multiple frame establishement support in DL,
						- op_mode_		= mode of operation,
						- lli_negot_	= logical link identifier negotiation,
						- assign_		= assignor/assignee,
						- band_negot_	= in-band/out-band negotiation,
						- stop_bit_		= number of stop bits,
						- data_bit_		= number of data bits,
						- parity_		= number of parity bit,
						- duplex_		= duplex mode,
						- modem_		= modem type,
						- protocol_id_2_= user information layer 2 protocol,
						- option_2_		= optional layer 2 protocol information,
						- protocol_id_3_= user information layer 3 protocol,
						- option_3_		= optional layer 3 protocol information.
			*/

#define Isdn_swd_store_llc_1_4(pr,coding_std_, trf_cap_, out_neg, trf_mode_, trf_rate_, structure_, dest_rate_)\
		Set_offset_swd	(pr,	llc);										\
		Set_val_swd		(llc,	coding_std,		coding_std_);				\
		Set_val_swd		(llc,	trf_cap,		trf_cap_);					\
		Set_val_swd		(llc,	out_band_neg,	out_neg);					\
		Set_val_swd		(llc,	trf_mode,		trf_mode_);					\
		Set_val_swd		(llc,	trf_rate,		trf_rate_);					\
		Set_val_swd		(llc,	structure,		structure_);				\
		Set_val_swd		(llc,	dest_rate,		dest_rate_)

#define Isdn_swd_store_llc_5_7(protocol_id_1_,	syn_asyn_, negotiation_, user_rate_, inter_rate_, nic_tx_, nic_rx_, fc_tx_,	fc_rx_,	hdr_, multi_frame_,	op_mode_, lli_negot_, assign_, band_negot_, stop_bit_, data_bit_, parity_, duplex_,	modem_,	protocol_id_2_,	option_2_, protocol_id_3_, option_3_) \
		Set_val_swd		(llc,	protocol_id_1,	protocol_id_1_);			\
		Set_val_swd		(llc,	syn_asyn,		syn_asyn_);					\
		Set_val_swd		(llc,	negotiation,	negotiation_);				\
		Set_val_swd		(llc,	user_rate,		user_rate_);				\
		Set_val_swd		(llc,	inter_rate,		inter_rate_);				\
		Set_val_swd		(llc,	nic_tx,			nic_tx_);					\
		Set_val_swd		(llc,	nic_rx,			nic_rx_);					\
		Set_val_swd		(llc,	fc_tx,			fc_tx_);					\
		Set_val_swd		(llc,	fc_rx,			fc_rx_);					\
		Set_val_swd		(llc,	hdr,			hdr_);						\
		Set_val_swd		(llc,	multi_frame,	multi_frame_);				\
		Set_val_swd		(llc,	op_mode,		op_mode_);					\
		Set_val_swd		(llc,	lli_negot,		lli_negot_);				\
		Set_val_swd		(llc,	assign,			assign_);					\
		Set_val_swd		(llc,	band_negot,		band_negot_);				\
		Set_val_swd		(llc,	stop_bit,		stop_bit_);					\
		Set_val_swd		(llc,	data_bit,		data_bit_);					\
		Set_val_swd		(llc,	parity,			parity_);					\
		Set_val_swd		(llc,	duplex,			duplex_);					\
		Set_val_swd		(llc,	modem,			modem_);					\
		Set_val_swd		(llc,	protocol_id_2,	protocol_id_2_);			\
		Set_val_swd		(llc,	option_2,		option_2_);					\
		Set_val_swd		(llc,	protocol_id_3,	protocol_id_3_);			\
		Set_val_swd		(llc,	option_3,		option_3_);					\
		Set_next_ie_swd	(llc)

			/*--------------------------------------------*/

			/*
			   Isdn_swd_store_llc_V110 : low layer compatibility for
											V110 rate adaption :

					INPUT :
						- pr			= primitive ID,
						- coding_std_	= coding standard,
						- trf_cap_		= information transfer capability,
						- out_neg		= out band negotiation,
						- trf_mode_		= transfer mode,
						- trf_rate_		= information transfer rate,
						- structure_	= structure,
						- dest_rate_	= destination rate,
						- syn_asyn_		= synchronous/asynchronous,
						- negotiation_	= negotiation,
						- user_rate_	= user rate,
						- inter_rate_	= intermediate rate,
						- nic_tx_		= network independent clock (NIC) on Transmission (Tx),
						- nic_rx_		= network independent clock (NIC) on Reception (Rx),
						- fc_tx_		= flow control on Transmission (Tx),
						- fc_rx_		= flow control on Reception (Rx),
						- stop_bit_		= number of stop bits,
						- data_bit_		= number of data bits,
						- parity_		= number of parity bit,
						- duplex_		= duplex mode,
						- modem_		= modem type,
						- protocol_id_2_= user information layer 2 protocol,
						- protocol_id_3_= user information layer 3 protocol,

			*/

#define Isdn_swd_store_llc_V110(pr, coding_std_, trf_cap_, out_neg, trf_mode_, trf_rate_, structure_, dest_rate_, syn_asyn_, negotiation_, user_rate_, inter_rate_, nic_tx_, nic_rx_, fc_tx_, fc_rx_, stop_bit_, data_bit_, parity_, duplex_, modem_, protocol_id_2_, protocol_id_3_) \
		Isdn_swd_store_llc_1_4(pr,			coding_std_,	trf_cap_,			\
							out_neg,		trf_mode_,		trf_rate_,			\
							structure_,		dest_rate_);						\
		Isdn_swd_store_llc_5_7(PROTOCOL_ID_1_V110,	syn_asyn_,	negotiation_,	\
							user_rate_,		inter_rate_,	nic_tx_,			\
							nic_rx_,		fc_tx_,			fc_rx_,				\
							BYTE_VAL_NIL,	BYTE_VAL_NIL,	BYTE_VAL_NIL,		\
							BYTE_VAL_NIL,	BYTE_VAL_NIL,	BYTE_VAL_NIL,		\
							stop_bit_,		data_bit_,		parity_,			\
							duplex_,		modem_,			protocol_id_2_,		\
							BYTE_VAL_NIL,	protocol_id_3_,	BYTE_VAL_NIL)

			/*--------------------------------------------*/

			/*
			   Isdn_swd_store_llc_V120 : low layer compatibility for
											V120 rate adaption :

					INPUT :
						- pr			= primitive ID,
						- coding_std_	= coding standard,
						- trf_cap_		= information transfer capability,
						- out_neg		= out band negotiation,
						- trf_mode_		= transfer mode,
						- trf_rate_		= information transfer rate,
						- structure_	= structure,
						- dest_rate_	= destination rate,
						- syn_asyn_		= synchronous/asynchronous,
						- negotiation_	= negotiation,
						- user_rate_	= user rate,
						- hdr_			= rate adaption header included or not,
						- multi_frame_	= multiple frame establishement support in DL,
						- op_mode_		= mode of operation,
						- lli_negot_	= logical link identifier negotiation,
						- assign_		= assignor/assignee,
						- band_negot_	= in-band/out-band negotiation,
						- stop_bit_		= number of stop bits,
						- data_bit_		= number of data bits,
						- parity_		= number of parity bit,
						- duplex_		= duplex mode,
						- modem_		= modem type,
						- protocol_id_2_= user information layer 2 protocol,
						- protocol_id_3_= user information layer 3 protocol,

			*/

#define Isdn_swd_store_llc_V120(pr, coding_std_, trf_cap_, out_neg, trf_mode_, trf_rate_, structure_, dest_rate_, syn_asyn_, negotiation_, user_rate_, hdr_, multi_frame_, op_mode_, lli_negot_, assign_, band_negot_, stop_bit_, data_bit_, parity_, duplex_, modem_, protocol_id_2_, protocol_id_3_) \
		Isdn_swd_store_llc_1_4(pr,			coding_std_,	trf_cap_,			\
							out_neg,		trf_mode_,		trf_rate_,			\
							structure_,		dest_rate_);						\
		Isdn_swd_store_llc_5_7(PROTOCOL_ID_1_V120,								\
							syn_asyn_,		negotiation_,	user_rate_,			\
							BYTE_VAL_NIL,	BYTE_VAL_NIL,	BYTE_VAL_NIL,		\
							BYTE_VAL_NIL,	BYTE_VAL_NIL,	hdr_,				\
							multi_frame_,	op_mode_,		lli_negot_,			\
							assign_,		band_negot_,	stop_bit_,			\
							data_bit_,		parity_,		duplex_,			\
							modem_,			protocol_id_2_, BYTE_VAL_NIL,		\
							protocol_id_3_,	BYTE_VAL_NIL)

/*-------------------------------------------------------------------------*/

			/*
				Isdn_swd_store_more_data : more data

					INPUT :
						- pr		= primitive ID,

			*/

#define Isdn_swd_store_more_data(pr) \
		Set_offset_swd	(pr,		more_data)

/*-------------------------------------------------------------------------*/

			/*
				Isdn_swd_store_notif_ind : notification indicator

					INPUT :
						- pr	= primitive ID,
						- descr	= description.

			*/

#define Isdn_swd_store_notif_ind(pr, descr)										\
		Set_offset_swd	(pr,		notif_ind);										\
		Set_val_swd		(notif_ind,	notif_descr,	descr);							\
		Set_next_ie_swd	(notif_ind)

/*-------------------------------------------------------------------------*/

			/*
				Isdn_swd_store_progress_ind : progress indicator

					INPUT :
						- pr				= primitive ID,
						- progress_loc_		= location,
						- progress_descr_	= description.

			*/

#define Isdn_swd_store_progress_ind(pr, progress_loc_, progress_descr_) \
		Set_offset_swd	(pr,			progress_ind);								\
		Set_val_swd		(progress_ind,	progress_loc,	progress_loc_);				\
		Set_val_swd		(progress_ind,	progress_descr,	progress_descr_);			\
		Set_next_ie_swd	(progress_ind)

/*-------------------------------------------------------------------------*/

			/*
				Isdn_swd_store_restart_ind : restart indicator

					INPUT :
						- pr	= primitive ID,
						- class_= class.

			*/

#define Isdn_swd_store_restart_ind(pr, class_)									\
		Set_offset_swd	(pr,			restart_ind);								\
		Set_val_swd		(restart_ind,	restart_class,	class_);					\
		Set_next_ie_swd	(restart_ind)

/*-------------------------------------------------------------------------*/

			/*
				Isdn_swd_store_uui : user to user information IE

					INPUT :
						- pr				= primitive ID,
						- protocol_discr_	= protocol discriminator,
						- p_text 			= address of the user info.
			*/

#define Isdn_swd_store_uui(pr, protocol_discr_, p_text)							\
		Set_offset_swd	(pr,	uui);												\
		Set_val_swd		(uui,	protocol_discr,	protocol_discr_);					\
		Set_str_swd		(uui, 	text,	p_text,	SWD_MX_SZ_UUI+1)

/*EOF*/

