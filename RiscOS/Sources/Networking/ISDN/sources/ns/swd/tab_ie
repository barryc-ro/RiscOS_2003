		/*
			Table of table items - the items are ordered according to the
			table ID number : table ID = index of the table.
		*/

Ns_swd_table_or_function (tabl, SWD_TAB_TABL)	/* { */
	Ns_swd_tab_tabl_entry (T_NIL,						P_NIL)
	Ns_swd_tab_tabl_entry (SWD_T_ADD_DIGIT,				SWD_ADD_DIGIT_S)
	Ns_swd_tab_tabl_entry (SWD_T_ASSIGN,				SWD_ASSIGN_S)
	Ns_swd_tab_tabl_entry (SWD_T_BAND,					SWD_BAND_S)
