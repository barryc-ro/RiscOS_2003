	Ns_swd_tab_tabl_entry (SWD_T_DUPLEX,				SWD_DUPLEX_S)
	Ns_swd_tab_tabl_entry (SWD_T_ESTABLISHMENT,			SWD_ESTABLISHMENT_S)
	Ns_swd_tab_tabl_entry (SWD_T_FLOW_CONTROL_RX,		SWD_FLOW_CONTROL_RX_S)
	Ns_swd_tab_tabl_entry (SWD_T_FLOW_CONTROL_TX,		SWD_FLOW_CONTROL_TX_S)
