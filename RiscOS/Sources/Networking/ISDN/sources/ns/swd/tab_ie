	Ns_swd_tab_tabl_entry (SWD_T_NIC_TX,				SWD_NIC_TX_S)
	Ns_swd_tab_tabl_entry (SWD_T_NOTIF_IND_DESCR,		SWD_NOTIF_IND_DESCR_S)
	Ns_swd_tab_tabl_entry (SWD_T_OP_MODE,				SWD_OP_MODE_S)
