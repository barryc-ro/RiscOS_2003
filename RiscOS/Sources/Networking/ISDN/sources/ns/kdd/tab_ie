	Ns_kdd_tab_tabl_entry (KDD_T_CALLED_NB_PLAN,		KDD_CALLED_NB_PLAN_S)
	Ns_kdd_tab_tabl_entry (KDD_T_CALLED_NB_TYPE,		KDD_CALLED_NB_TYPE_S)
	Ns_kdd_tab_tabl_entry (KDD_T_CALLING_NB_PLAN,		KDD_CALLING_NB_PLAN_S)
	Ns_kdd_tab_tabl_entry (KDD_T_CALLING_NB_PRES,		KDD_CALLING_NB_PRES_S)
	Ns_kdd_tab_tabl_entry (KDD_T_CALLING_NB_TYPE,		KDD_CALLING_NB_TYPE_S)
