	Ns_kdd_tab_tabl_entry (T_NIL,						P_NIL)
	Ns_kdd_tab_tabl_entry (KDD_T_ASSIGN,				KDD_ASSIGN_S)
	Ns_kdd_tab_tabl_entry (KDD_T_BAND,					KDD_BAND_S)
	Ns_kdd_tab_tabl_entry (KDD_T_C_C,					KDD_C_C_S)
