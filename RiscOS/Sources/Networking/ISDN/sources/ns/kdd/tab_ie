/*=========================================================================*/

		/* Table of list of tables */

Ns_kdd_table_or_function (tabx_list, KDD_TAB_C_V_S)	/* { */
	Ns_kdd_tabx_list_entry (KDD_C_V_NORMAL_EVENT_S)
	Ns_kdd_tabx_list_entry (KDD_C_V_RESOURCE_UNAVAILABLE_S)
	Ns_kdd_tabx_list_entry (KDD_C_V_SERVICE_NOT_AVAILABLE_S)
	Ns_kdd_tabx_list_entry (KDD_C_V_SERVICE_NOT_IMPLEMENTED_S)
	Ns_kdd_tabx_list_entry (KDD_C_V_INVALID_MESSAGE_S)
	Ns_kdd_tabx_list_entry (KDD_C_V_PROTOCOL_ERROR_S)
	Ns_kdd_tabx_list_entry (KDD_C_V_INTERWORKING_S)
	Ns_kdd_tabx_list_entry (KDD_C_V_NORMAL_EVENT_0_S)
Ns_kdd_end_table_or_function /* } */

Ns_kdd_table_or_function (tabx_list, KDD_TAB_CHAN_SEL_S)	/* { */
	Ns_kdd_tabx_list_entry (KDD_CHAN_SEL_BRI_S)
	Ns_kdd_tabx_list_entry (KDD_CHAN_SEL_PRI_S)
Ns_kdd_end_table_or_function /* } */

/*-------------------------------------------------------------------------*/

		/* Table of table items (indexed tables )*/

Ns_kdd_table_or_function (tabx, KDD_TAB_TABX)	/* { */
	Ns_kdd_tab_tabx_entry (T_NIL,				P_NIL)
	Ns_kdd_tab_tabx_entry (KDD_TX_C_V,			KDD_TAB_C_V_S)
	Ns_kdd_tab_tabx_entry (KDD_TX_CHAN_SEL,		KDD_TAB_CHAN_SEL_S)
Ns_kdd_end_table_or_function /* } */

/*

IE description structures :
-------------------------

					ie			tab_ie
	===============================================================
	| codeset | id | p_prog | size | p_struc | p_init| repetition |
	|---------|----|--------|------|---------|-------|------------|  meta-prog
	|         |    |  X----------------------------------------------> START
	|---------|----|--------|------|---------|-------|------------|       XXXXX
	|         |    |        |      |         |       |            |       YYYYY
	|---------|----|--------|------|---------|-------|------------|    END
	|         |    |        |      |         |       |            |
	|---------|----|--------|------|---------|-------|------------|
	|         |    |        |  X   |   X------------------------------> i_xxxx
	|---------|----|--------|--|---|---------|-------|------------|         Œ internal
	|         |    |        |  ---------------------------------------------| data
	|---------|----|--------|------|---------|-------|------------|         v structure
	|         |    |        |      |         |       |            |
	|---------|----|--------|------|---------|-------|------------|
	|         |    |        |      |         |   X--------------------> df_spdu_kdd_i_xxxx
	|---------|----|--------|------|---------|-------|------------|       Œ implicit   and df_ssdu_kdd_i_xxx
	|         |    |        |      |         |       |            |       | value or
	|---------|----|--------|------|---------|-------|------------|       v  P_NIL


		 Information Element (IE) table of item :
					- codeset,
					- ie identifier;
					- program sequence address,
					- internal structure size,
					- internal structure address,
					- initialization structure address,
					- maximum number of repeated IE.

*/

Ns_kdd_table_or_function (ie, KDD_TAB_IE)	/* { */

Ns_kdd_tab_ie_entry (CODESET_0,BC,					&KDD_BC_P,					i_bc,					&df_spdu_kdd_i_bc,					&df_ssdu_kdd_i_bc,					KDD_MX_BC)
Ns_kdd_tab_ie_entry (CODESET_0,CAUSE,				&KDD_CAUSE_P,				i_cause,				&df_spdu_kdd_i_cause,				&df_ssdu_kdd_i_cause,				KDD_MX_CAUSE)
Ns_kdd_tab_ie_entry (CODESET_0,CALL_STATE,			&KDD_CALL_STATE_P,			i_call_state,			&df_spdu_kdd_i_call_state,			&df_ssdu_kdd_i_call_state,			KDD_MX_CALL_STATE)
Ns_kdd_tab_ie_entry (CODESET_0,CHAN_ID,				&KDD_CHAN_ID_P,				i_chan_id,				&df_spdu_kdd_i_chan_id,				&df_ssdu_kdd_i_chan_id,				KDD_MX_CHAN_ID)
Ns_kdd_tab_ie_entry (CODESET_0,PROGRESS_IND,		&KDD_PROGRESS_IND_P,		i_progress_ind,			&df_spdu_kdd_i_progress_ind,		&df_ssdu_kdd_i_progress_ind,		KDD_MX_PROGRESS_IND)

Ns_kdd_tab_ie_entry (CODESET_0,CALLING_NB,			&KDD_CALLING_NB_P,			i_calling_nb,			&df_spdu_kdd_i_calling_nb,			&df_ssdu_kdd_i_calling_nb,			KDD_MX_CALLING_NB)
Ns_kdd_tab_ie_entry (CODESET_0,CALLING_SUB_ADD,		&KDD_CALLING_SUB_ADD_P,		i_calling_sub_add,		&df_spdu_kdd_i_calling_sub_add,		&df_ssdu_kdd_i_calling_sub_add,		KDD_MX_CALLING_SUB_ADD)
Ns_kdd_tab_ie_entry (CODESET_0,CALLED_NB,			&KDD_CALLED_NB_P,			i_called_nb,			&df_spdu_kdd_i_called_nb,			&df_ssdu_kdd_i_called_nb,			KDD_MX_CALLED_NB)
Ns_kdd_tab_ie_entry (CODESET_0,CALLED_SUB_ADD,		&KDD_CALLED_SUB_ADD_P,		i_called_sub_add,		&df_spdu_kdd_i_called_sub_add,		&df_ssdu_kdd_i_called_sub_add,		KDD_MX_CALLED_SUB_ADD)

Ns_kdd_tab_ie_entry (CODESET_0,RESTART_IND,			&KDD_RESTART_IND_P,			i_restart_ind,			&df_spdu_kdd_i_restart_ind,			&df_ssdu_kdd_i_restart_ind,			KDD_MX_RESTART_IND)
Ns_kdd_tab_ie_entry (CODESET_0,LLC,					&KDD_LLC_P,					i_llc,					&df_spdu_kdd_i_llc,					&df_ssdu_kdd_i_llc,					KDD_MX_LLC)
Ns_kdd_tab_ie_entry (CODESET_0,HLC,					&KDD_HLC_P,					i_hlc,					&df_spdu_kdd_i_hlc,					&df_ssdu_kdd_i_hlc,					KDD_MX_HLC)
Ns_kdd_tab_ie_entry (CODESET_0,UUI,					&KDD_UUI_P,					i_uui,					&df_spdu_kdd_i_uui,					&df_ssdu_kdd_i_uui,					KDD_MX_UUI)

#if MESSAGE_HEADER_IE == ON
		Ns_kdd_tab_ie_msg_header			
#endif

Ns_kdd_end_table_or_function /* } */

