			/* Numbering plan */

#define PLAN_UNKNOWN						G_4 (0,0,0,0)			/* unknown */
#define PLAN_ISDN							G_4 (0,0,0,1)			/* ISDN/telephony numbering plan (CCITT E.164/E.163) */
#define PLAN_TELEPHONE						G_4 (0,0,1,0)			/* telephony - not in CEPT */
