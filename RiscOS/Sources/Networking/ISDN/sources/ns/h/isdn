#define D_5									G_7 (0,1,1,0,1,0,1)		/* 5 */
#define D_6									G_7 (0,1,1,0,1,1,0)		/* 6 */
#define D_7									G_7 (0,1,1,0,1,1,1)		/* 7 */
#define D_8									G_7 (0,1,1,1,0,0,0)		/* 8 */
#define D_9									G_7 (0,1,1,1,0,0,1)		/* 9 */

#define D_STAR								G_7 (0,1,0,1,0,1,0)		/* * */
#define D_NUMBER							G_7 (0,1,0,0,0,1,1)		/* # */
#define D_A									G_7 (1,1,0,0,0,0,1)		/* A */
#define D_B									G_7 (1,1,0,0,0,1,0)		/* B */
#define D_C									G_7 (1,1,0,0,0,1,1)		/* C */
#define D_D									G_7 (1,1,0,0,1,0,0)		/* D */

#define MAXI_IA5							G_7 (1,1,1,1,1,1,1)		/* IA5 higher value */
#define MAXI_ASCII							G_7 (1,1,1,1,1,1,1)		/* ASCII higher value */

			/* Calling number presentation indicator */

#define PRES_ALLOWED						G_2 (0,0)	/* presentation allowed */
#define PRES_RESTRICTED						G_2 (0,1)	/* presentation restricted */
#define PRES_NOT_AVAILABLE					G_2 (1,0)	/* number not available due to interworking */
