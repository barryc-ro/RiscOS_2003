#ifndef NS_ISDN_H
#define NS_ISDN_H

/*-------------------------------------------------------------------------*/

					/* NS information elements fields values */

/*-------------------------------------------------------------------------*/

								/* Common coding */

					/* Type of call */

#define ANY_CALL							'A'						/* any call */
#define INCOMING_CALL						'I'						/* incoming call only */
#define OUTGOING_CALL						'O'						/* outgoing call only */
#define MIXED_CALL							'M'						/* mixed call (incoming and outgoing) */

#define FIELD_NIL 							G_8 (1,1,1,1,1,1,1,1)

					/* Coding standard */

#define CODING_STD_LGTH						2
#define CODING_STD_CCITT					G_2 (0,0)				/* CCITT */
#define CODING_STD_RESERVED					G_2 (0,1)				/* reserved */
#define	CODING_STD_ECMA						CODING_STD_RESERVED		/* ECMA standardized coding */
#define CODING_STD_NATIONAL					G_2 (1,0)				/* national (satellite network specific) */
#define CODING_STD_SPF						G_2 (1,1)				/* specific  */
#define CODING_STD_TFTS						G_2 (1,1)				/* terrestrial flight */

/*-------------------------------------------------------------------------*/
						/* ------------------------ */
						/* IE in alphabetical order */
						/* ------------------------ */
/*-------------------------------------------------------------------------*/

					/* Adjunct control (AT5) */

#define ADJUNCT_OFF							G_8 (0,0,0,0,0,0,0,0)	/* adjunct status is off */
#define ADJUNCT_ON							G_8 (0,0,0,0,0,0,0,1)	/* adjunct status is on */

/*-------------------------------------------------------------------------*/

					/* Advice of charge (NTT / BG_V1 / SWD_TVKT) */

/* NTT */

#define TOTAL_CHARGE						G_4 (0,0,1,0)			/* total charge */

/* BG_V1 / SWD_TVKT */

#define	NATIONAL_STD						G_1	(0)					/* bit 4, octet 3 is coded to 0 when national coding std is used (BG_V1) */
#define	CEPT_STD							G_1	(1)					/* else, bit 4, octet 3 is coded to 1 (BG_V1) */

			/* tax information type */

#define UNITS_TAX							G_8 (0,0,0,0,0,0,0,1)	/* TR6 */

	/* Type of charging */

#define	FREE_OF_CHARGE						G_3	(0,0,0)				/* free of charge */
#define	CHARGE_ON_DURATION					G_3	(0,0,1)				/* charge on duration */
#define	CHARGE_ON_ANSWER					G_3 (0,1,0)				/* charge on answer */
#define	CHARGE_INCREMENT					G_3 (0,1,1)				/* charge increment */
#define	SUB_TOTAL_CHARGE					G_3 (1,0,0)				/* (sub-) total charge */
#define	CHARGE_METER_VALUE					G_3	(1,0,1)				/* charge meter value (SWD_TVKT) */

	/* Charging period */

#define	CHARGING_PERIOD_0_1_S				G_7	(0,0,0,1,0,0,0)		/* 0.1 second */
#define	CHARGING_PERIOD_1_S					G_7	(0,0,0,1,0,0,1)		/* 1 second */
#define	CHARGING_PERIOD_10_S				G_7	(0,0,0,1,0,1,0)		/* 10 seconds */
#define	CHARGING_PERIOD_60_S				G_7	(0,0,0,1,0,1,1)		/* 1 minute */

/*-------------------------------------------------------------------------*/
					/* Associated status (AT5) */

#define SETUP_STATUS						G_8 (0,0,0,0,0,1,0,1)	/* setup status */
#define CONNECT_STATUS						G_8 (0,0,0,0,0,1,1,1)	/* connect status */
#define HOLD_STATUS							G_8 (0,0,0,0,1,0,0,0)	/* hold status */
#define RECONNECT_STATUS					G_8 (0,0,0,0,1,1,0,0)	/* reconnect status */
#define EXCLUSION_STATUS					G_8 (0,0,0,0,1,1,0,1)	/* exclusion status */
#define CONNECT_DENIED_STATUS				G_8 (0,0,0,0,1,1,1,0)	/* connect denied status */
#define CLEARING_DENIED_STATUS				G_8 (0,0,0,0,1,1,1,1)	/* clearing denied status */

/*-------------------------------------------------------------------------*/

					/* Bearer capability & Low Layer Compatibility */

	/* Information transfer attributes */

		/* 1 - Information tansfer mode */

#define TRF_MODE_LGTH						2
#define TRF_MODE_CIRCUIT					G_2 (0,0)				/* circuit mode */
#define TRF_MODE_PACKET						G_2 (1,0)				/* packet mode */

		/* 2 - Information transfer rate */

#define TRF_RATE_LGTH						5
#define TRF_RATE_PACKET_MODE				G_5 (0,0,0,0,0)			/* packet mode calls */
#define	TRF_RATE_INDEP_SIG_CONNECTION		G_5 (0,0,0,0,0)			/* call independent signalling connection */
#define TRF_RATE_64KBS						G_5 (1,0,0,0,0)			/* 64 kbits/s */
#define TRF_RATE_2x64KBS					G_5 (1,0,0,0,1)			/* 2 x 64 kbits/s */
#define TRF_RATE_384KBS						G_5 (1,0,0,1,1)			/* 384 kbits/s */
#define TRF_RATE_1536KBS					G_5 (1,0,1,0,1)			/* 1536 kbits/s */
#define TRF_RATE_1920KBS					G_5 (1,0,1,1,1)			/* 1920 kbits/s */
#define TRF_RATE_MULTI_RATE					G_5 (1,1,0,0,0)			/* multirate */

		/* 3 - Information transfer capability */

#define TRF_CAP_LGTH						5
#define TRF_CAP_SPEECH						G_5 (0,0,0,0,0)			/* speech */
#define TRF_CAP_UNRESTRICTED				G_5 (0,1,0,0,0)			/* unrestricted digital information */
#define TRF_CAP_RESTRICTED 					G_5 (0,1,0,0,1)			/* restricted digital information - NOT aplicable in CEPT for BC */
#define TRF_CAP_3_1_AUDIO 					G_5 (1,0,0,0,0)			/* 3.1 kHz audio */
#define TRF_CAP_7_AUDIO 					G_5 (1,0,0,0,1)			/* 7 kHz audio */
#define TRF_CAP_VIDEO 						G_5 (1,1,0,0,0)			/* video */

		/* 3-1 - Information transfer capability network specific */

#define TRF_CAP_NET_SPF_SPEECH				G_5 (0,0,0,0,1)			/* Telephone/Speech (64 kbps, PCM, A law Voice only) */
#define TRF_CAP_NET_SPF_FAX_ANALOG			G_5 (0,0,0,1,0)			/* Circuit mode Fax analog */
#define TRF_CAP_NET_SPF_MODEM_ANALOG		G_5	(0,0,0,1,1)			/* Circuit mode Modem analog */
#define TRF_CAP_NET_SPF_TDD_ANALOG			G_5 (0,0,1,0,0)			/* Circuit mode Telecommunications Devices for the Deaf analog */
#define TRF_CAP_NET_SPF_DATA				G_5 (0,1,0,0,0)			/* Digital mode data */

		/* 4 - Structure */

#define STRUCTURE_DEFAULT					G_3 (0,0,0)				/* default */
#define STRUCTURE_8KHZ_INT					G_3 (0,0,1)				/* 8kHz integrity */
#define STRUCTURE_SDU_INT					G_3 (1,0,0)				/* service data unit integrity */
#define STRUCTURE_UNSTRUCTURED				G_3 (1,1,1)				/* unstructured */

		/* 5 - Establishment of communication */

#define EST_DEMAND							G_2 (0,0)				/* demand */
#define EST_PERMANENT						G_2	(0,1)				/* permanent establishment */

		/* 6 - Communication configuration */

#define CONFIG_POINT_TO_POINT				G_2 (0,0)				/* point to point */
/* #define CONFIG_MULTIPOINT				G_2 (1,0)				 multipoint - NOT IMPLEMENTED */

		/* 7 - Symmetry */

#define BIDIRECTIONAL_SYMMETRY				G_2 (0,0)				/* bidirectional symmetry */
#define BIDIRECTIONAL_ASYMMETRY				G_2 (0,1)				/* bidirectional asymmetry - AT5 */
#define UNIDIRECTIONAL_ORIG_DEST			G_2 (1,0)				/* unidirectional origin to destination - AT5 */
#define UNIDIRECTIONAL_DEST_ORIG			G_2 (1,1)				/* unidirectional destination to origine - AT5 */

	/* Access attributes */

		/* 8 - Access channel rate */

		/* 9.1 - Signalling access protocol */

		/* 9.2 - Information access protocol */

#define LAYER_ID_LGTH						2
#define LAYER_ID_1							G_2 (0,1)				/* layer 1 identification */
#define LAYER_ID_2							G_2 (1,0)				/* layer 2 identification */
#define LAYER_ID_3							G_2 (1,1)				/* layer 3 identification */

			/* Protocol ID layer 1 */

#define PROTOCOL_ID_1_LGTH					5
#define PROTOCOL_ID_1_I412					G_5 (0,0,0,0,0)			/* I.412 - NOT APPLICABLE IN CEPT */
#define PROTOCOL_ID_1_V110					G_5 (0,0,0,0,1) 		/* CCITT V.110/X.30 */
#define PROTOCOL_ID_1_RA_I461				G_5 (0,0,0,0,1) 		/* AT5 - rate adaption as in I.461 */
#define PROTOCOL_ID_1_G711_u				G_5 (0,0,0,1,0)			/* CCITT G.711 u-law - NOT APPLICABLE IN CEPT */
#define PROTOCOL_ID_1_G711_A				G_5 (0,0,0,1,1)			/* CCITT G.711 A-law */
#define PROTOCOL_ID_1_G721					G_5 (0,0,1,0,0)			/* CCCITT G.721 */
#define PROTOCOL_ID_1_7KHZ					G_5 (0,0,1,0,1)			/* CCITT G.722 and G.725 7 kHz audio */
#define PROTOCOL_ID_1_VIDEO					G_5 (0,0,1,1,0)			/* CCCITT G.7xx 384 kbit/s video */
#define PROTOCOL_ID_1_RA_NON_CCITT			G_5 (0,0,1,1,1)			/* non-CCITT standardised rate adaptation */
#define PROTOCOL_ID_1_V120					G_5 (0,1,0,0,0)			/* CCITT rate adaptation V.120 - NOT APPLICABLE IN CEPT */
#define PROTOCOL_ID_1_X31					G_5 (0,1,0,0,1)			/* CCITT rate adaptation X.31 HDLC flag stuffing */

			/* Protocol ID layer 2 */

#define PROTOCOL_ID_2_UNDEFINED				G_5 (0,0,0,0,0)			/* undefined - NOT APPLICABLE IN CEPT */
#define PROTOCOL_ID_2_ISO_1745				G_5 (0,0,0,0,1)			/* basic mode ISO 1745 - NOT APPLICABLE IN CEPT */
#define PROTOCOL_ID_2_Q921					G_5 (0,0,0,1,0)			/* CCITT Q.921 (I.441) */
#define PROTOCOL_ID_2_X25_LL				G_5 (0,0,1,1,0)			/* CCITT X.25 link level */
#define PROTOCOL_ID_2_X25_ML				G_5 (0,0,1,1,1)			/* CCITT X.25 multilink level - NOT APPLICABLE IN CEPT */
#define PROTOCOL_ID_2_T71					G_5 (0,1,0,0,0)			/* extended LAPB; for half duplex operation (T.71) - NOT APPLICABLE IN CEPT */
#define PROTOCOL_ID_2_HDLC_ARM				G_5 (0,1,0,0,1)			/* HDLC ARM (ISO 4335) - NOT APPLICABLE IN CEPT */
#define PROTOCOL_ID_2_HDLC_NRM				G_5 (0,1,0,1,0)			/* HDLC NRM (ISO 4335) - NOT APPLICABLE IN CEPT */
#define PROTOCOL_ID_2_HDLC_ABM				G_5 (0,1,0,1,1)			/* HDLC ABM (ISO 4335) - NOT APPLICABLE IN CEPT */
#define PROTOCOL_ID_2_LAN_LLC				G_5 (0,1,1,0,0)			/* LAN logical link control (ISO 8802/2) - NOT APPLICABLE IN CEPT */
#define PROTOCOL_ID_2_X75_SLP				G_5 (0,1,1,0,1)			/* CCITT X.75 single link procedure (SLP) - NOT APPLICABLE IN CEPT */
#define	PROTOCOL_ID_2_USER_SPECIFIED		G_5 (1,0,0,0,0)			/* FRANCE TELECOM, protocol specified by the user */
#define	PROTOCOL_ID_2_DTE_DTE				G_5 (1,0,0,0,1)			/* FRANCE TELECOM, protocol ISO 7776, DTE - DTE use */

			/* Protocol ID layer 3 */

#define PROTOCOL_ID_3_UNDEFINED				G_5 (0,0,0,0,0)			/* undefined - NOT APPLICABLE IN CEPT */
#define PROTOCOL_ID_3_Q931					G_5 (0,0,0,1,0)			/* CCITT Q.931 (I.451) */
#define PROTOCOL_ID_3_X25_PL				G_5 (0,0,1,1,0)			/* CCITT X.25packet layer */
#define PROTOCOL_ID_3_ISO_8208				G_5 (0,0,1,1,1)			/* ISO 8208 (X.25 packet layer for data TE) - NOT APPLICABLE IN CEPT */
#define PROTOCOL_ID_3_ISO_8348				G_5 (0,1,0,0,0)			/* ISO 8348 (OSI connection oriented
																		network service specific subset of ISO 8208
																		and CCITT X.25 - NOT APPLICABLE IN CEPT */
#define PROTOCOL_ID_3_ISO_8473				G_5 (0,1,0,0,1)			/* ISO 8473 5OSI connectionless service - NOT APPLICABLE IN CEPT */
#define PROTOCOL_ID_3_T70					G_5 (0,1,0,1,0)			/* CCITT T.70 minimum network layer - NOT APPLICABLE IN CEPT */
#define	PROTOCOL_ID_3_ISO_TR_9577			G_5 (0,1,0,1,1)			/* ISO TR 9577 */
#define	PROTOCOL_ID_3_USER_SPECIFIED		G_5 (1,0,0,0,0)			/* FRANCE TELECOM, protocol specified by the user */

			/* Rate Adaptation (RA) */

#define RA_LGTH								5
#define RA_E_BITS							G_5 (0,0,0,0,0)			/* Rate is indicated by E-bits specified in Recommendation I.460 */
#define RA_UNDEFINED						G_5 (0,0,0,0,0)			/* AT5 - UNDEFINED */
#define RA_0_6KBS							G_5 (0,0,0,0,1)			/* 0.6 kbits/s CCITT V.6 and x.1 */
#define RA_1_2KBS							G_5 (0,0,0,1,0)			/* 1.2 kbits/s CCITT V.6 */
#define RA_2_4KBS							G_5 (0,0,0,1,1)			/* 2.4 kbits/s CCITT V.6 and X.1 */
#define RA_3_6KBS							G_5 (0,0,1,0,0)			/* 3.6 kbits/s CCITT V.6 */
#define RA_4_8KBS							G_5 (0,0,1,0,1)			/* 4.8 kbits/s CCITT V.6 and x.1 */
#define RA_7_2KBS							G_5 (0,0,1,1,0)			/* 7.2 kbits/s CCITT V.6 */
#define RA_8KBS								G_5 (0,0,1,1,1)			/* 8 kbits/s CCITT I.460 */
#define RA_9_6KBS							G_5 (0,1,0,0,0)			/* 9.6 kbits/s  CCITT V.6 and x.1 */
#define RA_14_4KBS							G_5 (0,1,0,0,1)			/* 14.4 kbits/s CCITT V.6 */
#define RA_16KBS							G_5 (0,1,0,1,0)			/* 16 kbits/s CCITT I.460 */
#define RA_19_2KBS							G_5 (0,1,0,1,1)			/* 19.2 kbits/s CCITT V.6 */
#define RA_32KBS							G_5 (0,1,1,0,0)			/* 32 kbits/s CCITT I.460 */
#define RA_38_4_KBS_NO_ETSI					G_5 (0,1,1,0,1)			/* 38.4 kbits/s extended V14 (VN3) NON ETSI */
#define RA_48KBS							G_5 (0,1,1,1,0)			/* 48 kbits/s CCITT V.6 and X.1 */
#define RA_56KBS							G_5 (0,1,1,1,1)			/* 56 kbits/s CCITT V.6 */
#define RA_64KBS							G_5 (1,0,0,0,0)			/* 64 kbits/s CCITT I.460 */
#define RA_38_4KBS							G_5 (1,0,0,0,1)			/* 38.4 kbits/s extended V14 (VN3) NON ETSI NON CCITT */
#define RA_57_6KBS							G_5 (1,0,0,1,0)			/* 57.6 kbits/s extended V14 (VN3) NON ETSI NON CCITT */
#define RA_0_1345KBS						G_5 (1,0,1,0,1)			/* 0.1345 kbits/s CCITT X.1 */
#define RA_0_100KBS							G_5 (1,0,1,1,0)			/* 0.100 kbits/s CCITT V.6 and X.1 */
#define RA_0_75_1_2KBS 						G_5 (1,0,1,1,1)			/* 0.075/1.2 kbits/s CCITT V.6 and x.1 */
#define RA_1_2_0_75KBS 						G_5 (1,1,0,0,0)			/* 1.2/0.075 kbits/s CCITT V.6 and x.1 */
#define RA_0_050KBS							G_5 (1,1,0,0,1)			/* 0.050 kbits/s CCITT V.6 and X.1 */
#define RA_0_075KBS							G_5 (1,1,0,1,0) 		/* 0.075 kbits/s CCITT V.6 and X.1 */
#define RA_0_110KBS							G_5 (1,1,0,1,1)			/* 0.110 kbits/s CCITT V.6 and X.1 */
#define RA_0_150KBS							G_5 (1,1,1,0,0)			/* 0.150 kbits/s CCITT V.6 and X.1 */
#define RA_0_200KBS							G_5 (1,1,1,0,1)			/* 0.200 kbits/s CCITT V.6 and X.1 */
#define RA_0_300KBS							G_5 (1,1,1,1,0)			/* 0.300 kbits/s CCITT V.6 and X.1 */
#define RA_12KBS							G_5 (1,1,1,1,1)			/* 12 kbits/s CCITT V.6 */

			/* Intermediate Rate Adaptation */

#define INTER_RA_UNUSED						G_2 (0,0)				/* not used */
#define INTER_RA_8KBS						G_2 (0,1)				/* 8 kbits/s */
#define INTER_RA_16KBS						G_2 (1,0)				/* 16 kbits/s */
#define INTER_RA_32KBS						G_2 (1,1)				/* 32 kbits/s */

			/* Network independent clock (NIC) on Transmission (Tx) and RECEPTION (Rx) (BT2) */

#define NIC_DATA							G_2 (1,1)				/* required to send / can accept data with NIC */
#define NIC_NOT_DATA						G_2 (0,0)				/* not required to send / cannot accept data with NIC */

			/* Network independent clock (NIC) on Transmission (Tx) */

#define NIC_RQ_TX_DATA						G_1 (1)					/* required to send data with NIC */
#define NIC_NOT_RQ_TX_DATA					G_1 (0)					/* not required to send data with NIC */

			/* Network independent clock (NIC) on Reception (Rx) */

#define NIC_ACCEPT_DATA						G_1 (1)					/* can accept data with NIC */
#define NIC_NOT_ACCEPT_DATA					G_1 (0)					/* cannot accept data with NIC */

			/* Flow control on Transmisssion (Tx) */

#define FLOW_RQ_TX_DATA						G_1 (1)					/* required to send data with flow control mechanism */
#define FLOW_NOT_RQ_TX_DATA					G_1 (0)					/* not required to send data with flow control mechanism */

			/* Flow control on Reception (Rx) */

#define FLOW_CAN_ACCEPT_DATA				G_1 (1)					/* can accept  data with flow control mechanism */
#define FLOW_CANNOT_ACCEPT_DATA				G_1 (0)					/* cannot accept data with flow control mechanism */

			/* Rate adaption header/no header - NOT APPLICABLE IN CEPT */

#define HEADER_NOT_INCLUDED					G_1 (0)					/* rate adaption header not included */
#define HEADER_INCLUDED						G_1 (1)					/* rate adaption header included */

			/* Multi frame establishment support in Data Link - NOT APPLICABLE IN CEPT */

#define MULTI_FRAME_NOT_SUPPORTED			G_1 (0)					/* not supported; only UI frames allowed */
#define MULTI_FRAME_SUPPORTED				G_1 (1)					/* supported */

			/* Mode of operation - NOT APPLICABLE IN CEPT */

#define BIT_TRANSPARENT						G_1 (0)
#define PROTOCOL_SENSITIVE					G_1 (1)

			/* Logical Link Identifier negotiation - NOT APPLICABLE IN CEPT */

#define DEFAULT_LLI							G_1 (0)					/* default, LLI=256 only */
#define FULL_PROTOCOL_LLI					G_1 (1)

			/* Message originator is : (Assignor/Assignee) - NOT APPLICABLE IN CEPT */

#define DEFAULT_ASSIGNEE					G_1 (0)
#define ASSIGNOR_ONLY						G_1 (1)

			/* In-band/Out_band negotiation - NOT APPLICABLE IN CEPT */

#define TSC									G_1 (0)					/* negotiation is done with USER INFORMATION messages
																		on a Temporary Signalling Connection */
#define LL0									G_1 (1)					/* negotiation is done In-band using Logical Link zero */

			/* Duplex mode */

#define FULL_DUPLEX							G_1 (1)					/* full duplex */
#define HALF_DUPLEX							G_1 (0)					/* half duplex */

			/* Modem type */

#define MODEM_RESERVED						G_6 (0,0,0,0,0,0)		/* reserved */
#define MODEM_V21							G_6 (0,0,0,0,0,1)		/* V.21 */
#define MODEM_V22							G_6 (0,0,0,0,1,0)		/* V.22 */
#define MODEM_V22_BIS						G_6 (0,0,0,0,1,1)		/* V.22 bis */
#define MODEM_V23							G_6 (0,0,0,1,0,0)		/* V.23 */
#define MODEM_V26							G_6 (0,0,0,1,0,1)		/* V.26 */
#define MODEM_V26_BIS						G_6 (0,0,0,1,1,0)		/* V26 bis */
#define MODEM_V26_TER						G_6 (0,0,0,1,1,1)		/* V.26 ter */
#define MODEM_V27							G_6 (0,0,1,0,0,0)		/* V.27 */
#define MODEM_V27_BIS						G_6 (0,0,1,0,0,1)		/* V.27 bis */
#define MODEM_V27_TER						G_6 (0,0,1,0,1,0)		/* V.27 ter */
#define MODEM_V29							G_6 (0,0,1,0,1,1)		/* V.29 */
#define MODEM_V32							G_6 (0,0,1,1,0,0)		/* V.32 */
#define MODEM_V35							G_6 (0,0,1,1,0,1)		/* V.35 */
#define MODEM_BELL_212A						G_6 (1,0,0,0,0,0)		/* A11 - Bell 212A */

			/* Modem type for Australian Telecom */

#define AU1_MODEM_UNDEFINED					G_6 (0,0,0,0,0,0)		/* undefined */
#define AU1_MODEM_V21						G_6 (0,0,0,1,0,0)		/* V.21 */
#define AU1_MODEM_V22						G_6 (0,0,0,1,0,1)		/* V.22 */
#define AU1_MODEM_V22_BIS					G_6 (0,0,0,0,1,0)		/* V.22 bis */
#define AU1_MODEM_V23						G_6 (0,0,0,0,0,1)		/* V.23 */
#define AU1_MODEM_V32						G_6 (0,0,0,0,1,1)		/* V.32 */

			/* Modem for France Telecom VN3/VN6 */

#define FT_MODEM_RESERVED					G_6 (0,1,0,0,0,0)		/* reserved */
