		/*--------- cause_class-value macro ----------*/

#define C_v(class, val) 					((((class) & Mask (C_C_LGTH)) << C_V_LGTH) | (val & Mask(C_V_LGTH)))

#define C_C_LGTH							3

		/* Macro to be used by Call Control */

#define C_v_user(class, val) 				((((class) & Mask (C_C_LGTH_USER)) << C_V_LGTH) | (val & Mask(C_V_LGTH)))

#define C_C_LGTH_USER						4						/* bit 7 is significant for Omnitel cause classes */

		/*-------- OMNITEL internal NS Classes -------*/

