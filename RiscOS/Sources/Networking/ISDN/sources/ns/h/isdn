			/* Calling number Screening indicator */

#define USER_PROVIDED						G_2 (0,0)				/* user-provided, not screened */
#define USER_PASSED							G_2 (0,1)				/* user-provided, verified and passed */
#define USER_FAILED							G_2 (1,0)				/* user-provided, verified and failed */
#define NETWORK_PROVIDED 					G_2 (1,1)				/* network provided */

			/* Pres_sceen macro */

#define Pres_screen(pres,screen)			((pres << 5) + screen)

