		/* B channel state */

#define	ACK_BASE							100
#define B_chan_state_ack(state)				(state + ACK_BASE)
#define B_chan_state_cancel_ack(state)		(state - ACK_BASE)

#define	Is_B_chan_state_wait_for_ack(state)	(state >= ACK_BASE)
#define	Is_maint_in_progress(int_id,b_chan)	Is_B_chan_state_wait_for_ack (Dl_b_chan_mode_int (int_id, b_chan))

#define	Is_call_upon_B_chan(int_id, b_chan)	(FALSE)

#define	MAINT_STATE_B_IS					0						/* B channel is IN service */
#define	MAINT_STATE_B_IS_MAINT_NE			1						/* B channel is IN service Maintenance Near End */
#define	MAINT_STATE_B_OOS_NE				2						/* B channel is OUT of service Near End */

#define	MAINT_STATE_B_OOS_FE				3						/* B channel is OUT of service Far End */
#define	MAINT_STATE_B_OOS_FE_ACK			(ACK_BASE+3)			/* B channel is OUT of service Far End waiting for ACK */
#define	MAINT_STATE_B_OOS_FE_REST			4						/* B channel is OUT of service Far End - Restart (National) */
#define	MAINT_STATE_B_OOS_MAINT_NE			7						/* B channel is Maintenance Near End. */
#define	MAINT_STATE_B_OOS_MAINT_NE_ACK		(ACK_BASE+7)			/* B channel is Maintenance Near End waiting for ACK */

#define	MAINT_STATE_B_MAINT_FE				8						/* B channel is Maintenance Far End. */
#define	MAINT_STATE_B_MAINT_FE_ACK			(ACK_BASE+8)			/* B channel is Maintenance Far End waiting for ACK */

#define	MAINT_STATE_B_IS_ACK				(ACK_BASE+0)			/* B channel is IN service, waiting for ACK  */
#define	MAINT_STATE_B_IS_MAINT_NE_ACK		(ACK_BASE+1)			/* B channel is IN service Maintenance Near End, waiting for ACK */

	/*-=-=-=-=-=-=-=-=-=*/
	/*	AT4 Specific	*/
	/*-=-=-=-=-=-=-=-=-=*/

#define	MAINT_STATE_B_MAINT_NE_ACK			(ACK_BASE+1)			/* B channel is in Maintenance Near End, waiting for ACK */
#define	MAINT_STATE_B_OOS_NE_ACK			(ACK_BASE+2)			/* B channel is Out Of Service service Near End, waiting for ACK */

#define	MAINT_STATE_B_UNDEF					255						/* Undefined state (internal use for Omnitel) */

