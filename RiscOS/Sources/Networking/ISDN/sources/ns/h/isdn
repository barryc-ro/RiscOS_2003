#define PLAN_NATIONAL						G_4 (1,0,0,0)			/* national standard numbering plan */
#define PLAN_PRIVATE						G_4 (1,0,0,1)			/* private numbering plan */
#define PLAN_RESERVED						G_4 (1,1,1,1)			/* reserved for extension */

#define NB_PLAN_LGTH						4

			/* Type_plan macro */

#define Type_plan(type,plan)				((type << NB_PLAN_LGTH) + plan)

			/* Number digit table */

#define D_0									G_7 (0,1,1,0,0,0,0) 	/* 0 */
#define D_1									G_7 (0,1,1,0,0,0,1)		/* 1 */
