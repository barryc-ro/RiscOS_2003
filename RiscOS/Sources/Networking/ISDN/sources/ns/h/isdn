						/* Call state */

#define STATE_NULL_0						0 						/* U0	- Null */
#define STATE_CALL_INIT_1					1						/* U1	- Call Initiated */
#define STATE_OVERLAP_SENDING_2				2						/* U2	- Overlap Sending */
#define STATE_OUTGOING_CALL_PROC_3			3						/* U3	- Outgoing Call Proceeding */
#define STATE_CALL_DELIVERED_4				4						/* U4	- Call Delivered */
#define STATE_NEGOTIATE_5					5						/* U5	- */
#define STATE_CALL_PRESENT_6				6						/* U6	- Call Present */
#define STATE_CALL_RECEIVED_7				7						/* U7	- Call Received */
#define STATE_CONNECT_REQUEST_8				8						/* U8	- Connect Request */
#define STATE_INCOMING_CALL_PROC_9			9						/* U9	- Incoming Call Proceeding */
#define STATE_ACTIVE_10						10						/* U10	- Active */
#define STATE_DISCONNECT_REQUEST_11			11						/* U11	- Disconnect Request */
#define STATE_DISCONNECT_INDICATION_12		12						/* U12	- Disconnect Indication */
#define STATE_PASSIVE_14					14						/* U14	- Passive state (1TR6) */
#define STATE_SUSPEND_REQUEST_15			15						/* U15	- Suspend Request */
#define STATE_LOCAL_SUSPEND_16				16						/* U16	- */
#define STATE_RESUME_REQUEST_17				17						/* U17	- Resume Request */
#define STATE_TONE_ACTIVE_18				18						/* U18	- Tone active */
#define STATE_RELEASE_REQUEST_19			19						/* U19	- Release Request */
#define STATE_REMOTE_FAC_REQUEST_20			20						/* U20	- */
#define STATE_LOCAL_FAC_REQUEST_21			21						/* U21	- */
#define STATE_CALL_ABORT_22					22						/* U22	- Call abort (NT2 only) */
#define	STATE_REGISTER_ACTIVE_23			23						/* U23  - Register active */
#define STATE_OVERLAP_RECEIVING_25			25						/* U25	- Overlap Sending */
#define STATE_CALL_INDEPENDENT_31			31						/* U31	- Call independent (Bearer Independent Transport) */
#define STATE_REST_0						0						/* REST0- Restart Null */
#define STATE_REST_1						61						/* REST1- Restart Request */
#define STATE_REST_2						62						/* REST2- Restart */
#define STATE_WAIT_DL						253						/* U253 - Awaiting DL connection */
#define STATE_REGISTRATION_REQUEST_254		254						/* U254 - Registration Request */
#define STATE_NIL							255						/* U255 - Null state */

#define	AUX_STATE_HOLD_IDLE_0				0						/* Auxiliary state hold idle */
#define	AUX_STATE_HOLD_RQ_1					1						/* Auxiliary state hold request */
#define	AUX_STATE_RET_RQ_2					2						/* Auxiliary state retrieve request */
#define	AUX_STATE_HOLD_ACTIVE_3				3						/* Auxiliary state hold active */

		/* D channel state */

#define	MAINT_STATE_IS						0						/* In Service */
#define	MAINT_STATE_STBY					1						/* Standby */
#define	MAINT_STATE_OOS						2						/* Out Of Service */
#define	MAINT_STATE_MOOS					3						/* Manual Out Of Service */
#define	MAINT_STATE_MB						4						/* Maintenance Busy */
#define	MAINT_STATE_WAIT					5						/* Wait for SERV_ACK */
#define	MAINT_STATE_INB						6						/* Installation busy */

		/* MSB = Active D channel state, LSB = Backup D channel state */

#define MAINT_STATE_IS_STBY					0x0001					/* Active = IS, 	Backup = STBY */
#define MAINT_STATE_STBY_IS					0x0100                  /* Active = STBY, 	Backup = IS */
#define MAINT_STATE_MB_WAIT					0x0405                  /* Active = MB, 	Backup = WAIT */
#define MAINT_STATE_WAIT_MB					0x0504                  /* Active = WAIT, 	Backup = MB */
#define MAINT_STATE_IS_OOS					0x0002                  /* Active = IS, 	Backup = OOS */
#define MAINT_STATE_OOS_IS					0x0200                  /* Active = OOS, 	Backup = IS */
#define MAINT_STATE_OOS_WAIT				0x0205                  /* Active = OOS, 	Backup = WAIT */
#define MAINT_STATE_OOS_OOS					0x0202                  /* Active = OOS, 	Backup = OOS */

/*-------------------------------------------------------------------------*/

						/* Calling and called number */

			/* Type of number */

#define NB_UNKNOWN							G_3 (0,0,0)				/* unknown */
#define NB_INTERNATIONAL					G_3 (0,0,1)				/* international number */
#define NB_NATIONAL							G_3 (0,1,0)				/* national number */
#define NB_NET_SPF							G_3 (0,1,1)				/* network specific number */
#define NB_SUBSCRIBER						G_3 (1,0,0)				/* subscriber number */
