/*
 * Generic (c.stop)
 *
 * © Acorn Computers Ltd. 1997
 */
#include <stdio.h>
#include <stdlib.h>
#include "kernel.h"
#include "swis.h"
#include <string.h>
#include "sys/errno.h"
#include "module.h"
#include "protocol.h"

_kernel_oserror *generic_stop(_kernel_swi_regs *r)
{
  	Session *ses;

	r->r[0] = 0;
	ses = find_session(r->r[1]);

  	#ifdef TRACE
  	protocol_debug("Stop SWI for session %p\n", ses);
  	#endif

	/* kill_session validates session pointer anyway */
	kill_session(ses);
  	
  	return NULL;
}
