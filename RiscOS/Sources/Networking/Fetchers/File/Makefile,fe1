# Project:   FileFetcher


# Toolflags:
CCflags = -c -depend !Depend -IC: -throwback -fahf -UTML -zps1 -zM
C++flags = -c -depend !Depend -IC: -throwback
Linkflags = -rmf -c++ -o $@
ObjAsmflags = -throwback -NoCache -depend !Depend
CMHGflags = -depend !Depend -p -throwback -IC: -d FileHdr.h
LibFileflags = -c -o $@
Squeezeflags = -o $@


# Final targets:
@.File:   @.o.module @.o.readdata @.o.ses_ctrl @.o.start @.o.status @.o.stop \
        @.o.utils @.o.messages @.o.FileHdr C:o.Stubs
        Link508 $(Linkflags) @.o.module @.o.readdata @.o.ses_ctrl @.o.start \
        @.o.status @.o.stop @.o.FileHdr @.o.utils @.o.messages C:o.Stubs
	Access $@ WR/R


# User-editable dependencies:
@.o.messages:   @.Resources.UK.Messages
        ResGen messages_file @.o.messages @.Resources.UK.Messages Resources.FileFetcher.Messages

clean:
	-wipe @.o.* ~v~cf
	-wipe @.od.* ~v~cf

# Static dependencies:
@.o.module:   @.c.module @.h.FileHdr
        cc $(ccflags) -o @.o.module @.c.module
@.o.readdata:   @.c.readdata
        cc $(ccflags) -o @.o.readdata @.c.readdata
@.o.ses_ctrl:   @.c.ses_ctrl
        cc $(ccflags) -o @.o.ses_ctrl @.c.ses_ctrl
@.o.start:   @.c.start
        cc $(ccflags) -o @.o.start @.c.start
@.o.status:   @.c.status
        cc $(ccflags) -o @.o.status @.c.status
@.o.stop:   @.c.stop
        cc $(ccflags) -o @.o.stop @.c.stop
@.o.utils:   @.c.utils
        cc $(ccflags) -o @.o.utils @.c.utils
@.o.FileHdr:   @.cmhg.FileHdr
        cmhg $(cmhgflags) -o @.o.FileHdr @.cmhg.FileHdr


# Dynamic dependencies:
o.utils:	c.utils
o.utils:	C:h.kernel
o.utils:	C:h.swis
o.utils:	h.utils
o.utils:	C:h.kernel
o.FileHdr:	cmhg.FileHdr
o.module:	c.module
o.module:	C:h.stdio
o.module:	C:h.stdlib
o.module:	C:h.kernel
o.module:	C:h.swis
o.module:	C:h.string
o.module:	C:h.errno
o.module:	h.module
o.module:	h.FileHdr
o.module:	h.utils
o.module:	C:h.kernel
o.readdata:	c.readdata
o.readdata:	C:h.kernel
o.readdata:	C:h.swis
o.readdata:	h.module
o.readdata:	h.utils
o.readdata:	C:h.kernel
o.readdata:	h.FileHdr
o.ses_ctrl:	c.ses_ctrl
o.ses_ctrl:	C:h.stdio
o.ses_ctrl:	C:h.stdlib
o.ses_ctrl:	C:h.kernel
o.ses_ctrl:	C:h.swis
o.ses_ctrl:	C:h.string
o.ses_ctrl:	C:h.errno
o.ses_ctrl:	h.module
o.start:	c.start
o.start:	C:h.stdio
o.start:	C:h.stdlib
o.start:	C:h.kernel
o.start:	C:h.swis
o.start:	C:h.string
o.start:	C:h.errno
o.start:	h.module
o.start:	h.utils
o.start:	C:h.kernel
o.status:	c.status
o.status:	C:h.stdio
o.status:	C:h.stdlib
o.status:	C:h.kernel
o.status:	C:h.swis
o.status:	C:h.string
o.status:	C:h.errno
o.status:	h.module
o.status:	h.utils
o.status:	C:h.kernel
o.stop:	c.stop
o.stop:	C:h.stdio
o.stop:	C:h.stdlib
o.stop:	C:h.kernel
o.stop:	C:h.swis
o.stop:	C:h.string
o.stop:	C:h.errno
o.stop:	h.module
