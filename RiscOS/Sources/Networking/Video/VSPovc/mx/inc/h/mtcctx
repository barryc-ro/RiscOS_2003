#ifndef MTCCTX_ORACLE
# define MTCCTX_ORACLE

#ifndef SYSI_ORACLE
#include <sysi.h>
#endif 

EXTC_START











typedef enum
{
   MTCCTXMGR,           
   MNETCTX,             
   MDSCTX,              
   MNSCCTX,             
   MZSILIST,            
   MZSILOCALDATA,       
   MZSISTATS,           
   MZSRC,               
   MDSCNVCTX,           
   MDSCPCTX,            
   MKECTX,		
   MZMGACTX,		
   MZMGSCTX,		
   MZLCTX,              
   MTTSIMCTX,		
   MZBCTX,              
   MKEVCTX,		
   MTPCTX,              
   MTUPTSLVINFO,        
   MZFCTX,              
   MTCTXLAST            
} mtcctxType;



#define MTCCTXSUCC      0                             
#define MTCCTXDUP       1                               
#define MTCCTXNOMEM     2                                   

















ub4 mtcctxInit(void);
































ub4 mtcctxPut(mtcctxType name, ub4 inst, dvoid *thectx);
























dvoid *mtcctxGet(mtcctxType name, ub4 inst);


















void mtcctxDone(void);

EXTC_END
#endif 
