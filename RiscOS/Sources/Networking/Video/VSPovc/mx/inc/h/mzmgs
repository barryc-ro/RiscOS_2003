#endif

EXTC_START

#ifndef MNRS_ORACLE
#include <mnrs.h>
#endif

#ifndef MZMGU_ORACLE
#include <mzmgu.h>
#endif
       




#define  MZMCMZSC	((ub4)0)	
#define  MZMCMZSI	((ub4)1)	
#define  MZMCMKP	((ub4)2)	
#define  MZMCUP		((ub4)3)	
#define  MZMCUT 	((ub4)4)	
#define  MZMCTEST	((ub4)5)	
#define  MZMCSRVMAX	((ub4)6)	



#define MZMGA_CMN_ONLY_SVCTYPE	((sb4)5)  
#define MZMGA_NAME_SVC_SVCTYPE	((sb4)10) 
#define MZMGA_SVC_CTRL_SVCTYPE	((sb4)11) 
#define MZMGA_STRM_SVCTYPE	((sb4)12) 
#define MZMGA_STRM_INST_SVCTYPE	((sb4)13) 
#define MZMGA_VPUMP_SVCTYPE	((sb4)14) 
#define MZMGA_DSMSTRM_SVCTYPE	((sb4)15) 
#define MZMGA_MZMSNMP_SVCTYPE	((sb4)16) 
#define MZMGA_MDS_SVCTYPE	((sb4)17) 



typedef void mzmgsStFunc( mzmguSStats *istats,
			  dvoid *arg,
			  mzmguSStats **ostats );




typedef void mzmgsAdminFunc( ub4 newState, dvoid *arg );







sb4 mzmgsAddCommonMib( mnrs *svcRpc, sb4 svcType,
		      mzmgsStFunc *svcStatFunc, dvoid *svcArg );




sb4 mzmgsCommonMib( mnrs *svcRpc, sb4 svcType,
		   mzmgsStFunc *svcStatFunc, dvoid *svcArg,
		   mzmgsAdminFunc *svcAdminFunc, dvoid *svcAdminArg );




sb4 mzmgsDelCommonMib( mnrs *svcRpc );





void mzmgsStatChange( mnrs *svc, ub4 new_state );




mzmguTimeTicks mzmgsTimeStamp(void);



ub4   mzmgsCpuCs(void);


EXTC_END
