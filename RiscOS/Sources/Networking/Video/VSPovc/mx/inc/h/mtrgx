#ifndef MTRGX_ORACLE
#define MTRGX_ORACLE

#ifndef SYSI_ORACLE
#include <sysi.h>
#endif 

EXTC_START

#define ESIZE 512
#define NBRA 9

typedef struct mtrgxCtx
{
   char expbuf[ESIZE];
   char *braslist[NBRA];
   char *braelist[NBRA];
   char circf;
} mtrgxCtx;



mtrgxCtx *mtrgxComp(char *s, CONST char **errstr);


sword mtrgxExec(mtrgxCtx *ctx, char *str);         

EXTC_END
#endif 

