#ifndef YSMI_ORACLE
#define YSMI_ORACLE

#ifndef SYSI_ORACLE
#include <sysi.h>
#endif
#ifndef YSM_ORACLE
#include <ysm.h>
#endif




typedef struct ysmahdr ysmahdr;                    
typedef struct ysmfhdr ysmfhdr;                         


struct ysmahdr
{
  sb4         sz;                             
  CONST char *tag;                                     
};

struct ysmfhdr
{
  sb4      sz;                                          
  ysmfhdr *prev;                              
  ysmfhdr *next;                                  
};

struct ysmhp
{
  ysmfhdr    *roveptr;              
  sb4         limit;                             
  sb4         inuse;                                        
  sb4         maxusg;                        
  sword       bhvr;                                   
  sb4         afcnt;                             
  ysmaf       af;                                      
  ysmrf       rf;                                    
  ysmff       ff;                                            
  ysbv       *bv;                                  
  sword       nbv;                     
};


#define YSM_MIN_SIZE ysRoundVal(sizeof(ysmfhdr), SYSM_STRICT_ALIGN)

externref CONST_DATA char YSMHP_TAG[];
externref CONST_DATA char YSMEOB_TAG[];

void ysmDump(ysmhp *hp);

#endif 
