#ifndef RTSPOVS_RTSPOErrors__

  #define RTSPOVS_RTSPOErrors__
  #include <VideoCtrl.h>

  /* Error messages */

  enum
  {
    rtspovs_ERROR_CLIENT_ID_NOT_FOUND = VIDEOCONTROL_ERROR1_BASE + RTSP_OVS_OFFSET,
    rtspovs_ERROR_ALREADY_SETUP,
    rtspovs_ERROR_NOT_SETUP_YET,
    rtspovs_ERROR_MESSAGE_NOT_FOUND,
    rtspovs_ERROR_CONNECT_GENERAL,
    rtspovs_ERROR_CONNECT_WRITE,
    rtspovs_ERROR_CONNECT_READ,
    rtspovs_ERROR_CONNECT_TIMEOUT,
    rtspovs_ERROR_CONNECT_DNS,
    rtspovs_ERROR_NO_SERVER_SESSION,

    rtspovs_ERROR_MEMORY_EXHAUSTED    = vc_ERROR_MEMORY_EXHAUSTED,
    rtspovs_ERROR_IN_PROGRESS         = vc_ERROR_IN_PROGRESS,
    rtspovs_ERROR_INVALID_OPTIONS     = vc_ERROR_INVALID_OPTIONS
  };

#endif /* RTSPOVS_RTSPOErrors__ */
