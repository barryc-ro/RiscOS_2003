#ifndef RTSPOVS_Module__

  #define RTSPOVS_Module__
  #include <time.h>

  /* Important constants. See also Headers.h, in particular. */

  #define RTSPOVS_FetchScheme "rtsp:"
  #define RTSPOVS_DefaultPort (5004)

  /* Function prototypes */

  extern _kernel_oserror * module_claim_event_vector     (void);
  extern _kernel_oserror * module_release_event_vector   (void);
  extern void              module_enable_internet_event  (void);
  extern void              module_disable_internet_event (void);

  extern void            * module_realloc                (void * ptr, size_t size);

#endif /* RTSPOVS_Module__ */
