#ifndef RTSPHSAC_RTSPHErrors__
  #define RTSPHSAC_RTSPHErrors__

  #ifndef VideoControl_VideoCtrl__
    #include <VideoCtrl.h>
  #endif

  /* Error messages */

  enum
  {
    rtsphsac_ERROR_CLIENT_ID_NOT_FOUND = VIDEOCONTROL_ERROR1_BASE + RTSP_HSAC_OFFSET,
    rtsphsac_ERROR_ALREADY_SETUP,
    rtsphsac_ERROR_NOT_SETUP_YET,
    rtsphsac_ERROR_MESSAGE_NOT_FOUND,
    rtsphsac_ERROR_CONNECT_GENERAL,
    rtsphsac_ERROR_CONNECT_WRITE,
    rtsphsac_ERROR_CONNECT_READ,
    rtsphsac_ERROR_CONNECT_TIMEOUT,
    rtsphsac_ERROR_CONNECT_DNS,
    rtsphsac_ERROR_NO_SERVER_SESSION,
    rtsphsac_ERROR_UNKNOWN_VERSION,

    rtsphsac_ERROR_MEMORY_EXHAUSTED    = vc_ERROR_MEMORY_EXHAUSTED,
    rtsphsac_ERROR_IN_PROGRESS         = vc_ERROR_IN_PROGRESS,
    rtsphsac_ERROR_INVALID_OPTIONS     = vc_ERROR_INVALID_OPTIONS
  };

#endif /* RTSPHSAC_RTSPHErrors__ */
