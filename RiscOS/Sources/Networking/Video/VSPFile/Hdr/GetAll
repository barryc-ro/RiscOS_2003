        GET     Hdr:CMOS
        GET     Hdr:ModHand
        GET     Hdr:Services
        GET     Hdr:Proc
        GET     Hdr:FSNumbers		; Jon Change (was file)
        GET     Hdr:NewErrors
        GET     Hdr:MsgTrans
        GET     Hdr:Debug

	GET	Hdr:HighFSI
        GET     Hdr:MPEGCtrl
	GET	Hdr:VSP

        GET     s.Module		; Jon added s.

        END
