#include "stubs.h"
#include "socklib.h"

int socketstat(int s, struct stat *buf)
{
    _kernel_swi_regs rin, rout;

    rin.r[0] = s;
    rin.r[1] = (int)buf;

    return(_makecall(Socketstat, &rin, &rout));
}

/* EOF socketstat.c */
