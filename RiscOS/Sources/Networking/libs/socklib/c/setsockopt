#include "stubs.h"
#include "socklib.h"

int setsockopt(int s, int level, int optname, void *optval, int optlen)
{
    _kernel_swi_regs rin, rout;

    rin.r[0] = s;
    rin.r[1] = level;
    rin.r[2] = optname;
    rin.r[3] = (int)optval;
    rin.r[4] = optlen;

    return(_makecall(Setsockopt, &rin, &rout));
}

/* EOF setsockopt.c */
