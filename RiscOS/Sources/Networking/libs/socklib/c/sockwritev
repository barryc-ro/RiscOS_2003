#include "stubs.h"
#include "socklib.h"

int socketwritev(int s, struct iovec *iov, int iovcnt)
{
    _kernel_swi_regs rin, rout;

    rin.r[0] = s;
    rin.r[1] = (int)iov;
    rin.r[2] = iovcnt;

    return(_makecall(Socketwritev, &rin, &rout));
}

/* EOF sockwritev.c */
