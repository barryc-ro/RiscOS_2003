#include "stubs.h"
#include "socklib.h"

int shutdown(int s, int how)
{
    _kernel_swi_regs rin, rout;

    rin.r[0] = s;
    rin.r[1] = how;

    return(_makecall(Shutdown, &rin, &rout));
}

/* EOF shutdown.c */
