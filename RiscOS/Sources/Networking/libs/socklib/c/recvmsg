#include "stubs.h"
#include "socklib.h"

int recvmsg(int s, struct msghdr *msg, int flags)
{
    _kernel_swi_regs rin, rout;

    rin.r[0] = s;
    rin.r[1] = (int)msg;
    rin.r[2] = flags;

    return(_makecall(Recvmsg, &rin, &rout));
}

/* EOF recvmsg.c */
