#include "stubs.h"
#include "socklib.h"

int socket(int domain, int type, int protocol)
{
    _kernel_swi_regs rin, rout;

    rin.r[0] = domain;
    rin.r[1] = type;
    rin.r[2] = protocol;

    return(_makecall(Socket, &rin, &rout));
}

/* EOF socket.c */
