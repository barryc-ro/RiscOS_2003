#include <string.h>

#include "unixlib.h"

void bcopy(char *src, char *dst, int length)
{
    memcpy(dst, src, length);
}

int bcmp(char *s1, char *s2, int length)
{
    return (memcmp(s1, s2, length) != 0);
}

char *bzero(char *s, int length)
{
    return(memset(s, 0, length));
}

/* EOF memops.c */
