#include "unixlib.h"
#include "stubs.h"

int getgroups(int ngrps, int *grparray)
{
    /* shut the compiler up */
    ngrps = ngrps;

    grparray[0] = UNIX_MagicNumber_GID_Nobody;
    return(1);
}

/* EOF getgroups.c */
