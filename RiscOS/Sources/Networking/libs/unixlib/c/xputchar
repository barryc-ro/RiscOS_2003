#include "kernel.h"
#include "swis.h"

#include "unixlib.h"

char xputchar(char ch)
{
    _kernel_swi_regs r;

    r.r[0] = (int)ch;
    (void)_kernel_swi(XOS_Bit | OS_WriteC, &r, &r);

    return(ch);
}

/* EOF xputchar.c */
