#include "kernel.h"
#include "swis.h"

#include "unixlib.h"

int chdir(char *dir)
{
    _kernel_swi_regs r;

    r.r[0] = 0;
    r.r[1] = (int)dir;
    (void)_kernel_swi(XOS_Bit | OS_FSControl, &r, &r);

    return (0);
}

/* EOF chdir.c */
