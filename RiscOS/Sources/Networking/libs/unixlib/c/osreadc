#include <stdio.h>

#include "kernel.h"
#include "swis.h"

#include "unixlib.h"

int osreadc(void)
{
    _kernel_swi_regs r;

    (void)_kernel_swi(XOS_Bit | OS_ReadC, &r, &r);

    return((r.r[0] == 0x1b) ? EOF : r.r[0]);
}

/* EOF osreadc.c */
