#include "kernel.h"
#include "swis.h"

#include "unixlib.h"

void flushinput(void)
{
    _kernel_swi_regs r;

    r.r[0] = 15;
    r.r[1] = 1;
    r.r[2] = 0;
    (void)_kernel_swi(XOS_Bit | OS_Byte, &r, &r);
}

/* EOF flushinput.c */
