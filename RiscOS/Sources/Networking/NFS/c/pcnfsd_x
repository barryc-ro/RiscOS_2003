#include <rpc/rpc.h>
#include "h.pcnfsd"


bool_t
xdr_arstat(xdrs, objp)
        XDR *xdrs;
        arstat *objp;
{
        if (!xdr_enum(xdrs, (enum_t *)objp)) {
                return (FALSE);
        }
        return (TRUE);
}




bool_t
xdr_pirstat(xdrs, objp)
        XDR *xdrs;
        pirstat *objp;
{
        if (!xdr_enum(xdrs, (enum_t *)objp)) {
                return (FALSE);
        }
        return (TRUE);
}




bool_t
xdr_psrstat(xdrs, objp)
        XDR *xdrs;
        psrstat *objp;
{
        if (!xdr_enum(xdrs, (enum_t *)objp)) {
                return (FALSE);
        }
        return (TRUE);
}




bool_t
xdr_auth_args(xdrs, objp)
        XDR *xdrs;
        auth_args *objp;
{
        if (!xdr_string(xdrs, &objp->aa_ident, 31)) {
                return (FALSE);
        }
        if (!xdr_string(xdrs, &objp->aa_password, 63)) {
                return (FALSE);
        }
        return (TRUE);
}




bool_t
xdr_auth_results(xdrs, objp)
        XDR *xdrs;
        auth_results *objp;
{
        if (!xdr_arstat(xdrs, &objp->ar_stat)) {
                return (FALSE);
        }
        if (!xdr_long(xdrs, &objp->ar_uid)) {
                return (FALSE);
        }
        if (!xdr_long(xdrs, &objp->ar_gid)) {
                return (FALSE);
        }
        return (TRUE);
}




bool_t
xdr_pr_init_args(xdrs, objp)
        XDR *xdrs;
        pr_init_args *objp;
{
        if (!xdr_string(xdrs, &objp->pia_client, MAXNAMLEN)) {
                return (FALSE);
        }
        if (!xdr_string(xdrs, &objp->pia_printername, MAXNAMLEN)) {
                return (FALSE);
        }
        return (TRUE);
}




bool_t
xdr_pr_init_results(xdrs, objp)
        XDR *xdrs;
        pr_init_results *objp;
{
        if (!xdr_pirstat(xdrs, &objp->pir_stat)) {
                return (FALSE);
        }
        if (!xdr_string(xdrs, &objp->pir_spooldir, MAXPATHLEN)) {
                return (FALSE);
        }
        return (TRUE);
}




bool_t
xdr_pr_start_args(xdrs, objp)
        XDR *xdrs;
        pr_start_args *objp;
{
        if (!xdr_string(xdrs, &objp->psa_client, MAXNAMLEN)) {
                return (FALSE);
        }
        if (!xdr_string(xdrs, &objp->psa_printername, 63)) {
                return (FALSE);
        }
        if (!xdr_string(xdrs, &objp->psa_username, 63)) {
                return (FALSE);
        }
        if (!xdr_string(xdrs, &objp->psa_filename, MAXNAMLEN)) {
                return (FALSE);
        }
        if (!xdr_string(xdrs, &objp->psa_options, 63)) {
                return (FALSE);
        }
        return (TRUE);
}




bool_t
xdr_pr_start_results(xdrs, objp)
        XDR *xdrs;
        pr_start_results *objp;
{
        if (!xdr_psrstat(xdrs, &objp->psr_stat)) {
                return (FALSE);
        }
        return (TRUE);
}


