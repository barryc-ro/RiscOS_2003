title-string:           ATM25stb

help-string:            ATM25stb 1.15 ATML ATM25 card driver

initialisation-code:    module_init

finalisation-code:      module_final

service-call-handler:   module_sc_handler &9b &9f &a2 &45

swi-chunk-base-number:  0x47b00

swi-handler-code:       module_swi_handler

swi-decoding-table:     ATM,
                        DCIVersion Inquire GetNetworkMTU SetNetworkMTU Transmit Filter,
                        DoNowt EmptyBuffer DoNothing EndOfPlay StartOfPlay ReadMacAddr,
                        OpenConn ClearConn SetMacAdr SetBufs GetStats TxFrame

