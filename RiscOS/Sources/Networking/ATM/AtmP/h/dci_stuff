// header file for dci_stuff.c

/* Begin-Prologue */
#ifndef DCI_STUFF_H
#define DCI_STUFF_H
#undef DECLARE
#ifdef PROTOTYPES_INTERNAL
#define DECLARE
#define INIT(x) =(x)
#else
#define DECLARE extern
#define INIT(x)
#endif
/* End-Prologue */

#define FRAME_TYPE_IP  0x0800

DECLARE int rx_dci(char *, int , unsigned short );
DECLARE int atm_filter(_kernel_swi_regs *);
DECLARE int atm_transmit(_kernel_swi_regs *);
DECLARE void atm_ref(unsigned char *, int *, int *);


/* Begin-Epilogue */
#undef PROTOTYPES_INTERNAL
#undef INIT
#undef DECLARE
#endif
/* End_Epilogue */

