// header file for resources.c

/* Begin-Prologue */
#ifndef RESOURCES_H
#define RESOURCES_H
#undef DECLARE
#ifdef PROTOTYPES_INTERNAL
#define DECLARE
#define INIT(x) =(x)
#else
#define DECLARE extern
#define INIT(x)
#endif
/* End-Prologue */

DECLARE int claim_call_resources(void);
DECLARE void clear_call_resources(struct mast *);
DECLARE void init_call_resources(struct mast *);

/* Begin-Epilogue */
#undef PROTOTYPES_INTERNAL
#undef INIT
#undef DECLARE
#endif
/* End_Epilogue */

