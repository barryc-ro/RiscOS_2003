// header file for nbit_mask.s

/* Begin-Prologue */
#ifndef NBIT_MASK_H
#define NBIT_MASK_H
#undef DECLARE
#ifdef PROTOTYPES_INTERNAL
#define DECLARE
#define INIT(x) =(x)
#else
#define DECLARE extern
#define INIT(x)
#endif
/* End-Prologue */
DECLARE int init_handle_handler(unsigned int *, unsigned int, unsigned int);
DECLARE int get_handle(unsigned int *);
DECLARE int free_handle(unsigned int *, int);
DECLARE int free_cmd_handle(unsigned int *);
DECLARE int enumerate_free_handles(unsigned int *);
DECLARE int enumerate_busy_handles(unsigned int *);

DECLARE unsigned int *handle_wspace INIT(NULL); /* pointer to wspace for handle handler */


/* Begin-Epilogue */
#undef PROTOTYPES_INTERNAL
#undef INIT
#undef DECLARE
#endif
/* End_Epilogue */

