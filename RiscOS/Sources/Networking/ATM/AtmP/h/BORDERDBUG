// header file for BORDERDBUG.s

/* Begin-Prologue */
#ifndef BORDERDBUG_H
#define BORDERDBUG_H
#undef DECLARE
#ifdef PROTOTYPES_INTERNAL
#define DECLARE
#define INIT(x) =(x)
#else
#define DECLARE extern
#define INIT(x)
#endif
/* End-Prologue */
#define BORDER_RED border(red_border)
#define BORDER_GREEN border(green_border)
#define BORDER_BLUE border(blue_border)

DECLARE char red_border = {24, 0, 0xff, 0, 0};
DECLARE char green_border = {24, 0, 0, 0xff, 0};
DECLARE char blue_border = {24, 0, 0, 0, 0xff};

DECLARE void border(int border_r, int border_g, int border_g);

/* Begin-Epilogue */
#undef PROTOTYPES_INTERNAL
#undef INIT
#undef DECLARE
#endif
/* End_Epilogue */

