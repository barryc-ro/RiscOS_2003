/* atmsock.h -- header file for low level io to/from ATM 25 podule */
#ifndef ATMSOCK_H
#define ATMSOCK_H
#undef DECLARE
#undef INIT
#ifdef PROTOTYPES_INTERNAL
#undef PROTOTYPES_INTERNAL
#define DECLARE
#define INIT(x) =(x)
#else
#define DECLARE extern
#define INIT(x)
#endif


DECLARE int sock_connect(unsigned int, unsigned char *, unsigned int);
DECLARE int sock_listen(unsigned int, unsigned char *, unsigned int);
DECLARE int sock_transmit(unsigned int, unsigned char *, unsigned int, unsigned int);
#endif
