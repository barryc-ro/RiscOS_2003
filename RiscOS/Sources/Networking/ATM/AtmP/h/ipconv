// header file for ipconv.s

/* Begin-Prologue */
#ifndef IPCONV_H
#define IPCONV_H
#undef DECLARE
#ifdef PROTOTYPES_INTERNAL
#define DECLARE
#define INIT(x) =(x)
#else
#define DECLARE extern
#define INIT(x)
#endif
/* End-Prologue */

DECLARE int ipuctos(unsigned char *, unsigned char *, unsigned int, unsigned int);
DECLARE int ipstouc(unsigned char *, unsigned char *, unsigned int);
DECLARE int ipuitos(unsigned int, unsigned char *, unsigned int);
DECLARE unsigned int ipstoui(unsigned char *);


/* Begin-Epilogue */
#undef PROTOTYPES_INTERNAL
#undef INIT
#undef DECLARE
#endif
/* End_Epilogue */

