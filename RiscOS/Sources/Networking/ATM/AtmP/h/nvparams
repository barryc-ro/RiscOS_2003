/* =============================================================================
 * Function prototype declarations
 */

extern int nvparams_init (unsigned int  podule);

extern int nvparams_read_pvc (void);
extern void nvparams_write_pvc (const int  pvc);

extern void nvparams_read_primary_server (unsigned char *  atm_addr);
extern void nvparams_write_primary_server (const unsigned char *  atm_addr);

extern int nvparams_read_dynamic_arp (void);
extern void nvparams_write_dynamic_arp (const int  dynamic_arp);

extern int nvparams_read_multiproto_enc (void);
extern void nvparams_write_multiproto_enc (const int  multiproto_enc);

extern unsigned int nvparams_read_upstream_bandwidth (void);
extern void nvparams_write_upstream_bandwidth (
                unsigned int  upstream_bandwidth);

extern unsigned int  nvparams_read_gateway (void);
extern unsigned int  nvparams_read_server (void);
extern unsigned int  nvparams_read_netmask (void);


#endif
