// header file for nvram.s

/* Begin-Prologue */
#ifndef NVRAM_H
#define NVRAM_H
#undef DECLARE
#ifdef PROTOTYPES_INTERNAL
#define DECLARE
#define INIT(x) =(x)
#else
#define DECLARE extern
#define INIT(x)
#endif
/* End-Prologue */

DECLARE unsigned int read_nvram_bit(unsigned int, unsigned int);
DECLARE void write_nvram_bit(unsigned int, unsigned int, unsigned int);
DECLARE unsigned int read_nvram_byte(unsigned int);
DECLARE void write_nvram_byte(unsigned int, unsigned int);
DECLARE unsigned int read_nvram_word(unsigned int);
DECLARE void write_nvram_word(unsigned int, unsigned int);


/* Begin-Epilogue */
#undef PROTOTYPES_INTERNAL
#undef INIT
#undef DECLARE
#endif
/* End_Epilogue */

