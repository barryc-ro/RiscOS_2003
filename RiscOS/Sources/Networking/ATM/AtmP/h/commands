#ifndef AtmP_commands_h
#define AtmP_commands_h

/*	COMMANDS.H

Project : ATML ATM25 card driver

Purpose: function prototypes for command functions


History:

Date      Who     Description of change
===========================================
21/1/97    CP      Picked up code, added header.
		   Added prototype for atm_changebufferpool

30/4/98   CP      Changed arguments for various functions to const inorder to
                  protect incoming data/fit with prototypes defined in cmhg.h

***************************************************************************/

#define ATM25info_Help "Hinfo"
#define ATM25info_Syntax "Sinfo"
#define ATM25connections_Help "Hconnections"
#define ATM25connections_Syntax "Sconnections"
#define ATM25boson_Help "Hboson"
#define ATM25boson_Syntax "Sboson"
#define ATM25pibs_Help "Hpibs"
#define ATM25pibs_Syntax "Spibs"
#define ATM25trail_Help "Htrail"
#define ATM25trail_Syntax "Strail"
#define ATM25cci_Help "Hcci"
#define ATM25cci_Syntax "Scci"
#define ATM25listen_Help "Hlisten"
#define ATM25listen_Syntax "Slisten"
#define ATM25status_Help "Hstatus"
#define ATM25status_Syntax "Sstatus"
#define ATM25buffers_Help "Hbuffers"
#define ATM25buffers_Syntax "Sbuffers"
#define ATM25pvc_Help "Hpvc"
#define ATM25pvc_Syntax "Spvc"
#define ATM25pvcdelete_Help "Hpvcdelete"
#define ATM25pvcdelete_Syntax "Spvcdelete"
#define ATM25arp_Help "Harp"
#define ATM25arp_Syntax "Sarp"
#define ATM25llc_Help "Hllc"
#define ATM25llc_Syntax "Sllc"
#define ATM25vc_Help "Hvc"
#define ATM25vc_Syntax "Svc"
#define ATM25data_Help "Hdata"
#define ATM25data_Syntax "Sdata"
#define ATM25changebufs_Help "Hchangebufs"
#define ATM25changebufs_Syntax "Schangebufs"
#define ATM25bw_Help "Hbw"
#define ATM25bw_Syntax "Sbw"
#define ATM25svc_Help "Hsvc"
#define ATM25svc_Syntax "Ssvc"
#define ATM25svcdelete_Help "Hsvcdelete"
#define ATM25svcdelete_Syntax "Ssvcdelete"

extern _kernel_oserror *  command_info (int  argc, char *  argv[]);
extern _kernel_oserror *  command_boson (int argc, char *  argv[]);
extern _kernel_oserror *  command_trail (int argc, char *  argv[]);
extern _kernel_oserror *  command_cci (int argc, char *  argv[]);
extern _kernel_oserror *  command_listen (int argc, char *  argv[]);
extern _kernel_oserror *  command_status (int argc, char *  argv[]);
extern _kernel_oserror *  command_buffers (int argc, char *  argv[]);
extern _kernel_oserror *  command_pvc (int argc, char *  argv[]);
extern _kernel_oserror *  command_pvcdelete (int argc, char *  argv[]);
extern _kernel_oserror *  command_svc (int argc, char *  argv[]);
extern _kernel_oserror *  command_svcdelete (int argc, char *  argv[]);
extern _kernel_oserror *  command_connections (int argc, char *  argv[]);
extern _kernel_oserror *  command_configure_arp (int argc, char *  argv[]);
extern _kernel_oserror *  command_configure_llc (int argc, char *  argv[]);
extern _kernel_oserror *  command_configure_vc (int argc, char *  argv[]);
extern _kernel_oserror *  command_configure_bw (int argc, char *  argv[]);
extern _kernel_oserror *  command_changebufs (int argc, char *  argv[]);

#endif
