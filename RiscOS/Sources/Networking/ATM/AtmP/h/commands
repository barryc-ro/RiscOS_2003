/*	COMMANDS.H

Project : ATML ATM25 card driver

Purpose: function prototypes for command functions


History:

Date      Who     Description of change
===========================================
21/1/97    CP      Picked up code, added header.
		   Added prototype for atm_changebufferpool

30/4/98   CP      Changed arguments for various functions to const inorder to
                  protect incoming data/fit with prototypes defined in cmhg.h

***************************************************************************/
// header file for

/* Begin-Prologue */
#ifndef COMMANDS_H
#define COMMANDS_H
#undef DECLARE
#ifdef PROTOTYPES_INTERNAL
#define DECLARE
#define INIT(x) =(x)
#else
#define DECLARE extern
#define INIT(x)
#endif
/* End-Prologue */

DECLARE void atm_info(void);
DECLARE void atm_mast_show(void);
DECLARE void atm_boson(void);
DECLARE void atm_pibs(void);
DECLARE void atm_trail(void);
DECLARE void atm_cci(const char *);
DECLARE void atm_listen(void);
DECLARE void atm_status(void);
DECLARE void atm_buffers(void);
DECLARE void atm_pvc(char *);
DECLARE void atm_pvcdelete(char *);
DECLARE void atm_mast_data(void);

/* 21/1/97:CP:Added prototype for new function; I don't like doing things
	      like this, but it fits with the existing coding style... :-( */
DECLARE void atm_changebufferpool(const char *);

/* Begin-Epilogue */
#undef PROTOTYPES_INTERNAL
#undef INIT
#undef DECLARE
#endif
/* End_Epilogue */

