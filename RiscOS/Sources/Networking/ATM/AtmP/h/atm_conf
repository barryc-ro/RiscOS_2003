// header file for atm_conf.c

/* Begin-Prologue */
#ifndef ATM_CONF_H
#define ATM_CONF_H
#undef DECLARE
#ifdef PROTOTYPES_INTERNAL
#define DECLARE
#define INIT(x) =(x)
#else
#define DECLARE extern
#define INIT(x)
#endif
/* End-Prologue */
DECLARE void atm_conf_arp(const char *);
DECLARE void atm_conf_llc(const char *);
DECLARE void atm_conf_vc(const char *);
DECLARE void atm_conf_pvc(const char *);


/* Begin-Epilogue */
#undef PROTOTYPES_INTERNAL
#undef INIT
#undef DECLARE
#endif
/* End_Epilogue */

