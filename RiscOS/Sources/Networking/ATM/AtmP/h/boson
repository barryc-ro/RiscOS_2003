// header file for boson.s

/* Begin-Prologue */
#ifndef BOSON_H
#define NOSON_H
#undef DECLARE
#ifdef PROTOTYPES_INTERNAL
#define DECLARE
#define INIT(x) =(x)
#else
#define DECLARE extern
#define INIT(x)
#endif
/* End-Prologue */

DECLARE void asm_get_msg(unsigned int, unsigned char *, unsigned int);
DECLARE void asm_junk_msg(unsigned int, unsigned int);
DECLARE int asm_write_fifo(unsigned int, const int, unsigned char *, unsigned int, unsigned int);
DECLARE void asm_enable_podule_irqs(unsigned int);
DECLARE void asm_disable_podule_irqs(unsigned int);
DECLARE void asm_buffer_recover(unsigned int);
DECLARE void multi_status(unsigned int, unsigned int *, unsigned int);
DECLARE int ensure_irqs_off(void);
DECLARE int ensure_irqs_on(void);
DECLARE void restore_irqs(int);

/* Begin-Epilogue */
#undef PROTOTYPES_INTERNAL
#undef INIT
#undef DECLARE
#endif
/* End_Epilogue */

