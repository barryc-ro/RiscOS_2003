// header file for pvc.c

/* Begin-Prologue */
#ifndef PVC_H
#define PVC_H
#undef DECLARE
#ifdef PROTOTYPES_INTERNAL
#define DECLARE
#define INIT(x) =(x)
#else
#define DECLARE extern
#define INIT(x)
#endif
/* End-Prologue */
DECLARE void pvc_create(unsigned char *, unsigned int, unsigned int);
DECLARE void pvc_delete(unsigned char *, unsigned int, unsigned int);
DECLARE void pvc_connect_boot(void);


/* Begin-Epilogue */
#undef PROTOTYPES_INTERNAL
#undef INIT
#undef DECLARE
#endif
/* End_Epilogue */

