#!/bin/sh
FROM=../../../Apps/NCFresco/webftp
TO=..
cp -p $FROM/c/dirparse $TO/dirparse.c
cp -p $FROM/c/misc $TO/misc.c
cp -p $FROM/c/q $TO/q.c
cp -p $FROM/c/trans $TO/trans.c
cp -p $FROM/c/webftp $TO/webftp.c
#
cp -p $FROM/h/all $TO/all.h
cp -p $FROM/h/autofns $TO/autofns.h
cp -p $FROM/h/csfs $TO/csfs.h
cp -p $FROM/h/pads $TO/pads.h
cp -p $FROM/h/privwebftp $TO/privwebftp.h
cp -p $FROM/h/q $TO/q.h
cp -p $FROM/h/webftp $TO/webftp.h
#
cp -p $FROM/c/files $TO/files.c
cp -p $FROM/h/files $TO/files.h
#
echo put files to NCFresco directory
