#!/bin/sh
FROM=../../../Apps/NCFresco/webftp
TO=..
cp -p $FROM/dirparse.c $TO/c/dirparse
cp -p $FROM/misc.c $TO/c/misc
cp -p $FROM/q.c $TO/c/q
cp -p $FROM/trans.c $TO/c/trans
cp -p $FROM/webftp.c $TO/c/webftp
#
cp -p $FROM/all.h $TO/h/all
cp -p $FROM/autofns.h $TO/h/autofns
cp -p $FROM/csfs.h $TO/h/csfs
cp -p $FROM/pads.h $TO/h/pads
cp -p $FROM/privwebftp.h $TO/h/privwebftp
cp -p $FROM/q.h $TO/h/q
cp -p $FROM/webftp.h $TO/h/webftp
#
cp -p $FROM/files.c $TO/c/files
cp -p $FROM/files.h $TO/h/files
#
echo got files from NCFresco directory
