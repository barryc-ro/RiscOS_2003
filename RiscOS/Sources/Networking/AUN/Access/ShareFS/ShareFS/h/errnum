/* Possible errors: */

#define FSERR           (0x100+RemoteFSNumber<<8)       /* filesystem errors */

#define ERRNotVol       FSERR+0xac
#define ERRNotDir       FSERR+0xc5
#define ERRNotFile      FSERR+0xc5
#define ERRUKTran       FSERR+0xac
#define ERRNoVol        FSERR+0xac
#define ERRreadonly     FSERR+0x4c
#define ERRaccess       FSERR+0xbd
#define ERRrename       FSERR+0xb0

#define ERC             0x806c00                        /* 256 allocated errors */

#define ERRStale        ERC
#define ERRConn         ERC+1
#define ERRBadFSOp	ERC+2
#define ERRNotLis       ERC+3
#define ERRLineJ        ERC+4
#define ERRAmbVol       ERC+5
#define ERRDiscName     ERC+6
#define ERRExport       ERC+7
#define ERRNotYet       ERC+8
/* #define ERRinuse        ERC+9 */
#define ERRbadref       ERC+10
#define ERRbadpath      ERC+11
/* #define ERRduse         ERC+12 */
#define ERRdcode        ERC+13
/* #define ERRdswi         ERC+14 */
#define ERRBadSWI	ERC+14
#define ERRddata        ERC+15
#define ERRdmany        ERC+16
#define ERRfree         ERC+17
#define ERRdexport      ERC+18
#define ERRnopar        ERC+19
#define ERRbaud         ERC+20
#define ERRlost         ERC+21
#define ERRtimeout      ERC+22
#define ERRdirok        ERC+23
/* #define ERRSerial       ERC+24 */
#define ERRNoTML        ERC+25
/* #define ERRTMLJam       ERC+26 */
#define ERRNotServ      ERC+27
#define ERRNoReply      ERC+28
#define ERRDeadServ     ERC+29
#define ERRInActive     ERC+30
#define ERRDupExprt     ERC+31
#define ERRRecurse      ERC+32
#define ERRVersion      ERC+33
#define ERRGone         ERC+34
#define ERRStolen       ERC+35
#define ERRTMLEco	ERC+36
#define ERRLongNam	ERC+37

/* EOF errnum.h */
