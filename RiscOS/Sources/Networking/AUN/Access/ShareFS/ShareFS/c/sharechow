#include <stdio.h>
#include <string.h>

#include "how.h"
#include "util.h"

#define DEFAULTBUFFERSIZE 32*1024

struct how defaulthow = {0, 1, 0, 0, 0, 0, 0, DEFAULTBUFFERSIZE};

int howread(struct how *h, char *s)
{
    s = s;
    return 1;
}

char *howprint(struct how h)
{
    static char buf[30];
    sprintf(buf, "%s%s%s", h.readonly ? "-readonly " : "",
	    h.owner ? "" : "-protected ",
	    h.hidden ? "-noicon " : "");
    return buf;
}

/* EOF sharechow.c */
