#include "ifscore.h"

#ifdef IMAGESUPPORT
void FileEntry_NewImage(int handle, int bufsize)
{
    handle = handle;
    bufsize = bufsize;
}

void FileEntry_OldImage(ImageEntry *image)
{
    image = image;
}

void FileEntry_ImageHandle(FileEntry *fe)
{
    fe = fe;
}
#else
/*
 * this is needed to shut the compiler up
 */
void NoImage(void)
{
}
#endif

