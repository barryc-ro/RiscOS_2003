	GET	Hdr:ListOpts
	GET	Hdr:Macros
	GET	Hdr:System
	GET	Hdr:EnvNumbers
	GET	Hdr:ModHand

;;	GET	ListOpts
;;	GET	Macros
;;	GET	System
;;	GET	EnvNumbers
;;	GET	ModHand

	EXPORT	m_dolink

	AREA	mlink,PIC,CODE,REL,READONLY

m_dolink	ROUT
	MOV	R12, R3
	MOV	PC, R2

	END

