//STBLED.c
//controls STB LEDs

#include "swis.h"
#define STBState_Control 0x4b406
void STBLED(char *data)
{
  int state =0;

  _kernel_swi_regs reg;

  if(data[0]=='1')   state = state|0x01;
  if(data[1]=='1')  state = state|0x02;

   reg.r[0] = state;

  _kernel_swi(STBState_Control, &reg, &reg);
}





