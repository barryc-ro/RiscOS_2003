
#ifndef _HANDLERS_H
#define _HANDLERS_H

#include "blclient.h"


/* toolbox event handler prototypes */
extern int handler_windowOpen (int event_code, ToolboxEvent* event, IdBlock* id_block, void* handle);
extern int handler_windowClose (int event_code, ToolboxEvent* event, IdBlock* id_block, void* handle);
extern int handler_tbObjectAutoCreated (int event_code, ToolboxEvent* event, IdBlock* id_block, void* handle);
extern int handler_clearWindow (int event_code, ToolboxEvent* event, IdBlock* id_block, void* handle);
extern int handler_keyPress (int event_code, WimpPollBlock* event, IdBlock* id_block, void* handle);
extern int handler_mouseClick (int event_code, WimpPollBlock* event, IdBlock* id_block, void* handle);
extern int handler_setMode (int event_code, ToolboxEvent* event, IdBlock* id_block, void* handle);
extern int handler_increaseWindowBufferSize (int event_code, ToolboxEvent* event, IdBlock* id_block, void* handle);
extern int handler_helpButton (int event_code, ToolboxEvent* event, IdBlock* id_block, void* handle);
extern int handler_setBaudRate (int event_code, ToolboxEvent* event, IdBlock* id_block, void* handle);
extern int handler_saveAsWindow (int event_code, ToolboxEvent* event, IdBlock* id_block, void* handle);
extern int handler_stopSaving (int event_code, ToolboxEvent* event, IdBlock* id_block, void* handle);
extern int handler_dataLoadMessage (WimpMessage *message, void *handle);
extern int handler_dataOpenMessage (WimpMessage* message, void* handle);
extern int handler_tbquit (int event_code, ToolboxEvent* event, IdBlock* id_block, void* handle);
extern int handler_quit (WimpMessage* message, void* handle);



#endif

