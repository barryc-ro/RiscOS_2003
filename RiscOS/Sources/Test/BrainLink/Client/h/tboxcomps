


#ifndef _TBOXCOMPONENTS_H
#define _TBOXCOMPONENTS_H


/* Component IDs */

/* brainlink set buffer size component IDs */
#define SET_BUFFER_SIZE_FIELD             (int)0

/* record output window component IDs */
#define FILE_LOCATION_FIELD               (int)0


#define BAUD_1200     (int)1
#define BAUD_2400     (int)2
#define BAUD_4800     (int)3
#define BAUD_9600     (int)4
#define BAUD_14400    (int)5
#define BAUD_19200    (int)6
#define BAUD_38400    (int)7
#define BAUD_57600    (int)8
#define BAUD_115200   (int)9


#define START_SPOOLING (int)1
#define STOP_SPOOLING  (int)2

#endif
