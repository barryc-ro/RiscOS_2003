//Test Menu
//Displays a Test Menu and allows selection of tests

#include <stdio.h>
#include <swis.h>
#include "support.h"

int main(void)
{
  standby_on();

  menu();

  off();

  do
  {
    switch (get())
    {
      case 1:
        run("Resources:$.Resources.Test.!UnitTest.start","!UnitTest");
      break;

      case 2:
        run("SoakTest","SoakTest module");
      break;

      case 5:
        on();
        return 0;
      break;
    }
  }
  while (1);

  return 0;
}
