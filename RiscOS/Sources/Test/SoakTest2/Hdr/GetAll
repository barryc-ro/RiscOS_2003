; > GetAll

        GBLL    international
international   SETL	{FALSE}

	GBLL	PokeCLINES		; Whether we have to poke CLINES to set up joystick ID pins as inputs
PokeCLINES	SETL 	{FALSE}

	GBLL	UseSTBStateModule
UseSTBStateModule SETL  {TRUE}

	GBLS	GetSTBState
 [ UseSTBStateModule
GetSTBState SETS "GET Hdr:STBState"
 |
GetSTBState SETS ""
 ]

        GET     Hdr:ListOpts
        GET     Hdr:Macros
        GET     Hdr:System
	GET	Hdr:Machine.<Machine>
	$GetIO
        GET     Hdr:CMOS
        GET     Hdr:ModHand
        GET     Hdr:Services
        GET     Hdr:Proc
        GET     Hdr:FSNumbers		; Jon Change (was file)
        GET     Hdr:NewErrors
        GET     Hdr:MsgTrans
        GET     Hdr:Debug
	GET	Hdr:Podule
	GET	Hdr:PoduleReg
	GET	Hdr:HighFSI		; ie old Hdr:File
	GET	Hdr:Sprite

        GET     Hdr:MPEGCtrl
        GET     Hdr:MPEGVideo
        GET     Hdr:MPEGAudio
	GET	Hdr:MPEGCard
	GET	Hdr:ResourceFS
	$GetSTBState

        GET     s.Module

        END
