dir <Src$Dir>.Lib.OSLib
Echo Cleaning <Src$Dir>.Lib.OSLib...
amu_machine clean_lib COMPONENT=OSLib TARGET=OSLib

dir <Src$Dir>.Lib.Desk
Echo Cleaning <Src$Dir>.Lib.Desk...
amu_machine clean_all COMPONENT=Desk TARGET=Desk

dir <Src$Dir>.Lib.RISC_OSLib
Echo Cleaning <Src$Dir>.Lib.RISC_OSLib
amu_machine clean_all COMPONENT=SharedRISC_OSLib TARGET=RISC_OSLib

dir <Src$Dir>.Internat.Messages
Echo Cleaning <Src$Dir>.Internat.Messages
amu_machine clean_all COMPONENT=Messages TARGET=Messages

