wimpslot -min 9000k
set mksystem$dir <obey$dir>
time
echo Cleaning messages ...
dir <src$dir>.Internat.Messages
amu_machine clean
time
echo Exporting resources ...
dir <mksystem$dir>
amu_machine -k resources
time
echo Installing components ...
dir <mksystem$dir>
amu_machine install_rom
time
echo Done
