wimpslot -min 5000k
set mksystem$dir <obey$dir>
echo Starting ...
time
|
echo Cleaning...
dir <mksystem$dir>
amu_machine -k clean
time
|
echo Exporting hdrs ...
dir <mksystem$dir>
amu_machine -k export PHASE=hdrs
|
echo Exporting libs ...
dir <mksystem$dir>
amu_machine -k export PHASE=libs
time
|
echo Copying Resource files to Messages module ...
dir <mksystem$dir>
amu_machine -k resources
|
echo Building components ...
dir <mksystem$dir>
amu_machine -k rom
|
echo Installing components ...
dir <mksystem$dir>
amu_machine -k install_rom 
|
echo All done.
time
