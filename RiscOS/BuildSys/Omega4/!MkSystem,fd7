wimpslot -min 5000k
echo Starting ...
time
|
set Build$SysDir <Obey$Dir>
echo Exporting hdrs ...
dir <Build$SysDir>
amu_machine -k export PHASE=hdrs
|
echo Exporting libs ...
dir <Build$SysDir>
amu_machine -k export PHASE=libs
time
|
echo Copying Resource files to Messages module ...
dir <Build$SysDir>
amu_machine -k resources
|
echo Building components ...
dir <Build$SysDir>
amu_machine -k rom
|
echo Installing components ...
dir <Build$SysDir>
amu_machine -k install_rom 
|
echo All done.
time
