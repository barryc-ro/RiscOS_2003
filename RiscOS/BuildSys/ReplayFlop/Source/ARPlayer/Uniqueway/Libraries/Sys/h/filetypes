#ifndef __filetypes_h
# define __filetypes_h

#define filetype_LoadExec               0x3000  /* as used in wimp data xfer protocol */
#define filetype_Application            0x2000
#define filetype_Directory              0x1000

#define filetype_Text                   0xFFF /* ASCII text file */
#define filetype_Command                0xFFE /* Command (Exec) file */
#define filetype_Data                   0xFFD
#define filetype_Utility                0xFFC /* Transient program */
#define filetype_BASIC                  0xFFB /* Tokenised BASIC program */
#define filetype_Module                 0xFFA /* Relocatable module */
#define filetype_Sprite                 0xFF9 /* Sprite or saved screen */
#define filetype_Absolute               0xFF8 /* Absolute application Loaded at &8000 */
#define filetype_BBCFont                0xFF7 /* BBC font file (sequence of VDU ops.) */
#define filetype_Font                   0xFF6 /* Fancy font (4bpp bitmap only) */
#define filetype_PostScript             0xFF5
#define filetype_DMData                 0xFF4 /* Dot Matrix data file */
#define filetype_LaserJet               0xFF3 /* HP LaserJet datafile */
#define filetype_Configuration          0xFF2 /* 240 bytes of CMOSRAM */
#define filetype_Tiff                   0xFF0

#define filetype_Diary                  0xFEF
#define filetype_NotePad                0xFEE
#define filetype_Palette                0xFED
#define filetype_Template               0xFEC
#define filetype_Obey                   0xFEB
#define filetype_Desktop                0xFEA
#define filetype_ViewWord               0xFE9
#define filetype_ViewPS                 0xFE8
#define filetype_ViewSheet              0xFE7

#define filetype_DOSFile                0xFE4
#define filetype_AtariFile              0xFE3
#define filetype_AmigaFile              0xFE2
#define filetype_Makefile               0xFE1
#define filetype_DeskTopAccessory       0xFE0 /* Used from BASIC: LIBRARY"filename" */

#define filetype_MasterUtilities        0xFDD
#define filetype_MIDI                   0xFD4
#define filetype_DebugImage             0xFD3
#define filetype_PCEmConfigure          0xFD0
#define filetype_FontCache              0xFCF
#define filetype_Device                 0xFCC
#define filetype_Squash                 0xFCA
#define filetype_SunRastr               0xFC9
#define filetype_DOSDisc                0xFC8
#define filetype_Patch                  0xFC3
#define filetype_AIFF                   0xFC2
#define filetype_EconetUtilities        0xF0E
#define filetype_WiniUtilities          0xF09

#define filetype_SDAF                   0xDAF
#define filetype_HighNoteCode           0xDAE
#define filetype_HighNote               0xDAD
#define filetype_HighNoteConfig         0xDAC

#define filetype_CSV                    0xDFE
#define filetype_Picture                0xDFA
#define filetype_DXF                    0xDEA
#define filetype_ProArt                 0xDE2

#define filetype_RendPic                0xD58
#define filetype_Armadeus               0xD3C

#define filetype_SnapShot               0xCC2

#define filetype_STSample               0xCB5       /* Sound trackers */
#define filetype_STModule               0xCB6

#define filetype_JPEG                   0xC85
#define filetype_Subtitle               0xC5F

#define filetype_ViA                    0xBFD
#define filetype_RIFF                   0xBF9
#define filetype_MPEG                   0xBF8

#define filetype_AudioWrk               0xBD6
#define filetype_Envelope               0xBD5

#define filetype_BBCRom                 0xBBC

#define filetype_DrawFile               0xAFF
#define filetype_MouseRecord            0xAFE
#define filetype_GCALsource             0xAFD
#define filetype_GCODEintermediate      0xAFC
#define filetype_DTPstyle               0xAFA
#define filetype_DTPdoc                 0xAF9
#define filetype_1stWordPlus            0xAF8
#define filetype_Help                   0xAF7
#define filetype_ASimTrace              0xAF6
#define filetype_MailSetup              0xAF5
#define filetype_MailFiled              0xAF4
#define filetype_MailPostbox            0xAF3
#define filetype_MailInTray             0xAF2
#define filetype_Maestro                0xAF1
#define filetype_ArcWriter              0xAF0
#define filetype_ARMovie                0xAE7

#define filetype_OutLineFont            0xADB

#define filetype_AIM                    0x004

#define filetype_Clear                  0x690
#define filetype_Degas                  0x691
#define filetype_Img                    0x692
#define filetype_AmigaIFF               0x693
#define filetype_MacPaint               0x694
#define filetype_GIF                    0x695
#define filetype_Pineapple              0x696
#define filetype_PCX                    0x697
#define filetype_QRT                    0x698
#define filetype_MTV                    0x699
#define filetype_CadSoft                0x69A
#define filetype_Irlam                  0x69B
#define filetype_BMP                    0x69C
#define filetype_Targa                  0x69D
#define filetype_PBMPlus                0x69E
#define filetype_ZVDA                   0x69F
#define filetype_MSX2                   0x6A0
#define filetype_RLE                    0x6A1
#define filetype_ColoRIX                0x6A2
#define filetype_FITS                   0x6A3
#define filetype_HawkV9                 0x6A4

#define filetype_601                    0x601
#define filetype_Irlam411               0x411

#define filetype_DataVox                0x108

#endif

/* eof h.filetypes */

