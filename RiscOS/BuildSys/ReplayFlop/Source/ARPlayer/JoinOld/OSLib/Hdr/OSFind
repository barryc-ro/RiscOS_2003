        [       :LNOT: :DEF: Get_Types
        GBLS    Get_Types
Get_Types                       SETS    "GET OS:Hdr.Types"
        |
Get_Types                       SETS    ""
        ]
        $Get_Types

        [       :LNOT: :DEF: Get_OS
        GBLS    Get_OS
Get_OS                          SETS    "GET OS:Hdr.OS"
        |
Get_OS                          SETS    ""
        ]
        $Get_OS

        [       :LNOT: :DEF: OSFind_Hdr
        GBLS    OSFind_Hdr
OSFind_ErrorIfAbsent            *       &8
OSFind_ErrorIfDir               *       &4


OSFind_Close                    *       &0
OSFind_Openin                   *       &40
OSFind_OpeninPath               *       &41
OSFind_OpeninPathVar            *       &42
OSFind_OpeninNoPath             *       &43
OSFind_Openout                  *       &80
OSFind_OpenoutPath              *       &81
OSFind_OpenoutPathVar           *       &82
OSFind_OpenoutNoPath            *       &83
OSFind_Openup                   *       &C0
OSFind_OpenupPath               *       &C1
OSFind_OpenupPathVar            *       &C2
OSFind_OpenupNoPath             *       &C3
        ]
        END
