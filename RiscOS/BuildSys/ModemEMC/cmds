echo Messages ...
cdir <install$dir>.EMCRom
dir <src$dir>.ModemEMC.EMCMsgs
amu MACHINE=Omega LOCALE=UK SYSTEM=Omega4 -k -o cmds ROMImage RESDIR=<src$dir>.ModemEMC.EMCMsgs.Resources.<Locale>.<System>.Resources INSTDIR=<install$dir>.EMCRom
echo CMOSReset ...
cdir <install$dir>.EMCRom
dir <src$dir>.ModemEMC.CMOSReset
amu MACHINE=Omega LOCALE=UK SYSTEM=Omega4 -k -o cmds ROMImage RESDIR=<src$dir>.ModemEMC.EMCMsgs.Resources.<Locale>.<System>.Resources INSTDIR=<install$dir>.EMCRom
echo UARTDriver ...
cdir <install$dir>.EMCRom
dir <src$dir>.HWSupport.UARTDriver
amu MACHINE=Omega LOCALE=UK SYSTEM=Omega4 -k -o cmds ROMImage RESDIR=<src$dir>.ModemEMC.EMCMsgs.Resources.<Locale>.<System>.Resources INSTDIR=<install$dir>.EMCRom
echo ROMImage ...
cdir <install$dir>.Images
dir <build$dir>.BuildSys.EMCRom.ROMImage
amu MACHINE=Omega LOCALE=UK SYSTEM=Omega4 -k -o cmds ROMImage RESDIR=<src$dir>.ModemEMC.EMCMsgs.Resources.<Locale>.<System>.Resources INSTDIR=<install$dir>.Images
