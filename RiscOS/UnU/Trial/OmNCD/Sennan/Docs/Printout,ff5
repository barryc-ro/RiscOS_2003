%%BeginFont: RO_Trinity.Bold_ELatin1

% Modify font Times-Bold to get font RO_Trinity.Bold_ELatin1
/Times-Bold findfont  dup length dict  begin
  {1 index /FID ne {def} {pop pop} ifelse} forall
  /Encoding Acorn_Latin1Encoding def
currentdict end
/RO_Trinity.Bold_ELatin1 exch definefont pop

%%EndFont


