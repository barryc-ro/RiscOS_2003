/* -*-c-*- */

#ifndef __urlopen_h
#define __urlopen_h

#define wimp_MOPENURL	0x4AF80
#define wimp_MURLHOTADD	0x4AF81
#define wimp_MURLHOTNEW	0x4AF82
#define wimp_MURLHOTDEL	0x4AF83

typedef union {
    char url[236];
    struct {
	int tag;
	char *url;
    } indirect;
} urlopen_data;

typedef struct {
    char *url;
    char *title;
    char appname[32];
} hotlist_msg_data;

#endif /* __urlopen_h */
