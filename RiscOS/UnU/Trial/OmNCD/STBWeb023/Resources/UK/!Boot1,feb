IconSprites <Obey$Dir>.!Sprites
|
Set STBWeb$Dir <Obey$Dir>
SetMacro STBWeb$Path <STBWeb$Dir>.
|
Set File$Type_695 GIF
Set File$Type_B28 URL
Set File$Type_B60 PNG
Set File$Type_B61 XBitMap
Set File$Type_B62 Gopher
Set File$Type_C85 JPEG
Set File$Type_FAF HTML
|
If "<Alias$URLOpen_HTTP>" = ""   Then Set Alias$URLOpen_HTTP   /STBWeb:!Run -URL %%*0
If "<Alias$URLOpen_GOPHER>" = "" Then Set Alias$URLOpen_GOPHER /STBWeb:!Run -URL %%*0
If "<Alias$URLOpen_FILE>" = ""   Then Set Alias$URLOpen_FILE   /STBWeb:!Run -URL %%*0
If "<Alias$URLOpen_FTP>" = ""    Then Set Alias$URLOpen_FTP    /STBWeb:!Run -URL %%*0
