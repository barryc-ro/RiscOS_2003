        BEQ      |L000988.J17.in_broadcast|
|L00096c.J7.in_broadcast|
        LDR      v2,[v2,#&40]
        TEQ      v2,#0
        BNE      |L000938.J4.in_broadcast|
|L000978.J5.in_broadcast|
        CMN      v1,#1
        TEQNE    v1,#0
        MOVNE    a1,#0
        LDMNEDB  fp,{v1,v2,fp,sp,pc}^
|L000988.J17.in_broadcast|
