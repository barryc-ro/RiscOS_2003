        MOVEQ    a1,a2
        MOVEQS   pc,lr
        LDR      a2,[a2,#&40]
        TEQ      a2,#0
        BNE      |L0008ec.J4.in_iaonnetof|
|L000908.J5.in_iaonnetof|
        MOV      a1,#0
        MOVS     pc,lr

        EXPORT  in_broadcast
in_broadcast
        MOV      ip,sp
        STMDB    sp!,{v1,v2,fp,ip,lr,pc}
        SUB      fp,ip,#4
        MOV      v1,a1
        LDR      a1,[pc, #L000104-.-8]
        LDR      ip,[sl,#-0]
        ADD      a1,ip,a1
        LDR      v2,[a1,#0]
        TEQ      v2,#0
        BEQ      |L000978.J5.in_broadcast|
|L000938.J4.in_broadcast|
