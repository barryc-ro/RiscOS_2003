        EXPORT  in_iaonnetof
in_iaonnetof
        LDR      a2,[pc, #L000104-.-8]
        LDR      ip,[sl,#-0]
        ADD      a2,ip,a2
        LDR      a2,[a2,#0]
        TEQ      a2,#0
        BEQ      |L000908.J5.in_iaonnetof|
|L0008ec.J4.in_iaonnetof|
