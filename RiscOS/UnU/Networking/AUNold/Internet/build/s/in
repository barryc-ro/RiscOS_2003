        BEQ      |L000860.J21.in_ifinit|
        STRB     v6,[v1,#&10]
        MOV      a2,#0
        STRB     a2,[v1,#&11]
        MVN      a2,#0
        BL       in_makeaddr
        STR      a1,[v1,#&14]
        ADD      a2,v1,#&28
        LDMIA    a2,{a1,a2}
        MVN      a2,a2
        ORR      a1,a1,a2
        BL       htonl
        STR      a1,[v1,#&38]
|L000860.J21.in_ifinit|
        LDR      a1,[v2,#&10]
        MOV      a1,a1,LSL #16
        MOV      a1,a1,ASR #16
        TST      a1,#8
        MOVNE    a2,v1
        MOVNE    a1,v1
        BNE      |L00088c.J36.in_ifinit|
        TST      a1,#&10
        BEQ      |L000894.J26.in_ifinit|
        MOV      a2,v1
        MOV      a1,v4
|L00088c.J36.in_ifinit|
        MOV      a4,#5
        B        |L0008b0.J33.in_ifinit|
|L000894.J26.in_ifinit|
