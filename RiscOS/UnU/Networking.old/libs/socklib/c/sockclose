#include "stubs.h"
#include "socklib.h"

int socketclose(int d)
{
    _kernel_swi_regs rin, rout;

    rin.r[0] = d;

    return(_makecall(Socketclose, &rin, &rout));
}

/* EOF sockclose.c */
