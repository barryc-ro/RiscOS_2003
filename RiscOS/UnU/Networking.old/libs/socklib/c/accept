#include "stubs.h"
#include "socklib.h"

int accept(int s, struct sockaddr *addr, int *addrlen)
{
    _kernel_swi_regs rin, rout;

    rin.r[0] = s;
    rin.r[1] = (int)addr;
    rin.r[2] = (int)addrlen;

    return(_makecall(Accept, &rin, &rout));
}

/* EOF accept.c */
