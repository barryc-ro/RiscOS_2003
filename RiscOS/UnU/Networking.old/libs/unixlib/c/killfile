#include "kernel.h"
#include "swis.h"

#include "unixlib.h"

void killfile(char *name)
{
    _kernel_swi_regs r;

    r.r[0] = 6;
    r.r[1] = (int)name;
    (void)_kernel_swi(XOS_Bit | OS_File, &r, &r);
}

/*
 * this is the proper UNIX function name
 */
int unlink(char *path)
{
    killfile(path);

    return(0);
}

/* EOF killfile.c */
