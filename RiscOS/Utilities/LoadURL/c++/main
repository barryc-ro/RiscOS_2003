/************************************************************************/
/* File:    main.c++                                                    */
/* Purpose: Little stub file to hold a C++ main function.               */
/*          This is required so that C++ static data can be initialised */
/*          at startup.                                                 */
/************************************************************************/

extern "C"
{
  #include "main.h"
}

int main (void)
{
  /* Call the proper C main function */
  return Cmain();
}
