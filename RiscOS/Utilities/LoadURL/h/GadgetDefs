/* Main Window */
#define MainWin_Target_Option		0x29
#define MainWin_Target			0x27
#define MainWin_URL			0x0b
#define MainWin_Open			0x0d
#define MainWin_Close			0x2a
#define MainWin_GenericURL		0x2b
#define MainWin_Radio_URL		0x2c
#define MainWin_Radio_URI		0x2d
#define MainWin_Launch			0x30

/* Iconbar menu */
#define IBarMenu_Filenames		0x02

