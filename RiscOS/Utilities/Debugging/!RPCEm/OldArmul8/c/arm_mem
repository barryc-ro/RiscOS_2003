#include "arm_mem.h"

void arm800_memory_earlyph1(state)
  armul8_state *state;
{

  arm800_mmu_earlyph1(state);

  return;
}

void arm800_memory_lateph1(state)
  armul8_state *state;
{

  arm800_mmu_lateph1(state);

  return;
}

