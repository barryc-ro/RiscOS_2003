/*******************************************************\
*                                                       *
* extend.c: routines to emulate the ARM8 core extension *
*                                                       *
\*******************************************************/

#include "state.h"
#include "extend.h"
#include "extend_i.h"
#include "multiplier.h"

/*****************************\
*                             *
* The "early" phase 1 routine *
*                             *
\*****************************/

void armul8extend_earlyph1(armul8_state *state)
{
  /* call any modules attrached to the core extension here */
  multiplier_earlyph1(state);

  return;
}

/****************************\
*                            *
* The "late" phase 1 routine *
*                            *
\****************************/

void armul8extend_lateph1(armul8_state *state)
{
  /* call any modules attrached to the core extension here */
  multiplier_lateph1(state);

  return;
}

/*****************************\
*                             *
* The "early" phase 2 routine *
*                             *
\*****************************/

void armul8extend_earlyph2(armul8_state *state)
{
  /* call any modules attrached to the core extension here */
  multiplier_earlyph2(state);

  return;
}

/****************************\
*                            *
* The "late" phase 2 routine *
*                            *
\****************************/

void armul8extend_lateph2(armul8_state *state)
{
  /* call any modules attrached to the core extension here */
  multiplier_lateph2(state);

  return;
}
