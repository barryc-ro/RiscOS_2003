#include "stdio.h"
#include "hwstate.h"

void EASIWrite(unsigned int addr,unsigned int val,unsigned int size)
{
  //fout ("Writing to EASI podule %d, address %x value %x\n",(addr >> 24)-8, addr,val);
}

unsigned int EASIRead(unsigned int addr,unsigned int size)
{
  //fout ("Reading EASI podule %d,address %x\n",(addr >> 24)-8);

  if (size) return 0xff;
  return 0xffffffffu;
}


