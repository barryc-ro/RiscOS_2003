-f ^.Sources...c.* Sources.*2.o.#0
#Makatic}

Libraries	=			\
			C:o.Stubs	\

!RunImage:	$(Objects) $(Libraries)
	link $(LinkExtra) -o $@ $(Objects) $(Libraries)

VPATH	= @.^

.SUFFIXES:	.o .c

.c.o:
	cc -c -fah -I,C: -depend !Depend -throwback $(CCExtra) -o $@ $<

