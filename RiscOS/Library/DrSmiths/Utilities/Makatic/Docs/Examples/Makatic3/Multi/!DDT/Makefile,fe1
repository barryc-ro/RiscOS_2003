# This makefile made by Makatic3


CCExtra=-g
LinkExtra=-d



ProjectName		=	!DDT

Objects	=	\
	Sources.o.Main

Libraries	=			\
			C:o.Stubs	\

!RunImage:	$(Objects) $(Libraries)
	link $(LinkExtra) -o $@ $(Objects) $(Libraries)

VPATH	= @.^

.SUFFIXES:	.o .c

.c.o:
	cc -c -fah -I,C: -depend !Depend -throwback $(CCExtra) -o $@ $<

# Dynamic dependencies:
Sources.o.Main:	^.Sources.c.Main
