# This makefile made by Makatic2


MemCheckCC=MemCheck.CC
CCExtra=-g
LinkExtra=-d


ObjectFiles	=		\
			o.Main	\


ProjectName		=	!MemCheck



Librarys	=					\
			MemCheck:o.MCFlex		\
			MemCheck:o.MC			\
			C:flexlib.o.flexlib		\
			C:o.Stubs			\

Target		=	!RunImage

ASMFlags	=	$(ASMExtra) -Stamp -NoCache -CloseExec -Quit -throwback
CCFlags		=	$(CCExtra) -fahi -depend !Depend -throwback -I,C:
CPPFlags	=	$(CPPExtra) -depend !Depend -throwback -I,C:
LinkFlags	=	$(LinkExtra)

ASM		=	objasm $(ASMFlags)
CC		=	$(MemCheckCC) cc -c $(CCFlags)
CPP		=	c++ -c $(CPPFlags)
LINK		=	Link -aif -c++ $(LinkFlags)



# Don't alter anything below this...

VPATH = @.^

.SUFFIXES:	.o .s .c .c++

$(Target):	$(ObjectFiles) $(Librarys)
	$(LINK) -o $@ $(ObjectFiles) $(Librarys)
	Settype $(Target) absolute

.s.o:
	$(ASM) -from $< -to $@

.c.o:
	$(CC) -o $@ $<

.c++.o:
	$(CPP) -o $@ $<


# Dynamic dependencies:
o.Main:	^.c.Main
o.Main:	MemCheck:h.MemCheck
o.Main:	MemCheck:ANSI.h.stdio
o.Main:	C:flexlib.h.flex
o.Main:	MemCheck:h.Flex
