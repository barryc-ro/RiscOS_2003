# This makefile made by Makatic3


CCExtra=-g
MemCheckCC=MemCheck.CC
LinkPrefix=MemCheck.DDTModLink



ProjectName		=	!Mod_MC

ObjectFiles	=	\
	o.Module\
	o.GetSL\


Librarys	=				\
			MemCheck:o.MC_M		\
			C:o.Stubs		\

Target		=	!RunImage



VPATH = @.^

.SUFFIXES:	.o .s .c .c++

$(Target):	$(ObjectFiles) $(Librarys) o.header
	$(LinkPrefix) link	-rmf $(LinkExtra) -o $@ $(ObjectFiles) $(Librarys) o.header

.s.o:
	objasm	$(ASMFlags) -from $< -to $@

.c.o:
	$(MemCheckCC) cc	-zm -c $(CCExtra) -fahi -depend !Depend -throwback -I,@.^,C: -o $@ $<

.cmhg.o:
	cmhg	$< $@

# Dynamic dependencies:
o.Module:	^.c.Module
o.Module:	C:h.stdlib
o.Module:	C:h.stdio
o.Module:	C:h.kernel
o.Module:	^.h.Module
o.Module:	C:h.kernel
o.Module:	MemCheck:h.MemCheck
o.Module:	C:h.stddef
o.Module:	C:h.stdio
o.Module:	C:h.stdarg
