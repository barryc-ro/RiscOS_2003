Librarys	=				\
			MemCheck:o.MC_M		\
			C:o.Stubs		\

Target		=	!RunImage



VPATH = @.^

.SUFFIXES:	.o .s .c .c++

$(Target):	$(ObjectFiles) $(Librarys) o.header
	$(LinkPrefix) link	-rmf $(LinkExtra) -o $@ $(ObjectFiles) $(Librarys) o.header

.s.o:
	objasm	$(ASMFlags) -from $< -to $@

.c.o:
	$(MemCheckCC) cc	-zm -c $(CCExtra) -fahi -depend !Depend -throwback -I,@.^,C: -o $@ $<

.cmhg.o:
	cmhg	$< $@

# Dynamic dependencies:
