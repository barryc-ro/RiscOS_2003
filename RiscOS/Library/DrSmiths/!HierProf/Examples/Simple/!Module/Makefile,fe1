# This makefile made by Makatic3


CCExtra=-DMODULE -DHierProf_PROFILE -DRUNTIME_PROFILING



ProjectName		=	!Module

Objects	=	\
	o.Main\
	o.CMHGHead\


Libraries	=	HierProf:o.HP_T1_M	\
			C:o.stubs		\

Target	= Module

All:	$(Target)

$(Target):	$(Objects) $(Libraries)
	Link $(LinkExtra) -rmf -o $@ $(Objects) $(Libraries)

VPATH = @.^

.SUFFIXES:	.o .cmhg .s .c .c++

.c.o:
	cc -c -zm -fah -depend !Depend -throwback $(CCExtra) -I,@.^.Sources,C: -o $@ $<

.cmhg.o:
	cmhg $< $@

.s.o:
	objasm -depend !Depend -throwback -IC:Desk.^ -PreDefine "Desk_AS_EMBED_FNNAMES SETA 0" -o $@ $<

# Dynamic dependencies:
o.Main:	^.c.Main
o.Main:	^.h.CMHGHead
o.Main:	C:h.kernel
o.Main:	HierProf:h.HierProf
