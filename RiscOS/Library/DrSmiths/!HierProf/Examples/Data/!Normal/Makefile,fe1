# This makefile made by Makatic2



ObjectFiles	=		\
			o.Main	\


ProjectName		=	!Normal



Librarys	=				\
			HierProf:o.Data.HPData	\
			C:o.Stubs		\

Target		=	!RunImage

ASMFlags	=	$(ASMExtra) -Stamp -NoCache -CloseExec -Quit -throwback
CCFlags		=	$(CCExtra) -fahi -depend !Depend -throwback -I,C:
CPPFlags	=	$(CPPExtra) -depend !Depend -throwback -I,C:
LinkFlags	=	$(LinkExtra)

ASM		=	objasm $(ASMFlags)
CC		=	cc -c $(CCFlags)
CPP		=	c++ -c $(CPPFlags)
LINK		=	Link -aif -c++ $(LinkFlags)



# Don't alter anything below this...

VPATH = @.^

.SUFFIXES:	.o .s .c .c++

$(Target):	$(ObjectFiles) $(Librarys)
	$(LINK) -o $@ $(ObjectFiles) $(Librarys)

.s.o:
	$(ASM) -from $< -to $@

.c.o:
	$(CC) -o $@ $<

.c++.o:
	$(CPP) -o $@ $<


# Dynamic dependencies:
o.Main:	^.c.Main
o.Main:	HierProf:h.Data
o.Main:	HierProf:h.HierProf
o.Main:	C:MemCheck.h.MemCheck
o.Main:	MemCheck:h.MemCheck
