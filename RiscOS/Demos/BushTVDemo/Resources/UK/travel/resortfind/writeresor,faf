<!--    This comment hides the script from non-scripting browsers
        browserName = navigator.appName;
    	browserVersion = parseInt( navigator.appVersion );
        
        function setBrowser() {
        if ( browserName == "Netscape" && browserVersion >= 3 ) {
                browserEquivalent = "ns3"; // Supports Netscape 3 features
                } else if ( browserName == "Netscape" && browserVersion == 2 ) {
                        browserEquivalent = "ns2"; // Supports Netscape 2 features
                } else if ( browserName == "Microsoft Internet Explorer" && browserVersion >= 2 ) {
                        browserEquivalent = "ie3"; // Supports Internet Explorer 3 features
                }
                return browserEquivalent;
        }
        
        //find object in a ways that compatible with all browsers
        function rtnObj(frm, nme) {
        var obj;
        for(i=0; i < frm.elements.length; i++) {
                if(frm.elements[i].name == nme) {
                        obj = frm.elements[i];
                        break;
                }
        }
        return obj
        }
        
        //check to see if item selected from a list
        function checkEntered(obj) {
        var ok = true;
        if(obj.options[obj.options.selectedIndex].value == "") {
                        ok = false;
                }
        return ok;
        }
        
        
        function validEmail(email) {
                invalidChars = " /:,;";
                if(email == "")
                        return false;

                for(i=0; i<invalidChars.length; i++)
                {
                        badChar = invalidChars.charAt(i);
                        if(email.indexOf(badChar,0) != -1)
                                return false;
                }
                
                atPos = email.indexOf("@",1);
                if(atPos == -1)
                        return false;

                if(email.indexOf("@",atPos+1) != -1) {
                        return false;
                }
                
                periodPos = email.indexOf(".",atPos);
                if(periodPos == -1)
                        return false;

                if(periodPos+3 > email.length)
                        return false;
                
                return true;
        }

        //validation form
        function validate(frm) {
                        
                var isOk = true; //set Ok by default
                var ver = setBrowser();
                var email = rtnObj(frm,"email").value;

                //basic check on name
                var name = rtnObj(frm,"name").value;
                if(isOk == true) {
                        if(name == "") {
                                alert("You must enter your name before you can continue.");
                                isOk=false;
                        }
                }
                
                //basic check on email
                if(isOk) {
                        if(!validEmail(email)) {
                                alert("You must enter a valid 'Email' address before you can continue.");
                                isOk=false;
                        }
                }

                //check summary
                if((rtnObj(frm,"summary").value == "" || rtnObj(frm,"summary").value == " ") && isOk == true) {
                        alert("You must enter a short summary in the box provided before you can continue.");
                        isOk = false;
                }
                
                //check review
                if((rtnObj(frm,"review").value == "" || rtnObj(frm,"review").value == " ") && isOk == true) {
                        alert("You must enter a review in the box provided before you can continue.");
                        isOk = false;
                }
                
                if((checkEntered(rtnObj(frm,"starRating")) == false) && isOk == true) {
                        alert("You must select a star rating for this restaurant");
                        isOk=false;
                }
                

                return isOk;
        }
//-->		
