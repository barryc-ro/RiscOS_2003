#define UNKNOWN_SWI_ERROR       0x840
#define STACK_OVERFLOW_ERROR    0x841
#define BAD_DBG_DATA            0x842
#define EXCEPTION_ERROR         0x843
#define DIV_ZERO_ERROR          0x844
#define READ_ADDRESS_ERROR      0x845
#define WRITE_ADDRESS_ERROR     0x846
#define CANT_DEBUG_ERROR        0x847
#define NULL_ENV_ERROR          0x848
#define MULT_DEBUG_ERROR        0x849
#define NO_FP_ERROR             0x84a
#define NO_COLOUR_ERROR         0x84b
#define NO_WRCHV_ROUTINE        0x84c
#define NO_BYTEV_ROUTINE        0x84d
#define NO_VECTOR_TABLE         0x84e
#define BAD_SPRITE              0x84f
#define OLD_FORMAT_WINDOW       0x850
#define NO_TEMPLATE             0x851
#define TOO_MANY_MENUS          0x852
#define DDT_IN_USE              0x853
#define DDT_HEAP_CORRUPT        0x854

