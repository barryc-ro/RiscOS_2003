#include "VersionNum"

tool_details_start
   name          "ToANSI";
   version       Module_MajorVersion;
   wimpslot      32k;
   has_extended_cmdline;
tool_details_end


metaoptions_start
   has_auto_run        on;
   has_auto_save     leafname from icn 3;
   has_text_window;
   has_summary_window;
metaoptions_end


fileoutput_start
   output_dft_is     produces_output;
fileoutput_end


dbox_start
   icons_start
      icn 3 maps_to string  not_saved;
   icons_end

   defaults
      icn 3 "";

   imports_start
      drag_to icn 3   inserts icn 3;
      drag_to any     inserts icn 3;
      drag_to iconbar inserts icn 3;
   imports_end
dbox_end

      






