/* Title:   MKinfo.c
 * Purpose: Display program info
 * Author:  IDJ
 * History: 5-Jul-90: IDJ: created
 *
 */

#include "dbox.h"

#include "MKinfo.h"

#include "VersionNum"

#define MKinfo_IName     1
#define MKinfo_IVersion  4

void MKinfo(void)
{
   dbox d;

   d = dbox_new("progInfo");
   if (d == 0) return;
   dbox_setfield(d, MKinfo_IVersion, Module_FullVersion);
   dbox_show(d);
   dbox_fillin(d);
   dbox_dispose(&d);
}
