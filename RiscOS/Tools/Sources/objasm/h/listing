extern void ListRegValue(CARDINAL reg);

extern void ListBoolValue(BOOLEAN b);

extern void PrintLineNumber(void);

extern void ListLineNumber(void);

extern void ListLine(void);

extern void PrintLine(void);

extern void ListWordValue(CARDINAL w);

extern void ListByteValue(char b);

