{
  /* STBot*/ 0,
  /* STTop*/ 0,
  /* Bra*/ 8,
  /* Ket*/ 0,
  /* LAnd*/ 1,
  /* LOr*/ 1,
  /* LEor*/ 1,
  /* Less*/ 2,
  /* OpEquals*/ 2,
  /* LessEquals*/ 2,
  /* Greater*/ 2,
  /* NotEquals*/ 2,
  /* GreaterEquals*/ 2,
