#include "VersionNum"

tool_details_start
   name          "ReadELF";
   version       Module_MajorVersion;
   wimpslot      192k;
   has_extended_cmdline;
tool_details_end


metaoptions_start
   has_auto_run;
   has_text_window;
   has_summary_window;
metaoptions_end


dbox_start
   icons_start
      icn 3  maps_to string  not_saved;
      icn 6  maps_to "-e";
      icn 9  maps_to "-s";
      icn 10 maps_to "-t";
      icn 11 maps_to "-S";
      icn 12 maps_to "-wliapr";
      icn 13 maps_to "-S";
      icn 14 maps_to "-r";
      icn 15 maps_to "-i";
      icn 16 maps_to "-c++";
   icons_end

   defaults
      icn 6  off,
      icn 9  on,
      icn 10 off,
      icn 11 off,
      icn 12 on,
      icn 13 on,
      icn 14 on,
      icn 15 on,
      icn 16 off;

   imports_start
      drag_to  icn 3   inserts icn 3 separator_is " ";
      drag_to  any     inserts icn 3 separator_is " ";
      drag_to  iconbar inserts icn 3;
   imports_end
dbox_end


deselections_start
   icn 6  deselects icn 9, icn 10, icn 11, icn 12, icn 13, icn 14, icn 15, icn 16;
   icn 9  deselects icn 6;
   icn 10 deselects icn 6;
   icn 11 deselects icn 6;
   icn 12 deselects icn 6;
   icn 13 deselects icn 6;
   icn 14 deselects icn 6;
   icn 15 deselects icn 6;
   icn 16 deselects icn 6;
deselections_end



